			IF CFLAG:MASTER:現在位置/100 == 8
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「えっと、ご苦労様…」
						PRINTFORML 椛の後輩らしき白狼天狗はオス臭い臭気に少し顔をしかめたが、
						PRINTFORML その発生源を察すると顔を真っ赤にして、挨拶もそこそこに去っていった…
						PRINTFORMW 「…うぅ」
					CASE 1
						PRINTFORML 「あの、お疲れ様です…」
						PRINTFORML おずおずとやってきた椛を見ると、同僚らしき白狼天狗はスンスンと鼻を鳴らした。
						PRINTFORML 仕事前くらい我慢しなさいよね、と軽くたしなめるように言うと、
						PRINTFORML 天狗はニヤニヤと笑みを浮かべながら椛の頭を小突いて去っていった…
						PRINTFORMW 「……はぁ」
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML （こんなに一杯になって…絶対同僚に気づかれちゃうよぉ…）
					CASE 1
						PRINTFORML （うう…あんまり歩くと、こぼれてきちゃいそう…）
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE RAND:3
				CASE 0
				PRINTFORML 「あ、もうこんな時間…」
				CASE 1
				PRINTFORML 「今日はどんなものが見つかりますかね…」
				CASE 2
				PRINTFORML 「さあ、頑張って見回りといきましょう！」
			ENDSELECT
		ENDIF
		PRINTFORML
	RETURN 1
	ENDIF
	;-------------------------------------------------
	;業務中
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF EX:膣内精液 + EX:アナル内精液 >= 100
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML （気づかれて…ない、よね？）
					PRINTFORML 椛はそわそわしながら周囲を見回している。
				CASE 1
					PRINTFORML （う…あんまり歩くと、こぼれてきちゃいそう…）
				CASE 2
					PRINTFORML \@ EX:膣内精液 >0 ? 胎 # 肛 \@内の精液が気になるのか、椛は仕事に身が入っていないようだ…
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 椛は凛とした表情で周囲を見回している。
				CASE 1
					PRINTFORML 刀を背負い、ぴんと背筋を伸ばした椛の姿は小柄ながらに威圧感を感じさせる。
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;仕事終了
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:3
			CASE 0
			PRINTFORML 「今日はここまでですね」
			CASE 1
			PRINTFORML 「交代の時間ですね」
			CASE 2
			PRINTFORML 「今日も無事、お勤め終了ですっ」
		ENDSELECT
	RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

