;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K19_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORML
		;ELSEIF TALENT:恋慕
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「あーあ。もう終わり？」
					PRINTFORMW 「また誘いなさいよ。絶対だからね」
				CASE 0
					PRINTFORML 「今日行ったところ、お姉ちゃんも気に入りそうね」
					PRINTFORMW 「教えてくれてありがとう%CALLNAME:MASTER%。バイバイ」
				CASEELSE
					PRINTFORML 「つまらなくは…なかったわ。でも勘違いしないでよ」
					PRINTFORMW 「今日はアタシの気まぐれで付き合ってあげただけなんだからね！」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「もう用は済んだでしょ？それじゃ」
					PRINTFORMW 
				CASE 0
					PRINTFORML 「リリーホワイトが心配してるから。早く戻らないと…」
					PRINTFORMW 
				CASEELSE
					PRINTFORML 「あぁ疲れた。早く家に帰って寝なくちゃ」
					PRINTFORMW 
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「…とりあえず一回抓らせて」
		PRINTFORML %CALLNAME:MASTER%の手の甲がギュウゥと捩じられる…。
		PRINTFORML 「ふぅスッキリした」
		PRINTFORMW 「これに懲りたらもう二度としないことね。わかった？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「…とりあえず一発はたかせて」
		PRINTFORML %CALLNAME:MASTER%の頬に軽やかな平手打ちが叩きこまれる…。
		PRINTFORML 「…うん、まだまだ気分が晴れないわ」
		PRINTFORMW 「あと百回はしないと気が済まないけど、今日のデートに免じて勘弁してあげるわ」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「…とりあえず一発ぶん殴らせて」
		PRINTFORML %CALLNAME:MASTER%の腹部に重い拳がミシリとめり込む…。
		PRINTFORML 「…うん、全然スッキリしないわ」
		PRINTFORMW 「あの時のことを謝る気があるならこれ以上の痛みぐらいを受け入れなさいよ。わかった？」
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

