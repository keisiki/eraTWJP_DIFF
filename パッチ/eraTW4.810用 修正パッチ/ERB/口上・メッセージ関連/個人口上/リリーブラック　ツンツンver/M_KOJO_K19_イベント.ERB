;==================================================
@M_KOJO_EVENT_K19_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF FLAG:時間停止
		ELSEIF CFLAG:318 == 1
			PRINTFORML  
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				;CASE 1
					;PRINTFORML 
					;PRINTFORMW 
				CASE 0
					PRINTFORML 「フン、%CALLNAME:MASTER%が来ても別に嬉しくともなんともないわ！」
					PRINTFORMW …そっぽむいてるリリーの羽根は、うれしそうに羽ばたいている。
				CASEELSE
					PRINTFORML 「ちちちちょっと！心の準備してないんだからいきなり現れないで！」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「ん、暇つぶしにはなりそう」
					PRINTFORMW 
				CASE 0
					PRINTFORML 「忙しそうね。ま、アタシには関係ないけど」
					PRINTFORMW 
				CASEELSE
					PRINTFORML 「%CALLNAME:MASTER%じゃない。暇なら付き合いなさいよ」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「ん？どこかで会った顔ね」
				CASE 0
					PRINTFORML リリーは警戒した瞳で%CALLNAME:MASTER%を一瞥した。
					PRINTFORML 「…」
				CASEELSE
					PRINTFORML 「…何か用？」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;リリーブラックの部屋に遭遇
		IF CFLAG:19:現在位置 == 709
			IF CFLAG:318 == 1
				PRINTFORML
			ELSEIF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 1
						;PRINTFORML 
						;PRINTFORMW 
					CASE 0
						PRINTFORML 「お、お茶ぐらいならだしてあげないこともないわ」
						PRINTFORML 「ぐうぜんおいしいお饅頭も残ってたし。そう、ぐうぜんに！」
					CASEELSE
						PRINTFORML 「会いに来る時は事前に連絡しなさいよぉ！」
						PRINTFORML 「%CALLNAME:MASTER%が良くてもアタシが色々準備しておきたいの！」
				ENDSELECT
			ELSEIF TALENT:思慕
				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「ここに来ても面白いものなんてないわよ」
					PRINTFORML 
				CASE 0
					PRINTFORML 「お姉ちゃんに会いに来たの？それともアタシ？」
				CASEELSE
					PRINTFORML 「ん、何か用？」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「よくアタシ達の住処だってわかったわね」
						PRINTFORML 「結構わかりにくくしたつもりなんだけどな」
					CASE 0
						PRINTFORML 「持て成すつもり何てないからね」
					CASEELSE
						PRINTFORML 「人の部屋に入るならノックぐらいしたらどう？」
				ENDSELECT
			ENDIF
		ENDIF
		IF FLAG:時間停止
		ELSEIF CFLAG:318 == 1
			PRINTFORML  
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML リリーはパァッと笑顔になったと思うとすぐムスッとした表情になってしまった。
					PRINTFORML 「こ、これはただの思い出し笑いだからね！」
				CASE 0
					PRINTFORML (あ、アタシの春が来てくれた）
					PRINTFORML 
				CASEELSE
					PRINTFORML  (…も、もしかしてアタシ見つけに来てくれたとか？)
					PRINTFORML 
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「ん、遊びにでも誘いに来た？」
				CASE 0
					PRINTFORML 「何かこう面白い事でも起きないかしら…」
				CASEELSE
					PRINTFORML 「あれ、%CALLNAME:MASTER%じゃない」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「妖精は好き勝手そこらへんにいるもんよ」
				CASE 0
					PRINTFORML 「んー、ここにもいないか」
				CASEELSE
					PRINTFORML 「何よ。アタシがいるのがそんなに気になるの？」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	ENDIF
