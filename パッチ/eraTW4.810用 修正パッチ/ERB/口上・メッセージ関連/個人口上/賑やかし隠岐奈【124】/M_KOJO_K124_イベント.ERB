;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K124_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORML 「はぁ…はぁ……ふぅぅ………」
		PRINTFORMW 「私としたことが、少しはしゃぎすぎたな、ふふっ」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORML 「%CALLNAME:MASTER%……」
		PRINTFORMW 隠岐奈は最後に優しく口づけ、%CALLNAME:MASTER%を解放した。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORML 「私はもう満足なんだが……まあ、お望みなら付き合ってやる」
		PRINTFORMW 隠岐奈は困った顔をしつつも笑みを浮かべた。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		PRINTFORML 「…ほう？そう言うのなら、お前が壊れるまでしてやろう」
		PRINTFORMW 隠岐奈は嗜虐心を煽られ、目を欲望にぎらつかせながら笑った。
		RETURN 1
	ENDIF
ENDIF
RETURN 1


