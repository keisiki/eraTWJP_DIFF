;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K124_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 
		IF CFLAG:TARGET:睡眠
			PRINTFORML 「すや………」
			PRINTFORML 隠岐奈は全く威厳のない、気の抜けた寝顔を晒している…。
		ELSEIF TALENT:恋人
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「ああ、%CALLNAME:MASTER%か」
				PRINTFORML 「……どうした？こっちに来ないのか？」
				PRINTFORMW 隠岐奈は早く来いとばかりに手招きしている。
			CASE 1
				PRINTFORML 「あっ、%CALLNAME:MASTER%！」
				PRINTFORML 隠岐奈は%CALLNAME:MASTER%の顔を見るや否や、ぱっと顔を輝かせた。
				PRINTFORMW 「ほら、早く来い！ 私とお喋りでもしようじゃないか！」
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「あ…あぁ、%CALLNAME:MASTER%か」
				PRINTFORML 「まあ、そろそろ来るのではないかとは思っていたが……」
				PRINTFORMW 隠岐奈は嬉しさを隠しきれずにニヤついてしまっている。
			CASE 1
				PRINTFORML 「えっ、あっ、%CALLNAME:MASTER%！？」
				PRINTFORML 「い、いや、何でもない！別に丁度お前のことを考えていたとか、そういうわけではないぞ！」
				PRINTFORMW 隠岐奈は頬を染め、あわあわと挙動不審になっている…。
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「おっ、%CALLNAME:MASTER%だ」
				PRINTFORML 「もしかして、私に会いに来たのかなー？」
				PRINTFORMW 隠岐奈は茶化すような口調でそう言いつつ、%CALLNAME:MASTER%に微笑みかけた。
			CASE 1
				PRINTFORML 「お前か。暇なら私が付き合ってやらなくもないぞ？」
				PRINTFORMW 隠岐奈はニヤニヤしながら%CALLNAME:MASTER%を見つめている。
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「ああ、%CALLNAME:MASTER%か」
				PRINTFORMW 隠岐奈は相変わらず偉そうだ。
			CASE 1
				PRINTFORML 「なんだ、お前か」
				PRINTFORML 「……能力を濫用するでないぞ」
				PRINTFORMW 隠岐奈はひっそりと、しかし威圧的な声調で%CALLNAME:MASTER%に警告した。
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「おーい、%CALLNAME:MASTER%」
				PRINTFORML 急に背後から声をかけられた。
				PRINTFORMW 「私だ、隠岐奈だよ……何だ、そのとぼけた顔は？」
			CASE 1
				PRINTFORML 肩をつつかれ振り向くと、そこには嬉しそうに微笑む隠岐奈がいた。
				PRINTFORML 「%CALLNAME:MASTER%、私が来たぞ♪ もちろん、構ってくれるな？」
				PRINTFORMW 隠岐奈の金色の目は期待に満ち満ちている…。
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「%CALLNAME:MASTER%」
				PRINTFORML いきなり背後から声をかけられた！ ……隠岐奈だ。
				PRINTFORMW 「ん、驚かせたか？ふふっ！お前も案外ビビりだな」
			CASE 1
				PRINTFORML 「あ……%CALLNAME:MASTER%……」
				PRINTFORML 「ご、ごきげんよう。まさかここで会うとはな…」
				PRINTFORMW %CALLNAME:MASTER%の顔を見て、隠岐奈はほんのりと頬を染めている。
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「邪魔するぞー」
				PRINTFORMW いつの間にか隠岐奈が%CALLNAME:MASTER%の背後に立っていた。
			CASE 1
				PRINTFORML 「お前の背中の扉、なかなかに使いやすいな」
				PRINTFORMW 背後の声に驚き振り向くと、隠岐奈が不敵な笑みを浮かべていた…。
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORML 「おっと！なーんだ、%CALLNAME:MASTER%かぁ」
