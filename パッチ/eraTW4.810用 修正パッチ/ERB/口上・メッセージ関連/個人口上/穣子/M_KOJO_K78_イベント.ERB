;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K78_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	CALL K78_HAIR_GROWTH,78
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF K78_CONSCIOUS_AND_TOLERANT()
			PRINTFORMW 「あ、%CALLNAME:MASTER%が入ってたんだ。ね、一緒に入っていい？」
		ENDIF
		SIF SHIRAHU(TARGET)
			TCVAR:78:352 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TCVAR:353 != ARG:1 && K78_CONSCIOUS_AND_TOLERANT()
			;静葉とデート
			IF ARG:1 == 77 && FLAG:なりきり != 77
				IF TALENT:恋人 || ( TALENT:恋慕 && !TALENT:77:恋人 )
					PRINTFORML 「う～　姉さんっ、べたべたしすぎっ」
					PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:77%への対抗心をあらわにして突っかかった。
				ELSE
					PRINTFORMW 「姉さんとお出かけ？ 楽しんできてね」
				ENDIF
			;さっきと違う相手とデート
			ELSEIF TCVAR:353 != 0
				IF TALENT:恋慕 || TALENT:恋人
					PRINTFORML 「また女をとっかえひっかえして…」
					PRINTFORMDW %CALLNAME:TARGET%ににらまれてしまった。
				ELSE
					PRINTFORML 「なんでデートの相手がさっきと変わってるのよ…」
					PRINTFORMDW %CALLNAME:TARGET%に呆れられた。
				ENDIF
			ELSE
				IF TALENT:恋慕 || TALENT:恋人
					PRINTFORML 「ずいぶん仲よさそうね……」
					PRINTFORMDW 女連れの%CALLNAME:MASTER%を見た%CALLNAME:TARGET%はおもしろくなさそうな様子だ。
				ELSEIF TALENT:思慕
					PRINTFORML 「ふーん。%CALLNAME:MASTER%って顔ひろいんだ…」
					PRINTFORMDW %CALLNAME:TARGET%は素っ気ない様子で%CALLNAME:MASTER%と%CALLNAME:(ARG:1)%を一瞥した。
				ELSE
					PRINTFORM 「あ、%CALLNAME:MASTER%。
					SELECTCASE TIME:0
					CASE IS <600
						PRINTFORMW おはよう」
					CASE 600 TO 1139
						PRINTFORMW こんにちは」
					CASE IS >= 1140
						PRINTFORMW こんばんは」
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
		IF SHIRAHU(TARGET)
			TCVAR:78:352 = 1
			TCVAR:78:353 = ARG:1
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1


