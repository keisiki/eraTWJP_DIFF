;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K78_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	SIF SHIRAHU(TARGET)
		TCVAR:78:352 = 1
	CALL K78_HAIR_GROWTH,78
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF K78_CONSCIOUS_AND_TOLERANT()
				IF TALENT:恋慕 || TALENT:愛欲
					CALL K78_HPH_PRINT,@"「ごめん、起こしちゃったかな。……なんだかあなたに会いたくなってHPH」","W"
				ELSEIF TALENT:思慕 || TALENT:セフレ
					PRINTFORML 「あ、おはよう」
					PRINTFORMDL なぜ%CALLNAME:TARGET%がいるのだろうか。
					PRINTFORMW 「な、なんとなく立ち寄ってみただけよ」
				ELSE
					PRINTFORMW 「起きた？ ちょっと物を借りるわよ」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF K78_CONSCIOUS_AND_TOLERANT()
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「おはよう%CALLNAME:MASTER%。さて、今日は畑の手入れでもしようかしら」
			CASE 2
				PRINTFORMDL 目を覚ますと%CALLNAME:MASTER%の目の前に%CALLNAME:TARGET%の顔があった。
				CALL K78_HPH_PRINT,@"「えへへ♪ あなたの寝顔がかわいくて見てたのHPH」","W"
			CASEELSE
				;TARGETと一緒に起きた
				IF TIME:0 == CFLAG:起床時間
					PRINTFORMW 「おはよう。今日は何しよっか」
				;TARGETより遅く起きた
				ELSE
					PRINTFORMW 「やっと起きたの？ お寝坊さんね」
				ENDIF
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

