		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「……浮気したら許しませんよ？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「では、またの機会にしますかね」
			ELSEIF TALENT:思慕
				PRINTFORMW 「残念です、また暇になったら誘いますよ？」
			ELSE
				PRINTFORMW 「そうですか、ではまたの機会に」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「暇なら私の部屋に来ませんか？今なら誰もいませんよ」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「私の部屋でゆっくりしませんか？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「私の部屋に来てみます？」
			ELSE
				PRINTFORMW 「暇なら私の部屋に来ませんか？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「では、楽しいことをしましょうね%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「では、寛いでくださいね%UNICODE(0x2665) *1%」
			ELSEIF TALENT:思慕
				PRINTFORMW 「お茶ぐらいは出しますよ、安心してください」
			ELSE
				PRINTFORMW 「何も企んでなどいないですから、安心してくださいね」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「……まさか浮気とかではありませんよね？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「何か警戒してますか？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「では、また今度」
			ELSE
				PRINTFORMW 「そうですか、残念ですね」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

;==================================================
;脱衣口上
;==================================================
@M_KOJO_EVENT_K130_28(ARG,ARG:1)
;口上主はTARGET
;ARGに脱がし方選択
;ARG:1（ARG=3の場合のみあり）地の文の反応
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;全裸にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「あまりジロジロ見ないでください」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「これでいいのですか？」
				ELSE
					PRINTFORMW 「脱げと言われて脱ぐのも恥ずかしいですね」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;下着姿にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「……恥ずかしいですね」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「可愛い下着とかでもないのですが……」
				ELSE
					PRINTFORMW 「あまり見ないでください」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;半脱ぎ
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「こちらの方が興奮しますか？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「服を汚したくないのですが」
				ELSE
					PRINTFORMW 「こういうのも悪くないかもしれませんね」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;パンツちょうだい
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
				;-------------------------------------------------
				CASE 0
				;交換です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「%CALLNAME:MASTER%のパンツを代わりにくれるのなら構いませんよ？」
						CALL ASK_YN("はい","はい")
						IF !RESULT
						ELSE
						ENDIF
						PRINTFORMW 「では交換しましょう」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「……では交換といきましょう」
						CALL ASK_YN("はい","はい")
						IF !RESULT
						ELSE
						ENDIF
					ELSE
						PRINTFORMW 「あげるだけというのは嫌ですね、交換しましょうか」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 1
				;OK（嬉々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「これで満足ですか？」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「はい、どうぞ」
					ELSE
						PRINTFORMW 「感謝しなさい、私があげるんですからね」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「まあ、構いませんが」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「ええ……構いませんけども」
					ELSE
						PRINTFORMW 「何に使う気ですか……全く」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「嫌ですよ」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「駄目ですね」
					ELSE
						PRINTFORMW 「気味の悪い趣味ですね」
					ENDIF
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

