			ELSE
				PRINTFORMW 「何のつもりですか全く……」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF

;==================================================
;相手からデートのお誘い
;==================================================
@M_KOJO_EVENT_K130_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「%CALLNAME:MASTER%、デートしませんか？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%、デートしてくれませんか？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、暇なら付き合ってもらえませんか？」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、私に付き合ってもらえませんか？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
