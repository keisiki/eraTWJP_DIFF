	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORMW 「もう…無理ぃ…%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
