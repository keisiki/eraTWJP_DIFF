	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;自室うふふ中
		IF CFLAG:[[キスメ]]:現在位置 == OMANEKIBEYA()
			IF TALENT:[[キスメ]]:恋人
