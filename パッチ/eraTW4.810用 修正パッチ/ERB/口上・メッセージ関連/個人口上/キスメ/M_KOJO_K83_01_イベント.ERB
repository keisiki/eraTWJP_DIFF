				ELSEIF TALENT:[[キスメ]]:思慕
					CALL HPH_PRINT(@"「……HPH」", "W")
				ELSE
					PRINTFORMW 「…………\/\/\/」
				ENDIF
				RETURN 1
			ENDIF
	ENDSELECT
ENDIF

