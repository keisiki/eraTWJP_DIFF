;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K85_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORML 「初めて挨拶したときは、こんな風に二人きりで遊びに行くとは思わなかったな…」
		PRINTFORMW 帰り道、並んで帰路を歩く中、ポツリと一輪が呟いた。
		PRINTFORMW 「私ね、最初は%CALLNAME:MASTER%のことを男としてみることは絶対に無いなって、思ってた」
		PRINTFORMW 「でも、一緒にいると、本当に楽しくて、嬉しくて…」
		PRINTFORMW 照れくさそうに少し俯き、再び顔を上げ
		PRINTFORMW 薄らと朱色に染まった顔で、その言葉を口にする――
		PRINTFORMW 「私は……」
		PRINTFORMW 「私は、%CALLNAME:MASTER%の事が好き」
		PRINTFORMW 「私と、付き合ってくれませんか？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 
		PRINTFORMW こちらも同じ想いであることを告げると、一輪は目に薄らと涙を浮かべる
		PRINTFORML 
		PRINTFORMW 「本当に……？」
		PRINTFORMW 「本当に、私で良いの？」
		PRINTFORML 
		PRINTFORMW まだ信じられないといった顔で聞き返す一輪に、%CALLNAME:MASTER%ははっきりと頷く
		PRINTFORMW 感極まった一輪は、思わず%CALLNAME:MASTER%に抱き着いてきた
		PRINTFORMW しばらくの抱擁の後、%CALLNAME:MASTER%は一輪が常にかぶっている頭巾に手をかけた
		PRINTFORMW 「あっ…//」
		PRINTFORMW 風に触れ、少し乱れた髪を撫でる。腕の中の一輪は、普段とは違って花のようにしおらしい。
		PRINTFORMW 「%CALLNAME:MASTER%、大好きっ…//」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
				PRINTFORML
		PRINTFORMW 「……」
		PRINTFORMW 「……あー、そっか…」
		PRINTFORMW 「私、ちょっと浮かれていたみたいね」
		PRINTFORMW どうにか平静を取り繕う一輪だが、明らかに落胆の様子が見て取れる
		PRINTFORMW 「今日は楽しかったわ、本当に」
		PRINTFORMW 「また、一緒に遊んでくれると嬉しいわ」
		PRINTFORMW 「あと、……その」
		PRINTFORMW 「いつか、絶対に振り向かせて見せるから」
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K85_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORML
		IF TALENT:恋人
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……あはは、やりすぎだって♪」
				PRINTFORML 他愛無い会話をする。
				PRINTFORML 手を繋ぎ、共に歩く。それだけで幸せな気持ちが湧いてくる
				PRINTFORML 「えへへ……私今、%CALLNAME:MASTER%と居て幸せ…」
				PRINTFORMW 同じ方向を向き、二人で寄り添いあいながら、ゆっくりと帰路についた
			CASE 1
				PRINTFORML 「うーん、次のデートはどこにしようか？」
				PRINTFORML 「私あそこに行ってみたいなー」
				PRINTFORML 帰路に着く二人は、早速次のデートの相談をしている
				PRINTFORML 「ああでも、あの景色もまた%CALLNAME:MASTER%と一緒に見たいな……なんて」
				PRINTFORMW 二人の帰り道に、話題と想いは尽きることなく続いていった……
			CASEELSE
				PRINTFORML 「ふふっ、やっぱり%CALLNAME:MASTER%と一緒にいるとあっという間ね」
				PRINTFORML 「ただ、楽しい時間は一瞬で過ぎ去っちゃうものだけど」
				PRINTFORML 「その一瞬がずっと続くなら、何も問題ないよね、%CALLNAME:MASTER%♪」
				PRINTFORML そういって一輪はぎゅっ、と%CALLNAME:MASTER%に抱き付いてきた
				PRINTFORML 歩き辛いと抗議しても無駄だと悟った%CALLNAME:MASTER%は、
				PRINTFORMW 一輪のぬくもりを感じながらのんびりと家路に着いた……
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 帰り道を歩くあなたと一輪は、今日のデートを振り返っている
				PRINTFORML 「いやあ、楽しかったねー♪」
				PRINTFORML 「そうだ、今度はあそこ行こうよ、あそこ！」
				PRINTFORML 「私のおススメを紹介してあげるね。ふっふっふ」
				PRINTFORML 抱き寄せられた腕に当たる胸の感触を楽しみつつ、
				PRINTFORMW 談笑しながら帰路へとついた――
			CASE 1
				PRINTFORML 「あ～あ、もうデートは終了か～……」
				PRINTFORML 「……%CALLNAME:MASTER%は楽しかった？」
				PRINTFORML %CALLNAME:MASTER%が頷くと、一輪は嬉しそうに微笑んだ
				PRINTFORML 「良かったぁっ、それじゃあ、また続きをしないと、ね♪」
				PRINTFORMW 次の逢瀬に想いを馳せながら、二人は帰り道を歩いて行った……
			CASEELSE
				PRINTFORML 「私、この帰りの時間ってのも結構好きなんだ～」
				PRINTFORML 「なんというか、そこまで含めてデートだなぁ、って……」
				PRINTFORML そういう一輪の横顔には、穏やかな充足感があった
				PRINTFORMW （%CALLNAME:MASTER%も同じように感じてくれてたら、嬉しいな……）
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「あー、もう帰る時間か……」
				PRINTFORML 「中々楽しい時間でした♪　次もよろしくね～」
				PRINTFORMW そう言って手を振りながら、朗らかに一輪は帰っていった……
			CASEELSE
				PRINTFORML 「いや～、やっぱり誰かと二人で出歩くのも良いもんだねー」
				PRINTFORML 「%CALLNAME:MASTER%、今日はありがとう」
				PRINTFORMW 「では、さようならー♪」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「じゃあ、そろそろ帰りますか」
				PRINTFORML 「良い気分転換になりました。機会があればまた誘ってください」
				PRINTFORMW 一輪は一礼すると、小さく手を振りながら帰っていった
			CASEELSE
				PRINTFORML 「っと、今日はこれで解散ですか」
				PRINTFORMW 「気を付けて帰ってくださいね。それでは」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

