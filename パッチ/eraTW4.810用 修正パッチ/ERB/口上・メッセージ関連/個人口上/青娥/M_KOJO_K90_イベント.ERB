			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「いや～ん%UNICODE(0x2665) *1%こんなあられもない姿にされてしまいましたわ%UNICODE(0x2665) *1%」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「あ～れ～♪」
				ELSE
					PRINTFORMW 「あーん%UNICODE(0x2665) *1%恥ずかしいですわ%UNICODE(0x2665) *1%」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;パンツちょうだい
		;記入チェック（=0, 非表示、1, 表示）
