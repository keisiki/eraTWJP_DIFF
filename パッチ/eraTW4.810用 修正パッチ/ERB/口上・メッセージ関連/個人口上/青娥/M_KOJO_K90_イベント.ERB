;==================================================
;相手からデートのお誘い
;==================================================
@M_KOJO_EVENT_K90_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「ねえ、デートしてくれないかしら、%CALLNAME:MASTER%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ねえ、%CALLNAME:MASTER%が良ければデートしてくださらない」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、今お暇でしたら…」
				PRINTFORMW 「ちょっと付き合ってほしいのだけれど…決して怪しいことをするわけではありませんわ」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、ちょっと付き合ってくださらない」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「ありがとう、さぁ、行きましょう%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ふふっ…ありがと」
			ELSEIF TALENT:思慕
				PRINTFORMW 「では参りましょう」
			ELSE
				PRINTFORMW 「お礼になにか奢ってあげますわ」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「残念、また誘うからその時はお願いしますわ%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「残念ですわね…」
			ELSEIF TALENT:思慕
				PRINTFORMW 「むぅ…残念ですわ」
			ELSE
				PRINTFORMW 「ケチ臭いですわねー」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「%CALLNAME:MASTER%、私の家に来ないかしら？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%、私の家に来ませんこと？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、お暇でしたら私の家に来ません？」
			ELSE
				PRINTFORMW 「ねえ、もしかして暇だったりしません？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「今日は一日ゆったりとしていきましょう%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「あまりじろじろ見ないでくださいまし…変なものがありますので」
			ELSEIF TALENT:思慕
				PRINTFORMW 「…面白いものが見れるといいですわね♪」
			ELSE
				PRINTFORMW 「ふふっ後悔しても遅いですわよ%UNICODE(0x2665) *1%」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「では、また今度ということで…約束ですわよ？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「あら？残念ですわ…お茶も用意していたのに…」
			ELSEIF TALENT:思慕
				PRINTFORMW 「むぅ…折角の機会だと思いましたのに…」
			ELSE
				PRINTFORMW 「あら？意外と身持ちが固いのですわね%UNICODE(0x2665) *1%」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

;==================================================
;脱衣口上
;==================================================
@M_KOJO_EVENT_K90_28(ARG,ARG:1)
;口上主はTARGET
;ARGに脱がし方選択
;ARG:1（ARG=3の場合のみあり）地の文の反応
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
