;==================================================
@M_KOJO_EVENT_K53_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;その日初めての遭遇
		IF TCVAR:53:350 == 0
			K53_合計絶頂数 = 0
			TCVAR:53:350 = 1
		ENDIF
		;発情中
		IF IsHeat(TARGET)
			PRINTFORML てゐは顔を赤らめて太腿を擦り合わせ、もじもじとしている…
			PRINTFORMW 「……んんっ……ふ…ふぅ………、………！……な、なに？」
		;欲求不満
		ELSEIF CFLAG:溜まってる度 >= 800
			IF ABL:自慰中毒 >= 4 && GET_TARGETNUM() == 1
				PRINTFORML %CALLNAME:MASTER%がてゐを視認するとバッと%CALLNAME:MASTER%の方を向き、股間から手を素早く離したように見えた
				PRINTFORMW 「な、なに！？………きゅ、急に入ってこないでよね………！」
			ELSE
				PRINTFORMW てゐはそわそわした様子で%CALLNAME:MASTER%の事をちらちらと気にかけている
			ENDIF
		ELSE
			;機嫌良い
			IF TALENT:機嫌 == 1	
				;販促
			;機嫌普通
			ELSEIF TALENT:機嫌 == 0
				;
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「ふふーん、今日はどんなイタズラをしよっかな～」
						PRINTFORMW 「げ、%CALLNAME:MASTER%！…べ、べつに何も企んでないウサよ？」
					CASE 1
						PRINTFORML 「ここにこれを仕掛けて～………………あっ」
						PRINTFORMW 「……………な、なにウサ？別に何もしてないウサ」
				ENDSELECT
			;機嫌悪い
			ELSEIF TALENT:機嫌 == -1
				;無視
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「………………………」
						PRINTFORMW 明らかにてゐと目が合ったがこちらを一瞥すると、あなたに関心を示すことなくそっぽを向いた
					CASE 1
						PRINTFORML 「ん？………ああなんだ%CALLNAME:MASTER%か…」
						PRINTFORMW てゐは%CALLNAME:MASTER%を一瞥するとそっぽを向いた
				ENDSELECT
			
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;その日初めての遭遇
		IF TCVAR:53:351 == 0
			K53_合計絶頂数 = 0
			TCVAR:53:351 = 1
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;仲が進んでおらず、既成事実が1未満か、恋慕が付いていない異性の場合に出ていく（他でも共通の風呂条件）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;異性で弱みを握られている（パンツ発見or情事を見られた）場合に発生
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「おっふろ～おっふろ～♪………あら？」
		PRINTFORML 「ん、ちょっと%CALLNAME:MASTER%！何私より先にお風呂はいってんの！」
		PRINTFORMW 「ほら私がお風呂入りたいって言ってるんだから、出てって出てって！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

