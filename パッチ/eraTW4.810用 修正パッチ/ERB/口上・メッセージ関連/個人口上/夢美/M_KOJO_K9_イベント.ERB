			LOCAL:1 = 1

			IF LOCAL:1
				;時間停止時
				IF FLAG:70
				;通常
				ELSE
					PRINTFORML 「あまり意味は感じないけど……」
					PRINTFORML 「全部脱ぐよりなんだか背徳的で……その、興奮するわね」
				ENDIF
				RETURN 1
			ENDIF
		;-------------------------------------------------
		;パンツちょうだい
		;-------------------------------------------------
		CASE 3
			;記入チェック（=0, 非表示、1, 表示）
			LOCAL:1 = 1

			IF LOCAL:1
				SELECTCASE ARG:1
					;-------------------------------------------------
					;交換です
					;-------------------------------------------------
					CASE 0
						;記入チェック（=0, 非表示、1, 表示）
						LOCAL:1 = 1

						IF LOCAL:1
							PRINTFORML 「いいけど、私も%CALLNAME:MASTER%のが欲しいわ」
							PRINTFORML 「だから、交換しましょ？」
							RETURN 1
						ENDIF
					;-------------------------------------------------
					;OK（嬉々
					;-------------------------------------------------
					CASE 1
						;記入チェック（=0, 非表示、1, 表示）
						LOCAL:1 = 1

						IF LOCAL:1
							PRINTFORML 「え？私のぱんつが欲しいの？いいわよ%UNICODE(0x2764)%」
							PRINTFORMDW そう言って夢美は目の前で%SHOW_BOTTOM(TARGET,1)%をたくし上げて下着を脱ぎ下ろした。
							PRINTFORMDW 夢美が下着を脱ぐ最中、露わになった秘部が見えた。年相応に陰毛が生え揃っている……
							PRINTFORMDW %CALLNAME:MASTER%が夢美の秘部に集中していると、目の前に脱ぎたての夢美の下着が差し出された。
							PRINTFORMDW ほんのり温かく、秘部が当たっていた箇所から漂う芳香……正直たまらん。
							PRINTFORML 「大事に使ってね、%CALLNAME:MASTER%%UNICODE(0x2764)%」
							RETURN 1
						ENDIF
					;-------------------------------------------------
					;OK（渋々
					;-------------------------------------------------
					CASE 2
						;記入チェック（=0, 非表示、1, 表示）
						LOCAL:1 = 1

						IF LOCAL:1
							PRINTFORML 「はぁ……ぱんつが欲しい……はぁ……えぇ……嫌なんだけど……」
							PRINTFORMDW そこをなんとか！人助けと思ってひとつ！
							PRINTFORML 「はぁ～……仕方ないわね……あげるわよ、ぱんつ」
							PRINTFORMDW そう言って夢美は目の前で%SHOW_BOTTOM(TARGET,1)%でぱんつを隠しながら脱ぎ下ろした。
							PRINTFORMDW 夢美が下着を脱いでいる間、なんとか%SHOW_BOTTOM(TARGET,1)%の中を覗けないかと思ったが、むだだった……。
							RETURN 1
						ENDIF
					;-------------------------------------------------
					;嫌です
					;-------------------------------------------------
					CASE 3
						;記入チェック（=0, 非表示、1, 表示）
						LOCAL:1 = 1

						IF LOCAL:1
							PRINTFORML 「パンツが欲しい？ダメに決まってるでしょうが。脳膿んでんじゃない？」
							RETURN 1
						ENDIF
						RETURN 1
				ENDSELECT
			ENDIF
	ENDSELECT
ENDIF
RETURN 1

