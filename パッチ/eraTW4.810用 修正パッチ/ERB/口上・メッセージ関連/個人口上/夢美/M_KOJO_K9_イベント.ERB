@M_KOJO_EVENT_K9_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;追い出される以外の遭遇を統合
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (ARG == 1 || ARG == 2 || ARG == 3 || ARG == 6 || ARG == 7)
		CALL M_KOJO_EVENT_K9_1_NOT_OIDASHI(ARG)
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「先客が居たのね……失礼したわ」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「今から私が入るから、あんたは出て行きなさい」
		PRINTFORMDW そんな横暴な……！%CALLNAME:MASTER%はすごすごと風呂を後にした
		RETURN 1
	ENDIF
ENDIF
RETURN 1

