	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「貧乏神が会いに来たよー」
			PRINTFORMW 「私の%CALLNAME:MASTER%は今日も素敵に元気だね」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「貧乏神が会いに来たよー」
			PRINTFORMW 「元気そうで何より…なんてね」
		ELSEIF TALENT:思慕
			PRINTFORMW 「貧乏神が会いに来たよー」
			PRINTFORMW 「…まだ元気そうだね」
		ELSE
			PRINTFORMW 「貧乏神が会いに来たよー」
			PRINTFORMW 「あんたは近い内に終わりだね」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
