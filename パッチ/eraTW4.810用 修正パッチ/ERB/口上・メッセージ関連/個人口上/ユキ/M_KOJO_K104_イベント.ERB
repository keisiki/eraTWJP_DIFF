	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
			PRINTFORML 「やったっ%UNICODE(0x2665) *1%　ありがと、%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
			PRINTFORML 「後で他の人が良いって言っても、もう駄目だからね！」
			PRINTFORMW 「大丈夫、退屈させないから%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
			PRINTFORML 「う～っ、ダメ？　どうしてよ～」
			PRINTFORMW 「ねぇ、どういうのが好みなの？」
			PRINTFORMW 「これぐらいじゃ諦めないからね！」
		RETURN 1
	ENDIF

ENDIF
RETURN 1

