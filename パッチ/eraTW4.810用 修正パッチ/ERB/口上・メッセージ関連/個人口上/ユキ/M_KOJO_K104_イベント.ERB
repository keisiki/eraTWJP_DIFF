@M_KOJO_EVENT_K104_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF CFLAG:104:オナバレ == 1
			PRINTFORML 「あ、%CALLNAME:MASTER%……あのさ、この前の……」
			PRINTFORML 「あ～……何でもないわ」
			PRINTFORMW 「はぁ……」
		ELSEIF CFLAG:104:オナバレ
			IF (TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ)
				PRINTFORML 「%CALLNAME:MASTER%……%UNICODE(0x2665) *1%」
				PRINTFORMW 「次も、ちょうどよく来てくれたら……相手してあげるからね」
			ELSE
				PRINTFORML 「っ、%CALLNAME:MASTER%……」
				PRINTFORML 「この前は流されちゃったけど、次はこうはいかないから！」
				PRINTFORMW 「気を付けてよね！」
			ENDIF
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「あっ、%CALLNAME:MASTER%！」
			PRINTFORML 「待ってたわ%UNICODE(0x2665) *1%」
		ELSEIF TALENT:思慕
			PRINTFORML 「あ、%CALLNAME:MASTER%！」
			PRINTFORMW 「今日は何してるの？」
		ELSE
			PRINTFORMW 「あら、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF CFLAG:104:オナバレ == 1
			PRINTFORML 「あ、%CALLNAME:MASTER%……あのさ、この前の……」
			PRINTFORML 「あ～……何でもないわ」
			PRINTFORMW 「はぁ……」
		ELSEIF CFLAG:104:オナバレ
			IF (TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ)
				PRINTFORML 「%CALLNAME:MASTER%……%UNICODE(0x2665) *1%」
				PRINTFORMW 「次も、ちょうどよく来てくれたら……相手してあげるからね」
			ELSE
				PRINTFORML 「っ、%CALLNAME:MASTER%……」
				PRINTFORML 「この前は流されちゃったけど、次はこうはいかないから！」
				PRINTFORMW 「気を付けてよね！」
			ENDIF
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「あっ、いたいた！」
			PRINTFORMW 「ね～、わたしの相手してよ」
		ELSEIF TALENT:思慕
			PRINTFORML 「あ、%CALLNAME:MASTER%……」
			PRINTFORMW 「あ～……暇なら、一緒にどこか行かない？」
		ELSE
			PRINTFORMW 「あら、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「あっ、入ってたんだ？」
			PRINTFORML 「背中でも流してあげよっか～」
		ELSEIF TALENT:思慕
			PRINTFORML 「わっと、%CALLNAME:MASTER%」
			PRINTFORMW 「え～と、一緒に入っていい？」
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%じゃない」
			PRINTFORMW 「あはは、なんか照れ臭いね」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「%CALLNAME:MASTER%が一緒じゃ、落ち着いてお風呂は無理ね」
			PRINTFORML 「ドキドキしちゃうし……%UNICODE(0x2665) *1%」
			PRINTFORMW 「後にしようかな」
		ELSEIF TALENT:思慕
			PRINTFORML 「あれ、誰か……%CALLNAME:MASTER%！？」
			PRINTFORMW 「え～っと、あ～……また来るわ」
		ELSE
			PRINTFORML 「ありゃ、先客？」
			PRINTFORMW 「ごゆっくりどうぞ～」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「%CALLNAME:MASTER%が一緒じゃ、落ち着いてお風呂は無理ね」
			PRINTFORML 「ドキドキしちゃうし……%UNICODE(0x2665) *1%」
			PRINTFORMW 「一人で先に入っていい？」
		ELSEIF TALENT:思慕
			PRINTFORML 「あれ、誰か……%CALLNAME:MASTER%！？」
			PRINTFORMW 「え～っと、あ～……ちょっと待っててくれない？」
		ELSE
			PRINTFORMW 「ごめんね、入ってるから……ちょっと待っててもらえる？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF CFLAG:104:オナバレ == 1
			PRINTFORML 「あ、%CALLNAME:MASTER%……あのさ、この前の……」
			PRINTFORML 「あ～……何でもないわ」
			PRINTFORMW 「はぁ……」
		ELSEIF CFLAG:104:オナバレ
			IF (TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ)
				PRINTFORML 「%CALLNAME:MASTER%……%UNICODE(0x2665) *1%」
				PRINTFORMW 「次も、ちょうどよく来てくれたら……相手してあげるからね」
			ELSE
				PRINTFORML 「っ、%CALLNAME:MASTER%……」
				PRINTFORML 「この前は流されちゃったけど、次はこうはいかないから！」
				PRINTFORMW 「気を付けてよね！」
			ENDIF
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「%CALLNAME:MASTER%じゃない%UNICODE(0x2665) *1%」
			PRINTFORMW 「折角だし、二人で過ごそうよ～」
		ELSEIF TALENT:思慕
			PRINTFORML 「あ、%CALLNAME:MASTER%……」
			PRINTFORMW 「あ～……暇なら、一緒にどこか行かない？」
		ELSE
			PRINTFORMW 「あら、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			IF FLAG:デート相手 == 105
				PRINTFORML 「あ、%CALLNAME:MASTER%%UNICODE(0x2665) *1%……と、マイ……」
					IF TALENT:104:恋人
						PRINTFORML 「あ、はは……二人、結構仲いいよね」
						PRINTFORMW （マイなら、大丈夫……だよね……？）
					ELSEIF TALENT:105:恋人
						PRINTFORML （……わたしも……）
						PRINTFORMW （だめだめ……何考えてるのよ、マイの恋人なんだから）
					ELSE
						PRINTFORMW 「うう～、なんか複雑……」
					ENDIF
			ELSE
				PRINTFORML 「あら、デート？」
				PRINTFORMW 「良いなぁ～、今度わたしともしてよ」
			ENDIF
		ELSEIF TALENT:思慕
			IF FLAG:デート相手 == 105
				PRINTFORML 「%CALLNAME:MASTER%と、マイ？　もしかしてデート？」
				PRINTFORMW 「……ふ～ん……そう、なんだ……」
			ELSE
				PRINTFORML 「あら、デート？」
				PRINTFORMW 「%CALLNAME:MASTER%って、そういう感じの子が好みなんだね」
			ENDIF
		ELSE
			IF FLAG:デート相手 == 105
				PRINTFORML 「わっ、マイがデートしてる！」
				PRINTFORMW 「%CALLNAME:MASTER%、マイに変なことしないでよ！」
			ELSE
				PRINTFORML 「あ、%CALLNAME:MASTER%……っと、デート中なんだ！」
				PRINTFORMW 「意外とやるのね、%CALLNAME:MASTER%！」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
