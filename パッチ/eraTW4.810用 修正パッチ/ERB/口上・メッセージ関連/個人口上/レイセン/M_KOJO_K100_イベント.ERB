		;外出先で遭遇した
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 6
			CALL REISEN_ENCOUNTER
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとデート中に遭遇した
		;ARG:1にデート相手
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 7
			PRINTFORML 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MAIN_MAP上で一日最初の挨拶
		;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 8
			PRINTFORML 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;外出先で一日最初の挨拶
		;ARG:1にデート相手、ARG:1が0の場合は非デート
		;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 9
			;ARG:1とデート中
			IF ARG:1
			;一人で外出
			ELSE
			ENDIF
			PRINTFORML 
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

