	SETBIT CFLAG:49:1490,1

;時姦される可能性を考慮して、生解禁までは紫に頭を下げて体内にスキマを仕込んでもらってるよ
IF (CFLAG:49:生解禁 == 1 && ESTRUS_CYCLE(49) == 150) || !CFLAG:49:生解禁
	EX:膣内精液 = 0
ENDIF

;時止めでお燐おくうこいしに手を出されると怒るよ
FOR LOCAL,36,39
	IF EXP:LOCAL:60 || EXP:LOCAL:61 || EXP:LOCAL:62 || EXP:LOCAL:63
		SETBIT CFLAG:49:1490,0
	ENDIF
NEXT

