;------------------------------------------
;名前を教えるイベ
;-------------------------------------------
@NAME_SET_K41
;/見事な響子口上の丸パクだと感心するがどこもおかしくはない
PRINTL 名前を入力してください(直接入力するか、ボタンで選択してください)
IF NAME:MASTER != "あなた"
	LOCALS = %NAME:MASTER%
ELSE
	LOCALS = %CALLNAME:MASTER%
ENDIF
PRINTBUTTON @"[名前(%LOCALS%)を名乗る]", @"%LOCALS%"
PRINTL 　
;名前を入力するループ
$INPUT_LOOP_ONAMAE
INPUTS
; 行頭行尾の空白文字削除
LOCALS = %RESULTS%
REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
;10文字以上は受け付けない
IF STRLENS(LOCALS) > 10
	CLEARLINE 1
	REUSELASTLINE 「…そんなに長いの？ニックネームとか教えなよ」
	GOTO INPUT_LOOP_ONAMAE
;空白は受け付けない
ELSEIF RESULTS == ""
	PRINTL 名前を入力してください
	GOTO INPUT_LOOP_ONAMAE
ELSE
	; 同姓同名
	IF FLAG:なりきり != 41 && STRCOUNT(LOCALS, "ぬえ")
		CLEARLINE 1
		REUSELASTLINE 「嘘付け、お前のどこが正体不明だよ」
		GOTO INPUT_LOOP_ONAMAE
	; 白蓮が主人ではなく、白蓮というフレーズが含まれている。もしくは
	; 星が主人ではなく、寅丸もしくは星というフレーズが含まれている
	ELSEIF (FLAG:なりきり != 55 && STRCOUNT(LOCALS, "聖\*\\s\*白蓮")) || (FLAG:なりきり != 87 && STRCOUNT(LOCALS, "\(寅丸\)\*\\s\*星\$"))
		CLEARLINE 1
		REUSELASTLINE 「はいはい、そういうのいいから」
		GOTO INPUT_LOOP_ONAMAE
	; 命蓮寺勢はじき
	; うんそうだね多々良勝五郎とか教える人もいるかもしれないもんね
	ELSEIF (FLAG:なりきり != 39 && LOCALS == "ナズーリン") || (FLAG:なりきり != 40 && STRCOUNT(LOCALS, "\(多々良\)\*\\s\*小傘")) || (FLAG:なりきり != 85 && STRCOUNT(LOCALS, "\(雲居\)\*\\s\*一輪")) || (FLAG:なりきり != 86 && STRCOUNT(LOCALS, "\(\(村紗\)*\\s\*水蜜\|ムラサ\)"))
		CLEARLINE 1
		REUSELASTLINE 「そういう名前の奴、他にもいるんだよね。愛称とかないの？」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FLAG:なりきり != 69 && STRCOUNT(LOCALS, "\(二ツ岩\)\*\\s\*マミゾウ")
		CLEARLINE 1
		REUSELASTLINE 「マミゾウ？いつのまに若返ったの」
		GOTO INPUT_LOOP_ONAMAE
	; 道教勢力はじき
	ELSEIF (FLAG:なりきり != 89 && STRCOUNT(LOCALS, "\(宮古\)\*\\s\*芳香")) || (FLAG:なりきり != 90 && STRCOUNT(LOCALS, "\^霍\*\\s\*青娥")) || (FLAG:なりきり != 91 && STRCOUNT(LOCALS, "\(蘇我\)\*\\s\*屠自古")) || (FLAG:なりきり != 92 && STRCOUNT(LOCALS, "\(物部\)\*\\s\*布都")) || (FLAG:なりきり != 56 && STRCOUNT(LOCALS, "\(\(豊聡耳\)\*\\s\*神子\|聖徳太子\)"))
		CLEARLINE 1
		REUSELASTLINE 
		GOTO INPUT_LOOP_ONAMAE
	; 博麗霊夢というフレーズが含まれてる。もしくは
	; 東風谷早苗というフレーズが含まれている
	ELSEIF STRCOUNT(LOCALS, "\(博麗\)\*\\s\*霊夢") || STRCOUNT(LOCALS, "\(東風谷\)\*\\s\*早苗")
		PRINTFORML 「あれ、巫女の親戚？なんでここにいるの」
	; 霧雨魔理沙というフレーズが含まれてる。
	ELSEIF STRCOUNT(LOCALS, "\(霧雨\)\*\\s\*魔理沙")
		PRINTFORML 「あの魔法使いの親戚？なんで仏門に入ったの」
	; 直接入力ならいけると思ったか？弾くぜ！
	ELSEIF FIRSTTIME("U41-1") && ABL:親密 < 4 && STRCOUNT(LOCALS, "\(お\(\(兄\|にい\)\|\(姉\|ねえ\)\)\(さん\|ちゃん\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「人の話聞いてる？」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U41-1") && ABL:親密 < 7 && STRCOUNT(LOCALS, "\(お\(\(兄\|にい\)\|\(姉\|ねえ\)\)\(様\|さま\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「人の話聞いてる？」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U41-1") && (ABL:従順 < 7 || ABL:奉仕精神 < 7) && STRCOUNT(LOCALS, "\(\(御\|ご\)\(主人\|しゅじん\)\(様\|さま\)\)$")
		CLEARLINE 1
		REUSELASTLINE 「人の話聞いてる？」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U41-1") && (!TALENT:恋人 || !TALENT:恋慕) && STRCOUNT(LOCALS, "\(\(旦那\|だんな\)\(様\|さま\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「人の話聞いてる？」
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTFORML 「%RESULTS%で良いの？」
	ENDIF
	PRINTL [0] はい
	PRINTL [1] いいえ
	;入力した名前を確定するループ
	$INPUT_LOOP_KAKUTEI_1
	INPUT
	IF RESULT == 0
		;あなたの名前を%MASTERNAME:41%に格納
		MASTERNAME:41 = %RESULTS%
		PRINTFORML 
	ELSEIF RESULT == 1
		PRINTL 名前を入力してください
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTL 名前を入力してください
		GOTO INPUT_LOOP_ONAMAE
	ENDIF
ENDIF
