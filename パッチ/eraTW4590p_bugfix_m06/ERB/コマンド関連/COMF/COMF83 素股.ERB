@COM_ABLE83
;素股実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(83)
	RETURN RESULT
;潤滑が高くないとダメ
SIF PALAM:潤滑 < 2000
	RETURN 0
;調教者がオトコか半人半妖かふたなりじゃないとダメ
SIF GETBIT(TALENT:PLAYER:性別,2)
	RETURN 0
;TARGETに男性器があるとだめ
SIF GETBIT(TALENT:性別,2)
	RETURN 0
;バイブ挿入中はダメ
SIF TEQUIP:13
	RETURN 0
;時間停止中は許可
IF FLAG:70 != 1
	;縄使用中は不可
	SIF TEQUIP:縄
		RETURN 0
ENDIF

;睡眠姦中
SIF CFLAG:イタズラ > 1
	RETURN 0
;下半身着衣
SIF TEQUIP:0 > 1
	RETURN 0
;お風呂場プレイ中はプレイマットが無いとダメ
IF TEQUIP:31
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
RETURN 1

