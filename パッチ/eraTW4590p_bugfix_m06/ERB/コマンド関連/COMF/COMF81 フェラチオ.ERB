;-------------------------------------------------
;フェラチオ
;奉仕系コマンド、レベル2
;-------------------------------------------------
@COM81
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;奉仕快楽
TCVAR:108 = 1

CSTR:PLAYER:10 += @"{TARGET}/"
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:PLAYER:2 = 4
;強度
TCVAR:PLAYER:4 = 1500

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 50
	SOURCE:快Ｍ = 50

	SOURCE:PLAYER:快Ｃ = 110
	;性技補正
	SOURCE:PLAYER:快Ｃ += MIN(ABL:舌 * 25,100)

	;調教者のTALENT:猫舌をみる
	IF TALENT:猫舌
		TIMES SOURCE:PLAYER:快Ｃ , 0.75
		;調教者が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
		EXP:嗜虐快楽経験 += 1
	ENDIF

ELSE
	DOWNBASE:体力 += 10
	DOWNBASE:気力 += 100
	SOURCE:PLAYER:快Ｃ = 110
	SOURCE:快Ｍ = 50
	SOURCE:情愛 = 100
	SOURCE:性行動 = 300
	SOURCE:達成 = 150
	SOURCE:屈従 = 700
	SOURCE:反感 = 400
	SOURCE:逸脱 = 100

	;上のほうで計算した汚れデータ
	SOURCE:不潔 = Y*10 + 60

	;-------------------------------------------------
	;特殊ソース処理
	;-------------------------------------------------
	;抑圧、抵抗持ちには、抑鬱ソース追加
	SIF TALENT:自己愛 < 0 || TALENT:抵抗
		SOURCE:鬱屈 += 400

	;性技補正
	IF ABL:舌 >= 4
		SOURCE:PLAYER:快Ｃ += 100
	ELSE
		SOURCE:PLAYER:快Ｃ += ABL:舌 * 25
	ENDIF

	;調教者のTALENT:舌使いをみる
	SIF TALENT:舌使い
		TIMES SOURCE:PLAYER:快Ｃ , 1.50

	;調教者のTALENT:猫舌をみる
	SIF TALENT:猫舌
		TIMES SOURCE:PLAYER:快Ｃ , 0.75

ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;奴隷の指⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:0

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;フェラ経験
;とりあえずM経験追加
EXP:Ｍ経験 += 1

;同性愛　レズなら戻り値1ゲイなら戻り値2
SIF HETEROSEX(TARGET,PLAYER)
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 7

IF !FLAG:70 && !CFLAG:イタズラ
	EXP:口淫経験 += 3
	;奴隷が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
	SIF TALENT:TARGET:猫舌
		EXP:嗜虐快楽経験 += 1
	;同性愛　レズなら戻り値1ゲイなら戻り値2
	SIF HETEROSEX(TARGET,PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 7
	IF HETEROSEX(TARGET,PLAYER)
		EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 7
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 7
	ENDIF
	;愛情経験
	SIF TALENT:恋慕 && ASSIPLAY == 0
		EXP:愛情経験 += 1
ELSE
	SIF !CFLAG:イタズラ
		EXP:PLAYER:時姦経験 += 1
ENDIF
IF CFLAG:PLAYER:イタズラ == 2
	EXP:PLAYER:睡眠姦経験 += 1
	EXP:TARGET:睡眠姦経験 += 1
ENDIF

;精液汚れ
SIF TCVAR:PLAYER:112 == 1 || TCVAR:PLAYER:112 == 2
	EXP:精飲経験 ++

RETURN 1


@CAN_COM81(ARG)
SKIPDISP ARG
PRINTL フェラチオ
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
IF FLAG:70 == 1 || CFLAG:イタズラ

ELSE
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:奉仕精神 * 4
	PRINT 奉仕精神
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF S
		PRINT  + 
	A += ABL:精液中毒 * 3
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 3,')
	S = 1
ENDIF

IF MARK:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:快楽刻印 * 1
	PRINT 快楽刻印
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 1,')
	S = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	S = 1
ENDIF
;恥じらい
IF TALENT:羞恥心 > 0
	PRINT  - 
	A -= 1
	PRINT 恥じらい
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭耐性 > 0
	SIF S
		PRINT  + 
	A += 1
	PRINT 汚臭鈍感
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭耐性 < 0
	PRINT  - 
	A -= 3
	PRINT 汚臭敏感
	PRINTV '(,3,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 6
	PRINT 献身的
	PRINTV '(,6,')
	S = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感応答 < 0 && !TALENT:淫乱
	PRINT  - 
	A -= 1
	PRINT 快感の否定
	PRINTV '(,1,')
	S = 1
ENDIF
;男嫌い（恋慕、親愛で無効）
IF OTOKOGIRAI(TARGET)
	PRINT  - 
	A -= 12
	PRINT 男嫌い
	PRINTV '(,12,')
	S = 1
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 恋慕
	PRINTV '(,5,')
	S = 5
ENDIF
;親愛
IF TALENT:恋慕 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 親愛
	PRINTV '(,5,')
	S = 5
ENDIF

;調教者がふたなり
IF (TALENT:PLAYER:2 & 3) == 3
	SIF S
		PRINT  + 
	A += 8
	PRINT ふたなり
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

Y = Y * (2 - TALENT:汚臭耐性) / 2

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:汚臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:汚臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT
SKIPDISP 0
;実行できない
SIF A < V
	RETURN 0
ENDIF
RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE81
;フェラチオ実行判定
SIF !TFLAG:100
	RETURN 0
SIF GLOBAL_COMABLE(81)
	RETURN RESULT
;調教者がオトコか半人半妖かふたなりじゃないとダメ
SIF !(TALENT:PLAYER:2 & 2)
	RETURN 0
;時間停止中は許可
IF FLAG:70 != 1
	;猫舌の奴隷の場合は技巧Lv2以上必要
	SIF TALENT:猫舌 && ABL:技巧 < 2
		RETURN 0
	;小人サイズだと口の中に入らない
	SIF 挿入不可(TARGET) == 3
		RETURN 0
ENDIF
;ボールギャグ使用中は不可
SIF TEQUIP:ボールギャグ
	RETURN 0
RETURN 1
