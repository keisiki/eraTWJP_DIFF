;-------------------------------------------------
;両穴フィスト
;ハードな調教コマンド、レベル4
;-------------------------------------------------
@COM143
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
IF ABL:Ｖ感覚 >= ABL:Ａ感覚
	TCVAR:TARGET:2 = 13
ELSE
	TCVAR:TARGET:2 = 12
ENDIF
;強度
TCVAR:TARGET:4 = 1800

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 500
	SOURCE:快Ｖ = 200
	SOURCE:快Ａ = 200
	SOURCE:苦痛 = 900

	;フィストなどのVへの大きな苦痛は先に処理しておく
	;EXP:Ｖ経験をみる
	IF EXP:Ｖ経験 < EXPLV:1
		TIMES SOURCE:苦痛 , 2.50
	ELSEIF EXP:Ｖ経験 < EXPLV:2
		TIMES SOURCE:苦痛 , 2.00
	ELSEIF EXP:Ｖ経験 < EXPLV:3
		TIMES SOURCE:苦痛 , 1.80
	ELSEIF EXP:Ｖ経験 < EXPLV:4
		TIMES SOURCE:苦痛 , 1.60
	ELSEIF EXP:Ｖ経験 < EXPLV:5
		TIMES SOURCE:苦痛 , 1.40
	ELSEIF EXP:Ｖ経験 >= EXPLV:5
		TIMES SOURCE:苦痛 , 1.20
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES SOURCE:苦痛 , 3.00
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES SOURCE:苦痛 , 2.50
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES SOURCE:苦痛 , 2.00
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES SOURCE:苦痛 , 1.50
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES SOURCE:苦痛 , 1.00
	ENDIF


	;フィストなどのAへの大きな苦痛は先に処理しておく
	;EXP:Ａ経験をみる
	IF EXP:Ａ経験 < EXPLV:1
		TIMES SOURCE:苦痛 , 3.00
	ELSEIF EXP:Ａ経験 < EXPLV:2
		TIMES SOURCE:苦痛 , 2.50
	ELSEIF EXP:Ａ経験 < EXPLV:3
		TIMES SOURCE:苦痛 , 2.40
	ELSEIF EXP:Ａ経験 < EXPLV:4
		TIMES SOURCE:苦痛 , 2.00
	ELSEIF EXP:Ａ経験 < EXPLV:5
		TIMES SOURCE:苦痛 , 1.80
	ELSEIF EXP:Ａ経験 >= EXPLV:5
		TIMES SOURCE:苦痛 , 1.60
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES SOURCE:苦痛 , 3.00
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES SOURCE:苦痛 , 2.50
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES SOURCE:苦痛 , 2.00
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES SOURCE:苦痛 , 1.50
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES SOURCE:苦痛 , 1.00
	ENDIF

ELSE

	DOWNBASE:体力 += 500
	DOWNBASE:気力 += 300

	SOURCE:快Ｖ = 200
	SOURCE:快Ａ = 200
	SOURCE:苦痛 = 1800
	SOURCE:露出 = 100
	SOURCE:屈従 = 2000
	SOURCE:逸脱 = 2000
	SOURCE:反感 = 2000


	;-------------------------------------------------
	;特殊ソース処理
	;-------------------------------------------------
	;フィストなどのVへの大きな苦痛は先に処理しておく
	;EXP:Ｖ経験をみる
	IF EXP:Ｖ経験 < EXPLV:1
		TIMES SOURCE:苦痛 , 2.50
		TIMES SOURCE:反感 , 4.00
	ELSEIF EXP:Ｖ経験 < EXPLV:2
		TIMES SOURCE:苦痛 , 2.00
		TIMES SOURCE:反感 , 3.50
	ELSEIF EXP:Ｖ経験 < EXPLV:3
		TIMES SOURCE:苦痛 , 1.80
		TIMES SOURCE:反感 , 3.00
	ELSEIF EXP:Ｖ経験 < EXPLV:4
		TIMES SOURCE:苦痛 , 1.60
		TIMES SOURCE:反感 , 2.50
	ELSEIF EXP:Ｖ経験 < EXPLV:5
		TIMES SOURCE:苦痛 , 1.40
		TIMES SOURCE:反感 , 2.00
	ELSEIF EXP:Ｖ経験 >= EXPLV:5
		TIMES SOURCE:苦痛 , 1.20
		TIMES SOURCE:反感 , 2.00
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES SOURCE:苦痛 , 3.00
		TIMES SOURCE:反感 , 2.00
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES SOURCE:苦痛 , 2.50
		TIMES SOURCE:反感 , 1.50
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES SOURCE:苦痛 , 2.00
		TIMES SOURCE:反感 , 1.20
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES SOURCE:苦痛 , 1.50
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES SOURCE:苦痛 , 1.00
	ENDIF


	;フィストなどのAへの大きな苦痛は先に処理しておく
	;EXP:Ａ経験をみる
	IF EXP:Ａ経験 < EXPLV:1
		TIMES SOURCE:苦痛 , 3.00
		TIMES SOURCE:反感 , 4.00
	ELSEIF EXP:Ａ経験 < EXPLV:2
		TIMES SOURCE:苦痛 , 2.50
		TIMES SOURCE:反感 , 3.50
	ELSEIF EXP:Ａ経験 < EXPLV:3
		TIMES SOURCE:苦痛 , 2.40
		TIMES SOURCE:反感 , 3.00
	ELSEIF EXP:Ａ経験 < EXPLV:4
		TIMES SOURCE:苦痛 , 2.00
		TIMES SOURCE:反感 , 2.50
	ELSEIF EXP:Ａ経験 < EXPLV:5
		TIMES SOURCE:苦痛 , 1.80
		TIMES SOURCE:反感 , 2.00
	ELSEIF EXP:Ａ経験 >= EXPLV:5
		TIMES SOURCE:苦痛 , 1.60
		TIMES SOURCE:反感 , 1.50
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES SOURCE:苦痛 , 3.00
		TIMES SOURCE:反感 , 2.00
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES SOURCE:苦痛 , 2.50
		TIMES SOURCE:反感 , 1.50
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES SOURCE:苦痛 , 2.00
		TIMES SOURCE:反感 , 1.20
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES SOURCE:苦痛 , 1.50
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES SOURCE:苦痛 , 1.00
	ENDIF
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Ｖ経験
EXP:Ｖ経験 += 24

;Ａ経験
EXP:Ａ経験 += 24

;Ｖ,Ａ拡張経験
IF !EXP:Ａ拡張経験 && !EXP:Ｖ拡張経験 && !GETBIT(CFLAG:3,5)
	EXP:異常経験 += 1
	SETBIT CFLAG:3,5
ENDIF

EXP:Ａ拡張経験 += 5
EXP:Ｖ拡張経験 += 5
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF FLAG:70 == 1
	IF HETEROSEX(TARGET,PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
	ENDIF
ELSE
	IF HETEROSEX(TARGET,PLAYER)
		EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 1
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
	ENDIF
ENDIF
RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE143
;両穴フィスト
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(143)
	RETURN RESULT
;バイブ使用中は不可
SIF TEQUIP:13
	RETURN 0
;アナルバイブ使用中は不可
SIF TEQUIP:14
	RETURN 0
;浣腸＋アナルプラグ使用中は不可
SIF TEQUIP:21
	RETURN 0
;アナルビーズ使用中は不可
SIF TEQUIP:15
	RETURN 0
;バルーン使用中は不可
SIF TEQUIP:22
	RETURN 0
;電極使用中は不可
SIF TEQUIP:23
	RETURN 0
;オトコだとダメ
SIF !(TALENT:2 & 1)
	RETURN 0
;ASSIの場合、従順4以上かサドじゃないとできない。
IF ASSIPLAY
	SIF ABL:ASSI:従順 < 4 && TALENT:ASSI:サド == 0
		RETURN 0
ENDIF
IF FLAG:70 == 1
ELSE
	;一定のＶ・Ａ経験が必要
	IF EXP:0 < 300 || EXP:1 < 300
		RETURN 0
	ENDIF
	;小柄体型なら従順が4以上かつマゾっ気が4以上かつ拡張経験10以上じゃないと実行できない
	IF TALENT:体型 < 0
		SIF ABL:従順 <= 3 || ABL:マゾっ気 <= 3 || EXP:Ｖ拡張経験 < 10 || EXP:Ａ拡張経験 < 10
			RETURN 0
	ENDIF
ENDIF
;調教対象が小人体型の場合は主人が禁断の知識持ちじゃないとダメ
SIF ABS(TALENT:体型 - TALENT:PLAYER:体型) > 2 && TALENT:MASTER:禁断の知識 == 0
	RETURN 0

;下半身着衣
SIF TEQUIP:0 > 1
	RETURN 0
;シャワー中はダメ
SIF TEQUIP:32
	RETURN 0
;お風呂場プレイ中はプレイマットが無いとダメ
IF TEQUIP:31
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
;新妻プレイ中はダメ
SIF TEQUIP:33
	RETURN 0
;ベッドルームでないとダメ
SIF !BEDROOM(CFLAG:PLAYER:現在位置)
	RETURN 0
RETURN 1

