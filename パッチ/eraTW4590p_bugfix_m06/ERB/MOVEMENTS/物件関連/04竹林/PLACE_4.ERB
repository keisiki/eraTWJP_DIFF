
@KICKOUT_4()
#FUNCTION
SELECTCASE CHK_INHOME(CFLAG:MASTER:現在位置)
	CASE 20
		RETURNF 419
	CASE 41
		RETURNF 434
	CASE 54
		RETURNF 465
ENDSELECT

@BATHROOM_4()
#FUNCTION
IF CFLAG:MASTER:現在位置 >= 401 && CFLAG:MASTER:現在位置 <= 430
	RETURNF 420
ELSE
	RETURNF 441
ENDIF
RETURNF 0

@BATHCHECK_4(ARG)
#FUNCTION
SELECTCASE CHK_INHOME(ARG)
	CASE 20,41,54
		RETURNF 1
	CASEELSE
		RETURNF 0
ENDSELECT
RETURNF 0

@MAP_UNIQUEMESSAGE_4
CALL DRAW_INFORMATIONLINE("Info",1)
IF WITH_MOB()
	IF FLAG:70
		PRINTFORML イナバ達が停止している
	ELSEIF GROUPMATCH(TIME:2,1,6,7)
		PRINTFORML イナバ達が眠っている…
	ELSEIF !DAY:3 % 10
		IF !TALENT:MASTER:2 & 2
			PRINTFORML 発情中のイナバ達は悶々としている……
		ELSE
			PRINTFORML 発情中のイナバ達から強烈な視線を感じる……
		ENDIF
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML イナバ達がうろついている
			CASE 1
				PRINTFORML イナバ達が遊んでいる
			CASE 2
				PRINTFORML イナバ達が話し合っている
			CASE 3
				PRINTFORML イナバとイナバがごっつんこしている
			CASE 4
				PRINTFORML イナバ達が積み重なっている
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;-----------------------------------------
;非自宅時に差し替える場所IDを返す関数
;ARG,対象ID
;RETURNF,差し替え済み対象ID
;-----------------------------------------

@MAP_SEARCH_REPLACEMENT_4(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 401 TO 406
		RETURNF 450
	CASE 407 TO 430
		RETURNF 480
	CASE 431
		RETURNF 440
	CASE 432,433,442,445
		RETURNF 430
	CASE 434,435,440,441
		RETURNF 420
	CASE 436,437
		RETURNF 460
	CASE 438,439
		RETURNF 470
	CASE 450 TO 466
		RETURNF 490
	CASE 443,444,446,447
		RETURNF 410
ENDSELECT
RETURNF 0



@MAP4_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 4
	SIF INRANGE(PLACE_ID, 401, 430)
		RETURNF "永遠亭"
	SIF INRANGE(PLACE_ID, 436, 437)
		RETURNF "無名の丘"
	SIF PLACE_ID == 438
		RETURNF "太陽の畑"
	SIF INRANGE(PLACE_ID, 450, 466)
		RETURNF "夢幻館"
	RETURNF "迷いの竹林"
ELSE
	SELECTCASE PLACE_ID
	CASE 450, 480
		RETURNF "永遠亭"
	CASE 460
		RETURNF "無名の丘"
	CASE 470
		RETURNF "太陽の畑"
	CASE 490
		RETURNF "夢幻館"
	CASEELSE
		RETURNF "迷いの竹林"
	ENDSELECT
ENDIF
