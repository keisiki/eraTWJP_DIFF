@MAP_SEARCH_REPLACEMENT_5(ARG)
#FUNCTION
SELECTCASE ARG
;森の入り口
	CASE 501,534 TO 537, 544
		RETURNF 510
;香霖堂
	CASE 502
		RETURNF 520
;古木の大樹
	CASE 540 TO 545
		RETURNF 530
;霧雨魔法店
	CASE 510 TO 515
		RETURNF 540
;人形の洋館
	CASE 520 TO 533
		RETURNF 550
;再思の道
	CASE 506
		RETURNF 560
;無縁塚
	CASEELSE
		RETURNF 570
ENDSELECT
RETURNF 0


@MAP5_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 5
	SIF INRANGE(PLACE_ID, 510, 515)
		RETURNF "魔理沙の店"
	SIF INRANGE(PLACE_ID, 520, 531)
		RETURNF "人形の洋館"
	SIF INRANGE(PLACE_ID, 534, 537)
		RETURNF "エレンの店"
	SIF PLACE_ID == 502
		RETURNF "香霖堂"
	SIF PLACE_ID == 506
		RETURNF "無縁塚"
	SIF PLACE_ID == 509
		RETURNF "再思の道"
	;SIF INRANGE(PLACE_ID, 343, 348)
	RETURNF "魔法の森"
ELSE
	SELECTCASE PLACE_ID
	CASE 520
		RETURNF "香霖堂"
	CASE 540
		RETURNF "魔理沙の店"
	CASE 550
		RETURNF "人形の洋館"
	CASE 560
		RETURNF "再思の道"
	CASE 570
		RETURNF "無縁塚"
	CASEELSE
		RETURNF "魔法の森"
	ENDSELECT
ENDIF
