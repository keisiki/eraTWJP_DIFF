;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K22_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORMW 「%CALLNAME:MASTER%」
		PRINTFORMW 帰り道、もうそろそろ二人を分断する分かれ道が見えるだろうと思っていたとき、ルナサは初めてあなたの名前を呼んだ
		PRINTFORMW 「今日は楽しかった、ありがとう。…いや、今日だけじゃない。%CALLNAME:MASTER%といるときはいつでも」
		PRINTFORMW ルナサは目を泳がせながら言葉を探し、少しずつ音にしていく
		PRINTFORMW 「…私、閻魔様に会ったことがあるの。悪いことしたわけじゃない、妹の付き添い…妹も悪いことはしてないはずだけど…」
		PRINTFORMW 「まあ、その時、言われたの。騒霊は拠り所がないといずれ壊れてしまうって」
		PRINTFORMW 「生きる意味を、存在する理由を考えろって、閻魔様に言われた」
		PRINTFORMW 「…私には妹がいるから、それでいいと思ってた。でも、妹は私を拠り所にしてくれるか、気づいたとき、とても怖くなったの」
		PRINTFORMW 「今こそ言うことを聞いてくれるし、仲も悪くないけど…それも突然崩れてしまうかもしれない」
		PRINTFORMW 「自由な子達だから、私なんか放っておいて遠くに行ってしまうかもしれないって思ってしまったの」
		PRINTFORMW 「私たち騒霊は輪廻に編み込まれることは決してない、消えてしまえば、どこにも行かない。どこの世界にもいない」
		PRINTFORMW 「私がいる意味を理解しなければ、跡も残さず、本当に消えてしまう。そんなこと…」
		PRINTFORMW 堰を切ったように不安をぶつけてから、はっと気づいて%CALLNAME:MASTER%の顔をようやく認識したようだった
		PRINTFORMW 「…違うの、聞かなかったことにして…。私は、…%CALLNAME:MASTER%をもう、手放せないことに気づいてしまった」
		PRINTFORMW 「%CALLNAME:MASTER%は、私のことを受け止めてくれるかしら。私が側にいることを許してくれるかしら」
		PRINTFORMW 「%CALLNAME:MASTER%のことが、好き。きっと、%CALLNAME:MASTER%が老いて、寿命を迎えても変わらない」
		PRINTFORMW 「…%CALLNAME:MASTER%は私のことを好きでいてくれる？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「ふ、ふふ、嬉しい、わ。ありがとう…」
		PRINTFORMW 「私と、ずっと一緒に…」
		PRINTFORMW 「きっと、いや、絶対、幸せにする」
		PRINTFORMW 「だから、私を置いてどこにも行かないで…。冥界にも、天国にも、地獄にも、ついていくから」
		PRINTFORMW 「いつまでも一緒よ…%CALLNAME:MASTER%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;重くし過ぎたごめんゆるして
		PRINTFORMW 「…いいのよ、そんな顔しないで」
		PRINTFORMW 「私が、貴方に合わなかっただけ、どうして気にするのかしら」
		PRINTFORMW 「いいの、本当に、…本当に。…その顔で見ないで。やめて…」
		PRINTFORMW 「私のこと、忘れて。お願い、私も、貴方のこと…わす、れ…」
		PRINTFORMW 「ごめんなさい、帰るわ。道中気を、つけて…」
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K22_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTFORMW 「…もう帰る時間なのね。また、明日…」
		ELSE
		PRINTFORMW 「楽しかったわ。道中気を付けて」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「…私も、ちょっとしつこかったわね。ごめん…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「…楽しかったわよ、また機会があるといいわね」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「貴方のこと、許したわけじゃない」
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K22_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「ん、こっちに来て…」
		ELSE
			PRINTFORMW 「…ああ、調子はどう？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「探したのよ…%CALLNAME:MASTER%」
		ELSE
			PRINTFORMW 「ああ、どうも」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「あっ…いいかしら」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「えっ、ご、ごめん」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
	;ルナサはこんなに図々しくない、はず
		PRINTFORMW 「入りたいの。…出てくれる？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「あら、こんにちは。今日の気圧は…」
		;天気予報させたいけど無理だよね？
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW 「妬けるわね」
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K22_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 「よく眠れたかしら」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
