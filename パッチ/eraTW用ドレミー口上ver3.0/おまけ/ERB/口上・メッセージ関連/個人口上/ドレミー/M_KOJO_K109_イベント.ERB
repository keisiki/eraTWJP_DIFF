;脱衣口上
;==================================================
@M_KOJO_EVENT_K109_28(ARG,ARG:1)
;口上主はTARGET
;ARGに脱がし方選択
;ARG:1（ARG=3の場合のみあり）地の文の反応
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;全裸にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSEIF TALENT:恋慕
				PRINTFORMDL 息を荒げながら衣服を脱いでいくドレミーの肢体を視姦する
				CALL HPH_PRINT,@"「さあ、続きを…きゃっHPH」","L"
				PRINTFORMDW 辛抱堪らなくなった%CALLNAME:MASTER%はドレミーに抱き着き体をまさぐった
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
				CALL HPH_PRINT,@"「はい、脱ぎ終わりましたよHPH」","W"
			ELSE
				PRINTFORMW 「やはり慣れないですね…」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;下着姿にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSEIF TALENT:恋慕
				PRINTFORMDL 生唾を飲み込む%CALLNAME:MASTER%の反応を楽しむようにドレミーがワンピースを脱いでいく
				PRINTFORMDL ゆっくりと露わになっていく肌に%CALLNAME:MASTER%は目が離せない
				CALL HPH_PRINT,@"「…はい、脱げましたよHPH」","W"
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
				CALL HPH_PRINT,@"「この下着…どうですか？HPH」","W"
			ELSE
				PRINTFORMW 「…はい、これでよろしいですか？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;半脱ぎ
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
			CALL HPH_PRINT,@"「着衣セックスですか？いいですよHPH」","W"
			ELSE
			PRINTFORMW 「現実問題として汚れた服を洗うのが大変なんですよ」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;パンツちょうだい
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
				;-------------------------------------------------
				CASE 0
				;交換です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORML 「等価交換です。私のパンツ半分あげるので貴方のパンツ半分ください」
					PRINTFORMDW パンツを真っ二つにするのか、持ってるパンツを半分ずつシェアするのか…
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 1
				;OK（嬉々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORML 「はいどうぞ」
					PRINTFORMDW ドレミーは丸めた下着を投げてよこした
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「貴方の変態趣味に付き合う私も私ですねぇ…」
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「ちょっと、いやかなりドン引きですよ」
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

