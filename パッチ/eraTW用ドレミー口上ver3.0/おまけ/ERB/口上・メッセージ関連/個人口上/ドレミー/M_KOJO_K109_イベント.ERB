;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;怒らせてしまった
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「………」
			PRINTFORMDW か、勘弁してくれ…ドレミー…勘弁してくれよ…っ
			PRINTFORMDW な…お、オレ…これからどうすりゃいいんだ…え？ドレミー…
			PRINTFORMDW 教えてくれ……教えてくれんのだな…もう…
		CASE 1
			PRINTFORML 「幻想郷で一番温厚な私を怒らせるとは、中々やりますね」
			PRINTFORMDW 知らなかったそんなの…
		ENDSELECT
	RETURN 1
ENDIF
	;-------------------------------------------------
	;泣かせてしまった　[臆病]持ち
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K109_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORMW 「疲れました…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORML 「ふぅ…」
		PRINTFORMDW ドレミーはスッキリした顔をしている
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORML 「ま、まだやるのですか…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		PRINTFORML 「別に、%CALLNAME:MASTER%を壊してしまっても構わんのだろう？」
		PRINTFORMDW ドレミーは舌なめずりをしながら愛撫を再開した
		RETURN 1
	ENDIF
ENDIF
RETURN 1


