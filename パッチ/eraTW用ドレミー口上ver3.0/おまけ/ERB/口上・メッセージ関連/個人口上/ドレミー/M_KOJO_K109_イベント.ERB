;==================================================
;相手からデートのお誘い
;==================================================
@M_KOJO_EVENT_K109_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF CFLAG:2 >= 50000
				PRINTFORML 「さあ%CALLNAME:MASTER%。デートに行きましょう」
				PRINTFORMDW ドレミーはこちらに期待した視線を送っている
			ELSEIF TALENT:恋慕
				PRINTFORMW 「暇なら一緒にデートでも…いかがですか？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「ちょっと出かけませんか？軽い息抜きですよ」
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
				CALL HPH_PRINT,@"「息抜きにラブホでも行きませんかHPH」","W"
			ELSE
				PRINTFORMW 「暇なら、ちょっと付き合ってくれませんか？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF CFLAG:2 >= 50000
				PRINTFORMW 「どこに行きましょうか…楽しみですねぇ」
			ELSEIF TALENT:恋慕
				PRINTFORML 「ありがとうございます」
				PRINTFORMDW ドレミーは小さくガッツポーズをした
			ELSEIF TALENT:思慕
				PRINTFORMW 「甘いものでも食べたいですね。桜餅とか？」
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
				CALL HPH_PRINT,@"「あ、連れ込み宿でもいいですよHPH」","W"
				PRINTFORMDW どっちも変わらないだろ
			ELSE
				PRINTFORMW 「では、行きましょうか」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF CFLAG:2 >= 50000
				PRINTFORMW 「そうですか…次回は絶対に付き合ってくださいね」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「それは残念です…次回を楽しみにしていますね」
			ELSEIF TALENT:思慕
				PRINTFORMW 「わかりました…しょうがないですよね」
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
				PRINTFORMW 「ヤる気満々でしたが…残念です」
			ELSE
				PRINTFORMW 「まあ%CALLNAME:MASTER%の都合もありますからね」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF CFLAG:2 >= 50000
				PRINTFORMW 「私の家で楽しいことでもしませんか？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「暇でしたら私の家でのんびりしませんか？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「もし用事がなければ、私の家でお茶でも飲んでいきませんか？」
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
				CALL HPH_PRINT,@"「私の部屋でセックスでもしませんか？HPH」","W"
			ELSE
				PRINTFORMW 「もしよかったら、私の家で話し相手にでもなってくれませんか？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF CFLAG:2 >= 50000
				PRINTFORML 「なんでも付き合いますよ。文字通り」
				PRINTFORMDW ドレミーはニタァと笑った
			ELSEIF TALENT:恋慕
				PRINTFORMW 「すぐにお茶とお菓子を用意しますね」
			ELSEIF TALENT:思慕
				PRINTFORMW 「あ、それに触ると幻想郷ごと吹き飛びますよ…冗談です」
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
				CALL HPH_PRINT,@"「すぐに精力剤を用意しますねHPH」","W"
			ELSE
				PRINTFORMW 「部屋の匂いを嗅ぐのはやめてください」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF CFLAG:2 >= 50000
				PRINTFORMW 「そうですか…ではまた今度誘いますね」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「仕方ないので、用意していたお菓子は私一人で食べることにします」
			ELSEIF TALENT:思慕
				PRINTFORMW 「残念です」
			ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
				PRINTFORMW 「まさか先約が…？」
			ELSE
				PRINTFORMW 「意外と硬派だったりしますか？」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

