;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K109_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF FIRSTTIME("告白")
			PRINTL 
			PRINTFORMDL いつものようにデートを終え二人帰路につく
			PRINTFORMDW %CALLNAME:MASTER%に寄り添いながら歩くドレミーは歩みを早めると神妙な面持ちで%CALLNAME:MASTER%の立ち塞がった
			PRINTFORMW 「%CALLNAME:MASTER%…今日はとても楽しかったです…それで、その…」
			PRINTFORMDW 言葉が喉に突っかかった様子のドレミーにどうしたと問うと、やがて覚悟を決めたのか%CALLNAME:MASTER%を真っ直ぐに見つめた
			PRINTW 
			PRINTFORMW 「%CALLNAME:MASTER%」
			PRINTFORMW 「貴方が私をどう思っているのか分からないけど…」
			PRINTFORMW 「好き。%CALLNAME:MASTER%が好きなの」
			PRINTFORMW 「現世の%CALLNAME:MASTER%も、夢の世界の%CALLNAME:MASTER%もすべて独り占めしたいほどに」
			PRINTFORMW 「……だから……」
			PRINTFORMW 「…私だけの、恋人になってくれませんか…」
			PRINTW 
			PRINTFORMDW 思いの丈を吐き出したドレミーは、震えながら%CALLNAME:MASTER%の言葉を待っている
		ELSE
			PRINTL 
			PRINTFORMDW  ふとドレミーが立ち止まると、%CALLNAME:MASTER%に縋るように抱き着いてきた
			PRINTW 
			PRINTFORMW 「%CALLNAME:MASTER%…やっぱり私、諦めきれないわ」
			PRINTFORMW 「私に夢を見させて…」
			PRINTFORMDW ドレミーは潤んだ瞳で%CALLNAME:MASTER%をじっと見つめている…
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
			PRINTL 
			PRINTFORMW 「…あぁ…」
			PRINTW 
			PRINTFORMDW ドレミーは感極まったように声を震わせると%CALLNAME:MASTER%の胸に顔を埋めた
			PRINTFORMW 「…夢が叶うって、こんなにも嬉しいものなのね…」
			PRINTFORMDW 思いを伝えあった二人はしっかりと抱き締め合う
			PRINTFORMW 「現でも夢の中でもずっと一緒よ…%CALLNAME:MASTER%…」
			PRINTFORMDW 自然と二人顔を近付け唇を重ねる。恋人とのキスは甘酸っぱい味がした
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
			PRINTW 
			PRINTFORMW 「………」
			PRINTFORMDL 付き合うことはできない
			PRINTFORMDW %CALLNAME:MASTER%の返答を聞いたドレミーは、背を向けて俯いた
			PRINTFORMW 「…夢は夢のまま…ということなのかしらね…」
			PRINTFORMDW %CALLNAME:MASTER%が止める間もなく、ドレミーは飛び去ってしまった
			PRINTFORML  
			PRINTFORMW  (もう二度とこんな気持ちになりたくないと思うけど…きっとまた私は%CALLNAME:MASTER%と会う）
			PRINTFORMW 
			PRINTFORMW （夢…見ていいでしょう？）
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K109_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
	PRINTL
	IF CFLAG:2 >= 50000
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「さて、次回のデートの予定を建てますか」
			PRINTFORMDW 気が早いドレミーと次回のデートの予定を考えた
		CASE 1
			PRINTFORML 「はぁ〜、楽しい時間はあっという間ですねぇ」
			PRINTFORMDW 若干の名残惜しさを感じながら別れた
		CASE 2
			PRINTFORML 「今日も良い夢を見せて貰いました。続きは%CALLNAME:MASTER%が寝てから…」
			PRINTFORMDW ニタァと笑うドレミーの頭を撫で別れた
		ENDSELECT
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「また誘ってくださいね」
			PRINTFORMDW ドレミーと次回の約束をして別れた
		CASE 1
			PRINTFORML 「次にデートすることがあったらあそこに行ってみたいですねぇ」
			PRINTFORMDW ドレミーと次回のデート先を語り合った
		CASE 2
			PRINTFORML 「今日は良い夢が見れそうですね？」
			PRINTFORMDW デートが終わっても暫く寄り添ったままだった
		ENDSELECT
	ELSEIF TALENT:思慕
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「良い気分転換になりました」
			PRINTFORMDW それはなにより
		CASE 1
			PRINTFORML 「なかなか楽しかったですよ」
			PRINTFORMDW どうやらお気に召したようだ
		CASE 2
			PRINTFORML 「%CALLNAME:MASTER%は楽しめましたか？それは何より」
			PRINTFORMW 手を振ってドレミーと別れた
		ENDSELECT
	ELSEIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「次のセックスの予定は？」
			PRINTFORMDW 気が向いたら
		CASE 1
			PRINTFORML 「ラブホ巡りとかもしてみたいですねぇ」
			PRINTFORMDW 結構です
		CASE 2
			PRINTFORML 「そういえば、帰る途中に男根に酷似したキノコを見つけまして…」
			PRINTFORMDW 知 ら な い よ
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「ふぅー…疲れました」
			PRINTFORMDW ドレミーと別れた
		CASE 1
			PRINTFORML 「仕事が進まなくて困りますね」
			PRINTFORMDW 親密な関係になるにはもう少し時間が掛かりそうだ
		CASE 2
			PRINTFORML 「さて…溜まった仕事を片付けないといけませんねぇ」
			PRINTFORMDW ドレミーはそう言うと去っていった
		ENDSELECT
		ENDIF
		PRINTL 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
			PRINTFORML 「もう怒っていませんよ。本当です」
			PRINTFORMDW どうやら機嫌を直してくれたようだ
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
			PRINTFORML 「どうやら反省しているようですね。その調子ですよ」
			PRINTFORMDW あと一押しといったところか
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
			PRINTFORML 「必死さが伝わってきましたよ。ですがまだ足りませんね」
			PRINTFORMDW まだ許してくれないらしい
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K109_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;仕事中ではなく、今日会ってない。
	IF !TCVAR:109:今日会ったか
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;正月、バレンタイン、ハロウィン、クリスマスの場合
		IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
			CALL SPECIALDAY_EVENT_K109
		ELSEIF CFLAG:318 == 1
			PRINTFORML 「今はちょっと話す気になれませんね」
			PRINTFORMDW ドレミーはムスっとしている
		ELSEIF CFLAG:2 >= 50000
			;DAY:2で季節を選択
			SELECTCASE DAY:2
			;春
			CASE 1
				;TIME:2で朝昼晩を選択
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORML 「おはようございます。%CALLNAME:MASTER%」
					PRINTFORML 「といっても、私はもうすぐ休むのですが…」
					PRINTFORMDW ドレミーは申し訳なさそうだ
				;昼
				CASE 3,4
					PRINTFORML 「こんにちは。%CALLNAME:MASTER%」
					PRINTFORML 「最近は暖かく過ごしやすいからか、夢の世界の住民も落ち着いています」
					PRINTFORMW 「というわけで、暇なのでデートにでも行きませんか？」
				;夜
				CASE 5,6,7
					PRINTFORML 「こんばんは。%CALLNAME:MASTER%」
					PRINTFORML 「これから眠る所ですか？それとも今起きたところ？」
					PRINTFORMW 「寝る前には白湯を飲むと心地よい眠りに就くことができますよ」
				ENDSELECT
			;夏
			CASE 2
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORML 「おはようございます。%CALLNAME:MASTER%」
					PRINTFORMW 「この時間帯はまだ涼しくて何かしらの活動をするには最適ですね」
				;昼
				CASE 3,4
					PRINTFORML 「こんにちは。%CALLNAME:MASTER%」
					PRINTFORML 「…ふぅ…暑いですねぇ…」
					PRINTFORMDW 汗で張り付いた衣服がなまめかしい
				;夜
				CASE 5,6,7
					PRINTFORML 「こんばんは。%CALLNAME:MASTER%」
					PRINTFORML 「夏は夜 月の頃はさらなり 闇もなほ 蛍の多く飛び違ひたる…」
					PRINTFORMW 「枕草子の一節ですよ」
				ENDSELECT
			;秋
			CASE 3
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORML 「おはようございます。%CALLNAME:MASTER%」
					PRINTFORMW 「涼しくなってきましたね。体調を崩さないように気を付けてください」
				;昼
