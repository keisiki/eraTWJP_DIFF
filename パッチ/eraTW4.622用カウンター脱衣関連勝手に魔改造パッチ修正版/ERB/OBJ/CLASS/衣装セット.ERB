;要素名→ID変換用関数の存否確認用関数
;変換可能（IDと要素名の関係が常に変化しない）なら設置
;状況によって名前が変わるようなのは不可能
@EXIST_衣装セット_ID
#LOCALSIZE 1
#LOCALSSIZE 1

;要素名→ID変換用関数
;必要なら各オブジェクトで同様に作成のこと
@衣装セット_ID(ARG:0, ARGS:0, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS V_NAME                  ;作成する変数名
#DIMS C_NAME, OBJ_ID_LAST + 1 ;データ保持
#DIM  O_ID                    ;参照用
IF ARGS:0 == "RESET"
	VARSET C_NAME
	FOR O_ID, 1, OBJ_ID_LAST + 1
		C_NAME:O_ID = %GET_STR(, "衣装セット", O_ID, "名前")%
	NEXT
ELSE
	O_ID = FINDELEMENT(C_NAME, ESCAPE(ARGS:0), , , 1)
	CALLF MAKE_INT(V_NAME, O_ID)
ENDIF



;存否関数
;F関数はTRYCALL系が使えないので通常関数で存否配列を作っちゃう
@EXIST_衣装セット1
#LOCALSIZE 1
#LOCALSSIZE 1

;オブジェクト本体
@衣装セット1(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "名前"
	CALLF MAKE_STR(V_NAME, "男装セット")
CASE "基本衣装セット"
	CALLF MAKE_INT(V_NAME, 1)

;こっから衣装設定系
CASE "靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "ズボン"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ズボン", "ズボン"))
CASE "上半身下着１"
;漢女はサラシをつける
	IF GETBIT(TALENT:ARG:2, 0)
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身下着１", "サラシ"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身下着１", "Ｔシャツ"))
	ENDIF
CASE "上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "上着"))
ENDSELECT



