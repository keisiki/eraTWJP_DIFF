RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_19_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
		IF RAND:6 == 0
			CALL HPH_PRINT,@"「ちゅっHPHじゅるっHPHんふ…HPH」","L"
			PRINTFORMDW %CALLNAME:MASTER%が舌を入れると、ドレミーは積極的に舐めしゃぶってきた
		ELSEIF RAND:5 == 0
			CALL HPH_PRINT,@"「ふ、んっHPHじゅぷっHPHんんんっ…HPH」","L"
			PRINTFORMDL ドレミーの歯の一本一本を数えるように舌を動かす
			PRINTFORMDW 顔を真っ赤にしたドレミーは仕返しとばかりに舌を絡めてきた
		ELSEIF RAND:4 == 0
			CALL HPH_PRINT,@"「れろっHPH……こうですか？…ん、んくっHPH」","L"
			PRINTFORMDL ドレミーに口を開けるように要求すると、%CALLNAME:MASTER%は溜めた唾液を垂らした
			PRINTFORMDW ドレミーは大人しく唾液を受け取ると、口内でくちゅくちゅと味わった後に嚥下した
		ELSEIF RAND:3 == 0
			CALL HPH_PRINT,@"「ぁっ…ちゅっHPHちゅぷっ…んっHPH」","L"
			PRINTFORMDW ドレミーの口内を味わい尽くすようにねっとりと舌を絡ませた
		ELSEIF RAND:2 == 0
			PRINTFORML 「ちゅぱっ…ん、ちゅっ」
			PRINTFORMDW 口でのセックスにドレミーはされるがままだ
		ELSE
			CALL HPH_PRINT,@"「もっと…んっ、ちゅっHPHれるっHPH」","L"
			PRINTFORMDW ドレミーは口の端から唾液が零れるのも構わず舌を絡ませてくる
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「ぁっ…ちゅっHPHちゅぷっ…んっHPH」","L"
			PRINTFORMDW ドレミーの口内を味わい尽くすようにねっとりと舌を絡ませた
		ELSEIF RAND:2 == 0
			PRINTFORML 「ちゅぱっ…ん、ちゅっ」
			PRINTFORMDW 口での濃厚なセックスにドレミーはされるがままだ
		ELSE
			CALL HPH_PRINT,@"「もっと…んっ、ちゅっHPHれるっHPH」","L"
			PRINTFORMDW ドレミーは口の端から唾液が零れるのも構わず舌を絡ませてくる
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「んん…はげしっ…ちゅぱっ…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ちゅっ…ずいぶん、手慣れていますね…」
		ELSE
			PRINTFORML 「んんっ！？…ふ、んぅ…」
			PRINTFORMDW 舌を吸い上げてやると、ドレミーは%CALLNAME:MASTER%の背中を弱々しく叩いてきた
		ENDIF
	ENDIF
ENDIF
RETURN 1

