;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_2(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P201人里の門
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P203南通り)
			RETURNF 3
	CASE P202中央広場
		SIF GROUPMATCH(ARG:1, P215花屋, P216食料品店, P220物見櫓, P227八百屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P203南通り, P204東通り, P205北通り, P206西通り, P207龍神像)
			RETURNF 3
	CASE P203南通り
		SIF GROUPMATCH(ARG:1, P204東通り, P211酒屋, P212カフェ, P214長屋前, P216食料品店, P218小料理屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P201人里の門, P202中央広場)
			RETURNF 3
	CASE P204東通り
		SIF GROUPMATCH(ARG:1, P203南通り, P205北通り, P213鈴奈庵, P217甘味所, P219集会所, P222寺子屋, P223銭湯, P224慧音宅, P227八百屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202中央広場)
			RETURNF 3
	CASE P205北通り
		SIF GROUPMATCH(ARG:1, P204東通り, P206西通り, P219集会所, P221稗田邸, P222寺子屋, P224慧音宅)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202中央広場, P207龍神像)
			RETURNF 3
	CASE P206西通り
		SIF GROUPMATCH(ARG:1, P205北通り, P214長屋前, P215花屋, P220物見櫓, P221稗田邸, P225連れ込み宿, P238鯢呑亭)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202中央広場)
			RETURNF 3
	CASE P207龍神像
		SIF GROUPMATCH(ARG:1, P202中央広場, P205北通り)
			RETURNF 3
	CASE P211酒屋, P212カフェ
		SIF GROUPMATCH(ARG:1, P203南通り)
			RETURNF 1
	CASE P213鈴奈庵
		SIF GROUPMATCH(ARG:1, P204東通り, P226小鈴私室)
			RETURNF 1
	CASE P214長屋前
		SIF GROUPMATCH(ARG:1, P203南通り, P206西通り)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P230空き部屋, P231蛮奇の部屋, P232蓮子の部屋, P233メリーの部屋, P234ユキの部屋, P235マイの部屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P208雷鼓の部屋, P209八橋の部屋, P210弁々の部屋, P241空き部屋, P236共同便所, P237共同井戸)
			RETURNF 1
	CASE P215花屋
		SIF GROUPMATCH(ARG:1, P202中央広場, P206西通り)
			RETURNF 1
	CASE P216食料品店
		SIF GROUPMATCH(ARG:1, P202中央広場, P203南通り)
			RETURNF 1
	CASE P217甘味所
		SIF GROUPMATCH(ARG:1, P204東通り)
			RETURNF 1
	CASE P218小料理屋
		SIF GROUPMATCH(ARG:1, P203南通り)
			RETURNF 1
	CASE P219集会所
		SIF GROUPMATCH(ARG:1, P204東通り, P205北通り)
			RETURNF 1
	CASE P220物見櫓
		SIF GROUPMATCH(ARG:1, P202中央広場, P206西通り)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P201人里の門, P203南通り, P204東通り, P205北通り, P207龍神像)
			RETURNF 2
	CASE P221稗田邸
		SIF GROUPMATCH(ARG:1, P205北通り, P206西通り, P229阿求私室)
			RETURNF 1
	CASE P222寺子屋
		SIF GROUPMATCH(ARG:1, P204東通り, P205北通り, P224慧音宅)
			RETURNF 1
	CASE P223銭湯
		SIF GROUPMATCH(ARG:1, P204東通り, P228貸切浴場)
			RETURNF 1
	CASE P224慧音宅
		SIF GROUPMATCH(ARG:1, P204東通り, P205北通り, P222寺子屋)
			RETURNF 1
	CASE P225連れ込み宿
		SIF GROUPMATCH(ARG:1, P206西通り)
			RETURNF 1
	CASE P226小鈴私室
		SIF GROUPMATCH(ARG:1, P213鈴奈庵)
			RETURNF 1
	CASE P227八百屋
		SIF GROUPMATCH(ARG:1, P202中央広場, P204東通り)
			RETURNF 1
	CASE P228貸切浴場
		SIF GROUPMATCH(ARG:1, P223銭湯)
			RETURNF 1
	CASE P229阿求私室
		SIF GROUPMATCH(ARG:1, P221稗田邸)
			RETURNF 1
	CASE P230空き部屋, P231蛮奇の部屋, P241空き部屋, P236共同便所, P237共同井戸
		SIF GROUPMATCH(ARG:1, P214長屋前)
			RETURNF 1
	CASE P232蓮子の部屋
		SIF GROUPMATCH(ARG:1, P214長屋前, P233メリーの部屋)
			RETURNF 1
	CASE P233メリーの部屋
		SIF GROUPMATCH(ARG:1, P214長屋前, P232蓮子の部屋)
			RETURNF 1
	CASE P234ユキの部屋
		SIF GROUPMATCH(ARG:1, P214長屋前, P235マイの部屋)
			RETURNF 1
	CASE P235マイの部屋
		SIF GROUPMATCH(ARG:1, P214長屋前, P234ユキの部屋)
			RETURNF 1
	CASE P208雷鼓の部屋
		SIF GROUPMATCH(ARG:1, P214長屋前, P209八橋の部屋, P210弁々の部屋)
			RETURNF 1
	CASE P209八橋の部屋
		SIF GROUPMATCH(ARG:1, P214長屋前, P208雷鼓の部屋, P210弁々の部屋)
			RETURNF 1
	CASE P210弁々の部屋
		SIF GROUPMATCH(ARG:1, P214長屋前, P209八橋の部屋, P208雷鼓の部屋)
			RETURNF 1
	CASE P238鯢呑亭
		SIF GROUPMATCH(ARG:1, P206西通り, P239美宵の部屋)
			RETURNF 1
	CASE P239美宵の部屋
		SIF GROUPMATCH(ARG:1, P238鯢呑亭)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P201人里の門
			RETURNF 1
ENDSELECT
RETURNF 0