;　　博　寺　里　湖　竹　森　川　山　守　地　月
;　　　　　　　　　　　　　　　　　　　　　　　
;博　－　30　20　50　40　50　70　40　60　20 120
;寺　30　－　10　40　20　40　60　70　90　50 110
;里　20　10　－　30　20　30　50　60　80　40 100
;湖　50　40　30　－　50　30　50　30　50　50　70
;竹　40　20　20　50　－　50　70　80 100　60 120
;森　50　40　30　30　50　－　20　60　80　70 100
;川　70　60　50　50　70　20　－　30　50　50　70
;山　80　70　60　30　80　60　30　－　20　20　40
;守 100　90　80　50 100　80　50　20　－　40　20
;地　20　50　40　50　60　70　50　20　40　－　60
;月 120 110 100　70 120 100　70　40　20　60　－
;山～森は川経由（手動）で行くと10分早い
;月は基本的に守+20
@SHORTCUT(ARG)
#FUNCTION
SELECTCASE ARG
	;守矢組
	CASE 1
		SIF GROUPMATCH(FLAG:デート相手, [[早苗]], [[神奈子]], [[諏訪子]]) || GROUPMATCH(TARGET, [[早苗]], [[神奈子]], [[諏訪子]])
			RETURNF 1
	;天界組
	CASE 2
		SIF GROUPMATCH(FLAG:デート相手, [[天子]], [[衣玖]]) || GROUPMATCH(TARGET, [[天子]], [[衣玖]])
			RETURNF 1
	;冥界組
	CASE 3
		SIF GROUPMATCH(FLAG:デート相手, [[妖夢]], [[紫]], [[映姫]], [[幽々子]], [[小町]], [[久侘歌]]) || GROUPMATCH(TARGET, [[妖夢]], [[紫]], [[映姫]], [[幽々子]], [[小町]], [[久侘歌]])
			RETURNF 1
	;天界（帰り）
	CASE 4
		IF CFLAG:MASTER:現在位置 == 天界
			SIF SHORTCUT(2)
				RETURNF 2
			RETURNF 1
		ENDIF
	;冥界（帰り）
	CASE 5
		IF INRANGE(CFLAG:MASTER:現在位置, 彼岸, 白玉楼)
			SIF SHORTCUT(3)
				RETURNF 2
			RETURNF 1
		ENDIF
	;竹林組
	CASE 6
		SIF GROUPMATCH(FLAG:デート相手, [[鈴仙]], [[妹紅]], [[輝夜]], [[影狼]], [[慧音]], [[幽香]], [[永琳]], [[メディスン]])
			RETURNF 1
		SIF GROUPMATCH(TARGET, [[鈴仙]], [[妹紅]], [[輝夜]], [[影狼]], [[慧音]], [[幽香]], [[永琳]], [[メディスン]])
			RETURNF 1
	;地獄・畜生組
	CASE 7
		SIF GROUPMATCH(FLAG:デート相手, [[魅魔]], [[映姫]], [[小町]], [[クラウンピース]], [[ヘカーティア]], [[久侘歌]], [[八千慧]], [[磨弓]], [[袿姫]], [[早鬼]])
			RETURNF 1
		SIF GROUPMATCH(TARGET, [[魅魔]], [[映姫]], [[小町]], [[クラウンピース]], [[ヘカーティア]], [[久侘歌]], [[八千慧]], [[磨弓]], [[袿姫]], [[早鬼]])
			RETURNF 1
	;地獄・畜生組（帰り）
	CASE 8
		IF INRANGE(CFLAG:MASTER:現在位置, 地獄, 霊長園)
			SIF SHORTCUT(7)
				RETURNF 2
			RETURNF 1
		ENDIF
ENDSELECT
RETURNF 0
