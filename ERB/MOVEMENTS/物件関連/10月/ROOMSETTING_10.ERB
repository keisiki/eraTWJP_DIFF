VARSET ROOMDATA
FOR LOCAL,1,100
	SIF GROUPMATCH(LOCAL, SPT鈴瑚宅, SPT清蘭宅)
		ROOMDATA:LOCAL |= 場所_ワンルーム

	SIF GROUPMATCH(LOCAL, SPTサグメ宅寝室, SPT空き家, SPTレイセン私室, SPT綿月亭空き部屋, SPT豊姫私室, SPT依姫私室, SPTドレミー宅寝室, SPTドレミー宅倉庫, SPT純狐邸空き部屋, SPTクラウンピース私室, SPT純狐私室, SPTヘカーティア私室)
		ROOMDATA:LOCAL |= 場所_布団

	SIF INRANGE(LOCAL, SPTサグメ宅居間, SPT八百屋)
		ROOMDATA:LOCAL |= 場所_屋内
	SIF INRANGE(LOCAL, SPT綿月亭門, SPT綿月亭空き部屋)
		ROOMDATA:LOCAL |= 場所_屋内
	SIF INRANGE(LOCAL, SPT四阿, SPT純狐邸風呂)
		ROOMDATA:LOCAL |= 場所_屋内
	SIF INRANGE(LOCAL, SPT純狐私室, SPTヘカーティア私室)
		ROOMDATA:LOCAL |= 場所_屋内

	SIF GROUPMATCH(LOCAL, SPT綿月亭風呂, SPTドレミー宅風呂, SPT純狐邸風呂)
		ROOMDATA:LOCAL |= 場所_風呂

	SIF GROUPMATCH(LOCAL, SPTサグメ宅居間, SPT空き家, SPT玉兎食堂, SPT厨房, SPTドレミー宅居間, SPT台所)
		ROOMDATA:LOCAL |= 場所_厨房

	SIF GROUPMATCH(LOCAL, SPT桃林, SPT砂浜, SPT入り江, SPT中庭, SPT屋上)
		ROOMDATA:LOCAL |= 場所_訓練

	SIF GROUPMATCH(LOCAL, SPTサグメ宅居間, SPT空き家, SPT玉兎詰め所, SPTレイセン私室, SPT綿月亭空き部屋, SPT四阿, SPT豊姫私室, SPT依姫私室, SPT書斎, SPTドレミー宅居間, SPT純狐邸空き部屋, SPT純狐私室, SPTヘカーティア私室)
