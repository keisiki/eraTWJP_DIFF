	;布団がある場所は自動設定
	SIF GROUPMATCH(LOCAL, SPT門番詰め所, SPT玉兎詰め所, SPT四阿, SPT応接間)
		ROOMDATA:LOCAL |= 場所_休憩

	SIF GROUPMATCH(LOCAL, SPTドレミー宅トイレ)
		ROOMDATA:LOCAL |= 場所_トイレ

	SIF GROUPMATCH(LOCAL, SPT月の都, SPT玉兎街, SPT広場, SPT桃林, SPT砂浜, SPT入り江, SPT中庭, SPT純狐邸中庭, SPT屋上)
		ROOMDATA:LOCAL |= 場所_青天井

;	SIF GROUPMATCH(LOCAL,)
;		ROOMDATA:LOCAL |= 場所_月見

	SIF GROUPMATCH(LOCAL, SPT四阿)
		ROOMDATA:LOCAL |= 場所_天の声

	SIF GROUPMATCH(LOCAL, SPT中庭)
		ROOMDATA:LOCAL |= 場所_菜園

;	SIF GROUPMATCH(LOCAL,)
;		ROOMDATA:LOCAL |= 場所_物干し

	SIF GROUPMATCH(LOCAL, SPT中庭, SPT純狐邸中庭)
		ROOMDATA:LOCAL |= 場所_水汲み

	SIF GROUPMATCH(LOCAL, SPT月の都, SPT玉兎街, SPT広場, SPT砂浜, SPT入り江, SPT一階回廊, SPT中庭, SPT二階回廊, SPT純狐邸中庭, SPT屋上)
		ROOMDATA:LOCAL |= 場所_見通し

	SIF GROUPMATCH(LOCAL, SPT食料庫, SPT宝物庫, SPTドレミー宅倉庫, SPTドレミー宅トイレ, SPT物置)
		ROOMDATA:LOCAL |= 場所_狭い

	;布団がある場所は自動設定
	SIF GROUPMATCH(LOCAL, SPT門番詰め所, SPT玉兎詰め所, SPT四阿, SPT応接間)
		ROOMDATA:LOCAL |= 場所_うたた寝_可能

	SIF GROUPMATCH(LOCAL, SPT綿月亭脱衣所, SPTドレミー宅洗面所, SPT純狐邸脱衣所)
		ROOMDATA:LOCAL |= 場所_身体測定

	SIF GROUPMATCH(LOCAL, SPT槐安通路, SPT玉兎街, SPT桃林, SPT綿月亭門, SPT綿月亭玄関, SPT一階回廊, SPT純狐邸玄関)
		ROOMDATA:LOCAL |= 場所_留まらない

	SIF GROUPMATCH(LOCAL, SPT砂浜, SPT門番詰め所, SPT玉兎詰め所, SPT食料庫, SPT宝物庫, SPTドレミー宅倉庫, SPT物置)
		ROOMDATA:LOCAL |= 場所_過疎

	SIF GROUPMATCH(LOCAL, SPTサグメ宅居間, SPTサグメ宅寝室, SPT空き家, SPT綿月亭脱衣所, SPT綿月亭風呂, SPT宝物庫, SPTレイセン私室, SPT綿月亭空き部屋, SPT豊姫私室, SPT依姫私室, SPTドレミー宅寝室, SPTドレミー宅風呂, SPTドレミー宅トイレ, SPT純狐邸空き部屋, SPTクラウンピース私室, SPT純狐邸脱衣所, SPT純狐邸風呂, SPT純狐私室, SPTヘカーティア私室)
		ROOMDATA:LOCAL |= 場所_プライベート

	SIF GROUPMATCH(LOCAL, SPT槐安通路, SPT月の都, SPT玉兎街, SPT広場, SPT食事処, SPT甘味処, SPT酒屋, SPT八百屋)
		ROOMDATA:LOCAL |= 場所_公共の場

	SIF GROUPMATCH(LOCAL, SPTサグメ宅居間, SPT食事処, SPT甘味処, SPT玉兎食堂, SPT食堂, SPTドレミー宅居間, SPT居間)
