;-----------------------------------------------------------
;地点関連性応答処理
;ARG		0(CASE指定):現在地点, 1:比較先地点
;RETURNF		1:移動可/見えない 2:移動不可/見える 3:移動可/見える
;-----------------------------------------------------------
@CAN_MOVE_10(ARG, ARG:1)
#FUNCTION
#DIM CONST SPT槐安通路 = 1001
#DIM CONST SPT月の都 = 1002
#DIM CONST SPT玉兎街 = 1003
#DIM CONST SPT広場 = 1004
#DIM CONST SPTサグメ宅居間 = 1005
#DIM CONST SPTサグメ宅寝室 = 1006
#DIM CONST SPT空き家 = 1007
#DIM CONST SPT鈴瑚宅 = 1008
#DIM CONST SPT清蘭宅 = 1009
#DIM CONST SPT食事処 = 1010
#DIM CONST SPT甘味処 = 1011
#DIM CONST SPT酒屋 = 1012
#DIM CONST SPT八百屋 = 1013
#DIM CONST SPT桃林 = 1014
#DIM CONST SPT砂浜 = 1015
#DIM CONST SPT入り江 = 1016

#DIM CONST SPT綿月亭門 = 1020
#DIM CONST SPT綿月亭玄関 = 1021
#DIM CONST SPT一階回廊 = 1022
#DIM CONST SPT門番詰め所 = 1023
#DIM CONST SPT玉兎食堂 = 1024
#DIM CONST SPT玉兎詰め所 = 1025
#DIM CONST SPT食堂 = 1026
#DIM CONST SPT厨房 = 1027
#DIM CONST SPT食料庫 = 1028
#DIM CONST SPT綿月亭脱衣所 = 1029
#DIM CONST SPT綿月亭風呂 = 1030
#DIM CONST SPT宝物庫 = 1031
#DIM CONST SPTレイセン私室 = 1032
#DIM CONST SPT綿月亭空き部屋 = 1033
#DIM CONST SPT中庭 = 1035
#DIM CONST SPT四阿 = 1036

#DIM CONST SPT二階回廊 = 1040
#DIM CONST SPT豊姫私室 = 1041
#DIM CONST SPT依姫私室 = 1042
#DIM CONST SPT書斎 = 1043
#DIM CONST SPT応接間 = 1044

#DIM CONST SPTドレミー宅居間 = 1050
#DIM CONST SPTドレミー宅寝室 = 1051
#DIM CONST SPTドレミー宅倉庫 = 1052
#DIM CONST SPTドレミー宅洗面所 = 1053
#DIM CONST SPTドレミー宅風呂 = 1054
#DIM CONST SPTドレミー宅トイレ = 1055

#DIM CONST SPT純狐邸玄関 = 1060
#DIM CONST SPT居間 = 1061
#DIM CONST SPT台所 = 1062
#DIM CONST SPT純狐邸空き部屋 = 1063
#DIM CONST SPTクラウンピース私室 = 1064
#DIM CONST SPT物置 = 1065
#DIM CONST SPT純狐邸脱衣所 = 1066
#DIM CONST SPT純狐邸風呂 = 1067
#DIM CONST SPT純狐邸中庭 = 1068
#DIM CONST SPT屋上 = 1070
#DIM CONST SPT純狐私室 = 1071
#DIM CONST SPTヘカーティア私室 = 1072

#DIM CONST SPT夢の向こう = 1073


SELECTCASE ARG
CASE SPT槐安通路
	SIF GROUPMATCH(ARG:1, SPT月の都, SPTドレミー宅居間)
		RETURNF 1
	SIF ARG:1 == SPT夢の向こう
		RETURNF 2
CASE SPT月の都
	SIF GROUPMATCH(ARG:1, SPT槐安通路, SPT玉兎街, SPTサグメ宅居間, SPT空き家)
		RETURNF 1
	SIF ARG:1 == SPT綿月亭門
		RETURNF 3
CASE SPT玉兎街
	SIF GROUPMATCH(ARG:1, SPT月の都, SPT鈴瑚宅, SPT清蘭宅, SPT食事処, SPT甘味処, SPT桃林)
		RETURNF 1
	SIF ARG:1 == SPT広場
		RETURNF 3
CASE SPT広場
	SIF GROUPMATCH(ARG:1, SPT食事処, SPT甘味処, SPT酒屋, SPT八百屋)
		RETURNF 1
	SIF ARG:1 == SPT玉兎街
		RETURNF 3
CASE SPTサグメ宅居間
	SIF GROUPMATCH(ARG:1, SPT月の都, SPTサグメ宅寝室)
		RETURNF 1
CASE SPTサグメ宅寝室
	SIF ARG:1 == SPTサグメ宅居間
		RETURNF 1
CASE SPT空き家
	SIF ARG:1 == SPT月の都
		RETURNF 1
CASE SPT鈴瑚宅
	SIF ARG:1 == SPT玉兎街
		RETURNF 1
CASE SPT清蘭宅
	SIF ARG:1 == SPT玉兎街
		RETURNF 1
CASE SPT食事処
	SIF GROUPMATCH(ARG:1, SPT玉兎街, SPT広場)
		RETURNF 1
CASE SPT甘味処
	SIF GROUPMATCH(ARG:1, SPT玉兎街, SPT広場)
		RETURNF 1
CASE SPT酒屋
	SIF ARG:1 == SPT広場
		RETURNF 1
CASE SPT八百屋
	SIF ARG:1 == SPT広場
		RETURNF 1
CASE SPT桃林
	SIF ARG:1 == SPT玉兎街
		RETURNF 1
	SIF ARG:1 == SPT砂浜
		RETURNF 3
CASE SPT砂浜
	SIF ARG:1 == SPT桃林
		RETURNF 3
	SIF ARG:1 == SPT入り江
		RETURNF 1
CASE SPT入り江
	SIF GROUPMATCH(ARG:1, SPT砂浜, SPT純狐邸玄関)
		RETURNF 1

CASE SPT綿月亭門
	SIF GROUPMATCH(ARG:1, SPT綿月亭玄関, SPT門番詰め所, SPT玉兎食堂)
		RETURNF 1
	SIF ARG:1 == SPT月の都
		RETURNF 3
CASE SPT綿月亭玄関
	SIF GROUPMATCH(ARG:1, SPT綿月亭門, SPT玉兎食堂, SPT食料庫)
		RETURNF 1
	SIF ARG:1 == SPT一階回廊
		RETURNF 3
CASE SPT一階回廊
	SIF GROUPMATCH(ARG:1, SPT食堂, SPT綿月亭脱衣所, SPT宝物庫, SPTレイセン私室, SPT綿月亭空き部屋, SPT二階回廊)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, SPT綿月亭玄関, SPT中庭)
		RETURNF 3
CASE SPT門番詰め所
	SIF ARG:1 == SPT綿月亭門
		RETURNF 1
CASE SPT玉兎食堂
	SIF GROUPMATCH(ARG:1, SPT綿月亭門, SPT綿月亭玄関, SPT玉兎詰め所)
		RETURNF 1
CASE SPT玉兎詰め所
	SIF ARG:1 == SPT玉兎食堂
		RETURNF 1
CASE SPT食堂
	SIF GROUPMATCH(ARG:1, SPT一階回廊, SPT厨房)
		RETURNF 1
CASE SPT厨房
	SIF GROUPMATCH(ARG:1, SPT食堂, SPT食料庫)
		RETURNF 1
CASE SPT食料庫
	SIF GROUPMATCH(ARG:1, SPT綿月亭玄関, SPT厨房)
		RETURNF 1
CASE SPT綿月亭脱衣所
	SIF GROUPMATCH(ARG:1, SPT一階回廊, SPT綿月亭風呂)
		RETURNF 1
CASE SPT綿月亭風呂
	SIF ARG:1 == SPT綿月亭脱衣所
		RETURNF 1
CASE SPT宝物庫
	SIF ARG:1 == SPT一階回廊
		RETURNF 1
CASE SPTレイセン私室
	SIF ARG:1 == SPT一階回廊
		RETURNF 1
CASE SPT綿月亭空き部屋
	SIF ARG:1 == SPT一階回廊
		RETURNF 1
CASE SPT中庭
	SIF GROUPMATCH(ARG:1, SPT一階回廊, SPT四阿)
		RETURNF 3
CASE SPT四阿
	SIF ARG:1 == SPT中庭
		RETURNF 3

CASE SPT二階回廊
	SIF GROUPMATCH(ARG:1, SPT一階回廊, SPT豊姫私室, SPT依姫私室, SPT書斎, SPT応接間)
		RETURNF 1
	SIF ARG:1 == SPT中庭
		RETURNF 2
CASE SPT豊姫私室
	SIF ARG:1 == SPT二階回廊
		RETURNF 1
CASE SPT依姫私室
	SIF ARG:1 == SPT二階回廊
		RETURNF 1
CASE SPT書斎
	SIF ARG:1 == SPT二階回廊
		RETURNF 1
CASE SPT応接間
	SIF ARG:1 == SPT二階回廊
		RETURNF 1

CASE SPTドレミー宅居間
	SIF GROUPMATCH(ARG:1, SPT槐安通路, SPTドレミー宅寝室, SPTドレミー宅倉庫, SPTドレミー宅洗面所)
		RETURNF 1
CASE SPTドレミー宅寝室
	SIF ARG:1 == SPTドレミー宅居間
		RETURNF 1
CASE SPTドレミー宅倉庫
	SIF ARG:1 == SPTドレミー宅居間
		RETURNF 1
CASE SPTドレミー宅洗面所
	SIF GROUPMATCH(ARG:1, SPTドレミー宅居間, SPTドレミー宅風呂, SPTドレミー宅トイレ)
		RETURNF 1
CASE SPTドレミー宅風呂
	SIF ARG:1 == SPTドレミー宅洗面所
		RETURNF 1
CASE SPTドレミー宅トイレ
	SIF ARG:1 == SPTドレミー宅洗面所
		RETURNF 1

CASE SPT純狐邸玄関
	SIF ARG:1 == SPT入り江
		RETURNF 1
	SIF ARG:1 == SPT居間
		RETURNF 3
CASE SPT居間
	SIF GROUPMATCH(ARG:1, SPT純狐邸空き部屋, SPTクラウンピース私室, SPT物置, SPT純狐邸脱衣所, SPT屋上)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, SPT純狐邸玄関, SPT台所, SPT純狐邸中庭)
		RETURNF 3
CASE SPT台所
	SIF ARG:1 == SPT居間
		RETURNF 3
CASE SPT純狐邸空き部屋
	SIF ARG:1 == SPT居間
		RETURNF 1
CASE SPTクラウンピース私室
	SIF ARG:1 == SPT居間
		RETURNF 1
CASE SPT物置
	SIF ARG:1 == SPT居間
		RETURNF 1
CASE SPT純狐邸脱衣所
	SIF GROUPMATCH(ARG:1, SPT居間, SPT純狐邸風呂)
		RETURNF 1
CASE SPT純狐邸風呂
	SIF ARG:1 == SPT純狐邸脱衣所
		RETURNF 1
CASE SPT純狐邸中庭
	SIF ARG:1 == SPT居間
		RETURNF 3
CASE SPT屋上
	SIF GROUPMATCH(ARG:1, SPT居間, SPT純狐私室, SPTヘカーティア私室)
		RETURNF 1
	SIF ARG:1 == SPT純狐邸中庭
		RETURNF 2
CASE SPT純狐私室
	SIF ARG:1 == SPT屋上
		RETURNF 1
CASE SPTヘカーティア私室
	SIF ARG:1 == SPT屋上
		RETURNF 1

CASE SPT夢の向こう
	SIF ARG:1 == SPT槐安通路
		RETURNF 1

ENDSELECT

RETURNF 0