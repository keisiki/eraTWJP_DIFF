@KICKOUT_10()
#FUNCTION
SELECTCASE CHK_INHOME(CFLAG:MASTER:現在位置)
	CASE 30
		RETURNF 1029
	CASE 54
		RETURNF 1053
	CASE 67
		RETURNF 1066
ENDSELECT

@BATHROOM_10()
#FUNCTION
SELECTCASE CHK_INHOME(CFLAG:MASTER:現在位置)
	CASE 20 TO 49
		RETURNF 30
	CASE 50 TO 59
		RETURNF 54
	CASE 60 TO 72
		RETURNF 67
ENDSELECT
RETURNF 0

@BATHCHECK_10(ARG)
#FUNCTION
SELECTCASE CHK_INHOME(ARG)
	CASE 30, 54, 67
		RETURNF 1
	CASEELSE
		RETURNF 0
ENDSELECT
RETURNF 0

@MAP_UNIQUEMESSAGE_10
IF WITH_MOB()
	IF FLAG:時間停止
		IF GROUPMATCH(CFLAG:MASTER:現在位置, 20, 23)
			PRINTFORML 門番達が停止している
		ELSE
			PRINTFORML 玉兎達が停止している
		ENDIF
	ELSE
		SELECTCASE CFLAG:MASTER:現在位置
		;玉兎街,広場
		CASE 1003, 1004
			PRINTFORML 玉兎達で賑わっている
		;桃林
		CASE 1014
			PRINTFORML 玉兎達が訓練している
		;綿月亭 門
		CASE 1020
			PRINTFORML 門番が見張っている
		;門番詰め所
		CASE 1023
			PRINTFORML 門番達が休息している
		;玉兎食堂
		CASE 1024
			PRINTFORML 玉兎達が食事をしている
		;玉兎詰め所
		CASE 1025
			PRINTFORML 玉兎達が休息している
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

;-----------------------------------------
;非自宅時に差し替える場所IDを返す関数
;ARG,対象ID
;RETURNF,差し替え済み対象ID
;-----------------------------------------

@MAP_SEARCH_REPLACEMENT_10(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 1001, 1050 TO 1059
	;槐安通路
		RETURNF 1010
	CASE 1002, 1005 TO 1007
	;月の都
		RETURNF 1050
	CASE 1020 TO 1049
	;綿月亭
		RETURNF 1060
	CASE 1003, 1004, 1008 TO 1013
	;玉兎の街
		RETURNF 1070
	CASE 1014, 1015
	;桃林～砂浜
		RETURNF 1080
	CASEELSE
	;静かの海
		RETURNF 1090
ENDSELECT
RETURNF 0



@MAP10_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 10
	SIF INRANGE(PLACE_ID, 1020, 1049)
		RETURNF "綿月亭"
	SIF INRANGE(PLACE_ID, 1050, 1059)
		RETURNF "ドレミー宅"
	SIF INRANGE(PLACE_ID, 1060, 1072)
		RETURNF "純狐邸"
	RETURNF "月の世界"
ELSE
	SELECTCASE PLACE_ID
	CASE 1010
		RETURNF "槐安通路"
	CASE 1050, 1060, 1070
		RETURNF "月の都"
	CASE 1080, 1090
		RETURNF "月都郊外"
	CASEELSE
		RETURNF "月面"
	ENDSELECT
ENDIF
