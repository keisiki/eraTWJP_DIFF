;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_1(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P101山門
		SIF GROUPMATCH(ARG:1, P102境内, P140お地蔵通り)
			RETURNF 3
	CASE P102境内
		SIF GROUPMATCH(ARG:1, P104講堂, P105経蔵, P107土間, P119離れ, P121墓地, P137マミゾウの棲家)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P101山門, P122梵鐘楼)
			RETURNF 3
	CASE P103本堂
		SIF GROUPMATCH(ARG:1, P104講堂)
			RETURNF 3
	CASE P104講堂
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P103本堂)
			RETURNF 3
	CASE P105経蔵
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 1
	CASE P106裏庭
		SIF GROUPMATCH(ARG:1, P120物置, P121墓地)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P110縁側)
			RETURNF 3
	CASE P107土間
		SIF GROUPMATCH(ARG:1, P102境内, P108居間, P112風呂)
			RETURNF 1
	CASE P108居間
		SIF GROUPMATCH(ARG:1, P107土間, P109女苑私室, P135廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P111台所)
			RETURNF 3
	CASE P109女苑私室
		SIF GROUPMATCH(ARG:1, P108居間, P118白蓮私室, P135廊下)
			RETURNF 1
	CASE P110縁側
		SIF GROUPMATCH(ARG:1, P113ナズーリン私室, P115星私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P106裏庭, P135廊下)
			RETURNF 3
	CASE P111台所
		SIF GROUPMATCH(ARG:1, P108居間)
			RETURNF 3
	CASE P112風呂
		SIF GROUPMATCH(ARG:1, P107土間)
			RETURNF 1
	CASE P113ナズーリン私室
		SIF GROUPMATCH(ARG:1, P110縁側, P116一輪私室, P135廊下)
			RETURNF 1
	CASE P114ムラサ私室
		SIF GROUPMATCH(ARG:1, P116一輪私室, P117ぬえ私室, P135廊下)
			RETURNF 1
	CASE P115星私室
		SIF GROUPMATCH(ARG:1, P110縁側, P118白蓮私室, P135廊下)
			RETURNF 1
	CASE P116一輪私室
		SIF GROUPMATCH(ARG:1, P113ナズーリン私室, P114ムラサ私室, P135廊下)
			RETURNF 1
	CASE P117ぬえ私室
		SIF GROUPMATCH(ARG:1, P114ムラサ私室, P136響子私室, P135廊下)
			RETURNF 1
	CASE P118白蓮私室
		SIF GROUPMATCH(ARG:1, P109女苑私室, P115星私室, P135廊下)
			RETURNF 1
	CASE P119離れ
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P112風呂)
			RETURNF 2
	CASE P120物置
		SIF GROUPMATCH(ARG:1, P106裏庭)
			RETURNF 1
	CASE P121墓地
		SIF GROUPMATCH(ARG:1, P102境内, P106裏庭)
			RETURNF 1
	CASE P122梵鐘楼
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 3

	CASE P123霊廟大門
		SIF GROUPMATCH(ARG:1, P124霊廟広場, P140お地蔵通り)
			RETURNF 3
	CASE P124霊廟広場
		SIF GROUPMATCH(ARG:1, P126道場, P133湯殿, P134蔵, P138鐘楼, P139邪仙楼)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P123霊廟大門)
			RETURNF 3
	CASE P125屠自古私室
		SIF GROUPMATCH(ARG:1, P130廊下)
			RETURNF 1
	CASE P126道場
		SIF GROUPMATCH(ARG:1, P124霊廟広場, P130廊下)
			RETURNF 1
	CASE P127布都私室
		SIF GROUPMATCH(ARG:1, P130廊下)
			RETURNF 1
	CASE P128神子私室
		SIF GROUPMATCH(ARG:1, P130廊下)
			RETURNF 1
	CASE P129客間
		SIF GROUPMATCH(ARG:1, P130廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P131枯山水)
			RETURNF 3
	CASE P130廊下
		SIF GROUPMATCH(ARG:1, P126道場, P125屠自古私室, P127布都私室, P128神子私室, P129客間, P132調薬室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P131枯山水)
			RETURNF 3
	CASE P131枯山水
		SIF GROUPMATCH(ARG:1, P129客間, P130廊下)
			RETURNF 3
	CASE P132調薬室
		SIF GROUPMATCH(ARG:1, P130廊下)
			RETURNF 1
	CASE P133湯殿
		SIF GROUPMATCH(ARG:1, P124霊廟広場)
			RETURNF 1
	CASE P134蔵
		SIF GROUPMATCH(ARG:1, P124霊廟広場)
			RETURNF 1

	CASE P135廊下
		SIF GROUPMATCH(ARG:1, P108居間, P109女苑私室, P113ナズーリン私室, P114ムラサ私室, P115星私室, P116一輪私室, P117ぬえ私室, P118白蓮私室, P136響子私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P110縁側)
			RETURNF 3
	CASE P136響子私室
		SIF GROUPMATCH(ARG:1, P117ぬえ私室, P135廊下)
			RETURNF 1
	CASE P137マミゾウの棲家
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 1
	CASE P138鐘楼
		SIF GROUPMATCH(ARG:1, P124霊廟広場)
			RETURNF 1
	CASE P139邪仙楼
		SIF GROUPMATCH(ARG:1, P124霊廟広場)
			RETURNF 1
	CASE P140お地蔵通り
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P101山門, P123霊廟大門)
			RETURNF 3

	CASE MAXROOM
		SIF ARG:1 == P140お地蔵通り
			RETURNF 1
ENDSELECT
RETURNF 0