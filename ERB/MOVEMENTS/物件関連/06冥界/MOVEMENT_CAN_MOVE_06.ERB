;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_6(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P601中有の道
	SIF GROUPMATCH(ARG:1, P602賽の河原, P630八雲邸玄関)
		RETURNF 1
CASE P602賽の河原
	SIF GROUPMATCH(ARG:1, P603積石塚)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P601中有の道, P603積石塚, P604彼岸, P608潤美の家, P630八雲邸玄関)
		RETURNF 1
CASE P603積石塚
	SIF GROUPMATCH(ARG:1, P602賽の河原)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P602賽の河原, P604彼岸, P609瓔花の家)
		RETURNF 1
CASE P604彼岸
	SIF GROUPMATCH(ARG:1, P602賽の河原, P603積石塚, P610是非曲直庁ホール, P620冥界の門, P640異界の関所)
		RETURNF 1
CASE P608潤美の家
	SIF GROUPMATCH(ARG:1, P602賽の河原)
		RETURNF 1
CASE P609瓔花の家
	SIF GROUPMATCH(ARG:1, P603積石塚)
		RETURNF 1
CASE P610是非曲直庁ホール
	SIF GROUPMATCH(ARG:1, P604彼岸, P612法廷, P613是非曲直庁廊下)
		RETURNF 1
CASE P612法廷
	SIF GROUPMATCH(ARG:1, P610是非曲直庁ホール, P613是非曲直庁廊下)
		RETURNF 1
CASE P613是非曲直庁廊下
	SIF GROUPMATCH(ARG:1, P610是非曲直庁ホール, P612法廷, P614是非曲直庁ラウンジ, P616小町の部屋, P617風呂, P618空き部屋)
		RETURNF 1
CASE P614是非曲直庁ラウンジ
	SIF GROUPMATCH(ARG:1, P613是非曲直庁廊下, P615映姫の部屋)
		RETURNF 1
CASE P615映姫の部屋
	SIF GROUPMATCH(ARG:1, P614是非曲直庁ラウンジ)
		RETURNF 1
CASE P616小町の部屋
	SIF GROUPMATCH(ARG:1, P613是非曲直庁廊下)
		RETURNF 1
CASE P617風呂
	SIF GROUPMATCH(ARG:1, P613是非曲直庁廊下)
		RETURNF 1
CASE P618空き部屋
	SIF GROUPMATCH(ARG:1, P613是非曲直庁廊下)
		RETURNF 1
CASE P620冥界の門
	SIF GROUPMATCH(ARG:1, P621庭)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P604彼岸)
		RETURNF 1
CASE P621庭
	SIF GROUPMATCH(ARG:1, P620冥界の門, P628居間)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P622廊下, P629西行妖)
		RETURNF 1
CASE P622廊下
	SIF GROUPMATCH(ARG:1, P621庭, P623幽々子私室, P624妖夢私室, P625空室, P626台所, P627風呂, P628居間)
		RETURNF 1
CASE P623幽々子私室
	SIF GROUPMATCH(ARG:1, P629西行妖)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P622廊下, P628居間)
		RETURNF 1
CASE P624妖夢私室
	SIF GROUPMATCH(ARG:1, P622廊下)
		RETURNF 1
CASE P625空室
	SIF GROUPMATCH(ARG:1, P622廊下)
		RETURNF 1
CASE P626台所
	SIF GROUPMATCH(ARG:1, P628居間)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P622廊下)
		RETURNF 1
CASE P627風呂
	SIF GROUPMATCH(ARG:1, P622廊下)
		RETURNF 1
CASE P628居間
	SIF GROUPMATCH(ARG:1, P621庭, P626台所)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P622廊下, P623幽々子私室)
		RETURNF 1
CASE P629西行妖
	SIF GROUPMATCH(ARG:1, P623幽々子私室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P621庭)
		RETURNF 1
CASE P630八雲邸玄関
	SIF GROUPMATCH(ARG:1, P601中有の道, P602賽の河原, P631八雲邸廊下, P632八雲邸庭, P634空部屋, P635橙私室)
		RETURNF 1
CASE P631八雲邸廊下
	SIF GROUPMATCH(ARG:1, P630八雲邸玄関, P633紫私室, P634空部屋, P635橙私室, P636八雲邸風呂, P637八雲邸台所, P638藍私室, P639八雲邸居間)
		RETURNF 1
CASE P632八雲邸庭
	SIF GROUPMATCH(ARG:1, P630八雲邸玄関, P633紫私室, P638藍私室, P639八雲邸居間)
		RETURNF 1
CASE P633紫私室
	SIF GROUPMATCH(ARG:1, P631八雲邸廊下, P632八雲邸庭, P639八雲邸居間)
		RETURNF 1
CASE P634空部屋
	SIF GROUPMATCH(ARG:1, P630八雲邸玄関, P631八雲邸廊下, P635橙私室)
		RETURNF 1
CASE P635橙私室
	SIF GROUPMATCH(ARG:1, P630八雲邸玄関, P631八雲邸廊下, P634空部屋)
		RETURNF 1
CASE P636八雲邸風呂
	SIF GROUPMATCH(ARG:1, P631八雲邸廊下)
		RETURNF 1
CASE P637八雲邸台所
	SIF GROUPMATCH(ARG:1, P631八雲邸廊下)
		RETURNF 1
CASE P638藍私室
	SIF GROUPMATCH(ARG:1, P631八雲邸廊下, P632八雲邸庭, P639八雲邸居間)
		RETURNF 1
CASE P639八雲邸居間
	SIF GROUPMATCH(ARG:1, P631八雲邸廊下, P632八雲邸庭, P633紫私室, P638藍私室)
		RETURNF 1
CASE P640異界の関所
	SIF GROUPMATCH(ARG:1, P641畜生メトロポリス)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P604彼岸)
		RETURNF 1
CASE P641畜生メトロポリス
	SIF GROUPMATCH(ARG:1, P640異界の関所)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P642霊長園, P647八千慧宅のリビング, P650早鬼宅のリビング, P653尤魔宅のリビング)
		RETURNF 1
CASE P642霊長園
	SIF GROUPMATCH(ARG:1, P641畜生メトロポリス, P643霊長園の風呂, P644袿姫の部屋, P645磨弓の部屋)
		RETURNF 1
CASE P643霊長園の風呂 TO P645磨弓の部屋
	SIF GROUPMATCH(ARG:1, P642霊長園)
		RETURNF 1
CASE P646八千慧宅の風呂
	SIF GROUPMATCH(ARG:1, P647八千慧宅のリビング)
		RETURNF 1
CASE P647八千慧宅のリビング
	SIF GROUPMATCH(ARG:1, P641畜生メトロポリス, P646八千慧宅の風呂, P648八千慧宅の寝室)
		RETURNF 1
CASE P648八千慧宅の寝室
	SIF GROUPMATCH(ARG:1, P647八千慧宅のリビング)
		RETURNF 1
CASE P650早鬼宅のリビング
	SIF GROUPMATCH(ARG:1, P641畜生メトロポリス, P651早鬼宅の寝室, P652早鬼宅の風呂)
		RETURNF 1
CASE P651早鬼宅の寝室, P652早鬼宅の風呂
	SIF GROUPMATCH(ARG:1, P650早鬼宅のリビング)
		RETURNF 1
CASE P653尤魔宅のリビング
	SIF GROUPMATCH(ARG:1, P641畜生メトロポリス, P654尤魔宅の寝室, P655尤魔宅の風呂)
		RETURNF 1
CASE P654尤魔宅の寝室, P655尤魔宅の風呂
	SIF GROUPMATCH(ARG:1, P653尤魔宅のリビング)
		RETURNF 1
CASE MAXROOM
	SIF ARG:1 == P601中有の道
		RETURNF 1
ENDSELECT

RETURNF 0