;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_7(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P701樹海道入り口
	SIF GROUPMATCH(ARG:1, P734小屋)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
	SIF GROUPMATCH(ARG:1, P702山道・沢へ続く道)
		RETURNF 3
CASE P702山道・沢へ続く道
	SIF GROUPMATCH(ARG:1, P715秋姉妹の家・畑, P716秋姉妹の家・土間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P701樹海道入り口, P703玄武の沢)
		RETURNF 3
CASE P703玄武の沢
	SIF GROUPMATCH(ARG:1, P704河童のアジト, P709リリーたちの家, P735水浴び場, P738森の深奥)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P702山道・沢へ続く道, P705山道・中腹)
		RETURNF 3
CASE P704河童のアジト, P709リリーたちの家, P735水浴び場
	SIF GROUPMATCH(ARG:1, P703玄武の沢)
		RETURNF 1
CASE P705山道・中腹
	SIF GROUPMATCH(ARG:1, P703玄武の沢, P707山道・分岐点)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P733正邪の隠れ家)
		RETURNF 1
CASE P707山道・分岐点
	SIF GROUPMATCH(ARG:1, P706厄神の家, P708大蝦蟇の池, P710猫の里, P711休憩所, P731帚木)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P705山道・中腹)
		RETURNF 3
CASE P711休憩所
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P712間欠泉地下センター, P732山頂への道, P736山姥の家)
		RETURNF 1
CASE P732山頂への道
	SIF GROUPMATCH(ARG:1, P711休憩所, P712間欠泉地下センター, P736山姥の家)
		RETURNF 1
CASE P734小屋
	SIF GROUPMATCH(ARG:1, P701樹海道入り口)
		RETURNF 1
CASE P733正邪の隠れ家
	SIF GROUPMATCH(ARG:1, P705山道・中腹)
		RETURNF 1
CASE P708大蝦蟇の池
	SIF GROUPMATCH(ARG:1, P707山道・分岐点)
		RETURNF 1
CASE P706厄神の家
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P710猫の里)
		RETURNF 1
CASE P710猫の里
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P706厄神の家, P737招き猫の家)
		RETURNF 1
CASE P737招き猫の家
	SIF GROUPMATCH(ARG:1, P710猫の里)
		RETURNF 1
CASE P712間欠泉地下センター, P736山姥の家
	SIF GROUPMATCH(ARG:1, P711休憩所, P732山頂への道)
		RETURNF 1

CASE P716秋姉妹の家・土間
	SIF GROUPMATCH(ARG:1, P702山道・沢へ続く道, P715秋姉妹の家・畑)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P718秋姉妹の家・居間)
		RETURNF 3
CASE P718秋姉妹の家・居間
	SIF GROUPMATCH(ARG:1, P717秋姉妹の家・寝室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P716秋姉妹の家・土間, P719秋姉妹の家・座敷)
		RETURNF 3
CASE P717秋姉妹の家・寝室
	SIF GROUPMATCH(ARG:1, P718秋姉妹の家・居間, P719秋姉妹の家・座敷)
		RETURNF 1
CASE P719秋姉妹の家・座敷
	SIF GROUPMATCH(ARG:1, P717秋姉妹の家・寝室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P715秋姉妹の家・畑, P718秋姉妹の家・居間)
		RETURNF 3
CASE P715秋姉妹の家・畑
	SIF GROUPMATCH(ARG:1, P702山道・沢へ続く道, P716秋姉妹の家・土間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P719秋姉妹の家・座敷)
		RETURNF 3
;仙人の屋敷華扇ちゃん！
CASE P731帚木
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P730仙人の屋敷・庭)
		RETURNF 1
CASE P730仙人の屋敷・庭
	SIF GROUPMATCH(ARG:1, P731帚木)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P729仙人の屋敷・玄関)
		RETURNF 3
CASE P729仙人の屋敷・玄関
	SIF GROUPMATCH(ARG:1, P720仙人の屋敷・台所, P721仙人の屋敷・風呂, P722仙人の屋敷・道場, P723仙人の屋敷・二階客間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P730仙人の屋敷・庭)
		RETURNF 3
CASE P721仙人の屋敷・風呂, P722仙人の屋敷・道場, P720仙人の屋敷・台所
	SIF GROUPMATCH(ARG:1, P729仙人の屋敷・玄関)
		RETURNF 1
CASE P723仙人の屋敷・二階客間
	SIF GROUPMATCH(ARG:1, P729仙人の屋敷・玄関, P726仙人の屋敷・居間, P724仙人の屋敷・書斎, P725仙人の屋敷・大鷲の部屋)
		RETURNF 1
CASE P724仙人の屋敷・書斎
	SIF GROUPMATCH(ARG:1, P723仙人の屋敷・二階客間)
		RETURNF 1
CASE P725仙人の屋敷・大鷲の部屋
	SIF GROUPMATCH(ARG:1, P723仙人の屋敷・二階客間)
		RETURNF 1
	;例外的に大鷲の部屋から庭にジャンプできる
	SIF GROUPMATCH(ARG:1, P730仙人の屋敷・庭)
		RETURNF 3
CASE P726仙人の屋敷・居間
	SIF GROUPMATCH(ARG:1, P723仙人の屋敷・二階客間, P727仙人の屋敷・華扇私室, P728仙人の屋敷・空き部屋)
		RETURNF 1
CASE P727仙人の屋敷・華扇私室, P728仙人の屋敷・空き部屋
	SIF GROUPMATCH(ARG:1, P726仙人の屋敷・居間)
		RETURNF 1
;深奥エリア
CASE P738森の深奥
	SIF GROUPMATCH(ARG:1, P703玄武の沢, P740山童のアジト)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P739秘天崖)
		RETURNF 3
CASE P739秘天崖
	SIF GROUPMATCH(ARG:1, P738森の深奥)
		RETURNF 3
CASE P740山童のアジト
	SIF GROUPMATCH(ARG:1, P738森の深奥)
		RETURNF 1

CASE MAXROOM
	SIF ARG:1 == P701樹海道入り口
		RETURNF 1
ENDSELECT

RETURNF 0