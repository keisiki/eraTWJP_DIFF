;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_0(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P1鳥居
		SIF GROUPMATCH(ARG:1, P2境内, P22参道)
			RETURNF 3
		SIF ARG:1 == MAXROOM
			RETURNF 2
	CASE P2境内
		SIF GROUPMATCH(ARG:1, P6物置, P16ボロ小屋, P20本殿の裏, P56あうんの棲家)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P1鳥居, P3賽銭箱, P5手水舎, P21守矢神社分社)
			RETURNF 3
	CASE P3賽銭箱
		SIF GROUPMATCH(ARG:1, P4本堂)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 3
	CASE P4本堂
		SIF GROUPMATCH(ARG:1, P3賽銭箱)
			RETURNF 1
	CASE P5手水舎
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 3
	CASE P6物置
		SIF GROUPMATCH(ARG:1, P2境内, P20本殿の裏)
			RETURNF 1
	CASE P7土間
		SIF GROUPMATCH(ARG:1, P9居間, P20本殿の裏)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P8台所)
			RETURNF 3
	CASE P8台所
		SIF GROUPMATCH(ARG:1, P9居間, P10縁側)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P7土間)
			RETURNF 3
	CASE P9居間
		SIF GROUPMATCH(ARG:1, P7土間, P8台所, P15霊夢私室, P17小さい納戸, P53納戸, P54小人の虫かご, P55和室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P10縁側)
			RETURNF 3
	CASE P10縁側
		SIF GROUPMATCH(ARG:1, P8台所, P12脱衣所, P14トイレ, P19大きい納戸, P54小人の虫かご)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P9居間, P11庭)
			RETURNF 3
	CASE P11庭
		SIF GROUPMATCH(ARG:1, P20本殿の裏, P52離れ)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P10縁側)
			RETURNF 3
	CASE P12脱衣所
		SIF GROUPMATCH(ARG:1, P10縁側, P13風呂)
			RETURNF 1
	CASE P13風呂
		SIF GROUPMATCH(ARG:1, P12脱衣所)
			RETURNF 1
	CASE P14トイレ
		SIF GROUPMATCH(ARG:1, P10縁側)
			RETURNF 1
	CASE P15霊夢私室
		SIF GROUPMATCH(ARG:1, P9居間, P17小さい納戸, P18押入れ)
			RETURNF 1
	CASE P16ボロ小屋
		SIF GROUPMATCH(ARG:1, P2境内, P20本殿の裏)
			RETURNF 1
	CASE P17小さい納戸
		SIF GROUPMATCH(ARG:1, P9居間)
			RETURNF 1
	CASE P18押入れ
		SIF GROUPMATCH(ARG:1, P15霊夢私室)
			RETURNF 1
	CASE P19大きい納戸
		SIF GROUPMATCH(ARG:1, P10縁側)
			RETURNF 1
	CASE P20本殿の裏
		SIF GROUPMATCH(ARG:1, P6物置, P7土間, P11庭, P16ボロ小屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 3
	CASE P21守矢神社分社
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 3
	CASE P22参道
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P1鳥居, P36夢幻遺跡前)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P23温泉・脱衣所, P25鎮守の森)
			RETURNF 1
	CASE P23温泉・脱衣所
		SIF GROUPMATCH(ARG:1, P22参道, P24温泉, P25鎮守の森)
			RETURNF 1
	CASE P24温泉
		SIF GROUPMATCH(ARG:1, P23温泉・脱衣所)
			RETURNF 1
	CASE P25鎮守の森
		SIF GROUPMATCH(ARG:1, P22参道)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P23温泉・脱衣所, P27三妖精の家・広間)
			RETURNF 1

	CASE P27三妖精の家・広間
		SIF GROUPMATCH(ARG:1, P25鎮守の森, P30三妖精の家・風呂, P31三妖精の家・二階廊下, P35三妖精の家・地下室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P29三妖精の家・台所)
			RETURNF 3
	CASE P29三妖精の家・台所
		SIF GROUPMATCH(ARG:1, P27三妖精の家・広間)
			RETURNF 3
	CASE P30三妖精の家・風呂
		SIF GROUPMATCH(ARG:1, P27三妖精の家・広間)
			RETURNF 1
	CASE P31三妖精の家・二階廊下
		SIF GROUPMATCH(ARG:1, P27三妖精の家・広間, P32サニー私室, P33ルナ私室, P34スター私室)
			RETURNF 1
	CASE P32サニー私室, P33ルナ私室, P34スター私室
		SIF GROUPMATCH(ARG:1, P31三妖精の家・二階廊下)
			RETURNF 1
	CASE P35三妖精の家・地下室
		SIF GROUPMATCH(ARG:1, P27三妖精の家・広間)
			RETURNF 1

	CASE P36夢幻遺跡前
		SIF GROUPMATCH(ARG:1, P22参道)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路)
			RETURNF 1
	CASE P37夢幻遺跡・一階通路
		SIF GROUPMATCH(ARG:1, P36夢幻遺跡前, P38夢幻遺跡・洗面所, P41夢幻遺跡・食堂兼談話室, P43夢美私室, P44ちゆり私室, P46夢幻遺跡・二階通路)
			RETURNF 1
	CASE P38夢幻遺跡・洗面所
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路, P39夢幻遺跡・浴室, P40夢幻遺跡・トイレ)
			RETURNF 1
	CASE P39夢幻遺跡・浴室, P40夢幻遺跡・トイレ
		SIF GROUPMATCH(ARG:1, P38夢幻遺跡・洗面所)
			RETURNF 1
	CASE P41夢幻遺跡・食堂兼談話室
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P42夢幻遺跡・調理室)
			RETURNF 3
	CASE P42夢幻遺跡・調理室
		SIF GROUPMATCH(ARG:1, P41夢幻遺跡・食堂兼談話室)
			RETURNF 3
	CASE P43夢美私室, P44ちゆり私室
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路)
			RETURNF 1
	CASE P46夢幻遺跡・二階通路
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路, P47夢幻遺跡・艦橋, P48夢幻遺跡・第一資料室, P49夢幻遺跡・研究室, P50夢幻遺跡・第二資料室, P51夢幻遺跡・倉庫)
			RETURNF 1
	CASE P47夢幻遺跡・艦橋
		SIF GROUPMATCH(ARG:1, P46夢幻遺跡・二階通路)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P36夢幻遺跡前)
			RETURNF 2
	CASE P48夢幻遺跡・第一資料室, P49夢幻遺跡・研究室, P50夢幻遺跡・第二資料室, P51夢幻遺跡・倉庫
		SIF GROUPMATCH(ARG:1, P46夢幻遺跡・二階通路)
			RETURNF 1

	CASE P52離れ
		SIF GROUPMATCH(ARG:1, P11庭)
			RETURNF 1
	CASE P53納戸, P55和室
		SIF GROUPMATCH(ARG:1, P9居間)
			RETURNF 1
	CASE P54小人の虫かご
		SIF GROUPMATCH(ARG:1, P9居間, P10縁側)
			RETURNF 1
	CASE P56あうんの棲家
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P22参道
			RETURNF 1
ENDSELECT
RETURNF 0