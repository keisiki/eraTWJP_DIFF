@DRAW_MAP_BG_0(SLICED)
#DIMS SLICED
SELECTCASE SLICED
;色つけたいときはこんな感じ
CASE "木","竹","林","森"
	SETCOLOR 0x90EE90
	PRINTS SLICED
	RESETCOLOR
;違う文字を表示させる例
CASE "#W"
	SETCOLOR 0x90EE90
	PRINT 木
	RESETCOLOR
CASE "#X"
	PRINT //
CASE "#E"
	SIF TALENT:MASTER:居場所察知 && COUNTCHARA:(MAXROOM) > 0 && FLAG:察知モード == 1
		SETCOLOR 255,100,200
	PRINT ∥
	RESETCOLOR
CASE "#A"
	IF HATAKE_NAME:0 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 　
	ENDIF
CASE "#B"
	IF HATAKE_NAME:1 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 　
	ENDIF
CASE "#C"
	IF HATAKE_NAME:2 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 　
	ENDIF
CASE "#D"
	IF HATAKE_NAME:3 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 　
	ENDIF
;鳥居用
;┳━┳
;╋━╋
;┃  ┃
CASE "#H"
	SETCOLOR C_RED
	PRINT ━
	RESETCOLOR
CASE "#I"
	SETCOLOR C_RED
	PRINT ┳
	RESETCOLOR
CASE "#J"
	SETCOLOR C_RED
	PRINT ┃
	RESETCOLOR
CASE "#K"
	SETCOLOR C_RED
	PRINT ╋
	RESETCOLOR

;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT