;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_4(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P401永遠亭入口
		SIF GROUPMATCH(ARG:1, P403待合室, P405調剤室, P433斜角の竹林, P435井戸)
			RETURNF 1
	CASE P403待合室
		SIF GROUPMATCH(ARG:1, P401永遠亭入口, P404診察室, P406来客用トイレ, P407廊下)
			RETURNF 1
	CASE P404診察室
		SIF GROUPMATCH(ARG:1, P403待合室, P405調剤室)
			RETURNF 1
	CASE P405調剤室
		SIF GROUPMATCH(ARG:1, P401永遠亭入口, P404診察室, P417台所, P422裏庭)
			RETURNF 1
	CASE P406来客用トイレ
		SIF GROUPMATCH(ARG:1, P403待合室)
			RETURNF 1
	CASE P407廊下
		SIF GROUPMATCH(ARG:1, P403待合室, P408中廊下, P409縁側, P418厠, P419脱衣所, P426二階廊下, P430地下室)
			RETURNF 1
	CASE P408中廊下
		SIF GROUPMATCH(ARG:1, P407廊下, P410てゐ私室, P411鈴仙私室, P412空き部屋, P413永琳私室, P414輝夜私室, P415広間, P416居間)
			RETURNF 1
	CASE P409縁側
		SIF GROUPMATCH(ARG:1, P407廊下, P412空き部屋, P413永琳私室, P414輝夜私室, P416居間, P424物置)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P421庭)
			RETURNF 3
	CASE P410てゐ私室
		SIF GROUPMATCH(ARG:1, P408中廊下, P411鈴仙私室)
			RETURNF 1
	CASE P411鈴仙私室
		SIF GROUPMATCH(ARG:1, P408中廊下, P410てゐ私室)
			RETURNF 1
	CASE P412空き部屋
		SIF GROUPMATCH(ARG:1, P408中廊下, P409縁側, P413永琳私室)
			RETURNF 1
	CASE P413永琳私室
		SIF GROUPMATCH(ARG:1, P408中廊下, P409縁側, P412空き部屋, P414輝夜私室)
			RETURNF 1
	CASE P414輝夜私室
		SIF GROUPMATCH(ARG:1, P408中廊下, P409縁側, P413永琳私室)
			RETURNF 1
	CASE P415広間
		SIF GROUPMATCH(ARG:1, P408中廊下, P416居間, P417台所)
			RETURNF 1
	CASE P416居間
		SIF GROUPMATCH(ARG:1, P408中廊下, P409縁側, P415広間, P417台所)
			RETURNF 1
	CASE P417台所
		SIF GROUPMATCH(ARG:1, P405調剤室, P415広間, P416居間, P422裏庭)
			RETURNF 1
	CASE P418厠
		SIF GROUPMATCH(ARG:1, P407廊下)
			RETURNF 1
	CASE P419脱衣所
		SIF GROUPMATCH(ARG:1, P407廊下, P420風呂)
			RETURNF 1
	CASE P420風呂
		SIF GROUPMATCH(ARG:1, P419脱衣所)
			RETURNF 1
	CASE P421庭
		SIF GROUPMATCH(ARG:1, P409縁側, P422裏庭)
			RETURNF 1
	CASE P422裏庭
		SIF GROUPMATCH(ARG:1, P405調剤室, P417台所, P421庭)
			RETURNF 1
	CASE P424物置
		SIF GROUPMATCH(ARG:1, P409縁側)
			RETURNF 1
	CASE P426二階廊下
		SIF GROUPMATCH(ARG:1, P407廊下, P427倉庫, P428二階空き部屋)
			RETURNF 1
	CASE P427倉庫
		SIF GROUPMATCH(ARG:1, P426二階廊下)
			RETURNF 1
	CASE P428二階空き部屋
		SIF GROUPMATCH(ARG:1, P426二階廊下)
			RETURNF 1
	CASE P430地下室
		SIF GROUPMATCH(ARG:1, P407廊下)
			RETURNF 1

	CASE P431兎の洞穴
		SIF GROUPMATCH(ARG:1, P433斜角の竹林)
			RETURNF 1
	CASE P432影狼の家
		SIF GROUPMATCH(ARG:1, P433斜角の竹林)
			RETURNF 1
	CASE P433斜角の竹林
		SIF GROUPMATCH(ARG:1, P401永遠亭入口, P431兎の洞穴, P432影狼の家, P434迷いの小道, P442空き小屋, P443ミスティアの家, P444夜雀の屋台)
			RETURNF 1
	CASE P434迷いの小道
		SIF GROUPMATCH(ARG:1, P433斜角の竹林, P435井戸, P440廃屋, P441温泉, P445妹紅の家, P446祠, P447竹林入口)
			RETURNF 1
	CASE P435井戸
		SIF GROUPMATCH(ARG:1, P401永遠亭入口, P434迷いの小道, P436無名の丘)
			RETURNF 1
	CASE P436無名の丘
		SIF GROUPMATCH(ARG:1, P438太陽の畑)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P435井戸, P437メディスンの家)
			RETURNF 1
	CASE P437メディスンの家
		SIF GROUPMATCH(ARG:1, P436無名の丘)
			RETURNF 1
	CASE P438太陽の畑
		SIF GROUPMATCH(ARG:1, P439ラルバの家, P450玄関)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P436無名の丘, P466庭)
			RETURNF 3
	CASE P439ラルバの家
		SIF GROUPMATCH(ARG:1, P438太陽の畑)
			RETURNF 1
	CASE P440廃屋
		SIF GROUPMATCH(ARG:1, P434迷いの小道, P447竹林入口)
			RETURNF 1
	CASE P441温泉
		SIF GROUPMATCH(ARG:1, P434迷いの小道)
			RETURNF 1
	CASE P442空き小屋
		SIF GROUPMATCH(ARG:1, P433斜角の竹林, P444夜雀の屋台)
			RETURNF 1
	CASE P443ミスティアの家
		SIF GROUPMATCH(ARG:1, P433斜角の竹林, P444夜雀の屋台)
			RETURNF 1
	CASE P444夜雀の屋台
		SIF GROUPMATCH(ARG:1, P433斜角の竹林, P442空き小屋, P443ミスティアの家, P446祠, P447竹林入口)
			RETURNF 1
	CASE P445妹紅の家
		SIF GROUPMATCH(ARG:1, P434迷いの小道, P447竹林入口)
			RETURNF 1
	CASE P446祠
		SIF GROUPMATCH(ARG:1, P434迷いの小道, P444夜雀の屋台)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P447竹林入口)
			RETURNF 3
	CASE P447竹林入口
		SIF GROUPMATCH(ARG:1, P434迷いの小道, P440廃屋, P444夜雀の屋台, P445妹紅の家)
			RETURNF 1
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P446祠)
			RETURNF 3

	CASE P450玄関
		SIF GROUPMATCH(ARG:1, P438太陽の畑, P451くるみ私室, P452エリー私室, P465ホール, P466庭)
			RETURNF 1
	CASE P451くるみ私室
		SIF GROUPMATCH(ARG:1, P450玄関)
			RETURNF 1
	CASE P452エリー私室
		SIF GROUPMATCH(ARG:1, P450玄関)
			RETURNF 1
	CASE P453ダイニング
		SIF GROUPMATCH(ARG:1, P465ホール)
			RETURNF 1
	CASE P454バス
		SIF GROUPMATCH(ARG:1, P465ホール)
			RETURNF 1
	CASE P455トイレ
		SIF GROUPMATCH(ARG:1, P465ホール)
			RETURNF 1
	CASE P456倉庫
		SIF GROUPMATCH(ARG:1, P465ホール)
			RETURNF 1
	CASE P457夢月私室
		SIF GROUPMATCH(ARG:1, P458廊下)
			RETURNF 1
	CASE P458廊下
		SIF GROUPMATCH(ARG:1, P457夢月私室, P459幻月私室, P461リビング, P463幽香私室, P465ホール)
			RETURNF 1
	CASE P459幻月私室
		SIF GROUPMATCH(ARG:1, P458廊下)
			RETURNF 1
	CASE P460空き部屋
		SIF GROUPMATCH(ARG:1, P465ホール)
			RETURNF 1
	CASE P461リビング
		SIF GROUPMATCH(ARG:1, P458廊下, P462テラス, P465ホール)
			RETURNF 1
	CASE P462テラス
		SIF GROUPMATCH(ARG:1, P461リビング, P466庭)
			RETURNF 3
	CASE P463幽香私室
		SIF GROUPMATCH(ARG:1, P458廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P464バルコニー)
			RETURNF 3
	CASE P464バルコニー
		SIF GROUPMATCH(ARG:1, P463幽香私室)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P466庭)
			RETURNF 2
	CASE P465ホール
		SIF GROUPMATCH(ARG:1, P450玄関, P453ダイニング, P454バス, P455トイレ, P456倉庫, P458廊下, P460空き部屋, P461リビング)
			RETURNF 1
	CASE P466庭
		SIF GROUPMATCH(ARG:1, P450玄関)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P438太陽の畑, P462テラス)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P464バルコニー)
			RETURNF 2

	CASE MAXROOM
		SIF ARG:1 == P447竹林入口
			RETURNF 1
ENDSELECT
RETURNF 0
