;-----------------------------------------------------------
;遠距離移動の設定
;ARG     目的地
;ARG:1   現在地
;RETURN  次の目的地
;-----------------------------------------------------------
@SKIP_MOVE_4(ARG, ARG:1)
#DIM 現在の位置
;現在位置取得
IF ARG:1
	現在の位置 = ARG:1
ELSE
	現在の位置 = CFLAG:MASTER:現在位置
ENDIF

;目的地に直接移動できる場合そこに移動
SIF CAN_MOVE(現在の位置, ARG) & 1
	RETURN ARG

;袋小路から行ける場所は固定なので、そこを目指す
SELECTCASE 現在の位置
CASE P406来客用トイレ
	RETURN P403待合室
CASE P418厠, P430地下室
	RETURN P407廊下
CASE P420風呂
	RETURN P419脱衣所
CASE P424物置
	RETURN P409縁側
CASE P427倉庫, P428二階空き部屋
	RETURN P426二階廊下
CASE P431兎の洞穴, P432影狼の家
	RETURN P433斜角の竹林
CASE P437メディスンの家
	RETURN P436無名の丘
CASE P439ラルバの家
	RETURN P438太陽の畑
CASE P441温泉
	RETURN P434迷いの小道
CASE P451くるみ私室, P452エリー私室
	RETURN P450玄関
CASE P453ダイニング, P454バス, P455トイレ, P456倉庫, P460空き部屋
	RETURN P465ホール
CASE P457夢月私室, P459幻月私室
	RETURN P458廊下
CASE P464バルコニー
	RETURN P463幽香私室
ENDSELECT

;直接移動できない場合、1つ近い位置を暫定目的地に設定し直してループ
;基本的に最短で行けるコースを設定する
;現在地によってコースが変わることも考慮して対応する
SELECTCASE ARG
	CASE P401永遠亭入口
		SELECTCASE 現在の位置
		CASE P415広間 TO P417台所, P421庭, P422裏庭
			RETURN P405調剤室
		CASE P404診察室 TO P430地下室
			RETURN P403待合室
		CASE P431兎の洞穴, P432影狼の家, P442空き小屋, P443ミスティアの家, P444夜雀の屋台
			RETURN P433斜角の竹林
		CASEELSE
			RETURN P435井戸
		ENDSELECT
	CASE P403待合室
		SELECTCASE 現在の位置
		CASE P405調剤室, P417台所, P422裏庭
			RETURN P404診察室
		CASE P408中廊下 TO P430地下室
			RETURN P407廊下
		CASEELSE
			RETURN P401永遠亭入口
		ENDSELECT
	CASE P404診察室
		SELECTCASE 現在の位置
		CASE P417台所, P422裏庭, P416居間, P415広間, P421庭
			RETURN P405調剤室
		CASEELSE
			RETURN P403待合室
		ENDSELECT
	CASE P405調剤室
		SELECTCASE 現在の位置
		CASE P421庭
			RETURN P422裏庭
		CASE P408中廊下 TO P416居間, P424物置
			RETURN P417台所
		CASE P403待合室, P406来客用トイレ, P407廊下, P418厠 TO P420風呂, P426二階廊下 TO P430地下室
			RETURN P404診察室
		CASEELSE
			RETURN P401永遠亭入口
		ENDSELECT
	CASE P406来客用トイレ
		RETURN P403待合室
	CASE P407廊下
		SELECTCASE 現在の位置
		CASE P420風呂
			RETURN P419脱衣所
		CASE P427倉庫, P428二階空き部屋
			RETURN P426二階廊下
		CASE P421庭, P422裏庭, P424物置
			RETURN P409縁側
		CASE P410てゐ私室 TO P417台所
			RETURN P408中廊下
		CASEELSE
			RETURN P403待合室
		ENDSELECT
	CASE P408中廊下
		SELECTCASE 現在の位置
		CASE P417台所, P422裏庭, P405調剤室, P424物置
			RETURN P416居間
		CASEELSE
			RETURN P407廊下
		ENDSELECT
	CASE P409縁側
		SELECTCASE 現在の位置
		CASE P417台所, P415広間, P405調剤室
			RETURN P416居間
		CASE P422裏庭
			RETURN P421庭
		CASEELSE
			RETURN P407廊下
		ENDSELECT
	CASE P410てゐ私室, P411鈴仙私室
		RETURN P408中廊下
	CASE P412空き部屋, P413永琳私室, P414輝夜私室
		SELECTCASE 現在の位置
		CASE P421庭, P422裏庭, P427倉庫
			RETURN P409縁側
		CASEELSE
			RETURN P408中廊下
		ENDSELECT
	CASE P415広間
		SELECTCASE 現在の位置
		CASE P409縁側, P424物置, P421庭
			RETURN P416居間
		CASE P403待合室, P407廊下, P410てゐ私室 TO P414輝夜私室, P418厠 TO P420風呂, P426二階廊下 TO P430地下室
			RETURN P408中廊下
		CASEELSE
			RETURN P417台所
		ENDSELECT
	CASE P416居間
		SELECTCASE 現在の位置
		CASE P424物置, P421庭
			RETURN P409縁側
		CASE P403待合室, P407廊下, P410てゐ私室 TO P414輝夜私室, P418厠 TO P420風呂, P426二階廊下 TO P430地下室
			RETURN P408中廊下
		CASEELSE
			RETURN P417台所
		ENDSELECT
	CASE P417台所
		SELECTCASE 現在の位置
		CASE P421庭
			RETURN P422裏庭
		CASE P407廊下 TO P420風呂, P424物置 TO P430地下室
			RETURN P416居間
		CASEELSE
			RETURN P405調剤室
		ENDSELECT
	CASE P418厠, P419脱衣所
		RETURN P407廊下
	CASE P420風呂
		RETURN P419脱衣所
	CASE P421庭
		SELECTCASE 現在の位置
		CASE P403待合室, P404診察室, P406来客用トイレ TO P416居間, P418厠 TO P420風呂, P424物置 TO P430地下室
			RETURN P409縁側
		CASEELSE
			RETURN P422裏庭
		ENDSELECT
	CASE P422裏庭
		SELECTCASE 現在の位置
		CASE P408中廊下, P410てゐ私室 TO P416居間
			RETURN P417台所
		CASE P407廊下, P409縁側, P418厠 TO P420風呂, P424物置 TO P430地下室
			RETURN P421庭
		CASEELSE
			RETURN P405調剤室
		ENDSELECT
	CASE P424物置
		RETURN P409縁側
	CASE P426二階廊下, P430地下室
		RETURN P407廊下
	CASE P427倉庫, P428二階空き部屋
		RETURN P426二階廊下

	CASE P431兎の洞穴, P432影狼の家
		RETURN P433斜角の竹林
	CASE P433斜角の竹林
		SELECTCASE 現在の位置
		CASE P440廃屋, P441温泉, P445妹紅の家 TO P447竹林入口
			RETURN P434迷いの小道
		CASEELSE
			RETURN P401永遠亭入口
		ENDSELECT
	CASE P434迷いの小道
		SELECTCASE 現在の位置
		CASE P431兎の洞穴, P432影狼の家, P442空き小屋 TO P444夜雀の屋台
			RETURN P433斜角の竹林
		CASEELSE
			RETURN P435井戸
		ENDSELECT
	CASE P435井戸
		SELECTCASE 現在の位置
		CASE P403待合室 TO P430地下室
			RETURN P401永遠亭入口
		CASE P437メディスンの家, P438太陽の畑, P439ラルバの家, P450玄関 TO P466庭
			RETURN P436無名の丘
		CASE P431兎の洞穴, P432影狼の家, P442空き小屋 TO P444夜雀の屋台
			RETURN P433斜角の竹林
		CASEELSE
			RETURN P434迷いの小道
		ENDSELECT
	CASE P436無名の丘
		SELECTCASE 現在の位置
		CASE P439ラルバの家, P450玄関 TO P466庭
			RETURN P438太陽の畑
		CASEELSE
			RETURN P435井戸
		ENDSELECT
	CASE P437メディスンの家
		RETURN P436無名の丘
	CASE P438太陽の畑
		SELECTCASE 現在の位置
		CASE P451くるみ私室 TO P461リビング, P463幽香私室 TO P465ホール
			RETURN P450玄関
		CASE P462テラス
			RETURN P466庭
		CASEELSE
			RETURN P436無名の丘
		ENDSELECT
	CASE P439ラルバの家
		RETURN P438太陽の畑
	CASE P440廃屋, P441温泉
		RETURN P434迷いの小道
	CASE P442空き小屋, P443ミスティアの家
		SELECTCASE 現在の位置
		CASE P446祠, P447竹林入口
			RETURN P444夜雀の屋台
		CASEELSE
			RETURN P433斜角の竹林
		ENDSELECT
	CASE P444夜雀の屋台
		SELECTCASE 現在の位置
		CASE P440廃屋, P445妹紅の家
			RETURN P447竹林入口
		CASEELSE
			RETURN P433斜角の竹林
		ENDSELECT
	CASE P445妹紅の家
		SELECTCASE 現在の位置
		CASE P444夜雀の屋台, P440廃屋, P446祠, P443ミスティアの家
			RETURN P447竹林入口
		CASEELSE
			RETURN P434迷いの小道
		ENDSELECT
	CASE P446祠
		SELECTCASE 現在の位置
		CASE P443ミスティアの家, P442空き小屋
			RETURN P444夜雀の屋台
		CASE P440廃屋, P445妹紅の家
			RETURN P447竹林入口
		CASEELSE
			RETURN P434迷いの小道
		ENDSELECT
	CASE P447竹林入口
		SELECTCASE 現在の位置
		CASE P443ミスティアの家, P442空き小屋
			RETURN P444夜雀の屋台
		CASEELSE
			RETURN P434迷いの小道
		ENDSELECT

	CASE P450玄関
		SELECTCASE 現在の位置
		CASE P453ダイニング TO P464バルコニー
			RETURN P465ホール
		CASEELSE
			RETURN P438太陽の畑
		ENDSELECT
	CASE P451くるみ私室, P452エリー私室
		RETURN P450玄関
	CASE P453ダイニング, P454バス, P455トイレ, P456倉庫, P460空き部屋
		RETURN P465ホール
	CASE P457夢月私室, P459幻月私室
		RETURN P458廊下
	CASE P458廊下
		SELECTCASE 現在の位置
		CASE P464バルコニー
			RETURN P463幽香私室
		CASE P462テラス, P466庭
			RETURN P461リビング
		CASEELSE
			RETURN P465ホール
		ENDSELECT
	CASE P461リビング
		SELECTCASE 現在の位置
		CASE P457夢月私室, P459幻月私室, P463幽香私室, P464バルコニー
			RETURN P458廊下
		CASE P466庭
			RETURN P462テラス
		CASEELSE
			RETURN P465ホール
		ENDSELECT
	CASE P462テラス
		SELECTCASE 現在の位置
		CASE P453ダイニング TO P460空き部屋, P463幽香私室 TO P465ホール
			RETURN P461リビング
		CASEELSE
			RETURN P466庭
		ENDSELECT
	CASE P463幽香私室
		RETURN P458廊下
	CASE P464バルコニー
		RETURN P463幽香私室
	CASE P465ホール
		SELECTCASE 現在の位置
		CASE P462テラス
			RETURN P461リビング
		CASE P457夢月私室, P459幻月私室, P463幽香私室, P464バルコニー
			RETURN P458廊下
		CASEELSE
			RETURN P450玄関
		ENDSELECT
	CASE P466庭
		SELECTCASE 現在の位置
		CASE P451くるみ私室 TO P456倉庫, P460空き部屋
			RETURN P450玄関
		CASE P457夢月私室 TO P459幻月私室, P461リビング TO P464バルコニー
			RETURN P462テラス
		CASEELSE
			RETURN P438太陽の畑
		ENDSELECT
ENDSELECT