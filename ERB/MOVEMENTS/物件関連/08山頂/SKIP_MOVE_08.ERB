@SKIP_MOVE_8(ARG,ARG:1)
;LOCAL一覧
;0	現在の位置

;現在位置取得
LOCAL = CFLAG:MASTER:現在位置
SIF ARG:1
	LOCAL = ARG:1
;暫定目的地に直接移動できる場合
IF CAN_MOVE(LOCAL, ARG) & 1
	;そこに移動
	RETURN ARG
ENDIF

;袋小路から行ける場所は固定なので、そこを目指す
SELECTCASE LOCAL
	CASE 806
		RETURN 802
	CASE 813
		RETURN 811
	CASE 824,825
		RETURN 822
	CASE 823
		RETURN 821
	CASE 833
		RETURN 832
	CASE 837
		RETURN 834
	CASE 841,842
		RETURN 838
ENDSELECT

;直接移動できない場合、1つ近い位置を暫定目的地に設定し直してループ
;地点追加時要フォロー（全ケース修正すること）
SELECTCASE ARG
	CASE 801
		RETURN 802
	CASE 802
		RETURN 803
	CASE 803
		IF GROUPMATCH(LOCAL:0,801,806,807)
			RETURN 802
		ELSE
			RETURN 804
		ENDIF
	CASE 804
		IF INRANGE(LOCAL:0,801,810)
			RETURN 803
		ELSEIF INRANGE(LOCAL:0,830,842)
			RETURN 805
		ELSE
			RETURN 811
		ENDIF
	CASE 805
		IF INRANGE(LOCAL:0,831,842)
			RETURN 830
		ELSE
			RETURN 804
		ENDIF
	CASE 806
		RETURN 802
	CASE 807
		RETURN 802
	CASE 808,809
		IF GROUPMATCH(LOCAL:0,801,806,807)
			RETURN 802
		ELSE
			RETURN 803
		ENDIF
	CASE 810
		IF GROUPMATCH(LOCAL:0,801,802,806,807,808,809)
			RETURN 803
		ELSE
			RETURN 804
		ENDIF
	CASE 811
		IF LOCAL:0 == 815
			RETURN 814
		ELSEIF GROUPMATCH(LOCAL:0,818,820,821)
			RETURN 822
		ELSEIF GROUPMATCH(LOCAL:0,816,817,819,826)
			RETURN 812
		ELSE
			RETURN 804
		ENDIF
	CASE 812
		IF INRANGE(LOCAL:0,816,822)
			RETURN 826
		ELSE
			RETURN 811
		ENDIF
	CASE 813
		RETURN 811
	CASE 814
		IF LOCAL:0 == 821
			RETURN 815
		ELSEIF INRANGE(LOCAL:0,817,819)
			RETURN 816
		ELSE
			RETURN 811
		ENDIF
	CASE 815
		IF GROUPMATCH(LOCAL:0,812,817,818,819,826)
			RETURN 816
		ELSEIF GROUPMATCH(LOCAL:0,820,822)
			RETURN 821
		ELSE
			RETURN 814
		ENDIF
	CASE 816
		IF LOCAL:0 == 814
			RETURN 815
		ELSEIF LOCAL:0 == 822
			RETURN 821
		ELSE
			RETURN 826
		ENDIF
	CASE 817
		RETURN 816
	CASE 818
		IF GROUPMATCH(LOCAL:0,812,814,815)
			RETURN 816
		ELSE
			RETURN 822
		ENDIF
	CASE 819
		IF GROUPMATCH(LOCAL:0,814,815,818,821)
			RETURN 816
		ELSE
			RETURN 826
		ENDIF
	CASE 820
		IF GROUPMATCH(LOCAL:0,814,815,821)
			RETURN 822
		ELSE
			RETURN 826
		ENDIF
	CASE 821
		IF GROUPMATCH(LOCAL:0,817,819)
			RETURN 816
		ELSEIF LOCAL:0 == 814
			RETURN 815
		ELSE
			RETURN 822
		ENDIF
	CASE 822
		IF LOCAL:0 == 815
			RETURN 821
		ELSEIF GROUPMATCH(LOCAL:0,812,817,819)
			RETURN 826
		ELSE
			RETURN 811
		ENDIF
	CASE 823
		RETURN 821
	CASE 824,825
		RETURN 822
	CASE 826
		IF GROUPMATCH(LOCAL:0,815,817)
			RETURN 816
		ELSEIF GROUPMATCH(LOCAL:0,818,821)
			RETURN 822
		ELSE
			RETURN 812
		ENDIF
	CASE 830
		IF INRANGE(LOCAL:0,831,842)
			RETURN 832
		ELSE
			RETURN 805
		ENDIF
	CASE 831
		IF INRANGE(LOCAL:0,835,842)
			RETURN 834
		ELSE
			RETURN 832
		ENDIF
	CASE 832
		IF INRANGE(LOCAL:0,834,842)
			RETURN 831
		ELSE
			RETURN 830
		ENDIF
	CASE 833
		RETURN 832
	CASE 834
		IF INRANGE(LOCAL:0,839,842)
			RETURN 838
		ELSE
			RETURN 831
		ENDIF
	CASE 835 TO 837
		RETURN 834
	CASE 838
		RETURN 834
	CASE 839 TO 842
		RETURN 838
ENDSELECT