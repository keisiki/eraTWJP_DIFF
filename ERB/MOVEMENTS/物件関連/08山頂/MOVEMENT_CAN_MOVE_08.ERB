;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_8(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P801山頂入り口
	SIF GROUPMATCH(ARG:1, P802橋)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P807椛宅)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
CASE P802橋
	SIF GROUPMATCH(ARG:1, P801山頂入り口, P803索道駅)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P806滝の裏, P807椛宅, P808文宅, P809はたて宅)
		RETURNF 1
CASE P803索道駅
	SIF GROUPMATCH(ARG:1, P802橋)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P804守矢神社鳥居, P808文宅, P809はたて宅, P810山小屋)
		RETURNF 1
CASE P804守矢神社鳥居
	SIF GROUPMATCH(ARG:1, P811境内)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P803索道駅, P805頂上, P810山小屋)
		RETURNF 1
CASE P805頂上
	SIF GROUPMATCH(ARG:1, P804守矢神社鳥居, P830玄雲海, P851久侘歌の小屋, P852駒草の小屋, P853魅須丸の小屋, P854龍宅, P855虹龍洞)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P801山頂入り口, P802橋, P803索道駅)
		RETURNF 2
CASE P806滝の裏
	SIF GROUPMATCH(ARG:1, P802橋)
		RETURNF 1
CASE P807椛宅
	SIF GROUPMATCH(ARG:1, P801山頂入り口, P802橋)
		RETURNF 1
CASE P808文宅, P809はたて宅
	SIF GROUPMATCH(ARG:1, P802橋, P803索道駅)
		RETURNF 1
CASE P810山小屋
	SIF GROUPMATCH(ARG:1, P803索道駅, P804守矢神社鳥居)
		RETURNF 1

CASE P811境内
	SIF GROUPMATCH(ARG:1, P804守矢神社鳥居, P813社務所, P814湖畔)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P812本殿, P821土間)
		RETURNF 1
CASE P812本殿
	SIF GROUPMATCH(ARG:1, P811境内, P826廊下)
		RETURNF 1
CASE P813社務所
	SIF GROUPMATCH(ARG:1, P811境内)
		RETURNF 3
CASE P814湖畔
	SIF GROUPMATCH(ARG:1, P811境内, P815庭)
		RETURNF 3
CASE P815庭
	SIF GROUPMATCH(ARG:1, P814湖畔, P816縁側)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P821土間)
		RETURNF 1
CASE P816縁側
	SIF GROUPMATCH(ARG:1, P817神奈子私室, P818諏訪子私室, P819居間, P821土間, P826廊下)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P815庭)
		RETURNF 3
CASE P817神奈子私室
	SIF GROUPMATCH(ARG:1, P816縁側, P818諏訪子私室, P819居間)
		RETURNF 1
CASE P818諏訪子私室
	SIF GROUPMATCH(ARG:1, P816縁側, P817神奈子私室, P820早苗私室, P821土間)
		RETURNF 1
CASE P819居間
	SIF GROUPMATCH(ARG:1, P816縁側, P817神奈子私室, P820早苗私室, P826廊下)
		RETURNF 1
CASE P820早苗私室
	SIF GROUPMATCH(ARG:1, P818諏訪子私室, P819居間, P821土間, P826廊下)
		RETURNF 1
CASE P821土間
	SIF GROUPMATCH(ARG:1, P811境内, P815庭, P826廊下, P816縁側, P818諏訪子私室, P820早苗私室, P823空室, P824トイレ, P825風呂)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P822台所)
		RETURNF 3
CASE P822台所
	SIF GROUPMATCH(ARG:1, P821土間)
		RETURNF 3
CASE P823空室, P824トイレ, P825風呂
	SIF GROUPMATCH(ARG:1, P821土間)
		RETURNF 1
CASE P826廊下
	SIF GROUPMATCH(ARG:1, P812本殿, P816縁側, P819居間, P820早苗私室, P821土間)
		RETURNF 1

CASE P830玄雲海
	SIF GROUPMATCH(ARG:1, P805頂上)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P832有頂天街)
		RETURNF 3
CASE P831比那名居邸前
	SIF GROUPMATCH(ARG:1, P832有頂天街, P834比那名居邸土間)
		RETURNF 1
CASE P832有頂天街
	SIF GROUPMATCH(ARG:1, P830玄雲海)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P831比那名居邸前, P833桃園)
		RETURNF 1
CASE P833桃園
	SIF GROUPMATCH(ARG:1, P832有頂天街)
		RETURNF 1
CASE P834比那名居邸土間
	SIF GROUPMATCH(ARG:1, P831比那名居邸前, P835比那名居邸応接間, P836比那名居邸厨房, P837比那名居邸風呂, P838比那名居邸居間)
		RETURNF 1
CASE P835比那名居邸応接間
	SIF GROUPMATCH(ARG:1, P834比那名居邸土間, P836比那名居邸厨房)
		RETURNF 1
CASE P836比那名居邸厨房
	SIF GROUPMATCH(ARG:1, P834比那名居邸土間, P835比那名居邸応接間)
		RETURNF 1
CASE P837比那名居邸風呂
	SIF GROUPMATCH(ARG:1, P834比那名居邸土間)
		RETURNF 1
CASE P838比那名居邸居間
	SIF GROUPMATCH(ARG:1, P834比那名居邸土間, P839比那名居邸和室, P840天子私室, P841衣玖私室, P842比那名居邸物置)
		RETURNF 1
CASE P839比那名居邸和室
	SIF GROUPMATCH(ARG:1, P838比那名居邸居間, P840天子私室)
		RETURNF 1
CASE P840天子私室
	SIF GROUPMATCH(ARG:1, P838比那名居邸居間, P839比那名居邸和室)
		RETURNF 1
CASE P841衣玖私室, P842比那名居邸物置
	SIF GROUPMATCH(ARG:1, P838比那名居邸居間)
		RETURNF 1

CASE P851久侘歌の小屋, P852駒草の小屋, P853魅須丸の小屋, P854龍宅
	SIF GROUPMATCH(ARG:1, P805頂上)
		RETURNF 1
CASE P855虹龍洞
	SIF GROUPMATCH(ARG:1, P805頂上, P856百々世の住処)
		RETURNF 1
CASE P856百々世の住処
	SIF GROUPMATCH(ARG:1, P855虹龍洞)
		RETURNF 1

CASE MAXROOM
	SIF ARG:1 == P801山頂入り口
		RETURNF 1
ENDSELECT

RETURNF 0