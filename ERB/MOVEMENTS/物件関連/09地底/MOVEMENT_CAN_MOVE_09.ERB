;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_9(ARG, ARG:1)
#FUNCTION
#DIM 参照先
SELECTCASE ARG
CASE P901地霊殿前
	SIF GROUPMATCH(ARG:1, P905一階廊下, P931大通り)
		RETURNF 1
CASE P904大広間, P906一階トイレ, P911お空私室, P912お燐私室
	SIF GROUPMATCH(ARG:1, P905一階廊下)
		RETURNF 1
CASE P905一階廊下
	SIF GROUPMATCH(ARG:1, P901地霊殿前, P904大広間, P906一階トイレ, P909厨房, P911お空私室, P912お燐私室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P913食堂, P914二階廊下, P923裏庭, P926中庭)
		RETURNF 1
CASE P909厨房
	SIF GROUPMATCH(ARG:1, P905一階廊下, P910食料庫, P913食堂, P925バラ園)
		RETURNF 1
CASE P910食料庫
	SIF GROUPMATCH(ARG:1, P909厨房)
		RETURNF 1
CASE P913食堂
	SIF GROUPMATCH(ARG:1, P905一階廊下, P909厨房, P923裏庭, P926中庭)
		RETURNF 1
CASE P914二階廊下
	SIF GROUPMATCH(ARG:1, P905一階廊下, P915バルコニー, P916物置, P917二階トイレ)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P918こいし私室, P919さとり私室, P920書斎, P922脱衣所)
		RETURNF 1
CASE P915バルコニー, P916物置, P917二階トイレ, P918こいし私室
	SIF GROUPMATCH(ARG:1, P914二階廊下)
		RETURNF 1
CASE P919さとり私室
	SIF GROUPMATCH(ARG:1, P914二階廊下, P920書斎)
		RETURNF 1
CASE P920書斎
	SIF GROUPMATCH(ARG:1, P914二階廊下, P919さとり私室)
		RETURNF 1
CASE P921風呂
	SIF GROUPMATCH(ARG:1, P922脱衣所)
		RETURNF 1
CASE P922脱衣所
	SIF GROUPMATCH(ARG:1, P914二階廊下, P921風呂)
		RETURNF 1
CASE P923裏庭
	SIF GROUPMATCH(ARG:1, P905一階廊下, P913食堂, P925バラ園)
		RETURNF 1
CASE P925バラ園
	SIF GROUPMATCH(ARG:1, P909厨房, P923裏庭)
		RETURNF 1
CASE P926中庭
	SIF GROUPMATCH(ARG:1, P905一階廊下, P913食堂, P928灼熱地獄跡)
		RETURNF 1
CASE P928灼熱地獄跡
	SIF GROUPMATCH(ARG:1, P926中庭, P929地獄の人工太陽)
		RETURNF 1
CASE P929地獄の人工太陽
	SIF GROUPMATCH(ARG:1, P928灼熱地獄跡, P930旧血の池地獄)
		RETURNF 1
CASE P930旧血の池地獄
	SIF GROUPMATCH(ARG:1, P929地獄の人工太陽)
		RETURNF 1
CASE P931大通り
	SIF GROUPMATCH(ARG:1, P901地霊殿前, P932裏通り, P933酒場, P937空き屋, P944通り)
		RETURNF 1
CASE P932裏通り
	SIF GROUPMATCH(ARG:1, P931大通り, P933酒場, P934温泉, P935ラブホ, P936勇儀の家, P938鬼の溜まり場, P939路地裏)
		RETURNF 1
CASE P933酒場
	SIF GROUPMATCH(ARG:1, P931大通り, P932裏通り)
		RETURNF 1
CASE P934温泉, P935ラブホ, P936勇儀の家, P938鬼の溜まり場, P939路地裏
	SIF GROUPMATCH(ARG:1, P932裏通り)
		RETURNF 1
CASE P937空き屋
	SIF GROUPMATCH(ARG:1, P931大通り)
		RETURNF 1
CASE P940パルスィの家, P941ヤマメの家, P942キスメの家
	SIF GROUPMATCH(ARG:1, P945川岸)
		RETURNF 1
CASE P943あばら屋
	SIF GROUPMATCH(ARG:1, P944通り)
		RETURNF 1
CASE P944通り
	SIF GROUPMATCH(ARG:1, P946橋)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P931大通り, P943あばら屋, P945川岸)
		RETURNF 1
CASE P945川岸
	SIF GROUPMATCH(ARG:1, P940パルスィの家, P941ヤマメの家, P942キスメの家, P944通り, P946橋)
		RETURNF 1
CASE P946橋
	SIF GROUPMATCH(ARG:1, P944通り, P948縦穴の底)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P945川岸)
		RETURNF 1
CASE P948縦穴の底
	SIF GROUPMATCH(ARG:1, P946橋)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, MAXROOM)
		RETURNF 2
CASE MAXROOM
	SIF ARG:1 == P948縦穴の底
		RETURNF 1
ENDSELECT

RETURNF 0