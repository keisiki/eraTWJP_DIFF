;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_3(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P301正門
		SIF GROUPMATCH(ARG:1, P303守衛小屋, P304空き小屋, P343湖畔)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P302庭
		SIF GROUPMATCH(ARG:1, P303守衛小屋, P306裏庭, P309広間)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P301正門, P307東テラス, P308西テラス)
			RETURNF 3
	CASE P303守衛小屋, P304空き小屋
		SIF GROUPMATCH(ARG:1, P301正門, P302庭, P306裏庭)
			RETURNF 1
	CASE P305納屋
		SIF GROUPMATCH(ARG:1, P306裏庭)
			RETURNF 1
	CASE P306裏庭
		SIF GROUPMATCH(ARG:1, P302庭, P303守衛小屋, P304空き小屋, P305納屋, P313厨房)
			RETURNF 1
	CASE P307東テラス
		SIF GROUPMATCH(ARG:1, P310図書館)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P308西テラス
		SIF GROUPMATCH(ARG:1, P309広間)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P309広間
		SIF GROUPMATCH(ARG:1, P302庭, P310図書館, P314食堂, P315応接間, P316一階トイレ, P319二階廊下, P328地下廊下)
			RETURNF 1
	CASE P310図書館
		SIF GROUPMATCH(ARG:1, P307東テラス, P309広間, P311パチュリー私室, P312小悪魔私室)
			RETURNF 1
	CASE P311パチュリー私室, P312小悪魔私室
		SIF GROUPMATCH(ARG:1, P310図書館)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P306裏庭)
			RETURNF 2
	CASE P313厨房
		SIF GROUPMATCH(ARG:1, P306裏庭, P314食堂)
			RETURNF 1
	CASE P314食堂
		SIF GROUPMATCH(ARG:1, P309広間, P313厨房)
			RETURNF 1
	CASE P315応接間
		SIF GROUPMATCH(ARG:1, P308西テラス, P309広間)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 2
	CASE P316一階トイレ
		SIF GROUPMATCH(ARG:1, P309広間)
			RETURNF 1
	CASE P319二階廊下
		SIF GROUPMATCH(ARG:1, P309広間, P320咲夜私室, P321空き部屋, P322二階トイレ, P323メイド妖精詰め所, P326レミリア私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P324大バルコニー)
			RETURNF 3
	CASE P320咲夜私室, P321空き部屋
		SIF GROUPMATCH(ARG:1, P319二階廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P306裏庭)
			RETURNF 2
	CASE P322二階トイレ, P323メイド妖精詰め所
		SIF GROUPMATCH(ARG:1, P319二階廊下)
			RETURNF 1
	CASE P324大バルコニー
		SIF GROUPMATCH(ARG:1, P319二階廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭, P301正門)
			RETURNF 2
	CASE P326レミリア私室
		SIF GROUPMATCH(ARG:1, P319二階廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P327小バルコニー)
			RETURNF 3
	CASE P327小バルコニー
		SIF GROUPMATCH(ARG:1, P326レミリア私室)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P302庭, P301正門, P324大バルコニー)
			RETURNF 2
	CASE P328地下廊下
		SIF GROUPMATCH(ARG:1, P309広間, P329倉庫, P330使用人部屋, P331地下室, P332脱衣所)
			RETURNF 1
	CASE P329倉庫, P330使用人部屋, P331地下室
		SIF GROUPMATCH(ARG:1, P328地下廊下)
			RETURNF 1
	CASE P332脱衣所
		SIF GROUPMATCH(ARG:1, P328地下廊下, P333大浴場)
			RETURNF 1
	CASE P333大浴場
		SIF GROUPMATCH(ARG:1, P332脱衣所)
			RETURNF 1

	CASE P334廃洋館・入り口
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P335廃洋館・広間)
			RETURNF 3
	CASE P335廃洋館・広間
		SIF GROUPMATCH(ARG:1, P338廃洋館・風呂, P339廃洋館・二階廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P334廃洋館・入り口, P337廃洋館・キッチン)
			RETURNF 3
	CASE P337廃洋館・キッチン
		SIF GROUPMATCH(ARG:1, P335廃洋館・広間)
			RETURNF 3
	CASE P338廃洋館・風呂
		SIF GROUPMATCH(ARG:1, P335廃洋館・広間)
			RETURNF 1
	CASE P339廃洋館・二階廊下
		SIF GROUPMATCH(ARG:1, P335廃洋館・広間, P340ルナサ私室, P341メルラン私室, P342リリカ私室)
			RETURNF 1
	CASE P340ルナサ私室, P341メルラン私室, P342リリカ私室
		SIF GROUPMATCH(ARG:1, P339廃洋館・二階廊下)
			RETURNF 1

	CASE P343湖畔
		SIF GROUPMATCH(ARG:1, P347霧の湖)
			RETURNF 3
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P301正門, P334廃洋館・入り口, P344レティのかまくら, P345チルノのかまくら, P346大妖精の棲家)
			RETURNF 1
	CASE P344レティのかまくら, P345チルノのかまくら, P346大妖精の棲家
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 1
	CASE P347霧の湖
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P348霧の湖・湖底)
			RETURNF 1
	CASE P348霧の湖・湖底
		SIF GROUPMATCH(ARG:1, P347霧の湖)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P343湖畔
			RETURNF 1
ENDSELECT
RETURNF 0