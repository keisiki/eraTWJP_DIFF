@DRAW_MAP_BG_3(SLICED)
#DIMS SLICED
SELECTCASE SLICED
;色つけたいときはこんな感じ
CASE "木"
	SETCOLOR 0x90EE90
	PRINTS SLICED
	RESETCOLOR
;違う文字を表示させる例
CASE "#W"
	SETCOLOR 0x90EE90
	PRINT 木
	RESETCOLOR
CASE "#A"
	IF HATAKE_NAME:0 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 　
	ENDIF
CASE "#B"
	IF HATAKE_NAME:1 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 　
	ENDIF
CASE "#C"
	IF HATAKE_NAME:2 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 　
	ENDIF
CASE "#D"
	IF HATAKE_NAME:3 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 　
	ENDIF
CASE "#E"
	SIF TALENT:MASTER:居場所察知 && COUNTCHARA:(MAXROOM) > 0 && FLAG:察知モード == 1
		SETCOLOR 255,100,200
	PRINT ←
	RESETCOLOR
;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT