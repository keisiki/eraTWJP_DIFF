#DIM EVENT_DAY,DAILY_EV_MAX

CALL FISHER_YATES_SHAFFLE(DAILY_EV_MAX)
ARRAYCOPY "SHAFFLE_ARRAY" , "EVENT_DAY"
;PRINTFORML デイリー発生判定
DRAWLINE
FOR LOCAL,0,DAILY_EV_MAX
	RESULT = 0
	TRYCALLFORM DAILY_CONDITION_EV{EVENT_DAY:LOCAL}
