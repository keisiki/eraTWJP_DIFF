@GOD_FUTANARI
CALL COLORMESSAGE("ほう、ヒトの身で余の正体に気づくとは",C_D_RED,2,1)
PRINTFORML 
PRINTFORMW 声の主の気配が強大なものへと変わった……
CALL COLORMESSAGE("そなたのふたなりおちんぽ愛は大したものよ",C_D_RED,1,1)
CALL COLORMESSAGE("褒美じゃ、ひとつ願いを言うがよい",C_D_RED,1,1)
$LOOP
CALL ASK_M("別にいいです",1,"ふたなりおちんぽを生やして",1,"ふたなりおちんぽを強化して",1,"ふたなりおちんぽしゃぶらせて",1,"パンツください",1)
SELECTCASE RESULT
	CASE 0
		CALL COLORMESSAGE("無粋な……己がいかなる僥倖に恵まれたか分らぬと見える",C_D_RED,1,1)
		CALL COLORMESSAGE("まあよいわ",C_D_RED,1,1)
	CASE 1
		PRINTFORML 誰に？
		DRAWLINE
		FOR LOCAL,0,CHARANUM
			SIF TALENT:LOCAL:性別 == 1
				PRINTFORMLC [{LOCAL}] %CALLNAME:LOCAL,15,LEFT%
		NEXT
		DRAWLINE
		PRINTFORML [999] やめる
		INPUT
		IF INRANGE(RESULT,0,CHARANUM) && TALENT:RESULT:性別 == 1
			TALENT:RESULT:性別 = 3
			MAXBASE:RESULT:精力 = 1000
			PRINTFORMW %CALLNAME:RESULT%はふたなりになった
			CALL COLORMESSAGE("容易いことよ",C_D_RED,1,1)
		ELSE
			GOTO LOOP
		ENDIF
	CASE 2
		PRINTFORML 誰の？
		DRAWLINE
		FOR LOCAL,1,CHARANUM
			SIF TALENT:LOCAL:性別 == 3 && MAXBASE:LOCAL:精力 < 1500
				PRINTFORMLC [{LOCAL}] %CALLNAME:LOCAL,15,LEFT%
		NEXT
		PRINTL 
		DRAWLINE
		PRINTFORML [999] やめる
		INPUT
		IF INRANGE(RESULT,1,CHARANUM) && TALENT:RESULT:性別 == 3 && MAXBASE:RESULT:精力 < 1500
			PRINTFORMW %CALLNAME:RESULT%の精力最大値が増えた
			MAXBASE:RESULT:精力 += 100
			CALL COLORMESSAGE("容易いことよ",C_D_RED,1,1)
		ELSE
			GOTO LOOP
		ENDIF
	CASE 3
		CALL COLORMESSAGE("フフ、まこと天晴れなふたなりおちんぽ愛よ",C_D_RED,1,1)
		CALL COLORMESSAGE("存分に味わうがよい、受け取め切れるならばな",C_D_RED,1,1)
