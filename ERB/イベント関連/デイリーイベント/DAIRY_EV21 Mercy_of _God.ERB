@DAILY_CONDITION_EV21
#DIM YOME
SIF !FLAG:子宝祈願
	RETURN -1
SIF FLAG:獲得パンツ種数 < 100
	RETURN -1
YOME = FLAG:子宝祈願

SIF RAND:30 > 0
	RETURN -1
SIF TALENT:YOME:妊娠
	RETURN -1
SIF !CHECK_CHARA(YOME, "幽霊") && !CHECK_CHARA(YOME, "人形") && !CHECK_CHARA(YOME, "蓬莱人") && !CHECK_CHARA(YOME, "神霊")
	RETURN -1
SIF CFLAG:YOME:子供人数 > 0
	RETURN -1
CALL DAILY_EV21(YOME)

RETURN 1


