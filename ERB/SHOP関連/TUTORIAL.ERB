;ARG:1 テキスト表記・１文字あたりwait時間 省略可
;-------------------------------------------------
@DQPRINT(ARGS, ARG:0 = 0, ARG:1)
#DIM 文字数
#DIM 検索数

LOCAL:0 = 0

IF ARG:1 == 0
	ARG:1 = 8 + FLAG:チュート文速度
ENDIF
STRLENFORM %ARGS%
文字数 = RESULT:0
検索数 = 文字数 + 1

FOR LOCAL:0, 0, 検索数
	IF LOCAL:0 == 文字数
		IF ARG:0
			PRINTFORM %ARGS%
		ELSE
			REUSELASTLINE %ARGS%
		ENDIF
		WAIT
	ELSE
		SUBSTRING ARGS, 0, LOCAL:0
		REUSELASTLINE %RESULTS:0%
		TWAIT ARG:1, 1
	ENDIF
NEXT
IF ARG:0 == 0
	REUSELASTLINE 
ELSEIF ARG:0 == 2
	CLEARLINE 1
	CALL COLORMESSAGE(@"%ARGS%", 0x696969, 1)
ENDIF

;-------------------------------------------------
;チュートリアル読破ボーナス贈呈
;-------------------------------------------------
@TUTORIAL_BONUS()
#DIM ボーナス内容
#DIM 授与個数

ボーナス内容 = 0

IF FLAG:591 >= TB_TOTALLINES()
	FLAG:592 ++
	ボーナス内容 = FLAG:592
ENDIF

SIF ボーナス内容
	TWAIT 2500, 1
SELECTCASE ボーナス内容
	CASE 1
		CALL DQPRINT("「夢の世界でのお喋りにお付き合いいただき、ありがとうございます」")
		CALL DQPRINT("「こちら、ほんの気持ちですが。お受け取りくださいな」")
	CASE 2
		CALL DQPRINT("「ずいぶん話し込んでしまいました。はい、こちら粗品となります」")
		CALL DQPRINT("「え？　夢でもらったものが持ち越せるのはなんでだって？」")
		CALL DQPRINT("「そこはそれ、夢の支配者たる私にとってはさしたる障害ではありませんよ」")
	CASE 3
		CALL DQPRINT("「なかなかの聞き上手とみました。あるいは生真面目なのか」")
		CALL DQPRINT("「それとも、景品があると燃えるタイプですかね？　ふふ♪」")
	CASE 4
		CALL DQPRINT("「ここまでお付き合いくださり、ありがとうございます」")
		CALL DQPRINT("「夢の世界でもくつろぎはじめるあたり、流石というかなんというか」")
		CALL DQPRINT("「さて、今回はとっておきの一品。現在提供中の、安眠枕の改良試作品です」")
		CALL DQPRINT("「夢の世界でもリラックスできるほどのあなたには必要ないかもしれませんが、どうぞお試しください」")
		CALL DQPRINT("「あ、後で使用感を聞かせてくださいね♪」")
ENDSELECT

IF ボーナス内容
	SELECTCASE ボーナス内容
		CASE 4
			授与個数 = 1
			
		CASEELSE
			授与個数 = 3
	ENDSELECT
	IF ITEM:TB_ITEM() + 授与個数 > 99
		ITEM:TB_ITEM() = 99
	ELSE
		ITEM:TB_ITEM() += 授与個数
	ENDIF
	IF ボーナス内容 == 4
		CALL COLORMESSAGE(@"%ITEMNAME:TB_ITEM()%をもらった！", C_YELLOW, 2, 1)
	ELSE
		CALL COLORMESSAGE(@"%ITEMNAME:TB_ITEM()%を{授与個数}個もらった！", C_YELLOW, 2, 1)
	ENDIF
	CLEARLINE 1
ENDIF

