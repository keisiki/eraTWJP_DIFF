;日常コマンドの成否判定
;CALL					USER
;ARG:0（条件最大値		コマンド事の成否条件の最大値
;ARG:1（成功値			コマンド事の成功値
;ARG:2（大成功補正値	コマンド事の大成功に対する補正値
;RETURN		VOID
;COMMENT	各コマンドに散らばっている成否判定を一括
;-------------------------------------------------
@SUC_FAIL(条件最大値,成功値,大成功補正値)
#DIM 条件最大値
#DIM 条件値
#DIM 成功値
#DIM 大成功補正値

SIF !条件最大値
	条件最大値 = 100

条件値 = RAND:条件最大値

SIF !成功値
	成功値 = MIN(90 + GET_SUCCESS_RATE() / 5, 99)

SELECTCASE TFLAG:192
	;通常
	CASE 0
		;大成功
		IF 条件値 <= 9 + 大成功補正値
			TFLAG:193 = 1
		;失敗
		ELSEIF 条件値 >= 成功値
			TFLAG:193 = -1
		;成功
		ELSE
			TFLAG:193 = 0
		ENDIF
	;強制成功or大成功(強制成功時は大成功しやすいように
	CASE 1
		;大成功
		IF 条件値 <= 19 + 大成功補正値 / 2
			TFLAG:193 = 1
		;成功
		ELSE
			TFLAG:193 = 0
		ENDIF
	;強制失敗orコマンド終了
	CASE -1,-2
		TFLAG:193 = -1
ENDSELECT
条件最大値 = 0
条件値 = 0
成功値 = 0
大成功補正値 = 0

@SOMATOMETRY(ARG)
PRINTFORML %CALLNAME:ARG%の身体測定を行った
CALL MAKE_SIZE(ARG)
CFLAG:ARG:身体測定 = 1
CALL SHOW_SIZE(ARG)
CALL ASK_YN("再測定する","これでよし")
IF !RESULT
	CFLAG:ARG:身体測定 = 2
	RESTART
ENDIF

