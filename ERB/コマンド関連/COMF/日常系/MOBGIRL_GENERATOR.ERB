体格差 = (TALENT:MASTER:体型 + 2 <= TALENT:ARG:体型)
IF TALENT:MASTER:幼稚 || TALENT:MASTER:幼児／幼児退行 || 年齢差 == -1
	IF TALENT:ARG:幼稚 || TALENT:ARG:幼児／幼児退行
		年齢差 = 0
	ELSE
		年齢差 = -1
	ENDIF
ELSEIF (TALENT:ARG:幼稚 || TALENT:ARG:幼児／幼児退行) && 年齢差 != 2
	年齢差 = 1
ENDIF
モブ子一人称 = %TEXTR(@"あたい/あたし/私/わたし/%CALLNAME:ARG%")%
IF CFLAG:ARG:性格傾向 == 2 ||  (TALENT:ARG:度胸 > 0 && TALENT:ARG:プライド > 0)
	SIF !RAND:5 && CSTR:ARG:モブ子種族 == "鬼"
		モブ子一人称 = %TEXTR("オレ/俺")%
	SIF !RAND:4
		モブ子一人称 = %TEXTR(@"ぼく/ボク/僕/%CALLNAME:ARG%")%
	モブ子二人称 = %TEXTR("おまえ/お前/あんた")%
	SIF STRCOUNT(モブ子二人称, "(お前|おまえ)") && (!RAND:4 || TALENT:ARG:風俗嬢)
		モブ子二人称 = %モブ子二人称%さん
	SIF RAND:3
		GOTO SKIP
	SIF !RAND:3 && !TALENT:ARG:人妻
		モブ子二人称 = %\@ 呼称 ? %TEXTR("兄さん/兄ちゃん/にいさん")% # %TEXTR("姉さん/姉ちゃん/姐さん")% \@%
	SIF RAND:2
		GOTO SKIP
	IF 年齢差 > 0
		IF 呼称
			IF CSTR:ARG:モブ子種族 == "鬼" && 敵対
				モブ子二人称 = %\@ 年齢差 == 2 ? じじい # おっさん \@%
			ELSEIF RAND:2
				モブ子二人称 = %\@ 年齢差 == 2 ? じい # おっ \@%ちゃん
			ELSE
				モブ子二人称 = おじ%\@ 年齢差 == 2 ? い # \@%さん
			ENDIF
		ELSE
			IF CSTR:ARG:モブ子種族 == "鬼" && 敵対
				モブ子二人称 = %\@ 年齢差 == 2 ? ババア # バアさん \@%
			ELSEIF RAND:2
				モブ子二人称 = %\@ 年齢差 == 2 ? ばあ # おば \@%ちゃん
			ELSE
				モブ子二人称 = おば%\@ 年齢差 == 2 ? あ # \@%さん
			ENDIF
		ENDIF
	ELSEIF 体格差 > 0
		IF 敵対 && CSTR:ARG:モブ子種族 == "鬼"
			モブ子二人称 = チビ%\@ 呼称 ? 助 # 女 \@%
		ELSE
			モブ子二人称 = おチビ%\@ 呼称 ? さん # ちゃん \@%
		ENDIF
	ELSEIF 年齢差 < 0 && 敵対 && CSTR:ARG:モブ子種族 == "鬼"
		モブ子二人称 = %\@ 呼称 ? クソ # メス \@%ガキ
	ENDIF
ELSE
	モブ子二人称 = %TEXTR("あんた/あなた")%
	SIF TALENT:ARG:風俗嬢
		モブ子二人称 = あなた
	IF CFLAG:ARG:性格傾向 == 4
		モブ子一人称 = %TEXTR("自分/わたし/私")%
		モブ子二人称 = あなた
		RETURN
	ELSEIF (RAND:2 && CFLAG:ARG:性格傾向 == 3) || !RAND:5
		モブ子一人称 = %TEXTR(@"ぼく/ボク/僕/%CALLNAME:ARG%")%
	ENDIF
	SIF RAND:3
		GOTO SKIP
	SIF !RAND:3 && !TALENT:ARG:人妻
		モブ子二人称 = %\@ 呼称 ? %TEXTR("兄さん/お兄さん")% # %TEXTR("姉さん/お姉さん")% \@%
	SIF RAND:2
		GOTO SKIP
	IF 年齢差 > 0 
		IF 呼称
			SELECTCASE CFLAG:ARG:性格傾向
				CASE 1
					モブ子二人称 = %\@ 年齢差 == 2 ? お爺 # おじ \@%さん
				CASE 3
					モブ子二人称 = %\@ 年齢差 == 2 ? じい # おっ \@%ちゃん
					SIF 年齢差 == 1 && RAND:3
						モブ子二人称 = %TEXTR("兄ちゃん/お兄ちゃん/おにいちゃん")%
				CASE 5
					モブ子二人称 = %\@ 年齢差 == 2 ? おじいさん # オジさん \@%
			ENDSELECT
		ELSE
			SELECTCASE CFLAG:ARG:性格傾向
				CASE 1
					モブ子二人称 = おば%\@ 年齢差 == 2 ? あ # \@%さん
				CASE 3
					モブ子二人称 = %\@ 年齢差 == 2 ? ばあ # おば \@%ちゃん
					SIF 年齢差 == 1 && RAND:3
						モブ子二人称 = %TEXTR("姉ちゃん/お姉ちゃん/おねえちゃん")%
				CASE 5
					モブ子二人称 = %\@ 年齢差 == 2 ? お婆 # オバ \@%さん
			ENDSELECT
		ENDIF
	ELSEIF 体格差 > 0 || 年齢差 < 0 
		モブ子二人称 = %\@ 呼称 ? 坊や # お嬢ちゃん \@%
	ENDIF
ENDIF
$SKIP
SIF モブ子一人称 == CALLNAME:ARG && !RAND:3
	モブ子一人称 += "ちゃん"
RETURN
