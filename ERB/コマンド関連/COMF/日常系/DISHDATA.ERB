RESULT = 1
;材料判定,いずれかの材料を1つでも持っていれば作れる
FOR LOCAL, 1, 11
	SIF DISH_ING:LOCAL < 0
		BREAK
	SIF ITEM:(DISH_ING:LOCAL) > 0
		RESULT = 10
NEXT

;必要技能判定
SIF ABL:MASTER:料理技能 < SKILL_LV
	RESULT = 2

;練習時は判定を無視してDISH_TYPEを決定する
IF 練習モード
	DISH_TYPE = %TYPECHECK%
ELSE
	;TYPECHECK判定
	SIF TYPECHECK != ARGS
		RESULT = 0

	;固有料理判定
	IF INRANGE(DISH_NUM, 400, 400 + CHARANUM)
		SIF CFLAG:SP_CHARA:固有料理 < 100
			RESULT = 0
	ENDIF
ENDIF

;存在判定
SIF DISH_NAME == ""
	RESULT = 0

RESULT:1 = SKILL_LV
RESULTS:1 = %TYPECHECK%
RETURN RESULT
;-------------------------------------------------
;料理の材料を設定する関数
;材料は１つで作れるので多いほど作る手段が多い≒作りやすい
;"汎用"を入れると汎用食材がセットされる
;"魚"を入れると魚食材がセットされる
;"作成不能"を入れるとどうやっても作れないようになる（表示はされる）
;-------------------------------------------------
@材料設定(ARGS:1, ARGS:2, ARGS:3, ARGS:4, ARGS:5, ARGS:6, ARGS:7, ARGS:8, ARGS:9, ARGS:10)
VARSET DISH_ING, -1
IF ARGS:1 == "汎用"
	DISH_ING:1 = GETNUM(ITEM, "高級食材")
	DISH_ING:2 = GETNUM(ITEM, "上質食材")
	DISH_ING:3 = GETNUM(ITEM, "安物食材")
	RETURN
ELSEIF ARGS:1 == "魚"
	DISH_ING:1 = GETNUM(ITEM, "うまい魚")
	DISH_ING:2 = GETNUM(ITEM, "魚")
	RETURN
ELSEIF ARGS:1 == "虫"
	DISH_ING:1 = GETNUM(ITEM, "レアな虫")
	DISH_ING:2 = GETNUM(ITEM, "虫")
	RETURN
ELSEIF ARGS:1 == "作成不能"
	FOR LOCAL, 1, 11
		DISH_ING:LOCAL = -1
	NEXT
	RETURN
ENDIF
FOR LOCAL, 1, 11
	IF ARGS:LOCAL == ""
		;ローターが0番なので無いものは-1を入れておく
		DISH_ING:LOCAL = -1
	ELSE
		DISH_ING:LOCAL = GETNUM(ITEM, ARGS:LOCAL)
		IF GETNUM(ITEM, ARGS:LOCAL) == -1
			PRINTFORML アイテム名が間違ってるよ！＞%ARGS:LOCAL%
			THROW
		ENDIF
	ENDIF
NEXT

