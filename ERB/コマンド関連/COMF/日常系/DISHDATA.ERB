	SELECTCASE ITEMNAME:ARG
	;肉はヤマドリ以外追加しない
	CASE "獣肉", "鳥肉"
	;アイスは追加しない
	CASE "アイス"
	;キノコ素材は[キノコ]を追加する
	CASE "普通のキノコ", "毒キノコ", "ねばねばキノコ", "冬虫夏草", "やばいキノコ", "魔法のキノコ"
		DISH_TASTE += "キノコ/"
	;イチゴと仙桃は[果物]と両方追加する
	CASE "イチゴ", "仙桃"
		DISH_TASTE += ITEMNAME:ARG + "/"
		DISH_TASTE += "果物/"
	;ほかはそのまま追加する
	CASEELSE
		DISH_TASTE += ITEMNAME:ARG + "/"
	ENDSELECT
