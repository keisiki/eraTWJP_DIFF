		なし崩し必要値 += 100
	SIF CFLAG:ARG:既成事実 & 合意_うふふ
		なし崩し必要値 -= 50
	SIF TALENT:ARG:度胸 < 0
		なし崩し必要値 -= 200
	CALL SHOW_LOVEPOWER(CFLAG:ARG:合意判定,50)
	CALL SHOW_REASONWALL(なし崩し必要値 ,50)
