@COM127
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;奉仕快楽フラグ
TCVAR:108 = 1

CSTR:PLAYER:10 += @"{TARGET}/"
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:PLAYER:2 = 12
;強度
TCVAR:PLAYER:4 = 300

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 100
DOWNBASE:気力 += 150

SOURCE:PLAYER:快Ａ = 200
SOURCE:性行動 = 300
SOURCE:達成 = 200
SOURCE:征服 = 800
SOURCE:不潔 = 200
SOURCE:反感 = 100

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 400

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;対象の手⇔調教者のＡの汚れが移動
STAIN:1 |= STAIN:PLAYER:4
STAIN:PLAYER:4 |= STAIN:1

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 7
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 7
ENDIF

STR:0 = Ａパールを入れられる

CALL PEARL_INSERT(TARGET, 5)

PRINTFORML %CALLNAME:TARGET%にアナルパールを入れてもらった
PRINTFORMW {TFLAG:パール期限-TIME}分間出し切らないようにがんばろう

FOR LOCAL, 0, CHARANUM
	CALL ENDUFUFU(LOCAL)
NEXT
TFLAG:106 = 0

RETURN 1

