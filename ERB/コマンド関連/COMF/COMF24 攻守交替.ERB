;-------------------------------------------------
;攻守交替
;-------------------------------------------------
@COM24
VARSET LOCAL
CFLAG:MASTER:うふふ = 2
TFLAG:COMABLE管理 = 3
FOR LOCAL,1,CHARANUM
	IF CFLAG:MASTER:現在位置 == CFLAG:LOCAL:現在位置
		CFLAG:LOCAL:うふふ = 2
		LOCAL:1 ++
	ENDIF
NEXT
PRINTFORML %CALLNAME:TARGET%\@ LOCAL:1 > 1 ? 達 #  \@に主導権を譲った
TIME += 5

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE24
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(150)
	RETURN RESULT
;相手の欲情
SIF GETPALAMLV(PALAM:欲情,10) < 5
	RETURN 0
SIF TCVAR:無理矢理
	RETURN 0
;時間停止中も駄目
SIF !SHIRAHU(TARGET)
	RETURN 0
;なんか装着してるとダメ
FOR LOCAL,11,25
	SIF TEQUIP:LOCAL
		RETURN 0
NEXT
;挿入中は駄目
SIF TEQUIP:50 != -1
	RETURN 0
SIF TEQUIP:51 != -1
	RETURN 0
;シャワー中はダメ
SIF TEQUIP:32
	RETURN 0
;乳首吸い中はダメ
SIF TEQUIP:41
	RETURN 0
;乳揉み中はダメ
SIF TEQUIP:42
	RETURN 0
;継続キス
SIF TEQUIP:継続キス
	RETURN 0

RETURN 1
