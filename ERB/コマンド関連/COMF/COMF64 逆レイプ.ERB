@COM_ABLE64
;逆レイプ実行判定
SIF !TFLAG:100
	RETURN 0
SIF GLOBAL_COMABLE(64)
	RETURN RESULT
;ある程度勃起してないとダメ
SIF BASE:勃起 < 1000
	RETURN 0
;調教者がオトコだとダメ
SIF !(TALENT:PLAYER:2 & 1)
	RETURN 0
;体格差がある場合は経験がないとダメ
SIF 挿入不可(TARGET) == 1 && EXP:Ｖ経験 < 50 && !TALENT:PLAYER:禁断の知識
	RETURN 0
;さらに体格差があると禁断の知識が必要
SIF 挿入不可(TARGET) == 2 && (!TALENT:PLAYER:禁断の知識 || EXP:Ｖ経験 < 50)
	RETURN 0
SIF 挿入不可(TARGET) == 3
	RETURN 0

;オトコ、半人半妖、ふたなりでないとダメ
SIF !(HAS_PENIS(TARGET)) 
	RETURN 0
;下半身着衣;ズボンは不可になってるけど、ズボンならおちんちん出せても問題なよね？
;SIF TEQUIP:0 & 4
	;RETURN 0
;下半身着衣(PLAYER);入れられる方はさすがにズボンはいてるなら無理だよね
SIF TEQUIP:PLAYER:0 & 4
	RETURN 0

;お風呂場プレイ中はプレイマットが無いとダメ
IF TEQUIP:31
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
;新妻プレイ中はダメ
SIF TEQUIP:33
	RETURN 0
;停止中は不可
;SIF FLAG:70 == 1
	;RETURN 0
RETURN 1

