;*********************************************************************************************
;カウンターコマンド：中出し後
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOMF9(ARG)

	;体勢の変更
	;=============================
	CALL TOUCH_RESET_T(ARG)

	;ソース計算および内部処理系統
	;=============================
	SOURCE:ARG:欲情 += 充填率(ARG,4) * 2
	SOURCE:ARG:屈従 += 充填率(ARG,4) * 2

	;マスターカウンター制御
	;=============================
	;なし

RETURN 1

;*********************************************************************************************
;カウンター実行可否出力関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ABLE9(ARG)

	SIF 充填率(ARG,4) < 15
			RETURN 0

RETURN 1

;*********************************************************************************************
;カウンター実行判定値算出関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ORDER9(コマンド番号, ARG, 基本値)
#DIM コマンド番号
#DIM 基本値
#DIMS カウンタータイプ

	;カウンタータイプ設定
	;=============================
	カウンタータイプ = コミュ

	;実行判定値算出式
	;=============================
	LOCAL = 基本値 + CFLAG:ARG:誘惑 + CFLAG:ARG:積極性

	;カウンタータイプ別計算処理 実質実行判定処理を含む
	;=============================
	LOCAL *= COUNTER_TYPE(カウンタータイプ, ARG)

RETURN LOCAL
