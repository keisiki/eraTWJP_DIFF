;*********************************************************************************************
;カウンターコマンド：正常位Ｖおねだり
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOMF41(ARG)

	;体勢の変更
	;=============================
	CALL DATUI_FOR_INSERT(ARG)

	;ソース計算および内部処理系統
	;=============================
	TCVAR:ARG:避妊 = 1
	SOURCE:ARG:誘惑 += 1000
	SOURCE:ARG:挑発 += 500
	SOURCE:ARG:強要 += 150

	;マスターカウンター制御
	;(ビット立ちで不可 0=尻を撫でる 1=抱き付く 2=キスする 3=胸愛撫 4=アナル愛撫 5=クリ愛撫 6=指挿入れ 7=スカートめくり)
	;=============================
	;CALL カウンター挿入され継続中処理(ARG)

RETURN 1

;*********************************************************************************************
;カウンター実行可否出力関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ABLE41(ARG)

	SIF 挿入不可(ARG) >= 2
		RETURN 0
	SIF !(TALENT:ARG:2 & 1)
		RETURN 0
	SIF TALENT:ARG:妊娠 && (CFLAG:ARG:903 <= 30 || CFLAG:ARG:903 >= 71)
		RETURN 0
	SIF CFLAG:ARG:育児人数 >= 1 && CFLAG:ARG:育児人数 <= 30
		RETURN 0
	SIF !(TALENT:PLAYER:2 & 2)
		RETURN 0
	SIF BASE:MASTER:勃起 < 1000
		RETURN 0
	SIF TFLAG:MASTERのＶ使用中 || TEQUIP:50 >= 0 || TEQUIP:51 >= 0 || TFLAG:MASTERのＡ使用中
		RETURN 0
	SIF TEQUIP:MASTER:50 >= 0 || TEQUIP:MASTER:51 >= 0
		RETURN 0
	SIF PALAM:ARG:潤滑 < PALAMLV:3
		RETURN 0
	SIF TEQUIP:ARG:バイブ
		RETURN 0
	SIF TEQUIP:ARG:体位
		RETURN 0
	SIF TEQUIP:MASTER:50 >= 0 || TEQUIP:MASTER:51 >= 0
		RETURN 0
	;IF 妊娠可否(TARGET) && CFLAG:TARGET:生解禁 != 2 && !TALENT:TARGET:無知 && !TCVAR:TARGET:泥酔
	;	SIF !TEQUIP:MASTER:コンドーム
	;		RETURN 0
	;ENDIF
	;貞操帯、触手下着、バイブパンツ、二穴バイブパンツをはいてるとダメ
	SIF EQUIP:下半身下着２ == 27 || EQUIP:下半身下着２ == 28 || EQUIP:下半身下着２ == 29 || EQUIP:下半身下着２ == 30
		RETURN 0

RETURN 1

;*********************************************************************************************
;カウンター実行判定値算出関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ORDER41(コマンド番号, ARG, 基本値)
#DIM コマンド番号
#DIM 基本値
#DIMS カウンタータイプ

	;カウンタータイプ設定
	;=============================
	カウンタータイプ = おねだり

	;実行判定値算出式
	;=============================
	LOCAL = 基本値 + ABL:ARG:Ｖ感覚 * 4 + EX:ARG:Ｖ寸止め * 10

	;カウンタータイプ別計算処理 実質実行判定処理を含む
	;=============================
	LOCAL = MAX(LOCAL + COUNTER_SIKOU:ARG:コマンド番号, 1)
	LOCAL *= COUNTER_TYPE(カウンタータイプ, ARG)

RETURN LOCAL
