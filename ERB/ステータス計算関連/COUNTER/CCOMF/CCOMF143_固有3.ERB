;*********************************************************************************************
;カウンターコマンド：固有3
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOMF143(ARG)
TRYCALLFORM UNIQUE_COUNTER3_SOURCE_K{ARG}
RETURN 1

;*********************************************************************************************
;カウンター実行可否出力関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ABLE143(ARG)
RESULT = 0
TRYCALLFORM UNIQUE_COUNTER3_ABLE_K{ARG}
RETURN RESULT

;*********************************************************************************************
;カウンター実行判定値算出関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ORDER143(コマンド番号, ARG, 基本値)
#DIM コマンド番号
#DIM 基本値

RESULT = 0
RESULTS =
TRYCALLFORM UNIQUE_COUNTER3_FREQUENCY_K{ARG}

	;=============================
	LOCAL = 基本値 + RESULT
	;カウンタータイプ別計算処理 実質実行判定処理を含む
	;=============================
	LOCAL *= COUNTER_TYPE(RESULTS, ARG)
RETURN LOCAL
