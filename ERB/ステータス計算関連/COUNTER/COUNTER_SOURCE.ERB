@EVENT_COUNTER_SOURCE(ARG,ARG:1)
SIF TCVAR:ARG:30 > 1
	RETURN 0
IF ARG:1 == 1

ENDIF

CALL カウンター挿入継続処理(ARG)

SELECTCASE TCVAR:ARG:カウンター行動
;ここからリアクション
	;膣性交
	CASE 500
		SELECTCASE TCVAR:ARG:21
			CASE 1
				TCVAR:PLAYER:4 += 700
				SOURCE:ARG:挑発 += 300
			;だいしゅきホールド
			CASE 2
				TCVAR:PLAYER:4 += 300
				SOURCE:ARG:誘惑 += 500
			;性交見せつけ
			CASE 3
				TCVAR:PLAYER:4 += 100
				SOURCE:ARG:挑発 += 500
				SOURCE:ARG:辱め += 500
			;ラブ握り
			CASE 7
				SOURCE:ARG:情愛 += 1000
				EXP:ARG:愛情経験 ++
		ENDSELECT
	CASE 501
		SELECTCASE TCVAR:ARG:21
			CASE 1
				TCVAR:PLAYER:4 += 700
				SOURCE:ARG:挑発 += 300
			;ラブ握り
			CASE 2
				SOURCE:ARG:情愛 += 1000
				EXP:ARG:愛情経験 ++
		ENDSELECT
	;手淫
	CASE 502
		SELECTCASE TCVAR:ARG:21
			CASE 1
				LOCALS = {ARG}/
				CSTR:PLAYER:10 += LOCALS
				TCVAR:PLAYER:2 = 3
				;強度
				TCVAR:PLAYER:4 += 200
				SIF !EXP_UP(24,ARG)
					EXP:ARG:手淫経験 ++
		ENDSELECT
	;フェラチオ
	CASE 503
		SELECTCASE TCVAR:ARG:21
			CASE 1
				LOCALS = {ARG}/
				CSTR:PLAYER:10 += LOCALS
				TCVAR:PLAYER:2 = 4
				;強度
				TCVAR:PLAYER:4 += 300
				SOURCE:ARG:快Ｍ += 200
				SIF !EXP_UP(25,ARG)
					EXP:ARG:口淫経験 ++
			CASE 2
				TCVAR:PLAYER:4 += 150
				SOURCE:ARG:奉仕 += 200
				SOURCE:ARG:快Ｍ += 200
			;ディープスロート
			CASE 3
				TCVAR:PLAYER:4 += 500
				SOURCE:ARG:奉仕 += 300
				SOURCE:ARG:苦痛 += 100
				SOURCE:ARG:快Ｍ += 300
			;フェラ自慰
			CASE 4
				SOURCE:ARG:快Ｃ += 100
				SOURCE:ARG:露出 += 200
				SOURCE:ARG:快Ｍ += 200
				EXP:ARG:自慰経験 ++
		ENDSELECT
	;パイズリ
	CASE 504
		SELECTCASE TCVAR:ARG:21
			CASE 1
				LOCALS = {ARG}/
				CSTR:PLAYER:10 += LOCALS
				TCVAR:PLAYER:2 = 5
				;強度
				TCVAR:PLAYER:4 += 300
				SIF !EXP_UP(26,ARG)
					EXP:ARG:パイズリ経験 += 2
			;縦パイズリ
			CASE 2
				TCVAR:PLAYER:4 += 300
				SOURCE:ARG:誘惑 += 300
				SOURCE:ARG:奉仕 += 300
			;谷間を作る
			CASE 3
				TCVAR:PLAYER:4 += 500
				SOURCE:ARG:奉仕 += 500
			CASE 4
				TCVAR:PLAYER:4 += 1000
				SOURCE:ARG:奉仕 += 200
				SOURCE:ARG:情愛 += 100
				SOURCE:ARG:達成 += 100
				SOURCE:ARG:快Ｍ += 200
				EXP:ARG:口淫経験 ++
				EXP:ARG:パイズリ経験 += 2
				;愛情経験
				SIF TALENT:ARG:恋慕 && ASSIPLAY == 0
					EXP:ARG:愛情経験 += 1
				;奴隷が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
				SIF TALENT:ARG:猫舌
					EXP:ARG:嗜虐快楽経験 ++
		ENDSELECT
	;素股
	CASE 505
		SELECTCASE TCVAR:ARG:21
			CASE 1
				LOCALS = {ARG}/
				CSTR:PLAYER:10 += LOCALS
				TCVAR:PLAYER:2 = 6
				;強度
				TCVAR:PLAYER:4 += 300
			;手淫追加
			CASE 2
				TCVAR:PLAYER:4 += 300
				SOURCE:ARG:奉仕 += 300
				EXP:ARG:手淫経験 ++
		ENDSELECT
	;無理矢理膣性交
	CASE 506
		SELECTCASE TCVAR:ARG:21
			;身をよじる
			CASE 1
				SOURCE:ARG:苦痛 += 300
				SOURCE:ARG:反感 += 2000
				DOWNBASE:ARG:体力 += 50
				DOWNBASE:ARG:気力 += 100
			;涙目
			CASE 2
				SOURCE:ARG:屈従 += 100
				SOURCE:ARG:反感 += 1000
				DOWNBASE:ARG:気力 += 50
		ENDSELECT
	;玉揉み
	CASE 600
		TCVAR:PLAYER:4 += 200
		SOURCE:ARG:誘惑 += 200
		SOURCE:ARG:奉仕 += 200
		SOURCE:ARG:挑発 += 200
		SOURCE:ARG:征服 += 300
		SIF TALENT:ARG:サド || ABL:ARG:サドっ気 > 4
			SOURCE:ARG:加虐 += 100
			TCVAR:ARG:嗜虐フラグ = 1
	;アナル愛撫
	CASE 601
		TCVAR:PLAYER:4 += 100
		SOURCE:ARG:辱め += 300
		SOURCE:ARG:加虐 += 100
		SOURCE:PLAYER:快Ａ += 300
		TCVAR:ARG:嗜虐フラグ = 1
ENDSELECT

CALL 胸弄り解除処理(ARG)
CALL カウンター挿入され継続中処理(ARG)

RETURN 1
