SIF TEQUIP:ARG:アイマスク
	CALLFORM EQUIP_COM{COMアイマスク}(ARG)

;縄で緊縛中
SIF TEQUIP:ARG:縄
	CALLFORM EQUIP_COM{COM縄}(ARG)

;ボールギャグ装着中
SIF TEQUIP:ARG:ボールギャグ
	CALLFORM EQUIP_COM{COMボールギャグ}(ARG)

;浣腸＋アナルプラグ装着中
SIF TEQUIP:ARG:浣腸
	CALLFORM EQUIP_COM{COM浣腸}(ARG)

;拡張バルーン装着中
SIF TEQUIP:ARG:拡張バルーン
	CALLFORM EQUIP_COM{COM拡張バルーン}(ARG)

;アナル電極装着中
SIF TEQUIP:ARG:アナル電極
	CALLFORM EQUIP_COM{COMアナル電極}(ARG)

;ビデオ撮影中
SIF ARG > 0 && TEQUIP:MASTER:ビデオ撮影
	CALLFORM EQUIP_COM{COMビデオ撮影}(ARG)

;羞恥プレイ中
;SIF TEQUIP:ARG:30
;	CALL EQUIP_COM186(ARG)

;お風呂場プレイ中
SIF TEQUIP:ARG:お風呂場プレイ
	CALLFORM EQUIP_COM{COMお風呂場プレイ}(ARG)

;シャワー使用中
SIF TEQUIP:ARG:シャワー
	CALLFORM EQUIP_COM{COMシャワー}(ARG)

;新妻プレイ中
SIF TEQUIP:ARG:新妻プレイ
	CALLFORM EQUIP_COM{COM新妻プレイ}(ARG)

;Ｖ挿入中
SIF TEQUIP:ARG:50 >= 0 && !SOURCE:ARG:快Ｖ
	CALL EQUIP_INCERT_V(ARG)

;Ａ挿入中
SIF TEQUIP:ARG:51 >= 0 && !SOURCE:ARG:快Ａ
	CALL EQUIP_INCERT_A(ARG)

;相合い傘中
SIF TEQUIP:ARG:相合い傘 && OUTROOF(CFLAG:ARG:現在位置) && ARG == TARGET
	CALLFORM EQUIP_COM{COM相合い傘}(ARG)
RETURN 1
