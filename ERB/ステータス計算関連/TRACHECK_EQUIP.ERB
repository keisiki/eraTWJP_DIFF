;アナルバイブ装着中
SIF TEQUIP:ARG:アナルバイブ
	CALLFORM EQUIP_COM{COMアナルバイブ}(ARG)

;アナルビーズ使用中
SIF TEQUIP:ARG:アナルビーズ
	CALLFORM EQUIP_COM{COMアナルビーズ}(ARG)

;ニプルキャップ装着中
SIF TEQUIP:ARG:ニプルキャップ
	CALLFORM EQUIP_COM{COMニプルキャップ}(ARG)

;搾乳器装着中
