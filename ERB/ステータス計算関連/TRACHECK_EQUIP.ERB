@EQUIP_CHECK(ARG)
;クリキャップ装着中
SIF TEQUIP:ARG:クリキャップ
	CALLFORM EQUIP_COM{COMクリキャップ}(ARG)

;オナホール装着中
SIF TEQUIP:ARG:オナホール
	CALLFORM EQUIP_COM{COMオナホール}(ARG)

