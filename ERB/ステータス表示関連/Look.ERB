			ELSEIF TEQUIP:拡張バルーン
				PRINT  バルーンで拡張されている
			ELSEIF TEQUIP:アナル電極
				PRINT  電極が挿入されている
			ENDIF
		ENDIF
		CALL LOOK_GIFT(TARGET)
	ENDIF
	IF FLAG:画像表示
		SIF !TFLAG:LOOK非表示
			PRINTL 　
		CALL PRINT_TARGET_IMAGE(TARGET)
	ENDIF
	IF !TFLAG:LOOK非表示
		IF TEQUIP:PLAYER:Ｖセックス == TARGET || TEQUIP:PLAYER:Ａセックス == TARGET
			SELECTCASE TEQUIP:MASTER:体位
				CASE 1 
					挿入状況2 = 正常位で
				CASE 2 
					挿入状況2 = 後背位で
				CASE 3 
					挿入状況2 = 騎乗位で
				CASE 4 
					挿入状況2 = 対面座位で
				CASE 5 
					挿入状況2 = 背面座位で
				CASE 6
					挿入状況2 = オナホのような扱いで
				CASEELSE
					挿入状況2 = 
			ENDSELECT

			PRINTL 
			PRINT 【ペニス】
			IF TEQUIP:PLAYER:Ｖセックス == TARGET
				PRINTFORM %CALLNAME:PLAYER%の膣内に%挿入状況2%挿入されている
			ELSE
				PRINTFORM %CALLNAME:PLAYER%のアナルに%挿入状況2%挿入されている
			ENDIF
		ENDIF
	ENDIF
ELSE
	IF !TFLAG:LOOK非表示
