;-------------------------------------------------
;69,対面座位アナル
;-------------------------------------------------
@MESSAGE_COM69
IF FLAG:70 == 1
	;連続
	IF SELECTCOM == PREVCOM && ASSIPLAY == TFLAG:150
		PRINTFORML %CALLNAME:PLAYER%は時間の止まった%CALLNAME:TARGET%にキスをして、舌を絡めながら再び腰を揺すり始めた…
		RETURN 0
	;抜かず
	ELSEIF TCVAR:101 == PLAYER
		PRINTFORM 腸内から
		;調教者についてる
		IF TALENT:PLAYER:2 & 2
			PRINTFORM %TINKO(PLAYER)%を
		ELSE
			PRINTFORM ディルドを
		ENDIF
			PRINTFORM 抜き去ることなく時間の止まった%CALLNAME:TARGET%の身体を背中から抱きしめ、
			PRINTFORML 身体を揺さぶって%CALLNAME:TARGET%を使った自慰を再開した…
	ELSE
		PRINTFORM 時間の止まった%CALLNAME:TARGET%の身体を抱き上げ
		SIF TEQUIP:0 & 2
			PRINTFORM %SHOW_BOTTOM(TARGET,2)%を強引にずらし
		PRINTFORM %TINKO(PLAYER)%目がけ降ろして
		;潤滑LV4以上
		SIF PALAM:潤滑 >= PALAMLV:4
			PRINT 粘液にまみれた
		PRINTFORM 尻穴へと挿入すると、
	ENDIF
	;ボールギャグの有無
	SIF TEQUIP:ボールギャグ == 0
		PRINT 口付けをしつつ
	PRINTL %CALLNAME:TARGET%のアナルで自慰するべく腰を揺り動かした…
ELSE
	;連続
	IF SELECTCOM == PREVCOM && ASSIPLAY == TFLAG:150
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:PLAYER%とキスをし、舌を絡めながら再び腰を揺すり始めた…
		RETURN 0
	;抜かず
	ELSEIF TCVAR:101 == PLAYER
		PRINTFORM 腸内から
		;調教者についてる
		IF TALENT:PLAYER:2 & 2
			PRINTFORM %TINKO(PLAYER)%を
		ELSE
			PRINTFORM ディルドを
		ENDIF
			PRINTFORM 抜き去ることなく%CALLNAME:TARGET%の身体をぎゅっと抱き寄せ、
	ELSE
		PRINTFORM %CALLNAME:TARGET%の身体を抱き上げ
		SIF TEQUIP:0 & 2
			PRINTFORM %SHOW_BOTTOM(TARGET,2)%を強引にずらし
		PRINTFORM ゆっくりと降ろしながら
		;潤滑LV4以上
		SIF PALAM:潤滑 >= PALAMLV:4
			PRINT 粘液にまみれた
		PRINTFORM アヌスへと挿入すると、
	ENDIF
	;ボールギャグの有無
	SIF TEQUIP:ボールギャグ == 0
		PRINT 口付けをしつつ
	PRINTL 腰を揺り動かした…
	;陥落済み
	SIF TALENT:恋慕
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:PLAYER%の首に手を回して抱きついてきた…
ENDIF

