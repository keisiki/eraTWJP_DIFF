;-------------------------------------------------
;310,尻を撫でる
;-------------------------------------------------
@MESSAGE_COM310
IF FLAG:70 == 1
	PRINTFORML %CALLNAME:PLAYER%は背後から%CALLNAME:TARGET%の%HIP(TARGET)%に手を伸ばした
	PRINTFORML 時間を止められている%CALLNAME:TARGET%は無抵抗のままお尻を撫でられている…
ELSEIF CFLAG:TARGET:睡眠 == 1
	PRINTFORML %CALLNAME:PLAYER%は横になって眠る%CALLNAME:TARGET%の%HIP(TARGET)%に手を伸ばした
	IF TALENT:恋慕
		PRINTFORML %CALLNAME:TARGET%は少し頬を染めて%CALLNAME:PLAYER%にされるがままになっている…
	ELSE
		PRINTFORML %CALLNAME:TARGET%は身をよじると無意識に%CALLNAME:PLAYER%の手を払いのけた…
	ENDIF
ELSE
	IF CFLAG:うふふ == 2
		PRINTFORM %CALLNAME:PLAYER%は
		SELECTCASE TCVAR:20
			;クンニ強制
			CASE 70
				IF !TEQUIP:0
					PRINTFORM %CALLNAME:TARGET%の%HIP(TARGET)%をゆっくりと揉みながら押し付けられる%SLIT(TARGET, 1)%に舌で奉仕した…
				ELSE
					GOTO NOMALSIRINADE
				ENDIF
			;アナル奉仕
			CASE 72
				IF !TEQUIP:0
					PRINTFORM %CALLNAME:TARGET%の尻たぶを広げ、尖らせた舌をアヌスに侵入させた…
				ELSE
					GOTO NOMALSIRINADE
				ENDIF
			;スカートの中を見せる
			CASE 35
				PRINTFORM %CALLNAME:TARGET%の尻をこちらに向けさせて撫で回した…
			;素股
			CASE 52
				PRINTFORM 腰の上にまたがった%CALLNAME:TARGET%の%HIP(TARGET)%を揉みしだいた…
			CASEELSE
				$NOMALSIRINADE
				;シックスナイン パンツ有り顔面騎乗(70～72)はこちら
				SIF TEQUIP:シックスナイン
					PRINTFORM 顔の上にまたがった
				PRINTFORM %CALLNAME:TARGET%の%HIP(TARGET)%を揉みしだいた…
		ENDSELECT
	ELSE
		PRINTFORML %CALLNAME:PLAYER%は\@ PREVCOM == 305 ? 膝に寝転んだまま # 背後から \@%CALLNAME:TARGET%の%HIP(TARGET)%に手を伸ばした
		SELECTCASE TFLAG:193
			CASE 1
				IF TALENT:TARGET:10 == -1
					PRINTFORML %CALLNAME:TARGET%はぎゅっと目をつぶり、目の端に涙を浮かべながら%CALLNAME:PLAYER%のセクハラに耐えている…
					PRINTFORML %CALLNAME:PLAYER%はちょっと罪悪感を覚えた
				ELSE
					PRINTFORML %CALLNAME:TARGET%は不快感をあらわにした表情で%CALLNAME:PLAYER%の手を払いのけた…
				ENDIF
			CASE 2,3
				IF OTOKOGIRAI(TARGET)
					PRINTFORML %CALLNAME:TARGET%は小さく悲鳴を上げて身を竦めた
					PRINTFORML %CALLNAME:PLAYER%はちょっと罪悪感を覚えた
				ELSE
					PRINTFORM %CALLNAME:TARGET%は
					IF TARGET == 57 && !TALENT:恋慕
						PRINTFORM 猿の面を浮かべ
					ELSE
						PRINTFORM 顔を赤らめて
					ENDIF
					PRINTFORML %CALLNAME:PLAYER%の手を払いのけた…
				ENDIF
			CASE 4
				PRINTFORM %CALLNAME:TARGET%は
				IF TARGET == 57 && !TALENT:恋慕
					PRINTFORM 猿の面と翁の面を交互に
				ELSE
					PRINTFORM 照れくさそうな表情を
				ENDIF
				PRINTFORML 浮かべながらも%CALLNAME:PLAYER%にされるがままになっている…
		ENDSELECT
		TFLAG:400 = RAND:2
		SIF TFLAG:400 && (DATE_HITOGOMI() || WITH_MOB() == 9)
			CALL DATE_HITOGOMI_MESSAGE(CFLAG:PLAYER:現在位置)
	ENDIF
ENDIF

