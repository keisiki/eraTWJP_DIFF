;-------------------------------------------
;刻印取得時のテキスト
;-------------------------------------------
@MARK_MESSAGE
M:1 = 0
SETCOLOR 247,171,166
CALL MARK_MESSAGE_A
CALL MARK_MESSAGE_P
CALL MARK_MESSAGE_S
SIF SHIRAHU(TARGET)
	CALL MARK_MESSAGE_R
CALL MARK_MESSAGE_T
RESETCOLOR
IF CFLAG:TARGET:口上セレクタ
	TRYCALLFORM M_KOJO_K{NO:TARGET}_{CFLAG:TARGET:口上セレクタ}
ELSE
	TRYCALLFORM M_KOJO_K{NO:TARGET}
ENDIF
;TARGET口上出力
SIF FLAG:口上色
	TRYCALLFORM M_KOJO%RESULTS%_COLOR_K{NO:TARGET}
@MARK_MESSAGE_A
;苦痛刻印
IF FLAG:70 == 1
	IF TFLAG:苦痛刻印取得 == 1
		PRINTFORML 時間の止まった%CALLNAME:TARGET%の身体に苦痛を刻み込んでいく……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV1を得た
	ELSEIF TFLAG:苦痛刻印取得 == 2
		PRINTFORML 時間の止まった%CALLNAME:TARGET%の身体に強い苦痛を刻み込んでいく……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV2を得た
	ELSEIF TFLAG:苦痛刻印取得 == 3
		PRINTFORML 時間の止まった%CALLNAME:TARGET%の身体に強烈な苦痛を刻み込んでいく……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV3を得た
		IF TALENT:PLAYER:サド
			PRINTFORML 【%CALLNAME:PLAYER%がサド】
			PRINTL 異常経験＋２
		ELSEIF TALENT:PLAYER:針さばき && SELECTCOM == 104
			PRINTFORML 【%CALLNAME:PLAYER%が針さばき】
			PRINTL 異常経験＋２
		ELSE
			PRINTL 異常経験＋１
		ENDIF
	ENDIF
ELSEIF CFLAG:MASTER:イタズラ == 2
	IF TFLAG:苦痛刻印取得 == 1
		PRINTFORML 悪夢にうなされる%CALLNAME:TARGET%の身体に苦痛を刻み込んでいく……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV1を得た
	ELSEIF TFLAG:苦痛刻印取得 == 2
		PRINTFORML 悪夢にうなされる%CALLNAME:TARGET%の身体に強い苦痛を刻み込んでいく……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV2を得た
	ELSEIF TFLAG:苦痛刻印取得 == 3
		PRINTFORML 悪夢にうなされる%CALLNAME:TARGET%の身体に強烈な苦痛を刻み込んでいく……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV3を得た
		IF TALENT:PLAYER:サド
			PRINTFORML 【%CALLNAME:PLAYER%がサド】
			PRINTL 異常経験＋２
		ELSEIF TALENT:PLAYER:針さばき && SELECTCOM == 104
			PRINTFORML 【%CALLNAME:PLAYER%が針さばき】
			PRINTL 異常経験＋２
		ELSE
			PRINTL 異常経験＋１
		ENDIF
	ENDIF
ELSE
	IF TFLAG:苦痛刻印取得 == 1
		PRINTFORM %CALLNAME:TARGET%は
		IF TARGET == 57 && !TALENT:恋慕
			PRINTFORM 猿と姥の面をうかべ
		ELSE
			PRINTFORM 顔を歪ませながら
		ENDIF
		PRINTFORML 苦痛を堪えている……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV1を得た
	ELSEIF TFLAG:苦痛刻印取得 == 2
		PRINTFORML %CALLNAME:TARGET%はしばらくの間、苦鳴を漏らし続けていた……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV2を得た
		IF TFLAG:刻印従順変化 == 1 && M:1 == 0
			PRINTL そして従順がLV1になった
			M:1 = 1
		ENDIF
	ELSEIF TFLAG:苦痛刻印取得 == 3
		PRINTFORM %CALLNAME:TARGET%は余りの苦痛に
		IF TARGET == 57 && !TALENT:恋慕
			PRINTFORM 表情は変えずに
		ENDIF
		PRINTFORML 泣き喚いていた……
		PRINTFORMW %CALLNAME:TARGET%は苦痛刻印LV3を得た
		IF TFLAG:刻印従順変化 == 1 && M:1 == 0
			PRINTL そして従順がLV1になった
			M:1 = 1
		ENDIF
		IF TALENT:PLAYER:サド
			PRINTFORML 【%CALLNAME:PLAYER%がサド】
			PRINTL 異常経験＋２
		ELSEIF TALENT:PLAYER:針さばき && SELECTCOM == 104
			PRINTFORML 【%CALLNAME:PLAYER%が針さばき】
			PRINTL 異常経験＋２
		ELSE
			PRINTL 異常経験＋１
		ENDIF
	ENDIF
ENDIF

@MARK_MESSAGE_T
;時姦刻印
IF TFLAG:時姦刻印取得 == 1
	PRINTFORML 時間の止まった%CALLNAME:TARGET%の身体に、性の悦びを教えてやった……
	PRINTFORMW %CALLNAME:TARGET%は時姦刻印LV1を得た
ELSEIF TFLAG:時姦刻印取得 == 2
	PRINTFORML 時間を止められている%CALLNAME:TARGET%の身体に、消しようもない快楽の記憶が刻み込まれた……
	PRINTFORMW %CALLNAME:TARGET%は時姦刻印LV2を得た
ELSEIF TFLAG:時姦刻印取得 == 3
	PRINTFORML 時間を止められている%CALLNAME:TARGET%の身体は、本人の意思を置き去りにして%CALLNAME:MASTER%の与える快楽に屈してしまった……
	PRINTFORMW %CALLNAME:TARGET%は時姦刻印LV3を得た
ENDIF
@MARK_MESSAGE_P
;快楽刻印
IF !FLAG:70
	IF CFLAG:MASTER:イタズラ == 2
		IF TFLAG:23 == 1
			PRINTFORML %CALLNAME:TARGET%は眠ったままの体に快楽を刻みこまされていく……
			PRINTFORMW %CALLNAME:TARGET%は快楽刻印LV1を得た
		ELSEIF TFLAG:23 == 2
			PRINTFORML %CALLNAME:TARGET%は眠りながらも淫乱な体へと変わっていく……
			PRINTFORMW %CALLNAME:TARGET%は快楽刻印LV2を得た
		ELSEIF TFLAG:23 == 3
			PRINTFORML %CALLNAME:TARGET%の体は眠ったまま淫靡な欲求に屈していった……
			PRINTFORMW %CALLNAME:TARGET%は快楽刻印LV3を得た
;		ELSEIF TFLAG:23 == 4
;			PRINTFORML 幼い身体が知るには早すぎた快楽が、%CALLNAME:TARGET%を「女」へと変えていく……
;			PRINTFORMW %CALLNAME:TARGET%は初潮を迎えてしまった
		ENDIF
	ELSE
		IF TFLAG:23 == 1
			PRINTFORML %CALLNAME:TARGET%は覚えたての快楽に身を打ち震わせていた……
			PRINTFORMW %CALLNAME:TARGET%は快楽刻印LV1を得た
		ELSEIF TFLAG:23 == 2
			PRINTFORM %CALLNAME:TARGET%は快楽の余韻に
			IF TARGET == 57 && !TALENT:恋慕
				PRINTFORM 翁の面をうかべ
			ELSE
				PRINTFORM 顔を緩ませてい
			ENDIF
			PRINTFORML た……
			PRINTFORMW %CALLNAME:TARGET%は快楽刻印LV2を得た
		ELSEIF TFLAG:23 == 3
			PRINTFORML %CALLNAME:TARGET%は強烈な快楽に身も心も焦がしているようだ……
			PRINTFORMW %CALLNAME:TARGET%は快楽刻印LV3を得た
			IF TFLAG:刻印従順変化 == 1 && M:1 == 0
			PRINTL そして従順がLV1になった
				M:1 = 1
			ENDIF
;		ELSEIF TFLAG:23 == 4
;			PRINTFORML 幼い身体が知るには早すぎた快楽が、%CALLNAME:TARGET%を「女」へと変えていく……
;			PRINTFORMW %CALLNAME:TARGET%は初潮を迎えてしまった
		ENDIF
	ENDIF
ENDIF

@MARK_MESSAGE_S
;不埒刻印
;快楽による刻印
IF TFLAG:24 == 11
	PRINTFORM %CALLNAME:TARGET%は思わず可愛らしい嬌声を上げたかと思うと、
	IF TARGET == 57 && !TALENT:恋慕
		PRINTFORM 猿の面をうかべ
	ELSE
		PRINTFORM 顔を赤くして
	ENDIF
	PRINTFORML こちらから目を逸らした……
	PRINTFORMW %CALLNAME:TARGET%は不埒刻印LV1を得た
ELSEIF TFLAG:24 == 12
PRINTFORML %CALLNAME:TARGET%は快楽に震えながら、物欲しげな視線を送っていた……
	PRINTFORMW %CALLNAME:TARGET%は不埒刻印LV2を得た
	IF TFLAG:刻印従順変化 == 1 && M:1 == 0
		PRINTL そして従順がLV1になった
		M:1 = 1
	ENDIF
ELSEIF TFLAG:24 == 13
	PRINTFORML %CALLNAME:TARGET%は快楽に乱れながらも、さらなる行為を懇願してきた……
	PRINTFORMW %CALLNAME:TARGET%は不埒刻印LV3を得た
	IF TFLAG:刻印従順変化 == 1 && M:1 == 0
		PRINTL そして従順がLV2になった
		M:1 = 1
	ENDIF
;屈服、恭順による刻印
ELSEIF TFLAG:24 == 31
	PRINTFORML %CALLNAME:TARGET%は媚びるような視線を%CALLNAME:MASTER%に投げかけた……
	PRINTFORMW %CALLNAME:TARGET%は不埒刻印LV1を得た
ELSEIF TFLAG:24 == 32
	PRINTFORML %CALLNAME:TARGET%はもっと%CALLNAME:MASTER%に喜んでもらいたいようだ……
	PRINTFORMW %CALLNAME:TARGET%は不埒刻印LV2を得た
	IF TFLAG:刻印従順変化 == 1 && M:1 == 0
		PRINTL そして従順がLV1になった
		M:1 = 1
	ENDIF
ELSEIF TFLAG:24 == 33
	PRINTFORM %CALLNAME:TARGET%は
	IF TARGET == 57 && !TALENT:恋慕
		PRINTFORM ひょっとこと翁の面をくるくると変えながら
	ELSE
		PRINTFORM どこか陶然とした表情で
	ENDIF
	PRINTFORML じっと%CALLNAME:MASTER%を見つめている……
	PRINTFORMW %CALLNAME:TARGET%は不埒刻印LV3を得た
	IF TFLAG:刻印従順変化 == 1 && M:1 == 0
		PRINTL そして従順がLV2になった
		M:1 = 1
	ENDIF
ENDIF


@MARK_MESSAGE_R
;反発刻印
IF TFLAG:反発刻印取得 == 1
	PRINTFORM %CALLNAME:TARGET%は
	IF TARGET == 57 && !TALENT:恋慕
		PRINTFORM 狐の面を
	ELSE
		PRINTFORM 鋭い視線を
	ENDIF
	PRINTFORML こちらに向けている……
	PRINTFORMW %CALLNAME:TARGET%は反発刻印LV1を得た
ELSEIF TFLAG:反発刻印取得 == 2
	PRINTFORM %CALLNAME:TARGET%は
	IF TARGET == 57 && !TALENT:恋慕
		PRINTFORML 般若の面を浮かべている……
	ELSE
		PRINTFORML 怒りを滲ませた表情をしている……
	ENDIF
	PRINTFORMW %CALLNAME:TARGET%は反発刻印LV2を得た
	IF TFLAG:刻印従順変化 == 1
		PRINTFORMW 従順が{ABL:従順}に下がった
		PRINTFORMW 親密が{ABL:親密}に下がった
		M:1 = 1
	ELSEIF TFLAG:刻印従順変化 == 2 && M:1 == 0
		PRINTFORMW 従順が{ABL:従順}に下がった
		M:1 = 1
	ENDIF
ELSEIF TFLAG:反発刻印取得 == 3
	PRINTFORM %CALLNAME:TARGET%は憤怒に
	IF TARGET == 57 && !TALENT:恋慕
		PRINTFORM 般若と狐の面を浮かべ
	ELSE
		PRINTFORM 顔を歪ませて
	ENDIF
	PRINTFORML 、恨み言を呟いている……
	PRINTFORMW %CALLNAME:TARGET%は反発刻印LV3を得た
	IF TFLAG:刻印従順変化 == 1 && M:1 == 0
		PRINTFORMW 従順が{ABL:従順}に下がった
		PRINTFORMW 親密が{ABL:親密}に下がった
		M:1 = 1
	ELSEIF TFLAG:刻印従順変化 == 2 && M:1 == 0
		PRINTFORMW 従順が{ABL:従順}に下がった
		PRINTFORMW 親密が{ABL:親密}に下がった
		M:1 = 1
	ENDIF
ENDIF

;-------------------------------------------
;SPイベント口上テキスト
;-------------------------------------------
;デートキス共通テキスト
@SPEVENT_MESSAGE_1(ARG,ARG:1)
;二重表示抑制
SIF LOCAL == FLAG:デート相手
	RETURN
PRINTL
PRINTFORML …デートからの帰り道で%CALLNAME:TARGET%はふと立ち止まると
PRINTFORMW 振り返って%CALLNAME:MASTER%の口唇を奪った…
PRINTL
LOCAL = FLAG:デート相手

;デート告白共通テキスト
@SPEVENT_MESSAGE_2(ARG,ARG:1)
IF ARG == 0
	PRINTL
	PRINTFORML デートからの帰り道で%CALLNAME:TARGET%は%CALLNAME:MASTER%を呼び止めた
	PRINTFORMW 僅かに声を震わせ、%CALLNAME:TARGET%は%CALLNAME:MASTER%への想いを告げた…
	PRINTL
ELSEIF ARG == 1
	PRINTFORML %CALLNAME:MASTER%が想いを受け入れると%CALLNAME:TARGET%は
	IF TARGET == 57 && !TALENT:恋慕
		PRINTFORMW 翁の面を乱舞させた…
	ELSE
		PRINTFORMW 喜びに頬を緩ませた…
	ENDIF
	PRINTL
ELSEIF ARG == 2
	PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の言葉に気を落とすと
	IF TARGET == 57 && !TALENT:恋慕
		PRINTFORMW 姥の面で顔を覆い隠した…
	ELSE
		PRINTFORMW 寂しそうにうつむいた…
	ENDIF
	PRINTL
ENDIF

;デート反発刻印消去共通イベント
