			PRINTFORML 「お゛お゛っ%UNICODE(0x2764)%ひぐぅ%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「あんっ%UNICODE(0x2764)%そんなぁふかいぃ%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「おくぅ%UNICODE(0x2764)%もっとぉ%UNICODE(0x2764)%いいぃぃ%UNICODE(0x2764)%すきぃ%UNICODE(0x2764)%%UNICODE(0x2764)%」
