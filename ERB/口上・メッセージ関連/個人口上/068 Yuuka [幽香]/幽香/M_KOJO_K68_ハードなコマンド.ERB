;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K68_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K68_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K68_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:イタズラ == 2
		RETURN 1
	ELSEIF TCVAR:68:泥酔 == 1
		
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「ん……ぐっ！？　っ、ぁ、が、ぐぅ……ッ！」
				SIF ABL:68:マゾっ気 >= 2
					PRINTFORML （苦しいのに、気持ちいい……どうして……？）
			CASEELSE
				PRINTFORML 「っぐ！？　ん、ぁ、ッは……！」
				SIF ABL:68:マゾっ気 >= 2
					PRINTFORML （これぐらい、何てことないわ……）
		ENDSELECT
		PRINTFORMDW 苦しさに顔を歪めながら、幽香は%CALLNAME:MASTER%を睨みつけてくる
	ENDIF
ENDIF

RETURN 1
