;==================================================
;150,胸の谷間を犯す
;==================================================
@M_KOJO_MESSAGE_COM_K68_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K68_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K68_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		PRINTL 
		PRINTFORMDL 幽香の豊満な胸は、まるでそれが使命であるかのように%CALLNAME:MASTER%のペニスを柔らかく包み込む
		PRINTFORMDL その中で激しく前後に腰を動かすと、ちゅこ、ちゅこという音と共に射精感が湧き上がってきた
		PRINTFORMDW 他人を寄せ付けない、暴力的なこの妖怪の厭らしい乳を性欲の吐け口にしていると思うと、自然と腰の動きが早まっていった
	;睡姦中
	ELSEIF CFLAG:MASTER:イタズラ == 2
		PRINTL 
		PRINTFORMDL %CALLNAME:MASTER%は幽香の乳を寄せると、その中にペニスを挟み彼女の寝顔の前でそれを突き動かした
		PRINTFORMDW 眼前で乳を犯されているのに眠り続ける幽香の様子に、まるで性交しているかのように腰が勝手に動いた
	ELSEIF TCVAR:68:泥酔 == 1
		
	;恋慕or恋人
	ELSEIF TALENT:68:恋人 || TALENT:68:恋慕 || TALENT:68:愛欲
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「んっ、ぁ、あっ
				CALL HEARTCB_K68(0xD02830)
				PRINTFORML 　もう、こんなのが気持ちいいの……？」
			CASEELSE
				PRINTFORM 「んっ
				CALL HEARTCB_K68(0xD02830)
				PRINTFORML ！　はぁ、胸とセックスしてるつもり……？」
		ENDSELECT
		PRINTFORMDW 胸で快感を貪る%CALLNAME:MASTER%に呆れつつも、幽香は突き上げに合わせて声を上げている
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「うぅ……こんな、恥ずかしいこと……」
			CASEELSE
				PRINTFORML 「んっ……早くイきなさいよ……！」
		ENDSELECT
		PRINTFORMDW 胸の中で性交しているかのように厭らしい音を立てる%CALLNAME:MASTER%のペニスから、幽香は目をそらしている
	ENDIF
ENDIF

RETURN 1
