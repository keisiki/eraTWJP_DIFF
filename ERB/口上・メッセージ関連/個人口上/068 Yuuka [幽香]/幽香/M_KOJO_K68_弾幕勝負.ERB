			ENDIF
		ENDIF
		CALL K68_DANMAKU_PENALTY()
	ENDIF
	;-------------------------------------------------
	;戦闘後
	;幽香の残機0
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "戦闘後" && 相手残機 == 0
		;童貞賭け
		IF TCVAR:68:398 == 3
			PRINTFORML 「嘘でしょ……ハンデ有りで負けるなんて……！」
			PRINTFORMW 「筆おろし……わかったわよ、やればいいんでしょ！」
		ELSE
			PRINTFORML 「……悔しいけど、私の負けね……自慢でも何でも、好きにすると良いわ」
			;勝利済みフラグ
			IF CFLAG:68:1800
				PRINTFORML 「……え？　見返り……って、また？　恥知らずな奴……」
			ELSE
				PRINTFORMW 「……え？　見返り……って、私に勝っただけじゃ物足りないというの？」
				PRINTFORML 「ふん……まぁいいわ、何か希望でも？」
			ENDIF
			;勝負済みフラグをオン
			CFLAG:68:1801 = 1
			;勝利済みフラグをオン
			CFLAG:68:1800 = 1
		ENDIF
		CALL K68_DANMAKU_REWARD()
	ENDIF
RESETCOLOR
ENDIF
RETURN 1

