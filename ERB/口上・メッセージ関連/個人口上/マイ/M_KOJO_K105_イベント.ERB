;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K105_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		;弱点は精神的な（意味のある）攻撃に繋がるということなので
		IF FIRSTTIME(1105) 
			PRINTFORML 「自分を全部見せるのって、怖いことだと思う」
			PRINTFORML 「すごく無防備じゃない？」
			PRINTFORMW 「自分の底が、弱点が、見えちゃう」
			PRINTFORML 
			PRINTFORML 「だから、できるだけ自分を隠してる」
			PRINTFORML 「他人がわたしをどう見ていても……好きでも、嫌いでも」
			PRINTFORML 「それは本当のわたしじゃない」
			PRINTFORML 「自分でいなければ、何があっても自分だけは傷つかないでしょ」
			PRINTFORMW 「それで十分だし……変に本気でぶつかり合っても、面倒ごとが増えるだけだから」
			PRINTFORML 
			PRINTFORML 「でも最近は、%CALLNAME:MASTER%にはわたしらしく接してるつもり」
			PRINTFORML 「被りものじゃなくって」
			PRINTFORMW 「わたしを、見てほしくなったから……」
			PRINTFORML 
			PRINTFORML 「わたしのことどう思った？」
			PRINTFORML 「隠してない……偽ってない、わたしのこと」
			PRINTFORML 「前の方が好きだった？それとも……」
			PRINTFORMW 「いまさら、戻すつもりもないけどね」
			PRINTFORML 
			PRINTFORML 「…………」
			PRINTFORML 「でも、本当はまだ少し我慢してる」
			PRINTFORMW 「まだ……不安なのかも」
			PRINTFORML 
			PRINTFORML 「%CALLNAME:MASTER%は変わらないで……受け入れて、くれるのかな」
			PRINTFORML 「"わたし"でいても」
			PRINTFORMW 「本気のわたし……」
			PRINTFORML 
			PRINTFORMW 「お願いがあるの」
			PRINTFORMW 「わたしは、%CALLNAME:MASTER%が一番好き」
			PRINTFORMW 「だから、%CALLNAME:MASTER%にとっても一番でいたい……」
			PRINTFORMW 「これで結構、負けず嫌いなんだよね」
			PRINTFORML 
			PRINTFORMW 「どう?」
			PRINTFORMW 「わたしを、一番に愛して欲しいの……約束してくれる？」
		;今度こそ貴様を倒す
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%」
			PRINTFORMW 「まだ、%CALLNAME:MASTER%のことは諦めてないんだけど……」
			PRINTFORML 
			PRINTFORMW 「やっぱり、わたしじゃダメ？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
			PRINTFORML
			PRINTFORMW 「良いの？」
			PRINTFORML 「……そっか、言ってみるもんだね」
			PRINTFORMW 「それじゃあ……」
			PRINTFORML 
			PRINTFORMW 「遠慮しないから%UNICODE(0x2665) *1%」
			PRINTFORML 
			PRINTFORMDL いたずらっぽく笑うマイの姿は、今までで一番楽しそうに見えた。
			PRINTFORMW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
			PRINTFORML
			PRINTFORMW 「そう」
			PRINTFORML 「……わたしも、それでいいと思う」
			PRINTFORMW 「でも……」
			PRINTFORML 
			PRINTFORMW 「めちゃ悔しいかも」
			PRINTFORML
			PRINTFORMDL そう言って顔をそむけたマイの表情を、%CALLNAME:MASTER%が見ることは無かった。
			PRINTFORMW
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K105_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORML 「…………」
			PRINTFORMW 「さよなら」
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋人
			PRINTFORMW 「次はどうしよっか」
			PRINTFORMW 「それとも、続きする？」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「こういうのもアリだね」
			PRINTFORMW 「また誘ってね……%UNICODE(0x2665) *1%」
		ELSEIF TALENT:思慕
			PRINTFORML 「……デート、か……」
			PRINTFORMW （また、一緒に……なんて、柄じゃないか）
			PRINTFORMDL マイは無表情だが、心なしか楽しそうに見える。
			PRINTFORMDW また誘ってもいいだろう。
		ELSE
			PRINTFORML 「……満足？」
			PRINTFORMDW %CALLNAME:MASTER%はとりあえず、自信ありげに頷いた。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORML 「ふふ、もういいよ」
			PRINTFORMW 「でも、なんでも許すわけじゃないからね……なんて」
			PRINTFORMDL マイは見慣れた表情に戻っている。
			PRINTFORMDW すっかり許してもらえたようだ。
		ELSE
			PRINTFORML 「…………」
			PRINTFORML 「……これで少し、お互いのことが分かったかもね」
			PRINTFORMDW とりあえず、許してもらえたようだ。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMDW 態度が柔らかくなったのが分かる。どうやら許してもらえたようだ。
			PRINTFORML 
			PRINTFORMW 「あんた、すっかり許してもらえたとか思ってないよね？」
			PRINTFORML 
			PRINTFORMDW ほどほどに許してもらえたようだ。
		ELSE
			PRINTFORML 「……はぁ」
			PRINTFORMW 「……まだ何か用？」
			PRINTFORMDL 心なしか態度が柔らかくなったように思う。
			PRINTFORMDW 印象はいくらかマシになったようだ。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORML 「少し頭が冷えたわ」
			PRINTFORMW 「でも、やり直せるかどうかはこれから次第」
			PRINTFORMDL まだ、何とかなる目はありそうだ。
		ELSE
			PRINTFORMW 「……いざとなれば、ここのルールがどうあれ"ごっこ"じゃ済まさない……」
			PRINTFORMDL 時を操る能力があるとはいえ、不意を突かれてはたまらない。
			PRINTFORMDW まだしばらくは警戒しておいたほうが良さそうだ……
		ENDIF
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K105_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:318 == 1
			PRINTFORML 「……」
			PRINTFORMW マイは%CALLNAME:MASTER%を一瞥したが、すぐに目をそらした。
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋人
			PRINTFORML 「ふふっ」
			PRINTFORML 「やっぱり、%CALLNAME:MASTER%がいないとね」
			PRINTFORMDW マイがすっと近寄り、腕に抱きついてきた。
		ELSEIF TALENT:恋慕
			PRINTFORML 「ああ、%CALLNAME:MASTER%」
			PRINTFORML 「……%UNICODE(0x2665) *1%」
			PRINTFORMDW マイが微笑みながらこちらを見つめている。
		ELSEIF TALENT:思慕
			PRINTFORML 「ん、%CALLNAME:MASTER%……」
			PRINTFORMDW マイがこちらを見ているようだ。
		ELSE
			PRINTFORML 「……」
			PRINTFORMDW マイは%CALLNAME:MASTER%に気づくと、無表情で軽く頭を下げた。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:318 == 1
			PRINTFORML 「……う」
			PRINTFORMW マイは%CALLNAME:MASTER%を見つけると、あからさまに嫌そうな顔をした。
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%」
			PRINTFORMDL 振り返ると、マイがそばに来ていた。
			PRINTFORMW 「ふふ、見つけた」
		ELSEIF TALENT:恋慕
			PRINTFORML 「あ……%CALLNAME:MASTER%」
			PRINTFORML 「……%UNICODE(0x2665) *1%」
			PRINTFORMDW マイは%CALLNAME:MASTER%を見つけると、柔らかく微笑んだ。
		ELSEIF TALENT:思慕
			PRINTFORML 「%CALLNAME:MASTER%……」
			PRINTFORMDW マイは%CALLNAME:MASTER%になにか言いかけたようだが、何も言わず向き直った。
		ELSE
			PRINTFORML 「……」
			PRINTFORMDW マイは%CALLNAME:MASTER%に気づくと、無表情で軽く頭を下げた。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF CFLAG:318 == 1
			PRINTFORML 「…………」
			PRINTFORMDW マイは%CALLNAME:MASTER%を非難するような目で見たが、何も言わなかった。
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%、入ってたんだ？」
			PRINTFORMDW マイは当たり前といった様子で%CALLNAME:MASTER%の側に入ってきた。
		ELSEIF TALENT:恋慕
			PRINTFORML 「っ……%CALLNAME:MASTER%……」
			PRINTFORMDL 風呂に入ってきたマイに、%CALLNAME:MASTER%は気さくに挨拶した。
			PRINTFORML 「もうちょっと動揺したりとか無いの？」
			PRINTFORMDW 呆れたような顔でマイが近くに寄ってきた。
		ELSEIF TALENT:思慕
		;多分死んでる？
			PRINTFORML 「っ……%CALLNAME:MASTER%……」
			PRINTFORMDL 風呂に入ってきたマイに、%CALLNAME:MASTER%は気さくに挨拶した。
			PRINTFORML 「え～……こんにちは？」
			PRINTFORMDW マイは身体を隠しつつ、%CALLNAME:MASTER%から離れたところで浴槽に入った。
		ELSE
		;実質セフレのみらしいから変える
			PRINTFORML 「…………」
			PRINTFORMDW マイは身体を隠しつつ、%CALLNAME:MASTER%から離れたところで浴槽に入った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF CFLAG:318 == 1
			PRINTFORML 「……」
			PRINTFORMDW マイは%CALLNAME:MASTER%がいるのを見ると、すっと風呂場から出ていった。
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%、入ってたんだ？」
			PRINTFORML 「ふふ……ごゆっくり%UNICODE(0x2665) *1%」
