;台詞一行のみをRESULTSに入れてください
;ムシの名前は %MUSHI_NAME% で表示できます
;ムシの名前の最長はアレキサンドラトリバネアゲハの全角14文字です
;文字数制限があります。はみ出ないように気をつけてください
;ARGは残りムシ数か順位です
;==================================================
@M_KOJO_MESSAGE_COM_K105_MUSHI_BATTLE(ARGS, ARG)
#DIM 残りムシ数
#DIM 順位
SELECTCASE ARGS
CASE "試合前"
	;-------------------------------------------------
	;キャラセレクト時,ARGは使えません
	;全角50文字くらいまで
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1
		IF TALENT:恋人 || TALENT:恋慕
			RESULTS = 「いや、わたしはムシなんて持ってないし……あれっ、持ってる！　なんで！？」
		ELSE
			RESULTS = 「あの……わたし、ムシなんて持って……え、持ってる……なにこれ」
		ENDIF
	ENDIF
CASE "戦場に出した"
	;-------------------------------------------------
	;"【（キャラ）】は《（ムシ）》を戦場に出した！」"の直後
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「ま、こいつでいいか……それっ」
	ENDIF
CASE "攻撃時"
	;-------------------------------------------------
	;"【（キャラ）】《（ムシ）》の『（攻撃名）』！」"の直前
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		PRINTFORML 
	ENDIF
CASE "倒れた時"
	;-------------------------------------------------
	;"【（キャラ）】《（ムシ）》は倒れた！！"の直後
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		SELECTCASE RAND:3
		CASE 0
			RESULTS = 「……儚いなぁ」
		CASE 1
			RESULTS = 「ああ……ちょっと、可哀想かもね……」
		CASEELSE
			RESULTS = 「……死んじゃった？」
		ENDSELECT
	ENDIF
CASE "敗退時"
	;-------------------------------------------------
	;"【（キャラ）】敗退！！"の直後
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「……そっか、これでおしまいだったね」
	ENDIF
CASE "タイマン：勝ち"
	;-------------------------------------------------
	;順位は1位
	;全角20文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「え、わたしの勝ち……そ、そう……」
	ENDIF
CASE "タイマン：負け"
	;-------------------------------------------------
	;順位は2位
	;全角20文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「わたしの負け……えーと、おめでとう」
	ENDIF
CASE "バトルロイヤル：優勝"
	;-------------------------------------------------
	;順位は1位
	;全角30文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「え、わたしの勝ち……そ、そう……」
	ENDIF
CASE "バトルロイヤル：２位以下"
	;-------------------------------------------------
	;順位は2～4位
	;全角30文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「良い勝負だった、のかな……？」
	ENDIF
CASE "チーム戦：勝ち"
	;-------------------------------------------------
	;順位は1位（やられていても）
	;全角32文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「まあ……味方が良かった、ってことかな……」
	ENDIF
CASE "チーム戦：負け"
	;-------------------------------------------------
	;順位は2位
	;全角32文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「ごめんなさい、わたしのせいかな……」
	ENDIF
ENDSELECT
SIF RESULTS != ""
	RETURN 1


