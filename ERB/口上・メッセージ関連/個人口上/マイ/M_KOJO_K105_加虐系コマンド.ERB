;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K105_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K105_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K105_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:イタズラ == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:泥酔 == 1
		PRINTFORML 
		RETURN 1
	;通常
	ELSE
		IF ABL:マゾっ気 < 2
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「いたっ！　ちょっと、何するの」
				PRINTFORMW 「悪いけど、そういうのは趣味じゃないんだよね」
			ELSE
				PRINTFORMW 「つっ……何、急に」
				SIF TALENT:思慕
					PRINTFORMW （いったぁ……思いっきりやってくれちゃって、とんだサディストだね）
			ENDIF
		ELSE
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「あぁっ%UNICODE(0x2665) *1%　もう、痛いって……」
				PRINTFORMW 「や、まぁ……その、ダメじゃ、ないけど……」
			ELSE
					PRINTFORML 「っ！　あ……%UNICODE(0x2665) *1%」
					PRINTFORMW 「もう……」
				SIF TALENT:思慕
					PRINTFORMW （やばい、ちょっと……気持ちいいっ……%UNICODE(0x2665) *1%）
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

