CALL M_KOJO_MESSAGE_COM_K105_105_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K105_105_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;装着
	IF TEQUIP:縄
		;特殊状況
		IF FLAG:70 ;時姦
			PRINTFORML 
			RETURN 1
		ELSEIF CFLAG:MASTER:イタズラ == 2
			PRINTFORML 
			RETURN 1
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
			RETURN 1
		;通常
		ELSE
			IF ABL:マゾっ気 < 2
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORML 「ああ、こういうのが好きなんだ……」
					PRINTFORMW 「あんまりキツくしないでよ？　痕ついたら嫌だし」
				ELSE
					PRINTFORMW 「えっ、ちょっと……何する気……？」
					SIF TALENT:思慕
						PRINTFORMW （……怖い、かも……大丈夫だよね……？）
				ENDIF
			ELSE
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORML 「あっ、縛っちゃう？」
					PRINTDATAW
						DATAFORM 「……その、してほしい縛り方が……あるんだけど、いい？」
						DATAFORM 「良いよ……ギチギチにやっちゃって%UNICODE(0x2665) *1%」
					ENDDATA
				ELSE
					PRINTFORML 「あ、きつ……んッ%UNICODE(0x2665) *1%」
					PRINTFORMW 「っ……良いよ、続けて……」
					SIF TALENT:思慕
						PRINTFORMW （ふふ……動けなくて……あは、興奮するわ……%UNICODE(0x2665) *1%）
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	;解除
	ELSE
		;特殊状況
		IF FLAG:70 ;時姦
			PRINTFORML 
			RETURN 1
		ELSEIF CFLAG:MASTER:イタズラ == 2
			PRINTFORML 
			RETURN 1
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
			RETURN 1
		;通常
		ELSE
			IF ABL:マゾっ気 < 2
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORML 「ん、もういいの？」
					PRINTFORMW 「あー、開放感……自由っていいね」
				ELSE
					PRINTFORMW 「終わり？　……ふぅ……お疲れさま」
					SIF TALENT:思慕
						PRINTFORMW （……まったく、良い趣味してるね）
				ENDIF
			ELSE
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORML 「あれ、ほどいちゃうの？」
					PRINTDATAW
						DATAFORM 「ふふ……ありがとうございました、ご主人様%UNICODE(0x2665) *1%」
						DATAFORM 「なんか、結構……ハマってきちゃったんだよね、これ」
					ENDDATA
				ELSE
					PRINTFORML 「終わり？　……ふぅ……お疲れさま」
					PRINTFORMW 「……%UNICODE(0x2665) *1%」
					SIF TALENT:思慕
						PRINTFORMW （やばいなぁ、なんか……癖になりそう）
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

