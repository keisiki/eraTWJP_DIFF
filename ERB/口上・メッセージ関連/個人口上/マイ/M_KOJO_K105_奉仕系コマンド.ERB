;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:イタズラ == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:泥酔 == 1
		IF TALENT:恋人 || TALENT:恋慕
