;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K15_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K15_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K15_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 時の止まった咲夜の、白魚のように細く白い手にペニスを擦り付ける
		PRINTFORMW 柔らかく華奢な手を犯していることに興奮を覚えた%CALLNAME:MASTER%は、
		PRINTFORMW 更なる快感を求め、咲夜の指を動かし更にペニスに纏わり付くように調整した……
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 「手でするの？」
		PRINTFORMW 咲夜はそう言うと、両手に唾液を塗してから片手で竿を握りもう片方で睾丸を揉み解した
		PRINTFORMW 更に開いた口では亀頭を舐め回している……
		PRINTFORMW 「んっ……いつでも出していいわよ？」
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		CALL HPH_PRINT("「手コキですね、了解いたしましたHPH」", "L")
		PRINTFORMW 咲夜はそう言うと、竿の上で口を広げ涎を垂らし、両手で塗り広げつつ絶妙な力加減で扱いた
		PRINTFORMW 更にそれに飽き足らず、開いた口に亀頭を含み、カリや鈴口を舌で愛撫してきた……
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「手で、ですか……分かりました」
		PRINTFORMW 咲夜はそう言い、手に唾液を落として両手で塗り広げると、
		PRINTFORMW 片手で竿を、もう片方の手で睾丸を揉み解してきた
		PRINTFORMW 「……」
		PRINTFORMW 咲夜は淡々と%CALLNAME:MASTER%の性処理をサポートしている……
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,フェラチオ
;==================================================
@M_KOJO_MESSAGE_COM_K15_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K15_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K15_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW %CALLNAME:MASTER%は咲夜の口をオナホールとして使うことにした
		PRINTFORMW 止まった時の中で咲夜の顎を掴み、無理やり口を開かせた
		PRINTFORMW %CALLNAME:MASTER%がそのまま事後の配慮もせず無遠慮にペニスを突き入れると、無防備な舌がペニスを迎えてくれた
		PRINTFORMW %CALLNAME:MASTER%のペニスは、柔らかく滑り、尚且つ少々の締りと吸い付きを持つ咲夜の口オナホを気に入ったようだ……
		PRINTFORMW そしてそのまま%CALLNAME:MASTER%は、咲夜の顔に腰を打ち付けるようにしていきり立ったペニスで咲夜の喉奥までを犯した
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF PREVCOM == 81
			SELECTCASE RAND:2
				CASE 0
				CALL HPH_PRINT("「じゅぐっHPH じゅるっHPH れろっHPH」", "L")
				PRINTFORMW 咲夜は一心不乱にペニスを頬張っている……
				CASEELSE
				CALL HPH_PRINT("「んぇ…っ くぷっ、んっ……HPH」", "L")
				PRINTFORMW 咲夜は自ら喉奥を使い竿を収めると、舌で根元を、喉全体で亀頭を満遍なく愛撫した
				ENDSELECT
		ELSE
		CALL HPH_PRINT("「失礼します……HPH」", "L")
		PRINTFORML %CALLNAME:MASTER%が足を開くと、咲夜は言われずとも自ら奉仕を始めた
		CALL HPH_PRINT("「ん……ぷはっ、すぅ～～っ……HPH」", "L")
		CALL HPH_PRINT("（すっごく濃い臭い……HPH 私を一匹のメスにする、“男”の臭い……っHPHHPH）", "L")
		PRINTFORML 口でズボンのチャックを下ろすと、顔を出したペニスの臭いを肺中に吸い込み、うっとりとした表情を見せた
		CALL HPH_PRINT("「では……頂きますっHPH はぷっHPH ん、むぅぅぅ……HPH」", "L") 
		PRINTFORML そしてそのまま亀頭を口に咥え、カリ裏を舌で掃除しつつ竿を根元まで飲み込んだ……
		RETURN 1
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 「ん、れろっ……どう？気持ち良い？んちゅっ」
		PRINTFORMW %CALLNAME:MASTER%の股座に顔を埋めて奉仕する咲夜に気持ち良いことを伝えると、
		PRINTFORMW 咲夜はペニスを頬張りながら嬉しそうに微笑んだ
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
			PRINTFORMW 咲夜に頼んでペニスをしゃぶって貰うことにした
			PRINTFORMW 「んっ……ぴちゃ……れろっ……」
			PRINTFORMW たしかに咲夜のフェラは気持ちの良いものだが、気持ちが入っていない
			PRINTFORMW 咲夜はそのまま機械的に奉仕を続けた……
			CASEELSE
			PRINTFORMW 「ぅ……ちゃんと洗っていますか、これ？」
			PRINTFORMW 咲夜はペニスに顔を近づけると、男性器特有のすえた臭いに顔を顰めた
			PRINTFORMW 「……わかってますよ。……ん、れろっ……ぅぇ……ちゅっ……」
			PRINTFORMW ペニスを見つめたまま暫し固まっていた咲夜だが、%CALLNAME:MASTER%に促され奉仕を始めた……
			ENDSELECT
	RETURN 1
	ENDIF
ENDIF
RETURN 1

