@M_KOJO_SUCCESS_COM_K104_301
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K104_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K104_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K104_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:318 == 1
			PRINTFORML 
	ELSE
		IF TCVAR:泥酔 == 1
			IF TALENT:恋慕 || TALENT:愛欲
				PRINTFORML 「んぇへへ、%CALLNAME:MASTER%のお茶～」
				PRINTFORMW 「んくっ、ん……ぷぁ～」
			ELSE
				PRINTFORML 「お茶だぁ、ありがと～」
				PRINTFORMW 「あ゛ぁ～、おいし」
			ENDIF
		ELSEIF TCVAR:媚薬 || TCVAR:発情
			PRINTFORML 「お茶ね、お茶……まぁ、良いんだけど」
			PRINTFORMW 「ちょっと今は、これくらいじゃリラックスできなそうかも……」
		ELSEIF TFLAG:193 == -1
			IF TALENT:恋慕 || TALENT:愛欲
				PRINTFORML 「うッ、これは苦手なやつだ」
				PRINTFORMW 「たまにクセのあるの淹れるよね～」
			ELSE
				PRINTFORML 「お茶を淹れてくれるのは良いんだけど……」
				PRINTFORMW 「これ、ちょっと苦手な味……うう」
			ENDIF
		ELSE
			IF TALENT:恋慕 || TALENT:愛欲
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ん～、やっぱりお茶は%CALLNAME:MASTER%が淹れたのに限るわ～」
					PRINTFORMW 「はぁ、リラックス%UNICODE(0x2665) *1%」
				CASE 1
					PRINTFORML 「来た来た、待ってたわ%UNICODE(0x2665) *1%」
					PRINTFORMW 「美味しいよ、さすがね！」
				CASE 2
					PRINTFORML 「あっ、ありがと」
					PRINTFORMW 「%CALLNAME:MASTER%といると、飲み物には困らないわ～」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「あら、どうも%UNICODE(0x2665) *1%」
					PRINTFORMW 「よく気が付くね～」
				CASE 1
					PRINTFORML 「わたしにもくれるの？」
					PRINTFORMW 「いただくわ、ありがとう！」
				CASE 2
					PRINTFORML 「こういうお茶、自分ではあんまり淹れないんだよね」
					PRINTFORMW 「ん～、たまにはいいかも？」
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
