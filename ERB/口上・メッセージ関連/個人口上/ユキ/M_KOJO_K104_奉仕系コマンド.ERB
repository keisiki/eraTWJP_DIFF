;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K104_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K104_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K104_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:イタズラ == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:泥酔 == 1
		IF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「はぁ～い、しこしこ～」
			PRINTFORMW 「どーかなぁ、こんなかんじでいい～？」
		ELSE
			PRINTFORML 「あっはは、ぐりぐり～」
			PRINTFORMW 「う～ん、なんかすごいねぇ」
		ENDIF
		RETURN 1
	;通常
	ELSE
		IF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「手で？　うんっ、任せてよ」
			PRINTFORMW 「わたしが、しっかり気持ちよくしてあげるからね……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORML 「どういう風にされるのが気持ちイイものなの？」
			PRINTFORMW 「いろいろ試してみるからね～」
		ENDIF
	ENDIF
ENDIF
RETURN 1

