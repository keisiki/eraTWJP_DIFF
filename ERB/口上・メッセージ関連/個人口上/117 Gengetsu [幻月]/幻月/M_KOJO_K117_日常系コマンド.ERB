@M_KOJO_SUCCESS_COM_K117_302
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K117_302
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K117_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K117_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;結構やる印象あるから増やしたい
	;怒り
	IF CFLAG:318
		PRINTFORML
		PRINTFORML 「チッ」
		PRINTFORMW 幻月はご立腹のようだ。
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:6
			CASE 5
				PRINTFORML
				PRINTFORML 幻月の手を取ってみた……。
				PRINTFORML 「どうしたの、突然？」
				PRINTFORMW 「……綺麗な手だって？　ふふ、ありがと」
			CASE 4
				PRINTFORML
				PRINTFORML 幻月の体を軽くつついてみた。
				PRINTFORML 「ふふ、くすぐったいわ」
				PRINTFORMW お返しにと彼女も%CALLNAME:MASTER%の体をつついてきた……。
			CASE 3
				PRINTFORML
				PRINTFORML 幻月の耳に息を吹きかけてみた。
				PRINTFORML 「ひゃっ！」
				PRINTFORMW 「もう……びっくりするじゃない……」
			CASE 2
				PRINTFORML
				PRINTFORML 幻月の頭を撫でてみた。
				PRINTFORML 「ん……いいわ」
				PRINTFORMW 幻月はリラックスした様子で、%CALLNAME:MASTER%に体を預けている。
			CASE 1
				PRINTFORML
				PRINTFORML 「ひゃっ！」
				PRINTFORML 羽の付け根のところを撫でると幻月はかわいい声をあげた。
				PRINTFORMW 「大丈夫、ちょっと驚いただけよ。続けてもいいわ」
			CASE 0
				PRINTFORML
				PRINTFORML 幻月の肩を揉んであげた。
				PRINTFORML 「そこよ、%CALLNAME:MASTER%。あっ、いいわ……」
				PRINTFORMW 結構凝っているようだった。
		ENDSELECT
	
	;思慕
	ELSEIF TALENT:思慕
		SELECTCASE RAND:6
			CASE 5
				PRINTFORML
				PRINTFORML 「あら、肩揉んでくれるの？」
				PRINTFORML なんとなく疲れているように見えたので。
				PRINTFORML 「んっ、なかなかうまいじゃない」
				PRINTFORMW 割と凝っているようだ。
			CASE 4
				PRINTFORML
				PRINTFORML 幻月の耳に息を吹きかけてみた。
				PRINTFORML 「ひゃっ！」
				PRINTFORMW 「ちょっと、突然なにするのよ！」
			CASE 3
				PRINTFORML
				PRINTFORML 幻月の肩を揉んであげた。
				PRINTFORML 「あら……なかなかうまいじゃない」
				PRINTFORMW 幻月は気持ちよさそうにしている。
			CASE 2
				PRINTFORML
				PRINTFORML 「ひゃっ！」
				PRINTFORML 羽の付け根を撫でると幻月はかわいい声をあげた。
				PRINTFORMW 「羽はやめてくれるとありがたいんだけど。……ひゃん！」
				PRINTFORMW 「だからやめてって！」
			CASE 1
				PRINTFORML
				PRINTFORML 幻月の髪に触れてみた。
				PRINTFORMW 「乱暴に扱わないでね」
			CASE 0
				PRINTFORML
				PRINTFORML 幻月の体を軽くつついてみた。
				PRINTFORML 「やり返してあげるわ」
				PRINTFORMW 楽しそうな彼女としばらくつつきあった……。
		ENDSELECT
	
	;それ以外
	ELSE
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML
				PRINTFORML 「髪の匂いを嗅ぎたい？」
				PRINTFORMW 「……変態なの？」
			CASE 2
				PRINTFORML
				PRINTFORML 「羽？　触らせないわよ」
				PRINTFORMW 柔らかくて気持ち良さそうなのだが許されなかった。
			CASE 1
				PRINTFORML
				PRINTFORML 幻月の体を軽くつついてみた。
				PRINTFORML 「突然何よ」
				PRINTFORMW 首をかしげて不思議そうにしている。
			CASE 0
				PRINTFORML
				PRINTFORML 「やめて」
				PRINTFORMW 幻月の髪を触ろうとすると手を払われてしまった。 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
