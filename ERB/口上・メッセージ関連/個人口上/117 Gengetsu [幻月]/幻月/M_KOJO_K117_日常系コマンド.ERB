;301,お茶を淹れる
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K117_301
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K117_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K117_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K117_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;失敗
	IF TFLAG:193 == -1
		PRINTFORML
		PRINTFORML 「不味いわね……」
		PRINTFORMW 「もっと精進なさい」
	;成功
	;恋慕とかでの分岐が浮かばない
	ELSE
		SELECTCASE RAND:5
			CASE 4
				PRINTFORML
				PRINTFORML 「あら、良い茶葉ね」
				PRINTFORML 「どこで手に入れたのかしら？」
				PRINTFORMW お気に召していただけたようだ。
			CASE 3
				PRINTFORML
				PRINTFORML 「ん、おいしい」
				PRINTFORMW 幻月の羽がパタパタと動いた。
			CASE 2
				PRINTFORML
				PRINTFORMW 「そういえば、いいお茶菓子があったわね……」
			CASE 1
				PRINTFORML
				PRINTFORML 「少し渋いわ」
				PRINTFORMW そう言いつつ幻月は全部飲んでくれた。
			CASE 0
				IF TALENT:恋慕
					PRINTFORML
					PRINTFORML 「夢月といい勝負ね」
					PRINTFORMW こぼれるように幻月はそんな感想をこぼした。
				ELSEIF TALENT:思慕
					PRINTFORML
					PRINTFORML 「お茶淹れてくれたの？　ありがと」
					PRINTFORML （夢月よりも……いや……）
					PRINTFORMW 幻月は難しい顔をしている。
				ELSE
					PRINTFORML
					PRINTFORMW 「お茶？　いただくわ」
					PRINTFORML 夢月が淹れたほうがおいしいわね……。
					PRINTFORMW そんなつぶやきが%CALLNAME:MASTER%の耳にとどいた。
				ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
