@M_KOJO_MESSAGE_COM_K14_303_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
		SELECTCASE TFLAG:193 
		CASE 0,1,2
			IF TALENT:恋慕
				;PRINTFORML 
			ELSEIF TALENT:思慕 || CFLAG:14:1002 == 1
				PRINTFORML 「ん…許す！」
