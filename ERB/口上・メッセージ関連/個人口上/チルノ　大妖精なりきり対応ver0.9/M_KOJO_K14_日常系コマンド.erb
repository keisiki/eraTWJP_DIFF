;日常系コマンド
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;300,会話
;TFLAG:193(1=大成功 0=成功 -1=失敗 -2=大失敗（TARGETがMASTERより教養が4以上高い)
;TCVAR:302(0=会話可能 1>=非恋慕時会話不能)
;MARK:反発刻印(3以上=会話不能)(4以上はバグ
;TCVAR:泥酔(1=通常成功or大失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K14_300
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K14_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「〜話をした。」とかが消えます。また、削除した場合は自動で口上色が付きません。
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K14_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K14_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
	IF FLAG:時間停止
	ELSEIF FLAG:なりきり == [[大妖精]] && !CFLAG:14:1002
	;大妖精に限り初対面口上へ移行
		CALL M_KOJO_ENCOUNTER_K14
	ELSEIF TCVAR:302 && !TALENT:恋慕 && !TALENT:思慕 && CFLAG:14:1002 != 1
		SELECTCASE RAND:3
			CASE 1
				CALL COLORMESSAGE14("　は、はくちゅん！",C_GREEN,1,0)
				PRINTFORMW 「ふ…あたいの近くにいると凍傷するぜ」
			CASE 0
				PRINTFORML 「ところでお前は誰だ？」
				CALL COLORMESSAGE14("(まだ名前覚えてもらえてない……）",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「…さっきからやけにつっかかってくるな？」
				CALL COLORMESSAGE14("　ご、ごめんなさい！",C_GREEN,2,0)
		ENDSELECT
	ELSEIF CFLAG:318 == 1
		PRINTFORML 「………」
	ELSEIF CFLAG:行動 == 5 && CFLAG:MASTER:現在位置 == 2 || CFLAG:行動 == 5 && CFLAG:MASTER:現在位置 == 202
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「アイス買いに来たの？1本500円ね」
				IF MONEY < 500
					CALL COLORMESSAGE14("　あう…お金足りないよぉ…。",C_GREEN,2,0)
				ELSE
					CALL COLORMESSAGE14("　おいしそうなアイス…。お金はあるから買っちゃおかな？",C_GREEN,2,0)
				ENDIF
			CASE 0
				PRINTFORML 「今仕事中なんだ。忙しすぎて目が回りそうだよ」
				PRINTFORMW 「人気者は辛いなー」
				CALL COLORMESSAGE14("（とても忙しそう…。手伝ってあげた方がいいのかな？)",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「太陽のあん畜生があたいとアイスを溶かそうとしてる…」
				PRINTFORMW 「うおー！負けるもんか！」
				CALL COLORMESSAGE14("　がんばってチルノちゃん！",C_GREEN,2,0)
		ENDSELECT
	;ELSEIF TALENT:恋慕 
		;PRINTFORML 
	ELSEIF TALENT:思慕 || CFLAG:14:1002 == 1
		SELECTCASE RAND:8
			CASE 6
				PRINTFORML 「凶器はナイフより氷の方が優れているわ！」
				PRINTFORML 「氷のナイフは溶けてしまえば証拠が隠滅するからね」
				CALL COLORMESSAGE14("　チルノちゃん。もしかして外の世界の探偵漫画を読んだ？",C_GREEN,1,0)
				CALL COLORMESSAGE14("「なんだ。大ちゃんも読んでたのかぁ」",C_AQUA,2,0)
			CASE 5
				CALL COLORMESSAGE14("「大ちゃんって何の妖精なの？」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　え？私？私は……何の妖精なんだろう。",C_GREEN,1,0)
				CALL COLORMESSAGE14("「ラルバみたいな蝶の妖精？」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　でも冬眠とかしないよ。…うーん私っていったい。",C_GREEN,1,0)
				PRINTFORMW 「…なんとなく何か異変が起きそうな予感」
				CALL COLORMESSAGE14("　私の正体で異変なんか起きないよぉ。",C_GREEN,2,0)
			CASE 4
				PRINTFORMW 「また日焼けしないかなー。最近の太陽は軟弱でいかん」
				CALL COLORMESSAGE14("　あの時のチルノちゃん、とても強くてかっこよかったよ。",C_GREEN,2,0)
			CASE 3
				PRINTFORMW 「鴉天狗には注意したほうがいいぞ。あることないこと記事にされるんだから！」
				CALL COLORMESSAGE14("　大蝦蟇さんの記事こと…？",C_GREEN,1,0)
				CALL COLORMESSAGE14("「そう！あの新聞はうそっぱち。あいつは大蝦蟇相手にもっと善戦したんだ」",C_AQUA,2,0)
			CASE 2
				PRINTFORML 「蛙でも居ないかなぁ」
				CALL COLORMESSAGE14("　また蛙を凍らせるの？可哀想だからやめようよ…。",C_GREEN,1,0)
				CALL COLORMESSAGE14("「ちゃんと蘇生させるから大丈夫だって」",C_AQUA,2,0)
			CASE 1
				PRINTFORML 「あーなんかヒマだなー。また異変でも起きないかなー」
				CALL COLORMESSAGE14("　平和なのが一番だよチルノちゃん。",C_GREEN,1,0)
				PRINTFORMW 「あたいにとっちゃ大手振って遊べる異変が起きた時の方が毎日楽しいよ」
			CASE 0
				PRINTFORML 「草原や砂利道で辺りを凍らして寝っ転がると楽しいよ」
				PRINTFORMW 「ツルツル滑って転がれて…もーサイコー！」
			CASEELSE
				PRINTFORML 「ほら、花びらを凍らすとすごいよ！」
				CALL COLORMESSAGE14("　ドライフラワーにするの？",C_GREEN,1,0)
				PRINTFORMW 「こうやって凍らせば凶器になるわ！」
				CALL COLORMESSAGE14("　わ、わぁ…。",C_GREEN,2,0)
		ENDSELECT
	ELSE
		SELECTCASE RAND:6
			CASE 4
				PRINTFORML 「あたいは夏の日の晴れた時だけ、人間の里でアイスを売ってるのさ」
				PRINTFORMW 「アイスを真夏の太陽の下で売るなんて芸当はあたいぐらしかできないから大儲けだ！」
				CALL COLORMESSAGE14("　私もアイス大好きなんです。もし見かけたら買わせてくださいね。",C_GREEN,2,0)
				SIF DAY:2 == 4
					CALL COLORMESSAGE14("　冬のアイスもおいしそうですけど、風邪ひいちゃいそう…。",C_GREEN,2,0)
			CASE 3
				PRINTFORML 「神社の近くにいる三妖精はすごいぞ」
				PRINTFORML 「サニーは気配を見えなくしルナは光を消せて、スターは…なんと音を感知できるんだ！」
				PRINTFORMW 「悔しいけど、あいつらでかくれんぼで右にでる奴はいないね」
				CALL COLORMESSAGE14("（あれ…何か違うような？)",C_GREEN,2,0)
			CASE 2
				IF DAY:2 == 4
					PRINTFORMW 「冬は退屈だなぁ。何にもしなくても周りは凍ってるし」
					CALL COLORMESSAGE14("　虫や夏の妖精の娘達も冬眠して遊べないから、冬はちょっと寂しいですよね",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「さーて今日は何を凍らせてやろうかな」
					CALL COLORMESSAGE14("　わ、私は凍らせないでくださいね…。",C_GREEN,2,0)
				ENDIF
			CASE 1
				PRINTFORML 「ところであんたの名前ってなんだっけ？」
				CALL COLORMESSAGE14("　私の名前ですか？私は…",C_GREEN,1,0)
				PRINTFORMW 「覚えるの面倒だから「あんた」のままでいいかな」
				CALL COLORMESSAGE14("　そんなぁ…。",C_GREEN,2,0)
			CASE 0
				PRINTFORML 「なんか大人しそうなやつだなー。ご飯食べ忘れたの？」
				CALL COLORMESSAGE14("　ちゃ、ちゃんと食べてますよ",C_GREEN,1,0)
				IF TALENT:MASTER:バストサイズ > -1
					CALL COLORMESSAGE14("「…じゃないとそんなに大きくならないか」",C_AQUA,1,0)
					CALL COLORMESSAGE14("　あ、あの…どこを見てるんですか？",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「本当？草ばかり食べてるんじゃない？」
				ENDIF
			CASEELSE
				PRINTFORML 「あんたはかくれんぼ知ってる？」
				CALL COLORMESSAGE14("　鬼が隠れている子を見つける遊び…ですよね？よく太陽の畑の娘達と遊んでます。",C_GREEN,1,0)
				PRINTFORML 「湖の妖精の間でも大流行中なんだ」
				PRINTFORML 「しかもいつもあたいが一人勝ちなのさ。エヘン！」
				PRINTFORML 「そうだ！今度やる時はあんたも誘ってあげる」
				CALL COLORMESSAGE14("　私にチルノさんを見つけられるかな…。がんばりますね。",C_GREEN,2,0)
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
;==================================================
;301,お茶を淹れる
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K14_301
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K14_301
CALL M_KOJO_MESSAGE_COM_K14_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K14_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
		IF FLAG:時間停止
		ELSEIF CFLAG:318 == 1
		;ELSEIF TALENT:恋慕
			;PRINTFORML 
		ELSEIF TALENT:思慕 || CFLAG:14:1002 == 1
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「あ、このお茶冷えてる」
					CALL COLORMESSAGE14("　それならチルノちゃんも飲めるもんね。",C_GREEN,1,0)
					PRINTFORMW 「もっと冷やしちゃお」
					CALL COLORMESSAGE14("　茶柱が凍って浮き上がってきちゃった…。",C_GREEN,2,0)
				CASE 0
					CALL COLORMESSAGE14("　チルノちゃんは紅茶にお砂糖何個いれる？",C_GREEN,1,0)
					PRINTFORMW 「あたいはお砂糖3個いれてちょーだい」
					CALL COLORMESSAGE14("　3個？甘いの3個欲しいんだ…。いやしんぼだね。",C_GREEN,2,0)
				CASEELSE
					PRINTFORML 「お茶おいしー」ｼｬｸｼｬｸ
					CALL COLORMESSAGE14("　チルノちゃん、その棒付きアイスってもしかして…",C_GREEN,1,0)
					PRINTFORMW 「もらったお茶をアイスキャンデーにしてみた」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「苦い！もう一杯！」
					CALL COLORMESSAGE14("　あの…少し溶けてますけど大丈夫ですか？",C_GREEN,2,0)
				CASE 0
					PRINTFORML 「熱いお茶なんて…こうだ！」
					CALL COLORMESSAGE14("　わぁ…何もないところから氷が…！",C_GREEN,1,0)
					PRINTFORML 「ふっ、最強ね」
					PRINTFORML 「…苦っ！」
					CALL COLORMESSAGE14("　今度は砂糖も用意しておきますね",C_GREEN,2,0)
				CASEELSE
					CALL COLORMESSAGE14("　あの、チルノさん。お、お茶飲みますか？",C_GREEN,1,0)
					PRINTFORML 「…あたいに熱い飲み物を渡すなんていやがらせ？」
					CALL COLORMESSAGE14("　あ…！ごめんなさい…。",C_GREEN,1,0)
					PRINTFORML 「なんの、これぐらい飲まないきゃ最強の名がすたる！」
					PRINTFORMW 「熱ッ！」
			ENDSELECT
		ENDIF
	RETURN 1
ENDIF
RESETCOLOR
RETURN 1
