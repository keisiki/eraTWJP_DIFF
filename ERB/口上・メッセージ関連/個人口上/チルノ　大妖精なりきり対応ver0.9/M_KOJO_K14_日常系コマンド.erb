@M_KOJO_MESSAGE_COM_K14_304
CALL M_KOJO_MESSAGE_COM_K14_304_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K14_304_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
		;時間停止中
		IF FLAG:70
		;人間の里の広場でアイス売り
		ELSEIF CFLAG:MASTER:現在位置 == 2 || CFLAG:MASTER:現在位置 == 202
			;IF TALENT:恋慕
				;PRINTFORML 
			IF TALENT:思慕 || CFLAG:14:1002 == 1
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「いやー暑い暑い」
						PRINTFORMW 「アイス食べないとやってられないね」
						CALL COLORMESSAGE14("　チ、チルノちゃん。お店のもの食べちゃっていいの？",C_GREEN,1,0)
						CALL COLORMESSAGE14("「はい、大ちゃんの分」",C_AQUA,1,0)
						CALL COLORMESSAGE14("　ゴクリ…。",C_GREEN,2,0)
					CASE 0
						PRINTFORML 「もっとチラシとかで宣伝するべきかなー。ある事ない事関わらずさぁ」
						CALL COLORMESSAGE14("　天狗の新聞さんに広告を載せてもらうの？",C_GREEN,1,0)
						CALL COLORMESSAGE14("「大蝦蟇をこらしめた最強チルノのサイキョーアイスとかね！」",C_AQUA,2,0)
					CASEELSE
						PRINTFORML 「知ってる？緑の巫女は人間の里で大声で駄弁ることしかないんだよ」
						CALL COLORMESSAGE14("　赤い巫女さんはよく妖怪退治してるね。",C_GREEN,1,0)
						PRINTFORMW 「巫女の違いがなんとなくわかってきたね」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						CALL COLORMESSAGE14("　あの、チルノさんは人間が怖くないんですか？",C_GREEN,1,0)
						PRINTFORML 「そーいや最初は妖精だと思って舐めてかかるヤローはいたかな。でも」
						PRINTFORMW 「うるさい人間は冷凍すると静かになるぞ」
						CALL COLORMESSAGE14("　ひぇ…",C_GREEN,2,0)
					CASE 0
						CALL COLORMESSAGE14("　人間の里でお店を開くなんてチルノさんは大胆ですね…",C_GREEN,1,0)
						PRINTFORMW 「どこに行って何をしようがあたいの自由だ！これぐらいお茶の子さいさいだぜー」
					CASEELSE
						CALL COLORMESSAGE14("　あ、アイスー…アイスはいりませんかー…",C_GREEN,1,0)
						PRINTFORMW 「声が小さいよ！もっとスペルカード宣言するみたいに！」
						CALL COLORMESSAGE14("　集客『アイスいりませんかー！！』",C_GREEN,2,0)
				ENDSELECT
