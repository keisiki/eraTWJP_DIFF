				PRINTFORML 「これぐらい懐は広くないと最強じゃないからな」
				CALL COLORMESSAGE14("「それに、大ちゃんならワザとやったんじゃないだろうし」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　ぐすっ…本当にごめんねチルノちゃん。",C_GREEN,2,0)
			ELSE
				PRINTFORML CALL COLORMESSAGE14("　チルノさん、この前は変なことして怒らせてごめんなさいっ！",C_GREEN,1,0)
				PRINTFORML 「………」
				CALL COLORMESSAGE14("………",C_GREEN,1,0)
				PRINTFORML 「…何かあった？」
				CALL COLORMESSAGE14("　えっ…。",C_GREEN,1,0)
				PRINTFORML 「いきなり謝られてビックリだよ」
				CALL COLORMESSAGE14("(もしかして忘れてる…？)",C_GREEN,2,0)
			ENDIF
		CASEELSE
			IF TALENT:恋慕
				;PRINTFORML 
			ELSEIF TALENT:思慕 || CFLAG:14:1002 == 1
				PRINTFORML 「…よく判らない事言って謝ってるフリしてるだけでしょ？」
				CALL COLORMESSAGE14("　私…そんなつもりじゃ…。",C_GREEN,2,0)
			ELSE
				PRINTFORML 「ふざけんな！お前があたいに何をした！？」
				PRINTFORML 「なる程判った！まだケンカやるきだな！いつでもかかってこい！」
				CALL COLORMESSAGE14("　ヒッ…！ご、ごめんなさぁい！！",C_GREEN,1,0)
				CALL COLORMESSAGE14("　ど、どうしよう。まだ怒ってるみたい…。",C_GREEN,2,0)
			ENDIF
		ENDSELECT
ENDIF
RESETCOLOR
RETURN 1
