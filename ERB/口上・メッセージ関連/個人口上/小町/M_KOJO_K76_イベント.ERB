;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K76_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORML 「%CALLNAME:MASTER%、ちょい待ち」
		PRINTW 
		IF TALENT:恋慕
			PRINTFORML 何事かと振り返ると、小町は後手を組んでもじもじしている
			PRINTW 
			PRINTFORML 「む～～、なんかこういうのはあたいの柄じゃないからはっきり言うよ」
			PRINTFORML 「あたい、%CALLNAME:MASTER%のことが……好きなんだよっ」
			PRINTFORMW 「あたいと、小野塚小町と一緒になっておくれっ！」
			PRINTW 
			PRINTW
			PRINTW  
			PRINTFORML 「いや、何か言っておくれよ！？　はいとかいいえとか。あ、いいえは嫌だなぁ……」
			PRINTW 
		ELSEIF TALENT:思慕
			PRINTFORML 「あたいと付き合ってくれないかい？」
			PRINTFORML 「今も付き合ってるだろうって？　そういう意味じゃなくって」
			PRINTFORML 「あたいの恋人になっておくれって言ってるんだよ」
			PRINTW 
			PRINTFORML 「いやー、今の関係も全然悪くなんだけどさ」
			PRINTFORML 「お前さんのこと、もっと知りたいんだよ」
			PRINTFORML 「あたいじゃ、駄目かい？」
		ELSE
			PRINTFORML 「あたいとさぁ、付き合ってみない？」
			PRINTFORML 「まー早い話が、お前さんのこと気にいっちゃったんだよね」
			PRINTFORML 「一緒にいて楽しいし、話もよく聞いてくれるしさ」
			PRINTW
			PRINTFORML 「ってわけで、%CALLNAME:MASTER%さえよけえば」
			PRINTFORML 「あたいとお付き合いしてくれないかな、どうかな？」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋慕
		PRINTFORML 「あぁ～～、よかったぁ。」
		PRINTFORML 小町は心底ホッとした表情を浮かべ脱力している
		PRINTW
		PRINTFORML 「こんなに緊張したのは四季様に初めて説教で呼び出されたとき以来……」
		PRINTFORML 説教と愛の告白を同列に語るのはどうか
		PRINTFORML 「そんな顔しないでおくれよ。とにかく、それくらいすごくドキドキしてたってことだよ」
		PRINTW 
		PRINTFORML 「これからどうしようかねぇ」
		PRINTFORML 「せっかく恋仲になるんだから、一緒に温泉に行ったり、部屋に泊まったり、それから……」
		PRINTFORML 小町はこれからの恋人生活を想像して舞い上がっている
		PRINTW
		ELSEIF TALENT:思慕
			PRINTFORML 「ホントに？　あたいでいいのかい？」
			PRINTFORML 「やったぁ、思い切ってみるもんだね」
			PRINTFORML 小町は%CALLNAME:MASTER%に向かってニカっと笑うとピースサインをしてみせた
			PRINTW
			PRINTFORML 「今も結構いい仲だと思ってるけどさ、もっともっとお前さんのこと教えておくれよ」
			PRINTFORML 「あたいのことも%CALLNAME:MASTER%に知ってほしいしさ」
			PRINTFORML 「おっぱいの大きさが知りたい？　そんなこと知ってどうするのさ。まあいいけど」
			PRINTW  
		ELSE
			PRINTFORML 「えっ、いいの？　ほんとに？」
			PRINTFORML 「よかったぁ、断られたらどうしようかと思ってたよ」
			PRINTW 
			PRINTFORML 「まあ、恋人同士になってすぐ世界が変わるかっていったらそうでもないけど」
			PRINTFORML 「あたいにできることだったらしてあげるよ」
			PRINTFORML 「せっかく、晴れて恋人同士になったわけだしね」
			PRINTW 
		ENDIF
		PRINTFORMW 「それにしても、お前さんも物好きだね。死神と付き合いたいなんて」
		PRINTFORML 「まあ、それを言ったらあたいだって変わり者か」
		PRINTFORML 「あんたと恋仲になりたいっていうんだから」
		PRINTW
		PRINTFORMW 「変わり者同士、これからよろしく頼むよ。%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
	IF TALENT:恋慕
	ELSEIF TALENT:思慕
		PRINTFORML 「そっか、お前さんを振り向かせるにはまだ魅力が足りないかねぇ」
		PRINTW 
		PRINTFORML 「でも、これで諦めたつもりはないよ」
		PRINTFORML 「そのうち、あたいに惚れさせてやるからね」
		PRINTFORMW そう言って小町はニカッ笑った
	ELSE
		PRINTFORMW 「そっかー、まぁ、まだ気が早かったかねぇ」
		PRINTFORMW 「でもさ、気が向いたらあたいと付き合っておくれよ」
		PRINTFORMW 「お前さんとなら気が合いそうな気がするんだよねぇ」
		PRINTL 
	ENDIF
ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K76_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:1100 == 1
			PRINTFORML 「楽しかったねぇ、%CALLNAME:MASTER%」
			PRINTFORML 「さて、この後、どうする？」
			IF CFLAG:MASTER:初期位置 == CFLAG:76:初期位置
				PRINTFORML 「部屋に帰って、朝までイチャイチャしちゃうかい」
				IF YOUBI_MATCH(DAY, "月水金")
					PRINTFORML 「明日は休みだし、一日中絡み合っていようよぉ%UNICODE(0x2665) *1%」
					PRINTFORML 「部屋の中、助平な臭いでむせ返っちゃうくらい、愛し合おうよ……%UNICODE(0x2665) *1%」
				ELSE
					PRINTFORML 「明日は仕事だけど、遅れちゃったらその時はその時で……%UNICODE(0x2665) *1%」
					PRINTFORML 「仕事は大事だけど、お前さんと気持ちいいことするほうがもっと大事だしね……%UNICODE(0x2665) *1%」
				ENDIF
				PRINTW 
			ELSE
				PRINTFORML 「うちに来るかい？　なんなら、泊まっていきなよ」
				IF YOUBI_MATCH(DAY, "月水金")
					PRINTFORML 「明日は休みだし、昼まで寝ることになっても、平気だしね……%UNICODE(0x2665) *1%」
				ELSE
					PRINTFORML 「明日は仕事だけど、遅れちゃったらその時はその時で……%UNICODE(0x2665) *1%」
				ENDIF
				PRINTW 
			ENDIF
		ELSEIF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%、また行こうね。とっても楽しかったよ」
			IF CFLAG:MASTER:初期位置 == CFLAG:76:初期位置
			ELSE
				PRINTFORML 「今日、どうする？　部屋泊まっていく？
			ENDIF
			PRINTW 
		ELSEIF TALENT:恋慕
			PRINTFORML 「楽しかったねぇ。また、二人で行きたいな」
			PRINTW 
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%って、面白いやつだねぇ」
			PRINTFORML 「よかったら、また誘っておくれよ」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「ま、反省してくれてるみたいだしね。許してあげるよ」
		PRINTFORML 「その代わり、またあたいをデートに誘うこと♪」
		PRINTW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML (まあ、ホントはもう許してあげてもいいんだけど）
		PRINTFORML 「んー、楽しかった、よ。でも、まだ許してないからね」
		PRINTW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「ま、楽しかったのは認めるけどさ」
		PRINTFORML 「でも、それとこれとは話は別。お前さんはまだ反省が必要だよ」
		PRINTW 
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K76_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;小町の部屋
		IF CFLAG:MASTER:現在位置 == 616
			;小町と映姫様が同室
			IF K76_FIND_AROUND() == 30
				;レズっ気ここまで上げるのってあなたと同性じゃなきゃ難しいからいいよね
				;逆に数値を上げすぎると死に口上になりそうな気がしないでもない
				IF ABL:レズっ気 >= 3 && ABL:(NO:30):レズっ気 >= 3
					;ガチ百合
					IF PALAM:欲情 >= PALAMLV:10 && PALAM:(NO:30):欲情 >= PALAMLV:10
						IF !TALENT:キス未経験 && !TALENT:(NO:30):キス未経験
							PRINTFORML 「四季様ぁ……%UNICODE(0x2665) *1%　ぷぁっ%UNICODE(0x2665) *1%」
							PRINTFORML 「えへへ、可愛いですよ、四季様……%UNICODE(0x2665) *1%」
							PRINTW 
							PRINTFORML 部屋をのぞくと、小町と映姫は愛し合っている最中だった
							PRINTFORML 十指を絡めながら手を繋ぎ、互いに貪るように唇を重ねあう
							PRINTFORML 二人の頬は紅潮し、夢見心地のような表情を浮かべている
							PRINTW 
							PRINTFORML 二人が唇を離したタイミングを見計らい、今入ってきた体を装って声を掛けた
							PRINTFORML 「%CALLNAME:MASTER%……%CALLNAME:MASTER%！？」
							PRINTFORML %CALLNAME:MASTER%の闖入に気付いた小町は、慌てて映姫から距離をとった
							PRINTFORML 二人の顔は先ほど情事に耽っていた時よりも真っ赤になっている
							PRINTW 
							PRINTFORML 「か、顔が赤いって？　ほら、今日は暑いからさー……あはは♪」
							PRINTFORML 下手糞な言い訳でこの場をごまかそうとする小町。一方の映姫は、真っ赤にしたまま顔を上げようともしない
							PRINTFORML 素晴らしいものを見せてもらったかわりに、今日のことは黙っておこう。%CALLNAME:MASTER%はそう思った
							PRINTW 
						ELSE
							PRINTFORML 「四季様……いい、ですよね……？」
							PRINTW 
							PRINTFORML 部屋をのぞくと、小町と映姫は秘め事の最中だった
							PRINTFORML 二人は十指を絡めながら手を繋ぎ、互いの吐息がかかるほど顔を近づけている
							PRINTW 
							PRINTFORML このまま覗いていたらどうなるだろうか、と固唾を飲んで見守っていたが、うっかり足元にあった小物を蹴飛ばしてしまった
							PRINTFORML 「……っ、%CALLNAME:MASTER%！？」
							PRINTW 
							PRINTFORML 物音に振り返った小町と映姫は、手を離すと慌てて距離を取り、互いに明後日の方向を向いている
							PRINTFORML 「や、やー……%CALLNAME:MASTER%。今日はいい天気だねぇ」
							PRINTFORML 屋内なのでいい天気もクソもあったものではないが、とりあえず会話を合わせてやった
							PRINTW 
						ENDIF
					;KENZEN
					ELSE
						SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「んっ……？」
							PRINTFORML 小町と映姫は、棒状のお菓子を互いに端から咥えて齧っている最中だったようだ
