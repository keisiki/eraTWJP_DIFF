;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K81_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K81_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K81_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「お互いに舐めあうなんて道化芝居よ」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%」
				PRINTFORMW 蓮子は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%ん～っ%UNICODE(0x2665) *1%」
				PRINTFORMW 蓮子は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%ん～っ%UNICODE(0x2665) *1%」
				PRINTFORMW 蓮子は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「……この臭い好きよ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「ん？」
				PRINTFORMW 蓮子は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「出しちゃえばいいのに%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……だんだんなんか出てきた……%UNICODE(0x2665) *1%」
				PRINTFORMW 蓮子は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっんっ%UNICODE(0x2665) *1%」
				PRINTFORMW 蓮子は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「ん……%UNICODE(0x2665) *1%ん～……%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んんんんっ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「私の舌使いも大したもんでしょ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「あっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「んんん？」
				PRINTFORMW 「もっと私の方も舐めてよねっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んっ……んんっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んんっ……っ%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん……んっ……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……んっ……あんっ……」
		ELSE
			PRINTFORMW 「……んっ……んっ……」
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K81_{SELECTCOM}_1
RETURN 1

