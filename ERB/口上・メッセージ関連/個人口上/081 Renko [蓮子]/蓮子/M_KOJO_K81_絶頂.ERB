		PRINTFORMW
		RETURN 1
	;TARGETだけ射精
	ELSEIF NOWEX:射精
		;PLAYERについてる
		IF GETBIT(TALENT:PLAYER:性別,1)
			PRINTFORMW
			RETURN 1
		;付いてない
