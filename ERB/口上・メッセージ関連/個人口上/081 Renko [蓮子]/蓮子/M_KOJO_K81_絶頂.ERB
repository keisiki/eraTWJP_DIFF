			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ＆Ｍ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;三重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
