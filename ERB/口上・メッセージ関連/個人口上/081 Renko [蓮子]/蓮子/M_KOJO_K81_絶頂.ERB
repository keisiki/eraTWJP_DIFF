		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で蓮子は絶頂した。
			ELSEIF RAND:5 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%あっ%UNICODE(0x2665)*1%おﾞっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「あっ%UNICODE(0x2665) *1%あんっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んんんっ%UNICODE(0x2665) *1%んﾞっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「あﾞっ%UNICODE(0x2665) *1%んﾞっ%UNICODE(0x2665) *1%駄目っ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:三重絶頂
			;Ｃ＆Ｖ＆Ａ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ｍ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｖ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;二重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:二重絶頂
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で蓮子は絶頂した。
			ELSEIF RAND:5 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%待ってっ%UNICODE(0x2665) *1%駄目っ%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%あっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「あっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「あﾞっ%UNICODE(0x2665) *1%駄目っ%UNICODE(0x2665) *1%っ%UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:二重絶頂
			;Ｃ＆Ｖ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｂ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ絶頂
		;時姦中
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で蓮子は絶頂した。
		ELSEIF RAND:5 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%くすぐったいっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「そこっ%UNICODE(0x2665) *1%敏感だからぁ%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「そこはっ%UNICODE(0x2665) *1%ダメぇっ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%んんんっ%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:Ｖ絶頂
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で蓮子は絶頂した。
		ELSEIF TEQUIP:50 == MASTER && TEQUIP:MASTER:コンドーム == 0
			IF RAND:5 == 0
				PRINTFORMW 「ダメっ%UNICODE(0x2665) *1%生チンポでイクっ%UNICODE(0x2665)*1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%イクっ%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%ああっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「っ%UNICODE(0x2665) *1%っ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSEIF TEQUIP:50 == MASTER
			IF RAND:5 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665)*1%いいっ%UNICODE(0x2665) *1%好きっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%これっ%UNICODE(0x2665) *1%好きっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%んﾞっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「っ%UNICODE(0x2665) *1%あんっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「あっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:5 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%ダメっ%UNICODE(0x2665)*1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%んあっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665) *1%もっとっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「あんっ%UNICODE(0x2665) *1%んあっ%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	ENDIF
	IF NOWEX:Ａ絶頂
		;時姦中
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で蓮子は絶頂した。
		ELSEIF RAND:5 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%おおﾞっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「お尻っ%UNICODE(0x2665) *1%ダメっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「あっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「あんっ%UNICODE(0x2665) *1%んあっ%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:Ｂ絶頂
		;時姦中
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で蓮子は絶頂した。
		ELSEIF RAND:5 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%ダメっ%UNICODE(0x2665) *1%」
