;==================================================
;押し倒した
;==================================================
@M_KOJO_ふらん_EVENT_K50_10(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;うふふ==2（開始時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;MASTERを押し倒した（開始時）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF TALENT:恋人
				IF TALENT:処女
				;押し倒し初を条件に入れる方法がわからぬ
					PRINTFORML フランは突如としてあなたを押し倒し、マウントポジションをとった。
					PRINTFORML 「ひどいよね、%CALLNAME:MASTER%は」
					PRINTFORML 「結構頑張って誘ってるのに、私はいつでもいいよってアピールしてるのに」
					PRINTFORML 「無視して、生殺し。一人遊びだけはかどっちゃう」
					PRINTFORML 「でも、もう我慢できないから。私から無理やり行くね？」
					PRINTFORMW 「それじゃあ、いただきます%UNICODE(0x2665)%」
				ELSE
					PRINTFORML 「ごめん、我慢できない」
					PRINTFORML 「%CALLNAME:MASTER%が、私の体に触れる度に、私の中で昂っちゃうの」
					PRINTFORML 「%CALLNAME:MASTER%が欲しくて、たまらない…って」
					PRINTFORMW 「だから、ごめんね。いただきます%UNICODE(0x2665)%」
				ENDIF
			ELSEIF TALENT:恋慕
				PRINTFORML 「その、ちょっとえっちな気分になっちゃったから」
				PRINTFORML 「本能的に、我慢できなくなっちゃった」
				PRINTFORML 「ごめんね。止められないや」
				PRINTFORMW 「・・・いただきます」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTERに抱きついた（開始時）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;なだめられた（終了時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;-------------------------------------------------
		;抜け出した
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF TALENT:恋人
				PRINTFORML 「……そんなに嫌だった？」
				PRINTFORML 「そうじゃないなら、今は、やめるから」
				PRINTFORMW 「後で、ちゃんとしてね」
			ELSEIF TALENT:恋慕
				PRINTFORML 「ごめん、その、ホントに嫌だったのかな」
				PRINTFORML 「ごめんね。我慢するから。……ごめんね」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;抱き着きモード満足終了
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF TALENT:恋人
				;IF (BASE:50:気力 <= (MAXBASE:気力 / 10) * 2)　みたいな
				;押し倒されてる間にどれだけ絶頂したかによって分岐させてみたいかも
				PRINTFORML 「んー、ふぅ」
				PRINTFORML 「まんぞく。私はしたけど」
				PRINTFORML 「%CALLNAME:MASTER%はどうかな？」
				PRINTFORMW 「もし、シたいなら……いいよ」
			ELSEIF TALENT:恋慕
				PRINTFORML 「その、ありがと。満足したよ」
				PRINTFORML 「%CALLNAME:MASTER%は、どう？」
				PRINTFORMW 「満足して、くれた…かな」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;抱き着きモード強制終了
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;抜け出す失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		;-------------------------------------------------
		;好きにして状態から抜け出せなかった
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;欲求不満が高くて抜け出せなかった
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			IF TALENT:恋人
				PRINTFORML 「だぁめ%UNICODE(0x2665)%」
				PRINTFORML 「こんなに私に我慢させて」
				PRINTFORML 「まだ全然解消させてくれないのに」
				PRINTFORML 「逃がさないよ」
				PRINTFORML 「まだまだ、足りないから、ね%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕
				PRINTFORML 「あっ、ダメ」
				PRINTFORML 「もうちょっとでいいから」
				PRINTFORMW 「もうちょっと、イかせて…%UNICODE(0x2665)%」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

