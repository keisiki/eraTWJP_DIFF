@M_KOJO_ふらん_MESSAGE_COM_K50_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CHK_DATENOW(CFLAG:LOCAL:デート中)
	ELSEIF CFLAG:睡眠
	ELSE
		IF TALENT:恋人
			PRINTDATAW
				DATALIST
					DATAFORM 「あったかいね」
					DATAFORM 「このぬくもりを覚えちゃったら、もう昔には、ひとりぼっちには戻れないや」
				ENDLIST
				DATALIST
					DATAFORM 「ずっとこのままがいい」
					DATAFORM 「なんて、わがままが過ぎるよね」
					DATAFORM 「でも、できるだけこうしていてほしい」
					DATAFORM 「ダメ、かな」
				ENDLIST
				DATALIST
					DATAFORM 「%CALLNAME:MASTER%が腰を落として、抱きしめてくれるとね」
					DATAFORM 「%CALLNAME:MASTER%の首筋が、目の前に見えるの」
					DATAFORM 「…その気になっちゃうね」
				ENDLIST
			ENDDATA
		;そのうち恋慕かつ恋人と恋慕で分けるかも
		;恋慕かつ恋人はすべての選択肢、恋慕は現状の二つのみみたいな感じで
		ELSEIF TALENT:恋慕
			PRINTDATAW
				DATALIST
					DATAFORM 「んー、ん」
					DATAFORM 「悪くない、というか、いいかも」
				ENDLIST
				DATALIST
					DATAFORM 「不思議と圧迫感が好ましいの」
					DATAFORM 「だから、もう少し、強く、ね？」
				ENDLIST
			ENDDATA
		ENDIF
	ENDIF
ENDIF
RETURN 1
;==================================================
;312,キスする
;TFLAG:193(99=拒絶(キス不可) 1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 50=手淫 60=キス 性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_ふらん_SUCCESS_COM_K50_312
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_ふらん_MESSAGE_COM_K50_312
CALL TRAIN_MESSAGE
CALL M_KOJO_ふらん_MESSAGE_COM_K50_312_1
RETURN RESULT

@M_KOJO_ふらん_MESSAGE_COM_K50_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
	ELSE
		IF TALENT:恋人
			IF EXP:TARGET:27 == 1
			;キス経験ゼロ条件だと通らない？
				PRINTDATAW
					DATALIST
						DATAFORM 「あ、してくれるんだ、キス」
						DATAFORM 「ううん、イヤなんてことないよ！むしろ、してほしい…」
						DATAFORM 「でもこっちから誘ってもなかなかしてくれなかったから」
						DATAFORM 「てっきり――人の血を吸ったような口とはキスしたくないのかなって思ってたの」
						DATAFORM 「うれしいな、ホント、ホントに……涙が出ちゃいそう」
						DATAFORM 「ねぇ、して？ゆっくり、優しく、情熱的に・・・」
						;KISS ME TENDERLY GENTLY VIOLENTLY
					ENDLIST
				ENDDATA
			ELSE
				PRINTDATAW
					DATALIST
						DATAFORM 「ん……ふ、ちゅ、る……ぷはぁ」
						DATAFORM 「情熱的だね。この先までしちゃう？」
					ENDLIST
					DATALIST
						DATAFORM 「え、私から？ちょ、ちょっと、恥ずかしいね」
						DATAFORM 「ん、ん〜っちゅっ」
						DATAFORM 唇と唇が少し触れて離れる。かわいらしいバードキスだ。
						DATAFORM 「……そんなほほえましい目で見ないで。恥ずかしいよ」
					ENDLIST
				ENDDATA
			ENDIF
		ELSEIF TALENT:恋慕
			PRINTDATAW
				DATALIST
					DATAFORM 「ん、じゅれろ、くちゅ…ぷ、はぁ」
					DATAFORM 「…もっと、する？」
				ENDLIST
			ENDDATA
		ELSEIF TALENT:思慕
			PRINTDATAW
				DATALIST
					DATAFORM 「ん……」
					DATAFORM 「満足？そ」
				ENDLIST
			ENDDATA
		ENDIF
	ENDIF
ENDIF
RETURN 1
