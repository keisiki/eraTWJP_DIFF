;==================================================
;140,イラマチオ
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K113_140
RETURN 0

@M_KOJO_MESSAGE_COM_K113_140
CALL M_KOJO_MESSAGE_COM_K113_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K113_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			CALL HPH_PRINT,@"「ふふ…HPH硬くて、美味しそうなチンポ……HPH」","L"
			CALL HPH_PRINT,@"「それじゃいただくわね……HPH  あぁ……♪」","L"
			PRINTFORMDL "―今だ"
			PRINTFORMDL 口を開けた瞬間に、ヘカーティアの後頭部を掴んで思い切り股間へと押し付ける。
			PRINTFORML 「んぶっ！？…ちょ……！くる、し……！」
			PRINTFORMDW 突然の事に目を白黒させるヘカーティアを無視して、快楽のままに腰を突き動かした……！
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ん゛んっ！！え゛ぶっ！ぐっ！ん゛お゛ぉぉっ！！」
					PRINTFORMDL ヘカーティアが抗議するような声を上げているが、%CALLNAME:MASTER%は無視してペニスを喉奥へ突き入れる。
					PRINTFORMDW やがて込み上げてきた射精感に、%CALLNAME:MASTER%は身を任せた……！
				CASE 1
					PRINTFORML 「ん゛んっ…！ぶっ！ずじゅっ！じゅぶっ！じゅぼぉっ！」
					PRINTFORMDL 激しい喉奥への抽挿により、ヘカーティアの口元は唾液でベトベトになっている。
					PRINTFORMDW 口元に陰毛を付着させながらも%CALLNAME:MASTER%をキッと睨む彼女を見て、興奮は更に高まり―
				CASE 2
					PRINTFORML 「ん゛ぶっ……ん゛……！んん゛ーーっ！」
					PRINTFORMDL 射精が近い事を悟ったのか、ヘカーティアは髪を振り乱して暴れ始めた。
					PRINTFORMDL %CALLNAME:MASTER%は彼女の頭をグッと抱き寄せ、喉奥へとペニスを捩じ込んだ。
					PRINTFORML 「―ごっ！？………ぉ……！」
					PRINTFORMDW ヘカーティアの眼球がぐるんと白目を剥き、喉がペニスをギュウギュウと締め上げる……！
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ん゛ん……っ！ん゛……！ぶっ……ぐ……！う゛ぅ……っ！」
					PRINTFORML （%CALLNAME:MASTER%の奴……！私をこんな…オナホみたいに……！）
					PRINTFORMDL 吐き気からか、或いは%CALLNAME:MASTER%に好き放題されているという不甲斐なさからか…
					PRINTFORMDW ヘカーティアは目尻に涙を浮かべている……！
				CASE 1
					PRINTFORML 「んぶっ！ん゛ん…！ぶっ！う゛ぅーっ！！」
					PRINTFORMDW ペニスで頬が膨らんだ醜い顔で、ヘカーティアは%CALLNAME:MASTER%を睨みつけている……！
				CASE 2
					PRINTFORML 「う゛っ…！ん゛っ…！え゛ほっ！げほっ！う゛っ！え゛う゛っ！」
					PRINTFORMDW 喉奥を亀頭で乱暴に突かれ、ヘカーティアは嘔吐いている……
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
