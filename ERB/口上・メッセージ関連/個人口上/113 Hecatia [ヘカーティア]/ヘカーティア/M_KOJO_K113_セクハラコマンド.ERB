IF LOCAL
TRYCALLFORM M_KOJO_COLOR_K113
	;アナル処女
	IF TCVAR:Ａ破瓜
		CALL HECA_SEX
		RETURN 1
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTL 
				PRINTFORML 「アナルが大好きな%CALLNAME:MASTER%の為に、今日は入念に準備しといてあげたわよん♪」
				CALL HPH_PRINT,@"「ここまでしてくれるご主人様に、たっぷり感謝なさい…HPH」","L"
				PRINTFORMDW 汚れ一つ無い尻穴に、%CALLNAME:MASTER%は怒張したペニスを突き入れた…！
			CASE 1
				PRINTL 
				CALL HPH_PRINT,@"「こんなにぐちょぐちょに濡れたおまんこより、ケツ穴を選ぶなんて…HPH」","L"
				CALL HPH_PRINT,@"「とんだド変態がいたものねぇ…？HPH HPH」","L"
				PRINTFORMDL ヘカーティアに罵られながらも、%CALLNAME:MASTER%はアナルへと挿入する。
				PRINTFORMDW 膣では味わえない極上の快感に、もはや抗うことはできなかった…！
		ENDSELECT
	ENDIF
RESETCOLOR
ENDIF
RETURN 1

