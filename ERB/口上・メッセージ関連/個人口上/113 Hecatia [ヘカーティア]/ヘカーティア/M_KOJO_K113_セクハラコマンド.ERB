@M_KOJO_MESSAGE_COM_K113_330_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		PRINTFORMDL 時を止めている為、へカーティアは一切身動きが取れない。
		PRINTFORMDL %CALLNAME:MASTER%は余裕を持って、目に痛い色合いのスカートをめくるとそこには…！？
		;穿いてる
		IF TEQUIP:0 > 1
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMDW へカーティアにしては珍しい、地味な色合いのパンツがあった…
				CASE 1
					PRINTFORMDW "GoToHell"と書かれた、変なパンツがあった…
			ENDSELECT
		;穿いてない
		ELSE
			PRINTFORMDL そこにある筈のパンツは無く、ヘカーティアはノーパンだった。
			PRINTFORMDW 丈の短いスカートで、よく平然としていられるものだ…
		ENDIF
	ELSEIF CFLAG:うふふ == 2
		CALL HPH_PRINT,@"「ふふふ……HPHどう？」","L"
		CALL HPH_PRINT,@"「貴方のチンポ…全部挿入ってるわよん…HPH」","L"
		PRINTFORMDW へカーティアはスカートをたくし上げ、繋がったお互いの性器を見せつけてきた…！
	ELSE
		;初回
		IF FIRSTTIME(SELECTCOM)
			PRINTFORMDL "ちょっと目を閉じて欲しい"
			PRINTFORML 「？…これでいいの？」
			PRINTFORML （ふふふ……♪ひょっとしてサプライズかしら？）
			PRINTFORMDW へカーティアが目を閉じた隙に、%CALLNAME:MASTER%はスカートをめくった！
			PRINTFORML 「あっ！？―ッ！！」
			;穿いてる
			IF TEQUIP:0 > 1
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMDW 拳骨を食らって昏倒するまでの一瞬に、純白のパンツを目に焼き付けた…！
					CASE 1
						PRINTFORMDL 拳骨を食らって昏倒する直前、"GoToHell"と書かれた変なパンツが見えた。
						PRINTFORMDW こんな物を見るためにリスクを犯した事を、%CALLNAME:MASTER%は心底後悔した…
				ENDSELECT
			;穿いてない
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%の視界が捉えたのは、秘密の花園だった。
				PRINTFORMDW 良い物を見せて貰ったお礼に、へカーティアの強烈なお仕置を受け入れる事にした……
			ENDIF
		
		SELECTCASE TFLAG:193
			;ビンタ
			CASE -5
				PRINTFORML 「胴体と泣き別れしなかっただけ、有難いと思いなさいな」
				PRINTFORMDW 首が捻じれそうな程の勢いで頬を張られた……
			;つねられる
			CASE -3
				PRINTFORML 「おイタしちゃう悪い手は、この手かしら～？」
				PRINTFORMDW へカーティアに手をつねられた。すごく痛い……
			;嫌がる
			CASE -1
					SELECTCASE RAND:2
					CASE 0
						PRINTFORMDL "ちょっと目を…"
						PRINTFORMW 「もうその手には乗らないわよ」
					CASE 1
						PRINTFORML 「ちょっ！？何してんのよ！」
						PRINTFORMDW へカーティアのスカートをめくろうとしたが、見つかってしまった…
				ENDSELECT
				
					
			;嫌がる（嬉しげ）
			CASE 0
				CALL HPH_PRINT,@"「や～ん♪%CALLNAME:MASTER%さんのエッチ～HPH」","L"
				PRINTFORMDW 軽い口調だが、へカーティアは%CALLNAME:MASTER%の足を強く踏みにじっている……
			;成功
			CASE 1
				IF ABL:露出癖 <= 2
					PRINTFORMDL "あ！%TEXTR("純狐が知らないおじさんに首輪付けて散歩させてる！/クラウンピースが玉乗りしながら松明をジャグリングしてる！/映姫が地獄Tシャツ着てハイグレ踊りしてる！")%"
					PRINTFORML 「%TEXTR("何ですって/嘘っ/えっ")%！？」
					PRINTFORMDW へカーティアの注意が逸れた隙に、%CALLNAME:MASTER%はスカートをめくった！
					PRINTFORML 「あっ！？―ッ！！」
					;穿いてる
					IF TEQUIP:0 > 1
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMDW 拳骨を食らって昏倒するまでの一瞬に、純白のパンツを目に焼き付けた…！
							CASE 1
								PRINTFORMDL 拳骨を食らって昏倒する直前、"GoToHell"と書かれた変なパンツが見えた。
								PRINTFORMDW こんな物を見るためにリスクを犯した事を、%CALLNAME:MASTER%は心底後悔した…
						ENDSELECT
					;穿いてない
					ELSE
						PRINTFORMDL %CALLNAME:MASTER%の視界が捉えたのは、秘密の花園だった。
						PRINTFORMDW 良い物を見せて貰ったお礼に、へカーティアの強烈なお仕置を受け入れる事にした……
					ENDIF
				ELSEIF ABL:露出癖 <= 4
					PRINTFORML 「…見られたものは、仕方ないわねぇ」
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%～HPH何か言い遺す言葉はある？」","L"
					;穿いてる
					IF TEQUIP:0 > 1
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMDW "可愛い下着だった"と褒めてみたが、返ってきたのはビンタだった…
							CASE 1
								PRINTFORMDL どうせ痛い目を見るのなら、率直な感想を言うとしよう…
								PRINTFORMDW "そのパンツは、ちょっとどうかと思う"
						ENDSELECT
					;穿いてない
					ELSE
						PRINTFORMDW "可愛いお尻だった"と言うと、へカーティアの拳が硬さを増した気がした……
					ENDIF
				ELSE
					CALL HPH_PRINT,@"「あんっHPHダメよ、こんなトコでサカっちゃ…HPH」","L"
					PRINTFORMDW %CALLNAME:MASTER%にスカートをめくられるヘカーティアは、どこか嬉しそうだ…
					;穿いてる
					IF TEQUIP:0 > 1
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMDL へカーティアにしては地味な下着だった。
								PRINTFORMW （何か、反応がイマイチねぇ……）
							CASE 1
								PRINTFORMDL "GoToHell"と書かれた変なパンツだ。
								PRINTFORMW 「私の勝負パンツよん♪」
						ENDSELECT
					;穿いてない
					ELSE
						PRINTFORMDL へカーティアはパンツを履いておらず、股座は微かだが濡れていた…！
						CALL HPH_PRINT,@"「ふふふ…♪バレちゃったわねぇ…HPH」","L"
						CALL HPH_PRINT,@"「どう？興奮した…？HPH」","W"
					ENDIF
				ENDIF
		ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
