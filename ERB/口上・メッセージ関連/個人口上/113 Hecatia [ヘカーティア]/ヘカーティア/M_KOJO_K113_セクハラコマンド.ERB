	ELSEIF CFLAG:睡眠
		CALL HPH_PRINT,@"「んん…っ…HPHん……ふ…HPH…んんっ…HPH」","W"
	ELSEIF CFLAG:うふふ == 2
		SELECTCASE TCVAR:20
			;クンニ強制
			CASE 70
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あっHPHちょっとぉっHPHお尻触らないのぉHPH HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「あんっHPHそこはおまんこじゃないわよぉっHPH HPH」","W"
					ENDSELECT
			;アナル奉仕
			CASE 72
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「んっ…あぁっHPHそう…HPHその調子よん…HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「ふふふ…HPHご主人様のお尻、気に入った？HPH」","W"
				ENDSELECT
			;スカートの中を見せる
			CASE 35
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あぁっHPHちょっと、お尻触んないのHPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「んん…っ…HPH貴方、お尻の方が好きなのかしら…？HPH」","W"
				ENDSELECT
			;素股
			CASE 52
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あんっHPHちょっと、邪魔しないのHPH」","W"
					CASE 1
						PRINTFORMW 「こら、ジッとしてなさい♪」
				ENDSELECT
			CASEELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「んん…っ…HPH貴方、お尻の方が好きなのかしら…？HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「ふふふ…HPHご主人様のお尻、気に入った？HPH」","W"
				ENDSELECT
		ENDSELECT
	ELSE
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%～♪この手は何かしら？」
					PRINTFORMDW へカーティアに気付かれ、手を捻り上げられた……
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「あっHPHちょっと、部屋まで我慢できないの？」","W"
						CASE 1
							CALL HPH_PRINT,@"「あぁんっHPHちょっと、お尻触んないのHPH」","W"
						CASE 2
							CALL HPH_PRINT,@"「あぁっ…HPHんもう、このスケベ…HPH」","W"
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「あんっHPHもっと優しく触ってちょうだい…HPH」","W"
						CASE 1
							CALL HPH_PRINT,@"「んんっ…HPHあ、はぁ…んっHPHふっ…HPHんん…ぁ…HPH」","L"
							PRINTFORMDW %CALLNAME:MASTER%に尻を撫でられ、へカーティアは切ない声を漏らしている…！
						CASE 2
							CALL HPH_PRINT,@"「ふふふ…HPH貴方のお尻も悪くないわよ…HPH」","L"
							PRINTFORMDW へカーティアの手が、%CALLNAME:MASTER%の尻に触れている…！
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%～♪この手は何かしら？」
					PRINTFORMDW へカーティアに気付かれ、手を捻り上げられた……
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「んんっ…HPHちょっと…！止めな、さ…あぁ…っHPH」","L"
						CASE 1
							CALL HPH_PRINT,@"「んっ…HPHく…っ…！んん…HPH貴方…いい加減にぃ…っ…HPH」","L"
					ENDSELECT
					PRINTFORMDW へカーティアは%CALLNAME:MASTER%のボディタッチを受け入れている…！
					
				;されるがまま
				CASE 4
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「思い残す事の無いよう、たっぷり堪能なさい♪」
							PRINTFORMDW へカーティアの笑顔が怖い……
						CASE 1
							IF TALENT:111:恋慕 || TALENT:111:思慕
								PRINTFORML （我慢…我慢よへカーティア…！）
								PRINTFORML （こんなのでも一応は、ピースの友達なのだから…！）
								PRINTFORMDW へカーティアは眉間に青筋を立てながら、%CALLNAME:MASTER%のセクハラを受け入れている……
							ELSE
								PRINTFORML 「思い残す事の無いよう、たっぷり堪能なさい♪」
								PRINTFORMDW へカーティアの笑顔が怖い……
							ENDIF
						CASE 2
							IF TALENT:112:恋慕 || TALENT:112:思慕
								PRINTFORML （我慢…我慢よへカーティア…！）
								PRINTFORML （こいつを傷つけたら、きっと純狐が悲しむわ…！）
								PRINTFORMDW へカーティアは眉間に青筋を立てながら、%CALLNAME:MASTER%のセクハラを受け入れている……
							ELSE
								PRINTFORML 「思い残す事の無いよう、たっぷり堪能なさい♪」
								PRINTFORMDW へカーティアの笑顔が怖い……
							ENDIF
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

RETURN 1
