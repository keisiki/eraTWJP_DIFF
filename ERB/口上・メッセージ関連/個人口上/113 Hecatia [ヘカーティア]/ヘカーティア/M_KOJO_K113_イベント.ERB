;==================================================
@M_KOJO_EVENT_K113_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;正月、バレンタイン、ハロウィン、クリスマスの場合
		IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
			CALL SPECIALDAY_EVENT_K113
		;不機嫌
		ELSEIF CFLAG:318 == 1
			IF !TCVAR:113:今日会ったか
				PRINTFORML 「……ああ、%CALLNAME:MASTER%。居たの？」
				PRINTFORMDW ヘカーティアは機嫌が悪いようだ……
			ELSE
				PRINTFORML 「さっきからウロウロと……何をしてるの？」
				PRINTFORMDW ヘカーティアの機嫌はまだ直っていないようだ……
			ENDIF
		ELSE
			IF !TCVAR:113:今日会ったか
				SELECTCASE TIME:2
					;朝
					CASE 1,2
						PRINTFORML 「おっはよ～ん！」
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「今日も楽しい事が起こるといいわねぇ♪」
							CASE 1
								PRINTFORMW 「清々しい朝ねぇ、%CALLNAME:MASTER%♪」
						ENDSELECT
					;昼
					CASE 3,4
						PRINTFORML 「こんにちはよん♪」
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「"語呂が悪い"？そんな事無いでしょう？」
							CASE 1
								PRINTFORMW 「%CALLNAME:MASTER%、用事はもう終わったのかしら？」
						ENDSELECT
