@M_KOJO_MESSAGE_MARKCNG_MESSAGECHECK_K113
RETURN 0

@M_KOJO_MESSAGE_MARKCNG_K113
;このままだと地の文が表示されません
;地の文を表示したい方は、下記のコメントアウトを削除して下さい
CALL MARK_MESSAGE
;苦痛刻印
;恐怖するようなタイプとは思えないので趣向を変えてみた
IF !FLAG:70 && !CFLAG:MASTER:イタズラ
	IF TFLAG:苦痛刻印取得 == 1
		PRINTFORML 「痛いわねぇ！何すんのよ！」
		PRINTFORMDW へカーティアに怒られた……
	ELSEIF TFLAG:苦痛刻印取得 == 2
		CALL HPH_PRINT,@"「いぃんっHPHや、やめ……！」","L"
		PRINTFORMDW %CALLNAME:MASTER%に叩かれ、へカーティアは艶めかしい声を上げた……
	ELSEIF TFLAG:苦痛刻印取得 == 3
		CALL HPH_PRINT,@"「あぁっHPHあひぃHPHひぎぃぃっHPH」","L"
		CALL HPH_PRINT,@"「%CALLNAME:MASTER%……HPHもっとぉ……HPH」","L"
		PRINTFORMDW へカーティアの中にある、イケナイ扉を開いてしまったようだ……！
		;PRINTFORMW ヘカーティアはマゾになった
		;TALENT:113:マゾ = 1
	ENDIF
ENDIF

;時姦刻印
IF TFLAG:時姦刻印取得 == 1
	PRINTFORMDL へカーティアの肢体を好き放題に弄ぶ%CALLNAME:MASTER%。
	PRINTFORMDL 地獄を支配する女神といえど、時を止めて動きを封じればただの女に過ぎない。
	PRINTFORMDW 日頃から彼女に振り回されている鬱憤を晴らすべく、%CALLNAME:MASTER%は両手に力を込めて胸を揉みしだいた……！
ELSEIF TFLAG:時姦刻印取得 == 2
	PRINTFORMDL 時の止まった世界の中で、%CALLNAME:MASTER%は一息つく。
	PRINTFORMDL 今の今まで愛撫されていた乳首や陰核は、時を止められているにも関わらず、破裂しそうな程に勃起している。
	PRINTFORMDL "何て淫乱な女神様だ"%CALLNAME:MASTER%は何気無く呟いた。
	CALL HPH_PRINT,@"（……ッHPH）","L"
	PRINTFORMDW ほんの一瞬……ヘカーティアの身体が、まるで絶頂したかの様に跳ね上がったように見えた……！
ELSEIF TFLAG:時姦刻印取得 == 3
	CALL HPH_PRINT,@"（ふぁぁっHPHあんっHPHわ、わたひっHPH%CALLNAME:MASTER%なんかにHPH好き放題されてるぅっHPHHPH）","L"
	CALL HPH_PRINT,@"（こ……こんなあぁっHPH時間止められ、てっHPH動けないまま…あちこちイジられてぇっHPH HPH）","L"
	CALL HPH_PRINT,@"（あ……HPHこれくるHPHくるっHPH大きいの―）","W"
	PRINTFORML （……は？ちょ……！何で止め……お、"おねだり"！？）
	CALL HPH_PRINT,@"（%CALLNAME:MASTER%のヤツ……！口動かせないの分かって……くぅぅっ……HPH）","W"
	PRINTFORMDL 表情は変わらないが、恐らくヘカーティアは時を止められている事を認識している。
	PRINTFORMDL 流石は地獄の女神と言いたい所だが……
	PRINTFORMDL 意識があっても動けないのであれば、それは時姦をより楽しむためのスパイスにしかならない。
	IF TALENT:恋慕 || TALENT:思慕
		PRINTFORMDW 時の止まった%CALLNAME:MASTER%の世界では、主従関係は逆転する。
	ELSE
		PRINTFORMDW 普段は傍若無人に振る舞う女神に、少し灸を据えてやるとしよう。
	ENDIF
	PRINTFORMDW 再び時が動き出し、彼女に制裁されるまで……
ENDIF

;快楽刻印
IF TFLAG:23 == 1
	CALL HPH_PRINT,@"「んうっHPHんくぅっ…！んんっ……HPH HPH」","L"
	PRINTFORMDW 絶頂したのか、ヘカーティアは身体をビクビクと震わせている……
ELSEIF TFLAG:23 == 2
	CALL HPH_PRINT,@"「ああぁぁっHPH HPHいくっHPHイクうぅぅぅっHPH HPH HPH」","L"
	PRINTFORMDW ヘカーティアが大きな嬌声を上げて絶頂した……！
ELSEIF TFLAG:23 == 3
	CALL HPH_PRINT,@"「あぁぁぁっHPH HPH HPH」","L"
	CALL HPH_PRINT,@"「あは……HPHいいわよ%CALLNAME:MASTER%……HPHもっと激しく突いて…HPH」","L"
	PRINTFORMDW ヘカーティアは秘裂から愛液を滴らせ、恍惚とした表情で身震いをしている……！
ENDIF

;不埒刻印
;快楽、屈服、恭順による刻印
;LV1
IF TFLAG:24 == 11 || TFLAG:24 == 31
	IF CFLAG:うふふ
		CALL HPH_PRINT,@"「あぁ……HPHん……♪おっ、きぃのが……HPH奥までぇ……HPH」","L"
		CALL HPH_PRINT,@"（もう……指じゃ満足出来ないわ……HPH）","W"
	ELSE
		PRINTFORML 「…………♪」
		PRINTFORMDL ヘカーティアがTシャツの襟元をずり下げている。もう少しで乳首が見えそうだ……
		PRINTFORMDL 意地の悪い笑みを浮かべる彼女を見て意図を察した%CALLNAME:MASTER%は、目を逸らして見なかった事にした……
		CALL HPH_PRINT,@"（んもう……%CALLNAME:MASTER%ったら、必死に目ぇ逸らしちゃって……可愛いんだからHPH）","W"
		PRINTFORMW （次はどんな事をしてからかってやろうかしらねぇ……♪）
	ENDIF
;LV2
ELSEIF TFLAG:24 == 12 || TFLAG:24 == 32
	IF CFLAG:うふふ
		CALL HPH_PRINT,@"「はぁ……HPHはぁ…HPH……%CALLNAME:MASTER%……！」","L"
		CALL HPH_PRINT,@"「もっと激しく……シて……HPH」","L"
		PRINTFORMDW ヘカーティアは瞳を潤ませ、熱い吐息を吐いた……！
	ELSE
		PRINTFORML 「%CALLNAME:MASTER%。ちょっと目、閉じてて♪」
		PRINTFORMDL 今度は何を企んでいるのかと―ほんの僅かな期待と共に―訝しみながら、%CALLNAME:MASTER%は目を閉じる。
		PRINTFORMDW するとヘカーティアは、すかさず後ろに回り込んで―
		CALL HPH_PRINT,@"「んふふ……♪あぁ……HPHぺろっ…HPHれるぅ……HPHちゅ…HPHれろぉぉ……HPH」","L"
		CALL HPH_PRINT,@"「あぁん……駄目よ開けちゃ……♪私がいいって言うまで目を閉じてなさい……HPH」","W"
		CALL HPH_PRINT,@"「ふふ……そうそう……♪ん……HPHぢゅ…HPH」","L"
		CALL HPH_PRINT,@"「ふぉうやっへ……HPHしふぁいをふさはえはまま、みみなめらへるの……HPHふぉうふん、ふるれしょ……HPH」","L"
		;こうやって……視界を塞がれたまま、耳舐められるの……興奮、するでしょ
		PRINTFORMDW 舌の生暖かい感触と甘い囁きに、%CALLNAME:MASTER%は理性が飛びそうになった……！
	ENDIF
;LV3
ELSEIF TFLAG:24 == 13 || TFLAG:24 == 33
	IF CFLAG:うふふ
		CALL HPH_PRINT,@"「あぁぁっHPH HPHこれっHPHいいっHPHすきぃHPH HPH」","L"
		CALL HPH_PRINT,@"「チンポHPH%CALLNAME:MASTER%のHPH%CALLNAME:MASTER%チンポぉHPHもっとおくまでほしいぃぃっHPH HPH HPH」","L"
		PRINTFORMDW 普段のヘカーティアからは想像もできない程の乱れぶりに、%CALLNAME:MASTER%はゾクゾクと身震いした……！
	ELSE
		CALL HPH_PRINT,@"「……っ……HPHは……！んん……っHPHふ……んぅ……HPH」","L"
		PRINTFORML 「んっ……？どうしたの%CALLNAME:MASTER%？股なんか押さえちゃって……♪」
		PRINTFORMDL ヘカーティアは%CALLNAME:MASTER%が隣にいても構うこと無く自慰をしている……
		PRINTFORMDW まるで見せつける様に開かれた二本の指には、ねっとりとした濃い愛液が糸を引いていた……！
		CALL HPH_PRINT,@"（んもう……！%CALLNAME:MASTER%ったら、焦らしてくれるわ……HPH）","L"
		PRINTFORMW （でも我慢よん……今日のプレイは、"ケダモノになった%CALLNAME:MASTER%に犯される"って決めたんだから……♪）
	ENDIF
