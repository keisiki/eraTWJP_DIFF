@M_KOJO_MESSAGE_COM_K113_402_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SETCOLOR 0xC90B39
	IF CFLAG:睡眠
		PRINTFORML 「ぐ～……Zzz……」
		PRINTFORMDW ヘカーティアを起こさないよう、そっと彼女の部屋を後にした……
	ELSEIF CFLAG:MASTER:初期位置 == CFLAG:113:初期位置 && CFLAG:MASTER:現在位置 == CFLAG:113:初期位置
		;川の字で寝る
		IF TFLAG:193 == 1
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「よしよーし……早く寝ちゃいなさい、%CHILDNAME:113:(TALENT:113:育児中)%……♪」
					PRINTFORMDL %CHILDNAME:113:(TALENT:113:育児中)%を間に挟み、へカーティアと寝床に入った。
					PRINTFORMDW 左右から頭を撫でられ、%CHILDNAME:113:(TALENT:113:育児中)%は気持ち良さそうに目を閉じて眠った……
				CASE 1
					CALL HPH_PRINT,@"「ふふ……♪%CHILDNAME:113:(TALENT:113:育児中)%……HPH」","L"
					PRINTFORMDL へカーティアは%CHILDNAME:113:(TALENT:113:育児中)%を抱きしめ、幸せそうな寝顔を浮かべている。
					PRINTFORMDW 一方の%CHILDNAME:113:(TALENT:113:育児中)%は、少し寝苦しそうだ……
				CASE 2
					PRINTFORML 「あら？……パパ、寝ちゃったわねぇ……♪」
					PRINTFORML 「……ねぇ%CHILDNAME:113:(TALENT:113:育児中)%、パパは好き？」
					CALL HPH_PRINT,@"「そう……♪ママもパパが大好きよHPH」","W"
			ENDSELECT
		ELSEIF TALENT:恋慕 || TALENT:思慕
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ふふふ…♪今夜は寝かせないわよ、%CALLNAME:MASTER%…HPH」","L"
					PRINTFORML 「今夜は朝までたっぷりと……」
					PRINTFORML 「ぐぅ～……Zzz……」
					PRINTFORMDW へカーティアは寝てしまった……
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%、ベッドメークは終わった？」
					PRINTFORML 「……オーケー、上出来！じゃあ寝ましょうか♪」
					PRINTFORMDW ぽんぽんと自分の隣を叩くへカーティアのお言葉に甘え、一緒に寝る事にした。
				CASE 2
					PRINTFORML 「いい？%CALLNAME:MASTER%。ご主人様をしっかり寝かしつけるのよん♪」
					PRINTFORMDW へカーティアに添い寝し、眠くなるまでお喋りをした。
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「%CALLNAME:MASTER%、私の隣で寝られる事を光栄に思いなさい♪」
					PRINTFORMDW 今夜はへカーティアと一緒に寝る事にした。
				CASE 1
					PRINTFORML 「んぅ……ぐぅ～……♪」
					PRINTFORMDW 寝返りを打ったへカーティアの裏拳が腹に直撃した……
				CASE 2
					PRINTFORML 「私を途中で起こしたりしたら、承知しないわよ……じゃ、おやすみ」
					PRINTFORMDW へカーティアは目を閉じ、寝息を立て始めた。
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE RAND:3
			CASE 0
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「ご主人様より先に寝るとは、感心しないわねぇ…」
					PRINTFORMW 「……まあ、今日だけは許してあげましょう。おやすみ♪」
				ELSE
					PRINTFORML 「もう寝るの？つまらないわねぇ……」
					PRINTFORMDW へカーティアは帰って行った。
				ENDIF
			CASE 1
				PRINTFORML 「おやすみ、%CALLNAME:MASTER%」
				PRINTFORMW 「明日も楽しいコトしましょうねぇ♪」
			CASE 2
				PRINTFORML 「そうだ%CALLNAME:MASTER%、私達が子守唄を歌ってあげましょう♪」
				SETCOLOR 0x2459F0
				PRINTFORML 「よかったわねぇ♪子守唄の三重奏<トリオ>なんてそうそう聴けないわよ？」
				SETCOLOR 0xF0D124
				CALL HPH_PRINT,@"「今夜はぐっすり眠れるわねぇ%CALLNAME:MASTER%HPH」","L"
				PRINTFORMDW この後、%CALLNAME:MASTER%は悪夢にうなされた……
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
