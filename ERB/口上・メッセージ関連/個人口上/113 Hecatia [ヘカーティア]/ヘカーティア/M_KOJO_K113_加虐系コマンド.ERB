;加虐系コマンド
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;100,スパンキング
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K113_100
RETURN 0

@M_KOJO_MESSAGE_COM_K113_100
CALL M_KOJO_MESSAGE_COM_K113_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K113_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「あ゛あんっHPH HPH HPH」","L"
		PRINTFORML 「……な、何すんのよ！変な声出ちゃったじゃない……！」
		PRINTFORMDL 臀部を叩かれるという屈辱に、ヘカーティアは憤っている。
		PRINTFORMDW だが今の反応を見るに、満更でもないのかもしれない……
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:TARGET:マゾ == 1 || ABL:TARGET:マゾっ気 >= 2
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「ひぃっHPH HPHやめなさ…あぁぁっHPH HPH」","L"
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%……ご主人様のお尻をなんだとぉぉっHPH HPH」","L"
				PRINTFORMDW %CALLNAME:MASTER%に尻を叩かれる度、ヘカーティアは淫らに嬌声を上げている…！
			CASE 1
				CALL HPH_PRINT,@"「ひぎぃっHPH HPHいいっHPH あぁぁっHPH HPH」","L"
				CALL HPH_PRINT,@"「こ、こんな…！地獄の女神が、貴方なんかにぃっHPH HPH」","L"
				PRINTFORMDW ヘカーティアは尻を叩かれ、悔しさと悦びの混ざった表情を向けてくる…！
			CASE 2
				CALL HPH_PRINT,@"「あひぃっ HPH HPH%CALLNAME:MASTER%っHPH HPHも、もっとぉっHPH」","L"
				PRINTFORML 「い…いや、何でもないわ。忘れ―」
				CALL HPH_PRINT,@"「てぇっHPH HPHあひっHPHひい゛いぃっHPH HPH」","L"
				PRINTFORMDW 自分もヘカーティアも、引き返せない所まで来てしまったのかもしれない……！
		ENDSELECT
		RETURN 1
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「あぎぃっHPH HPHあぁっHPH HPH%CALLNAME:MASTER%…！後で覚えて―」","L"
				CALL HPH_PRINT,@"「あ゛ぁっHPH HPHちょっ…！とおっHPHい、いい加減にひぃっHPH HPH」","L"
				PRINTFORMDW どうせ後でお仕置されるのだ……叩けるうちに叩いておこう。
			CASE 1
				CALL HPH_PRINT,@"「はひぃっHPHいぃっHPHいうぅんっHPH HPH HPH」","L"
				PRINTFORMDW 一定のリズムを刻むように、連続でヘカーティアの尻を叩いた。
			CASE 2
				CALL HPH_PRINT,@"「あひぃっHPH HPH」","L"
				PRINTFORML 「あ、貴方ねぇ…！誰に向かってこんな事―」
				CALL HPH_PRINT,@"「あ゛あぁぁんっHPH HPH」","L"
				PRINTFORMDW 言葉を遮るように、%CALLNAME:MASTER%はヘカーティアの尻を叩いた。
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1

