RETURN RESULT

@M_KOJO_MESSAGE_COM_K123_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
	ELSE
		IF TALENT:狂気
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「っ！触らないで！」
					PRINTFORMW 里乃はぱしんと%CALLNAME:MASTER%の手を払った。
				;恥ずかしがる
				CASE 2,3
					PRINTFORML 「んっ……もう、やめてよねー%UNICODE(0x2764)%」
					PRINTFORMW そう言って、里乃は慣れた様子で%CALLNAME:MASTER%の手から逃れた。
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「貴方もその辺のスケベジジイと一緒ねー？」
						PRINTFORMW 里乃は呆れ笑いを浮かべ、%CALLNAME:MASTER%を見つめている。
					CASE 1
						PRINTFORML 「みんな、これくらいで済ませてくれるならいいんだけどねー」
						PRINTFORMW 里乃は嫌な記憶を思い出し、乾いた笑い声を上げた。
					ENDSELECT
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「きゃっ！？もう、やめてってば！」
					PRINTFORML 「それ以上するなら、ケーサツにツーホーするわよー？」
					PRINTFORMW 里乃はむすっとして%CALLNAME:MASTER%に抗議している。
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「お尻なんて触って、楽しいの……？」
							PRINTFORMW 里乃は恥ずかしがって、%CALLNAME:MASTER%の手から逃れようとしている。
						CASEELSE
							PRINTFORML 「何考えてるのよー……」
							PRINTFORMW 里乃は恥ずかしがって、%CALLNAME:MASTER%の手から逃れようとしている。
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「%CALLNAME:MASTER%にだけは、許してあげるわ」
						CASEELSE
							PRINTFORML 「もー、そんなに私のお尻が好きなの？」
							PRINTFORMW 「仕方ないわねぇ、もっと触らせてあげる♪」
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「嫌っ！触らないでっ！」
					PRINTFORMW 里乃はばしっと%CALLNAME:MASTER%の手を引っぱたいた。
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「やだ、もう……」
							PRINTFORMW 里乃は居心地悪げに身じろぎしている。
						CASEELSE
							PRINTFORMW 「手つきが気持ち悪すぎるんだけどー……」
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「下心丸出し…って感じねー」
							PRINTFORMW 里乃はじっとりと%CALLNAME:MASTER%を見つめている。
						CASEELSE
							PRINTFORMW 「ほら、もう十分触ったでしょー？」
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
