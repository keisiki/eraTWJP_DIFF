;-------------------------------------------------
IF LOCAL
;時姦中
	IF FLAG:70
		;PRINTFORMW 
	ELSEIF TALENT:MASTER:濃厚精液
		IF RAND:3 == 0
			PRINTFORMW 「こんなにたくさん出してくれたんですね…ゼリーみたい…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 大妖精は興味津々に精液が詰ったコンドームを突いている。
			RETURN 1
		ELSE
			PRINTFORMW 「私の膣内…こんなに出されてたら妊娠しちゃいますね」
			RETURN 1
		ENDIF
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「精液…まだ温かいですね…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「白くて濃くていっぱい…」
			RETURN 1
		ELSE
			PRINTFORMW 「や…見てるだけでお腹の奥が切なくなっちゃいます…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

