;==================================================
@M_KOJO_SPEVENT_K66_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_1(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORMW 「つ～ん……まだちょっと怒ってます」
		ELSEIF TALENT:恋人
			PRINTFORMW 「楽しかったわ～、もっともっとたくさん遊びましょうね%UNICODE(0x2665) *1%？」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「…うふふっ…じゃあまた今度…遊びましょうか？」
		ELSEIF TALENT:思慕
			PRINTFORMW 「ふ～ん…結構楽しかったわね…」
		ELSE
			PRINTFORMW 「つかれた～…ふよふよ浮くのもちょっと一苦労なものね…」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「はい、もう怒っていません」
			PRINTFORMW 「よく頑張ってくれました、幽々子ちょっとうれしいです」
			PRINTFORMW 「ふふっ……じゃあ仲直りっと」
		ELSE
			PRINTFORMW 「ん～？怒ってないわよもう全然」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「…え～それで終わり？　もう少し悪戯たかったんだけれど……」
			PRINTFORMW 「もう少し遊ばないと満足できないわよ？」
		ELSE
			PRINTFORMW 「ふ～ん……まぁ結構よかったんじゃない？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「…う～んもう…謝るなら別方法でも全然よかったんだけれど」
			PRINTFORMW 「この方法が一番いいわね…ふふっ！あっまだ怒ってま～す」
		ELSE
			PRINTFORMW 「…はい亡霊ポイント1減りました～」
			PRINTFORMW 「さて今何ポイント目でしょうか？」
		ENDIF
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

