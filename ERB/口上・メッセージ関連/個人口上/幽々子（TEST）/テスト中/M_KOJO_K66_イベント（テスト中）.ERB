;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;仕事中、今日会ってない。
	IF !CFLAG:行動 == 5 && !TCVAR:90:今日会ったか
		PRINTFORMW 「%CALLNAME:MASTER%、今は忙しいのよ～」
	;今日会ってない。
	ELSEIF !TCVAR:90:今日会ったか
		;-------------------------------------------------
		;小分岐
		;-------------------------------------------------
		;MASTERが入ってきた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 1
			;正月、バレンタイン、ハロウィン、クリスマスの場合
			IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
				CALL SPECIALDAY_EVENT_K66
