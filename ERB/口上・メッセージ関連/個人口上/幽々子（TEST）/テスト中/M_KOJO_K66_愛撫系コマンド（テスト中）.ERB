	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	;ELSEIF TALENT:恋慕
		RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
		;RETURN 1
