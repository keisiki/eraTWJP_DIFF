			PRINTFORML …少々恥ずかしい
			PRINTW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋慕 && TALENT:恋人
		IF TALENT:妊娠願望
			PRINTFORML 「えぇ～～、付けちゃうの～？」
			PRINTFORML 「子種、ほしーのにぃ…%MASTERNAME:42%の赤ちゃん、産みたいな…」
			SIF ESTRUS_CYCLE(TARGET) == 150
				PRINTFORML 「はたての準備できた赤ちゃんのお部屋に、たくさん注いでほしーなー…」
			PRINTW 
		ELSE
			IF ESTRUS_CYCLE(TARGET) == 15
				PRINTFORML 「今日、安全日だったんだけど、ちゃんと付けてくれるんだ」
				PRINTFORML 「%MASTERNAME:42%って、しっかりしてるよね♪」
				PRINTW 
			ELSEIF ESTRUS_CYCLE(TARGET) == 150
				IF CFLAG:TARGET:生解禁 == 2
					PRINTFORML 「えーっ、付けちゃうのー？」
					PRINTFORML 「生のほーが気持ちいーよー？　%MASTERNAME:42%はぁ、はたてに種付けしたくないのぉ～？」
					PRINTFORML はたてが胸元にのの字を書いて誘惑してきた…
					PRINTW 
				ELSE
					PRINTFORML 「あ、ちゃんと付けてくれるんだー♪」
					PRINTFORML 「赤ちゃんできたら大変だもんね。そこはちゃんとしなきゃ」」
					PRINTW 
				ENDIF
			ELSE
				PRINTFORML 「付けないとデきちゃうかもしれないもんね♪」
				PRINTW 
			ENDIF
		ENDIF
	ELSEIF TALENT:恋慕 || TALENT:恋人
		IF TALENT:妊娠願望
			PRINTFORML 「えぇ～～、付けちゃうの～？」
			PRINTFORML 「子種、ほしーのにぃ…%MASTERNAME:42%の赤ちゃん、産みたいな…」
			PRINTW 
		ELSEIF ESTRUS_CYCLE(TARGET) == 15
			PRINTFORML 「今日、安全日だったんだけど、ちゃんと付けてくれるんだ」
			PRINTFORML 「%MASTERNAME:42%って、しっかりしてるよね♪」
			PRINTW 
		ELSEIF ESTRUS_CYCLE(TARGET) == 150
			IF CFLAG:TARGET:生解禁 == 2
				PRINTFORML 「えーっ、付けちゃうのー？」
				PRINTFORML はたては不服そうだ…
				PRINTW 
			ELSE
				PRINTFORML 「あ、ちゃんと付けてくれるんだー♪」
				PRINTFORML 「赤ちゃんできたら大変だもんね。そこはちゃんとしなきゃ」」
				PRINTW 
			ENDIF
		ELSE
			PRINTFORML 「付けないとデきちゃうかもしれないもんね♪」
			PRINTW 
		ENDIF
		RETURN 1
	ELSEIF TALENT:思慕
		IF ESTRUS_CYCLE(TARGET) == 15
			PRINTFORML 「今日、安全日だったんだけど、ちゃんと付けてくれるんだ」
			PRINTFORML 「%MASTERNAME:42%って、意外とちゃんとしてるのね♪」
			PRINTW 
		ELSEIF ESTRUS_CYCLE(TARGET) == 150
			PRINTFORML 「あ、ちゃんと付けてくれるんだー♪」
			PRINTFORML 「そーいうとこ、ポイント高いぞ♪」
			PRINTW 
		ELSE
			PRINTFORML 「安全日以外、ちゃんと付けないとね」
			PRINTFORML 「ホントは安全日も付けなきゃダメだよ？」
			PRINTW 
		ENDIF
		RETURN 1
	ELSE
		RETURN 1
	ENDIF
ENDIF
RETURN 1