			PRINTW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;装着
	IF TEQUIP:ニプルキャップ
		;時姦中
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			IF TALENT:恋慕 || TALENT:恋人
				PRINTFORML 「駄目じゃないけど…それ、ホントーは乳首につけるものじゃないよね…？」
				PRINTFORML 「ううっ、乳首がキュってなっちゃう…」
				PRINTW 
				RETURN 1
			ELSE
				PRINTFORML 「またその変なの付けるの…『おっぱいが大きくなる』？　なるわけないでしょ！　馬鹿！」
				PRINTFORML 「ひううっ、ちくびっ、とれちゃううっ」
				PRINTW 
				RETURN 1
			ENDIF
		ENDIF
	;解除
	ELSE
		IF ABL:Ｂ感覚 >= 3
			PRINTFORML 「あンッ、とっちゃうの…って、今のはなんでもないっ！」
			PRINTW 
		ELSE
			PRINTFORML 「やっととってくれた～。もうっ、変なモノ付けないでよね…」
			PRINTW 
		ENDIF
	ENDIF
ENDIF
RETURN 1
