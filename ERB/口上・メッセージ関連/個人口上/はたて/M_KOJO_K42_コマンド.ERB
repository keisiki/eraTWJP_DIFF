;==================================================
;189,新妻プレイ
;==================================================
@M_KOJO_MESSAGE_COM_K42_189
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K42_189_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K42_189_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			RETURN 1
		ELSE
			;恋慕時のみ可能
			PRINTFORML 「え、これを付けてほしいの…？」
			PRINTFORML 「こ、こんな感じでいーのかな、似合う？」
			PRINTW 
			PRINTFORML 「ホント？　ふふ、ありがと、あ・な・た%UNICODE(0x2665) *1%」
			PRINTFORML 「きゃー、言っちゃったー%UNICODE(0x2665) *1%」
			PRINTFORML はたてはいやんいいやんと首を振っている
			PRINTW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;開始
	IF TEQUIP:新妻プレイ
		IF CFLAG:K42C_結婚 >= 1
			IF TEQUIP:0 == 0 && TEQUIP:1 == 0
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「えへへ…どーかな、ダーリン%UNICODE(0x2665) *1%」
					PRINTFORM エプロンからのぞく
					IF TALENT:TARGET:バストサイズ > 0
						PRINTFORML はちきれんばかりの胸に、視線が釘付けになる…
					ELSEIF TALENT:TARGET:バストサイズ == -1
						PRINTFORML 愛らしい小ぶりな胸がチラチラと見え隠れするのにドキドキする…
					ELSEIF TALENT:TARGET:バストサイズ == -2
						PRINTFORML のは悲しいまでの絶壁だったが、乳首が見え隠れするのがなんともエロい
					ELSE
						PRINTFORML 形の良い胸に、視線が釘付けになる…
					ENDIF
					PRINTW 
					PRINTFORML 「あー、胸ばっかり見てー。エッチなんだからぁ…%UNICODE(0x2665) *1%」
					PRINTFORML はたてが怒ったふりをしておでこをつついてきた
					PRINTW 
				CASE 1
					PRINTFORML 「こーいうのが嬉しーんだよね…%UNICODE(0x2665) *1%」
					PRINTFORML 裸エプロン姿のはたては、こちらにお尻を突き出して見せている…
					PRINTW 
				CASE 2
					PRINTFORML 「こ、こーいう恰好すればいーの…？」
					PRINTFORML はたてはもじもじと恥ずかし気にしている
					PRINTW 
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ダーリン、お帰りなさい%UNICODE(0x2665) *1%」
					PRINTFORML 「ご飯にする？　お風呂にする？　それとも、わ・た・し？」
					PRINTW 
					PRINTFORML 「自分で言っといてなんだけど、けっこー恥ずかしーわね、これ…」
					PRINTFORML 「やんっ、服を脱ぎながら飛び込んできちゃ駄目ぇ%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 1
					PRINTFORML 「ねーねー似合うー？」
					CALL ASK_YN("とっても似合ってる","はたてちゃん最高に可愛いさすが嫁")
					IF !RESULT
						PRINTFORML 「ほんと？　うれしー%UNICODE(0x2665) *1%」
						PRINTFORML はたてが笑顔で抱きついてきた
						PRINTW 
					ELSE
						PRINTFORML 「も、も～～、褒めすぎだってばぁ…\/\/\/」
						PRINTFORML はたては気恥ずかしいのか、ほにゃほにゃと笑っている
						PRINTW 
					ENDIF
				CASE 2
					PRINTFORML 「えへへ、もう新婚ごっこじゃなくて、ちゃんと結婚してるんだもんね…♪」
					PRINTFORML 「幸せだよ、あ・な・た%UNICODE(0x2665) *1%」
					PRINTW 
				ENDSELECT
			ENDIF
			RETURN 1
		ELSE
			IF TEQUIP:0 == 0 && TEQUIP:1 == 0
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「冷静に考えたら、裸にエプロンで料理とかしたら危ないよね」
					PRINTFORML 「私は妖怪だから油が跳ねたくらいじゃひどい怪我とかはしないけどさー」
					PRINTW 
				CASE 1
					PRINTFORML 「裸にエプロンって…なんだか恥ずかしーね」
					PRINTFORML 「でも、%MASTERNAME:42%、なんか嬉しそーなんですけどー…♪」
					PRINTW 
				CASE 2
					PRINTFORML 「あっ、私、こーいうの本で見たことあるよっ」
					PRINTFORML 「裸エプロンで料理している妻を、夫が後ろから抱きしめて…エッチ、しちゃうんでしょ…%UNICODE(0x2665) *1%」
					PRINTW 
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「えへへ、なんかこれ付けてると本当に新妻になった気分…%UNICODE(0x2665) *1%」
					PRINTFORML 「え、えーと、ね、笑わないでね…あ・な・た」
					PRINTFORML 「…………キャーーッ、もー、恥ずかしー！　あなただってー！やだー\/\/\/」
					PRINTFORML 一人で舞い上がり一人で恥ずかしがるはたては見ていて飽きなかった
					PRINTW 
				CASE 1
					PRINTFORML 「えへへ、なんか、こーいうのいいね…%UNICODE(0x2665) *1%」
					PRINTFORML はたてはエプロンに手を当て、感慨深げな表情を浮かべた
					PRINTW 
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
		RETURN 1
	;終了
	ELSE
		PRINTFORML 「あーん、もーちょっと着けてたかったかなー」
		PRINTW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1
