				PRINTFORML 共に、死合おうぞ…！
				PRINTFORML 
				PRINTFORML 『うぉおおおおおおおおおおーーーーーーーーーーーー！』
				PRINTFORML 
				PRINTFORML 
				PRINTFORML はたてちゃんの新たな活躍にご期待ください！
				PRINTFORML 
				PRINTFORML 
				PRINTW 
			CASE 3 && TALENT:MASTER:年齢 == -1 && (TALENT:MASTER:応答 == 1 || TALENT:MASTER:態度 == 1)
				PRINTFORML 「うわ。クソジャリ、むかつくー！」
				PRINTFORML 「奪えるものなら奪ってみなさいっての！」
				PRINTW 
			ENDSELECT
		ENDIF
	CASE -1
		IF TALENT:恋慕 || TALENT:恋人
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「う～ん…%MASTERNAME:42%のお願いでもこればっかりは駄目ー！」
				PRINTW 
			CASE 1
				PRINTFORML 「ごめんね。これは渡せないの」
				PRINTW 
			CASE 2
				PRINTFORML 「えっ、駄目ダメ！　やっと捕まえたんだもん！」
				PRINTFORML 「私も結構真剣に集めてるんだよ」
				PRINTW 
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「どーしよっかなー。んー、やっぱりだーめ」
				PRINTW 
			CASE 1
				PRINTFORML 「違うのだったらあげてもよかったんだけどねー」
				PRINTFORML 「%MUSHI_NAME%はあげられないよ」
				PRINTW 
			CASE 2
				PRINTFORML 「えぇ～…それは無理かなー」」
				PRINTW 
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「はぁ？　あげるわけないじゃん」
				PRINTW 
			CASE 1
				PRINTFORML 「やーよ。あんたにあげる理由がないもん」
				PRINTW 
			CASE 2
				PRINTFORML 「だーめ。これは私のだかんね！」
				PRINTW 
			ENDSELECT
		ENDIF
	;捕獲失敗
	CASE 0
		IF TALENT:恋慕 || TALENT:恋人
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「あーん、逃げられちゃったー」
				PRINTW 
			CASE 1
				PRINTFORML 「惜しかったねー。もーちょっとだったのに」
				PRINTW 
			CASE 2
				PRINTFORML 「あああ、あんな隙間に…」
				PRINTW 
			CASE 3
				PRINTFORML 「失敗する時だってあるよ。次がんばろ♪」
				PRINTW 
			CASE 4
				PRINTFORML 「ちょこざいなー！」
				PRINTW 
			ENDSELECT
		ELSE
			SELECTCASE RAND:6
			CASE 0
				PRINTFORML 「ぷぷー♪　逃げられてやんの♪」
				PRINTW 
			CASE 1
				PRINTFORML 「う、うっさいわね！　今のは逃がしてやったのよ！」
				PRINTW 
			CASE 2
				PRINTFORML 「意外とすばしっこいわねぇ」
				PRINTW 
			CASE 3
				PRINTFORML 「あーもう、あんたがしっかり構えてないからー」
				PRINTFORML 「なによー、はたてちゃんのせいだっての！？」
				PRINTW 
			CASE 4
				PRINTFORML 「ふふふ、幻想郷一の虫取り名人と言われたはたてちゃんが…あれー？」
				PRINTW 
			CASE 5
				PRINTFORML 「あーあ、逃げられちゃったー」
				PRINTW 
			ENDSELECT
		ENDIF
	;MASTERが捕った
	CASE 1
		IF TALENT:恋慕 || TALENT:恋人
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「%MASTERNAME:42%、すごーい♪」
				PRINTW 
			CASE 1
				PRINTFORML 「おおっ、お見事♪」
				PRINTW 
			CASE 2
				PRINTFORML 「やったね、%MASTERNAME:42%♪」
				PRINTW 
			CASE 3
				PRINTFORML 「あっ、%MUSHI_NAME%じゃん！　見せて見せてー」
				PRINTW 
			CASE 4
				PRINTFORML 「よく捕まえられたわねー」
				PRINTW 
			ENDSELECT
		ELSE
			SELECTCASE RAND:8
			CASE 0
				PRINTFORML 「ぬぐぐ…それは私の獲物だったのに…」
				PRINTW 
			CASE 1
				PRINTFORML 「ま、まーまーじゃん」
				PRINTW 
			CASE 2
				PRINTFORML 「うっさいわね！　私だってすぐ捕まえるんだから！」
				PRINTW 
			CASE 3
				PRINTFORML 「見てなさいよー。私だって！」
				PRINTW 
			CASE 4
				PRINTFORML 「へぇ、思ってたよりやるじゃん」
				PRINTW 
			CASE 5
				PRINTFORML 「意外とやるわねー」
				PRINTW 
			CASE 6
				PRINTFORML 「やるじゃん！」
				PRINTW 
			CASE 7
				PRINTFORML 「へぇ、すごーい！」
				PRINTW 
			ENDSELECT
		ENDIF
	;やるじゃん
	CASE 2
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「えへへー、すごいでしょ！」
			PRINTW 
		CASE 1
			PRINTFORML 「妖怪の山の虫取りプリンセスといえば、この私よ！」
			PRINTW 
		CASE 2
			PRINTFORML 「もっと褒めてもいーよ♪」
			PRINTW 
		ENDSELECT
	;渋々譲った
	CASE 3
		IF TALENT:恋慕 || TALENT:恋人
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「どーしてもっていうならあげる」
				PRINTW 
			CASE 1
				PRINTFORML 「んー、しょーがないなぁ」
				PRINTW 
			CASE 2
				PRINTFORML 「同じのが捕れたら次は私にちょーだい」
				PRINTW 
			CASE 3
				PRINTFORML 「もー、そんな目しないでよぉ。はいどーぞ」
				PRINTW 
			CASE 4
				PRINTFORML 「もー我が儘なんだからぁ。しょーがないなぁ」
				PRINTW 
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「次は私にも譲りなさいよね！」
				PRINTW 
			CASE 1
				PRINTFORML 「しょーがないわね、今回だけ譲ってあげる」
				PRINTW 
			CASE 2
				PRINTFORML 「あんたねぇ…必死すぎでしょ」
				PRINTW 
			CASE 3
				PRINTFORML 「分かった！　譲るから！　近づきすぎだっての！」
				PRINTW 
			CASE 4
				PRINTFORML 「しょーがないわね、そこまで完璧な土下座を見せられちゃ…」
				PRINTW 
			ENDSELECT
		ENDIF
	;快く譲った
	CASE 4
		IF TALENT:恋慕 || TALENT:恋人
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「はい、どーぞ♪」
				PRINTW 
			CASE 1
				PRINTFORML 「いいよー♪　大事に飼ってね♪」
				PRINTW 
			CASE 2
				PRINTFORML 「ムシバトルがんばってね！」
				PRINTW 
			CASE 3
				PRINTFORML 「このムシがほしかったの？　別にいーけど」
				PRINTW 
			CASE 4
				PRINTFORML （すっごく喜んでる。そんなにほしかったんだなー）
				PRINTW 
			ENDSELECT
		ELSE
			SELECTCASE RAND:6
			CASE 0
				PRINTFORML 「まあ、私には必要ないムシだしいいわよ」
				PRINTW 
			CASE 1
				PRINTFORML 「はいどーぞ」
				PRINTW 
			CASE 2
				PRINTFORML 「感謝しなさいよねー」
