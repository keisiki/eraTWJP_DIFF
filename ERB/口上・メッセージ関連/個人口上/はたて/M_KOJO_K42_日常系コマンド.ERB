;==================================================
;448,罠を仕掛ける／虫トラップ
;TFLAG:193,
;   -1=罠が壊れた
;    0=罠を仕掛けた
;    1=獲物が得られた
;   10=虫トラップを仕掛けた
;   11=虫トラップを破棄した
;   12=ムシを捕まえた
;FLAG:194,獲物の番号
;ムシの名前は MUSHI_NAME で表示したり判別したりできます
;==================================================
@M_KOJO_MESSAGE_COM_K42_448
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K42_448_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K42_448_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE TFLAG:193
	;罠が壊れた
	CASE -1
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「うわ、グシャグシャじゃん」
			PRINTW 
		CASE 1
			PRINTFORML 「あちゃー、大物がかかっちゃったのかな」
			PRINTW 
		CASE 2
			PRINTFORML 「あーん、せっかく仕掛けたのにー」
			PRINTW 
		ENDSELECT
	;罠を仕掛けた
	CASE 0
		IF TALENT:恋慕 || TALENT:恋人
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「これで、よしと」
				PRINTW 
			CASE 1
				PRINTFORML 「なーにっがとっれるかなー♪」
				PRINTW 
			CASE 2
				PRINTFORML 「うまくかかるといいねー」
				PRINTW 
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「そんなんでだいじょぶ？　ホントーに捕まるの？」
				PRINTW 
			CASE 1
				PRINTFORML 「捕れたら私にちょーだい。えー、いーじゃーん」
				PRINTW 
			CASE 2
				PRINTFORML 「あっ！　かかってる！」
				PRINTW 
			ENDSELECT
		ENDIF
	;獲物が得られた
	CASE 1
		SELECTCASE RAND:4
		CASE 0
			PRINTFORML 「やったー♪　かかったー♪」
			PRINTW 
		CASE 1
			PRINTFORML 「見て、%MASTERNAME:42%。何かいる」
			PRINTW 
		CASE 2
			PRINTFORML 「あっ！　かかってる！」
			PRINTW 
		CASE 3
			PRINTFORML 「%CritterName%がかかってるわ」
			PRINTW 
		ENDSELECT
	;虫トラップを仕掛けた
	CASE 10
		IF TALENT:TARGET:育児中
			PRINTFORML 「捕まえられたら、%CHILDNAME:42:(TALENT:TARGET:育児中)%が喜ぶね♪」
			PRINTW 
		ELSEIF TALENT:恋慕 || TALENT:恋人
			PRINTFORML 「明日が楽しみだねー、%MASTERNAME:42%」
			PRINTW 
		ELSE
			PRINTFORML 「なーにがっかっかるっかなー♪」
			PRINTW 
		ENDIF
	;虫トラップを破棄した
	CASE 11
		PRINTFORML 「ここの罠、外しちゃう？　おっけー」
		PRINTW 
	;虫を捕まえた
	CASE 12
		PRINTFORML 「お、%MUSHI_NAME%だー」
		PRINTW 
	ENDSELECT
ENDIF
RETURN 1
