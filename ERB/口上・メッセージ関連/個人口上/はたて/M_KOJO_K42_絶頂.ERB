;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精した
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	IF TALENT:恋慕 || TALENT:恋人
	ELSEIF TALENT:愛欲 || TALENT:セフレ
		IF TEQUIP:50 == MASTER
		ELSEIF TEQUIP:51 == MASTER
		ELSE
		ENDIF
	ELSEIF TALENT:思慕
	ELSE
	ENDIF
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「あっ、あああああ～～～～～～～～っ…」
		PRINTFORML （漏らしちゃった…やっちゃいけないのにっ、気持ちいいっ、なんで…っ？）
		PRINTW 
	CASE 1
		PRINTFORML 「あっ、あああああっ、お漏らし、しちゃったぁ…\/\/\/」
		PRINTW 
	CASE 2
		PRINTFORML 「やだっ、とまって%UNICODE(0x2665) *1%　とまってぇ～～%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
		PRINTW 
	ENDSELECT
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
