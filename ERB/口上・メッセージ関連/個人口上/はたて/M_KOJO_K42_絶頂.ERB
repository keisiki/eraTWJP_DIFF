					PRINTW 
				ENDIF
			CASE 2
				;ちんぽねじ込まれて絶頂
				IF TEQUIP:TARGET:口接触部位 == 1
					;口内射精されて絶頂
					IF GETBIT(TFLAG:1,4) || TCVAR:MASTER:12 == 4
