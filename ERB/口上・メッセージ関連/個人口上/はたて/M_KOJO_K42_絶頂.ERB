			CASE 1
				PRINTFORMW 「やだっ、これやだっ、怖いっ、とんじゃうっとんじゃうっ！」
				PRINTW 
		ENDSELECT

		IF NOWEX:Ｃ絶頂
			;ローター
			SIF SELECTCOM == 40 && CFLAG:K42C_はたてと通販 != 2
