;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K42_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K42_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K42_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORML 「おごっ…おっ、ぐっ、んぐっ…！」
			PRINTFORML 「がはっ、ごほっ、げぇっ…」
			IF TALENT:恋慕 || TALENT:恋人
				PRINTFORML 「はぁっ、はぁっ…ねぇ、無理やりはやめてよ…！」
				PRINTFORML 「おクチではしてあげるからさ…」
				PRINTW 
			ELSE
				PRINTFORML 「ちょっと、乱暴は止めてよ…！」
				PRINTW 
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		IF TALENT:恋慕 || TALENT:恋人
			IF ABL:Ｍ感覚 >= 5
				PRINTFORML ぢゅぽっ！　ぶちゅっ！　ぶぼっ！　ぐぽっ！　ごつっ！　ごつっ！
				PRINTFORML 「%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　も゛っ%UNICODE(0x2665) *1%　も゛っ%UNICODE(0x2665) *1%」
				PRINTFORML はたては喉奥をゴツゴツと突かれてなお、幸せそうに肉棒に吸い付いている
				PRINTW 
			ELSEIF ABL:Ｍ感覚 >= 2
				PRINTFORML ぢゅぽっ！　ぶちゅっ！　ぶぼっ！　ぐぽっ！
				PRINTFORML 「お゛っ%UNICODE(0x2665) *1%　ごっ%UNICODE(0x2665) *1%」
				PRINTFORML （乱暴なのにっ、苦しいのにっ、気持ちいいっ%UNICODE(0x2665) *1%　どうしてっ！？）
				PRINTW 
			ELSE
				PRINTFORML ぢゅぽっ！　ぶちゅっ！　ぶぼっ！　ぐぽっ！
				PRINTFORML 「お゛ぼっ！？　お゛っ！？　お゛っ！！　お゛え゛っ！」
				PRINTFORML はたてが太ももを叩いて抗議しているが、無視して喉奥まで突き入れた
				PRINTW 
			ENDIF
			RETURN 1
		ELSEIF TALENT:思慕
			IF ABL:Ｍ感覚 >= 5
				PRINTFORML ぢゅぽっ！　ぶちゅっ！　ぶぼっ！　ぐぽっ！　ごつっ！　ごつっ！
				PRINTFORML 「お゛っ、ぐもっ、　ふーっ%UNICODE(0x2665) *1%　ふーっ%UNICODE(0x2665) *1%」
				PRINTFORML はたては苦痛と快楽がない交ぜになった表情で肉棒に吸い付いている
				PRINTW 
			ELSEIF ABL:Ｍ感覚 >= 2
				PRINTFORML ぢゅぽっ！　ぶちゅっ！　ぶぼっ！　ぐぽっ！
				PRINTFORML 「お゛っ、ぐもっ、んぶっ、んぶっ」
				PRINTFORML はたては苦痛に顔を歪めつつも、わずかに頬を上気させている
				PRINTW 
			ELSE
				PRINTFORML ぢゅぽっ！　ぶちゅっ！　ぶぼっ！　ぐぽっ！
				PRINTFORML 「お゛ぼっ！？　お゛っ！？　お゛っ！！　お゛え゛っ！」
				PRINTFORML はたてが太ももを叩いて抗議しているが、無視して喉奥まで突き入れた
				PRINTW 
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

