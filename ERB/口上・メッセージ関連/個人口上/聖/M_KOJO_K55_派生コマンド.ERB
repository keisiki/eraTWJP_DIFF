	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「ふわぁっ%UNICODE(0x2764)%！んっ……なんですか……今の……」
		PRINTFORML  あなたが白蓮の弱い所を見つけると何度も虐めるように小突く
		PRINTFORML 「そこっ%UNICODE(0x2764)%きもちいぃぃ%UNICODE(0x2764)%だめっあたままっひろにぃぃ%UNICODE(0x2764)%%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回射精した
		IF TCVAR:104
			PRINTFORML 「いまぁ%UNICODE(0x2764)%射精したばっかなのにぃ%UNICODE(0x2764)%」
			PRINTFORML 「そこっいいっ%UNICODE(0x2764)%だめぇ%UNICODE(0x2764)%いじめひゃらめぇ%UNICODE(0x2764)%%UNICODE(0x2764)%」
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		PRINTFORML 「ひゃうぅ%UNICODE(0x2764)%そこしゅきぃぃ%UNICODE(0x2764)%もっろいひめてぇ%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「んっ%UNICODE(0x2764)%そこはよわいからぁ%UNICODE(0x2764)%だめぇっ%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「きもちいとこばっかりぃ%UNICODE(0x2764)%おかしくなっちゃぅ%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
