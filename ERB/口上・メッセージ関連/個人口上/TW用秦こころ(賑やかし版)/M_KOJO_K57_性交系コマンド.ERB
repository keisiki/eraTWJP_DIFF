		PRINTFORMW 
		RETURN 1
	ELSEIF ABL:膣 >= 3
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「なぁ、手を繋いでほしいHPH ん…そう、こうやってると安心するHPH」","L"
				CALL HPH_PRINT,@"「いいよHPH このまま私のナカに、%CALLNAME:MASTER%のおちんちん挿入れて欲しいHPH」","L"
				CALL HPH_PRINT,@"「んHPH ふあぁぁHPH 入って、きたぁHPH HPH」","W"
			CASE 1
				CALL HPH_PRINT,@"「この体位…%CALLNAME:MASTER%と愛し合ってる感じがして、私は好きだ…HPH」","L"
				CALL HPH_PRINT,@"「ん、ふぅぅHPH …はぁHPH こうやって向かい合って、お前を受け入れるの…堪らないHPH」","L"
				CALL HPH_PRINT,@"「さぁ…愛し合おう%CALLNAME:MASTER%HPH お前のおちんちんで、私の膣内を掻き回してHPH HPH」","L"
				PRINTFORMW こころは%CALLNAME:MASTER%の後ろ首に手を回し、甘く囁くようにピストンをねだっている
			CASE 2
				CALL HPH_PRINT,@"「んHPH 私の膣内…すっかりお前の形を覚えてしまったな……」","L"
				CALL HPH_PRINT,@"「不思議な感じだけど…悪くはない…かな…HPH」","L"
				CALL HPH_PRINT,@"「…いいよ、お前の好きに動いてくれHPH」","W"
		ENDSELECT
		RETURN 1
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「くぅ…HPH はぁ…はぁ…入った？」","L"
				PRINTFORMW 「ん…まだちょっと、苦しい…かも」
			CASE 1
				CALL HPH_PRINT,@"「ふうぅHPH …くぅ…未だに、こんなに大きなのが私のナカに入ってるの…信じられない…」","L"
				PRINTFORMW 「おなか…へんなかんじする」
			CASE 2
				PRINTFORML 「ふぅ…ぐ、ぅ……おおきい……」
				PRINTFORMW 「はっ…はっ……まだ、慣れないな…この感覚……」
		ENDSELECT
		RETURN 1
