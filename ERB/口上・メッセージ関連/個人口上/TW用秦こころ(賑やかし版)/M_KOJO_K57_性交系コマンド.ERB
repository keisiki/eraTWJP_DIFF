						CALL HPH_PRINT,@"「あ、んんっHPH まってっHPH おく……あぁぁぁっHPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「…奥、当たって……ッHPH、っん、くぅ……っHPH」","L"
						CALL HPH_PRINT,@"「だめ、なにか…ッHPH へんなの、くるッHPH」","W"
					CASE 2
						CALL KOKORO_SEX
				ENDSELECT
				RETURN 1
			;前回射精した
			ELSEIF TCVAR:102
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「……あんっHPH まだ、するのか？ んHPH 音が…っHPH 恥ずかしい……」","W"
					CASE 1
						CALL HPH_PRINT,@"「まだ、するの？ んHPH あっHPH おく、さっきの熱いのが…HPH くぅHPH」","W"
					CASE 2
						CALL KOKORO_SEX
				ENDSELECT
				RETURN 1
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「はHPH あッHPH だんだん、気持ちよくなってきた…かもHPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「んHPH こうしてるとお前の顔がよく見えるけど…人間って、気持ちいいときはそんな顔をするのね」","W"
					CASE 2
						CALL KOKORO_SEX
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
