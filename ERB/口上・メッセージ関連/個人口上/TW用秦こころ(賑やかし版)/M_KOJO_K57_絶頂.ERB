					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「んHPH お腹の中…%CALLNAME:MASTER%の精液…注がれてる……HPH」","L"
					PRINTFORMW 
				ENDIF
			CASEELSE
				CALL HPH_PRINT,@"「あ、はぁ…HPH ん……HPH ガッチリ固定されて、精液どくどくって……HPH」","L"
				CALL HPH_PRINT,@"「……あぁ……子宮が、お前の子種で満たされていく……HPH」","L"
				PRINTFORML こころはトロトロに蕩けた表情で%CALLNAME:MASTER%の射精を受け入れている
				PRINTFORMW 
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:MASTER:濃厚精液
			IF RAND:3 == 0
				PRINTFORML 「あ、……ナカ……に……こんなに……」
				PRINTFORMW 
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORML 「は……ぁ……これが、精液……」
				PRINTFORMW 
				RETURN 1
			ELSE
				PRINTFORML 「あ……あぁ……これ、すごい……かも……」
				PRINTFORMW 
				RETURN 1
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORML 「あ、はぁ……おなか、あつい……」
				PRINTFORML 「……これが、精液の感覚……」
				PRINTFORMW 
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORML 「は……ぁ、こんな……だめ、なのに……」
				PRINTFORMW 
				RETURN 1
			ELSE
				PRINTFORML 「はっ…はッ…ん……ふ、ぅ……」
				PRINTFORML 「まだ、射精してる……おなか……疼いて……」
				PRINTFORMW 
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
