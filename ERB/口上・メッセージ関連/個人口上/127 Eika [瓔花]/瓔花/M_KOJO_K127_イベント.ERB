;==================================================
;押し倒した
;==================================================
@M_KOJO_EVENT_K127_10(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERを押し倒した（開始時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「も…もうダメ……我慢できないよ……」
		PRINTFORML 「%CALLNAME:MASTER%が悪いんだから……%UNICODE(0x2764)%」
		;妊娠中
		IF TALENT:127:妊娠 && CFLAG:127:妊娠自覚状態 == 1
			PRINTFORML 
			PRINTFORMW 「ほらパパ%UNICODE(0x2764)%お腹の中の赤ちゃんに挨拶しようね%UNICODE(0x2764)%」
		;危険日
		ELSEIF CFLAG:127:生理周期 == 7 && !TALENT:127:妊娠 && CFLAG:127:生解禁 == 2
			PRINTFORML 
			PRINTFORMW 「子供作るって決めたんだし…ちゃんと計画だって立てたし…良いよね？」
			PRINTFORMW 「%CALLNAME:MASTER%の子供……作りたいな%UNICODE(0x2764)%」
		ENDIF
		TCVAR:127:351 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;なだめられた（終了時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE ARG:1
		CASE 0
			;抜け出した
			IF TALENT:恋人 || TALENT:恋慕
				;妊娠中
				IF TALENT:127:妊娠 && CFLAG:127:妊娠自覚状態 == 1
					PRINTFORMW 「うう…よく考えたら確かにお腹の赤ちゃんには負担だよね……」
				;危険日
				ELSEIF CFLAG:127:生理周期 == 7 && !TALENT:127:妊娠 && CFLAG:127:生解禁 == 2
					PRINTFORML 「まだこども出来てないと思うのにー！！」
					PRINTFORMW 「まあ…確かに急ぎ過ぎたとも思うけど…」
				ELSE
					PRINTFORMW 「ううう…ちょっと自制心が足りなかったよ。ごめんね…」
				ENDIF
			ELSE
				PRINTFORMW 「ううう…ちょっと自制心が足りなかったよ。ごめんね…」
			ENDIF
		CASE 1
		;抱き着きモード満足終了
			RETURN 1
		CASE 2
		;抱き着きモード強制終了
			RETURN 1
		ENDSELECT
		TCVAR:127:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;抜け出す失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「お願い…もうちょっと付き合って…」
		PRINTFORMW 「あと少しで崩し切れるから…」
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

