;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K127_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF FIRSTTIME("初告白")
			PRINTFORMW 「ねえ%CALLNAME:MASTER%」
			PRINTFORMW 「私は水子の霊だからあんまりよくわからないんだけど」
			PRINTFORML 「生きている人たちは一緒にいて楽しい人と」
			PRINTFORMW 「恋人っていうのになるんでしょ？」
			PRINTFORMW 「私は%CALLNAME:MASTER%といて楽しいよ？」
			PRINTFORMW 「一緒に積み石したいし…それ以外のことも一緒なら楽しいんだ」
			PRINTFORMW 「これきっと恋なんだと思う」
			PRINTFORML
			PRINTFORMW 「だからもし%CALLNAME:MASTER%が私と一緒にいて楽しいなら」
			PRINTFORMW 「どう…かな…？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「やったー！！」
		PRINTFORMDW 瓔花は両手を挙げて喜んだ
		PRINTFORMW 「これでもっといっぱい楽しい思い出を積めるね」
		PRINTFORMW 「水子だから愛とか恋とか分からないけど」
		PRINTFORMW 「今%CALLNAME:MASTER%に感じてるのがきっとそういうことなんだろうから…」
		IF TALENT:127:81 == 3
			TALENT:127:81 = 0
			CALL COLORMESSAGE(@"瓔花は【人間不信】を失った",C_YELLOW,1)
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「しくしくしく…告白が失敗するなんて…」
		PRINTFORMW 「しょうがない。また一から積み上げていくしかないよね」
		PRINTFORMW 「%CALLNAME:MASTER%……これからも宜しくね」
		RETURN 1
	ENDIF
ENDIF
RETURN 1


