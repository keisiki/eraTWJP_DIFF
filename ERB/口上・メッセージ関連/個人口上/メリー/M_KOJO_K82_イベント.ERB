;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K82_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		;初回
		IF FIRSTTIME(1026)
			PRINTFORMW 「%CALLNAME:MASTER%、話があるの」
			PRINTFORMW メリーはいつになく緊張した口調で話しかけてきた。
			PRINTFORMW 「私ね、少し不安だったの」
			PRINTFORMW 「知らない世界で、いえ、知っている世界だったとしても」
			PRINTFORMW 「蓮子と一緒とはいえ分からないことばかりだらけの世界は確かに楽しかったわ」
			PRINTFORMW 「でも%CALLNAME:MASTER%に会えたことが、私にとって一番の幸せだったのかもしれないの」
			PRINTFORMW そう言ってメリーは%CALLNAME:MASTER%の眼を深く見つめ
			PRINTFORMW 
			PRINTFORMW 「―――%CALLNAME:MASTER%、貴方のことが好きです」
			PRINTFORMW 
			PRINTFORMW 「だから、私と恋人になってもらえませんか？」
			PRINTFORMW 少し怯えながらそう告げた。
		;二回目の告白
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%、女の子に何度も告白させるものじゃないわ」
			PRINTFORMW メリーは少しおどけながら言葉を紡いでいった。
			PRINTFORMW 「ねえ、私と付き合ってはくれないの？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「……うん、私を受け入れてくれるのね、%CALLNAME:MASTER%」
		PRINTFORMW 「これからもずっと私と一緒にいてね、%CALLNAME:MASTER%」
		PRINTFORMW 「ああ、でも浮気は駄目よ、私だけを見ていればいいんだから」
		PRINTFORMW そう言いながら彼女は身を翻し、そっと%CALLNAME:MASTER%に口づけをした。
		PRINTFORMW 「誓いの口づけよ、これでもう裏切れないわね♪」
		PRINTFORMW 満面の笑みで彼女は嬉しそうにそう言った。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「……これでも勇気出したんですけど」
		PRINTFORMW メリーはそう言いながら少しむくれている。
		PRINTFORMW 「いいわ、いつか後悔させてあげるんだから」
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K82_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORMW 「……帰るわ、じゃあね」
			PRINTFORMW メリーは怒りながら帰っていった。
		ELSEIF TALENT:恋人
			PRINTFORMW 「帰るわ、また明日会いましょうね♪」
			PRINTFORMW メリーは名残惜し気に別れて行った。
		ELSEIF TALENT:恋慕
			PRINTFORMW 「そろそろお開きね、楽しかったわ」
		ELSEIF TALENT:思慕
			PRINTFORMW 「じゃあ、また今度、会いましょうね」
		ELSE
			PRINTFORMW 「それじゃ、また誘ってちょうだい」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

