;==================================================
;310,尻を撫でる
;TFLAG:193(1=不快(TALENT:臆病なら涙目) 2&&3=恥ずかしがる(TALENT:男嫌い兼MASTERが男なら、身をすくめる) 4=されるがまま)
;CFLAG:うふふ==2&&TCVAR:20(パンツ無し顔面騎乗(70=クンニ強制 71=フェラチオ強制 72=アナル奉仕) 35=スカート内見せつけ 52=素股)
;CFLAG:うふふ==2&&TEQUIP:シックスナイン(パンツありTCVAR:20(70～72)はこちら)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_SUCCESS_COM_K122_310
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K122_310
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K122_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K122_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
	ELSE
		IF TALENT:狂気
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「うわ、いきなり触らないでよ。気持ちわるー」
					PRINTFORMW 舞は不快そうに%CALLNAME:MASTER%の手を払った。
				;恥ずかしがる
				CASE 2,3
					PRINTFORML 「もー！イヤらしい手つきで触ってー%UNICODE(0x2764)%」
					PRINTFORMW 舞は珍しくはにかんだ笑みを浮かべている。
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「ぷぷぷー！君もその辺のスケベ親父と同レベルだねー」
						PRINTFORMW 舞はニヤニヤしながら、なじるように%CALLNAME:MASTER%を見つめている。
					CASE 1
						PRINTFORML 「あははっ！%CALLNAME:MASTER%ってば痴漢みたい！」
						PRINTFORMW 舞はけらけらと乾いた笑い声をあげている。
					ENDSELECT
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「ひゃあっ！？い、いきなりやめてよ！」
					PRINTFORML 「もう……そういうの、痴漢っていうんだよ？」
					PRINTFORMW 舞は頬を膨らませて%CALLNAME:MASTER%に抗議している。
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「うぅぅ……は、恥ずかしいからやめてよぉ……」
						CASEELSE
							PRINTFORMW 「%CALLNAME:MASTER%のヘンタイ……」
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「えへへ…僕のお尻、どう？」
						CASEELSE
							PRINTFORML 「%CALLNAME:MASTER%って、お尻が好きなの？」
							PRINTFORMW 「好きなら……もっと触っても、大丈夫だよ？」
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「ひぃぃっ！！へ、へんたいーっ！」
					PRINTFORMW 舞は%CALLNAME:MASTER%の手を振り払った。
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「や、やめてよ………」
							PRINTFORMW 舞は居心地悪げに身じろぎしている。
						CASEELSE
							PRINTFORMW 「うぅ…こんなことして、楽しいの……？」
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「触りすぎないでよね……」
							PRINTFORMW 舞はじっとりと%CALLNAME:MASTER%を見つめている。
						CASEELSE
							PRINTFORMW 「なんというか……%CALLNAME:MASTER%って、変態だね……」
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

RETURN 1
