;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K140_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K140_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K140_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「大天狗に手淫して欲しいだなんてね%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「大天狗の手でイかされるなんて贅沢ね%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「こんなに大きくなるの天狗でも中々見ないわね%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「もう出そうなの？いいよ、出しても%UNICODE(0x2665) *1%」
		ENDIF
	;技巧4以上
	ELSEIF ABL:技巧 > 3
		IF RAND:3 == 0
			PRINTFORMW 「少しは手慣れてきたでしょ%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%臭い……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「ふふっ、上手いもんでしょ%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「こう？気持ちよくできてる？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「全く大天狗に手淫させるなんてね」
		ELSE
			PRINTFORMW 「こんなのがいいの？」
		ENDIF
	ENDIF
ENDIF
RETURN 1

