;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K140_80
CALL M_KOJO_MESSAGE_COM_K140_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K140_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
CALL M_KOJO_COLOR_K140
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「大天狗に手淫して欲しいだなんてねHPH」","L"
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「大天狗の手でイかされるなんて贅沢ねHPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「こんなに大きくなるの天狗でも中々見ないわねHPH」","L"
		ELSE
			CALL HPH_PRINT,@"「もう出そうなの？いいよ、出してもHPH」","L"
		ENDIF
	;技巧4以上
	ELSEIF ABL:技巧 > 3
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「少しは手慣れてきたでしょHPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「んっHPH臭い……HPH」","L"
		ELSE
			CALL HPH_PRINT,@"「ふふっ、上手いもんでしょHPH」","L"
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「こう？気持ちよくできてる？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「全く大天狗に手淫させるなんてね」
		ELSE
			PRINTFORMW 「こんなのがいいの？」
		ENDIF
	ENDIF
ENDIF
RETURN 1

