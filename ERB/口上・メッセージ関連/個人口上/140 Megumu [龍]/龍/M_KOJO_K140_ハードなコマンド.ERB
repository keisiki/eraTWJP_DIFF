;==================================================
;150,胸の谷間を犯す
;==================================================
@M_KOJO_MESSAGE_COM_K140_150
CALL M_KOJO_MESSAGE_COM_K140_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K140_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
CALL M_KOJO_COLOR_K140
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「ん……強引だねHPH」","L"
		RETURN 1
	ENDIF
	;基本セット
	;胸3以上
	IF ABL:胸 > 2
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「無理やりするほど私の胸は良かったのかなHPH」","L"
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んんっ……そんなにおっぱい好き？」
		ELSE
			CALL HPH_PRINT,@"「んっHPH強引にされるの好きかも……HPH」","L"
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「そんなに強引にしなくてもおっぱいは逃げないのに……HPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「んっHPHいいよ、おっぱい犯してHPH」","L"
		ELSE
			CALL HPH_PRINT,@"「そんなことしなくても大天狗のおっぱいは君の物だよHPH」","L"
		ENDIF
	ENDIF
ENDIF
RETURN 1
