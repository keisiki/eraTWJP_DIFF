;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K140_140
CALL M_KOJO_MESSAGE_COM_K140_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K140_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
CALL M_KOJO_COLOR_K140
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「んんん～～～！」
		CALL HPH_PRINT,@"「んあっ……もう無理やりなんて……HPH」","L"
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「ん～～～HPH」","L"
			PRINTFORMDW 龍は%CALLNAME:MASTER%の射精を感じ取ると喉で巧みにそれを促している。
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「んHPHんあっHPH」","L"
			PRINTFORMDW 龍は%CALLNAME:MASTER%の射精を感じ取ると喉で巧みにそれを促している。
		ELSE
			CALL HPH_PRINT,@"「んんっHPHんんんHPH」","L"
			PRINTFORMDW 龍は%CALLNAME:MASTER%の射精を感じ取ると喉で巧みにそれを促している。
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん～！」
			PRINTFORMDW 龍は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んん～～～！！！」
			PRINTFORMDW 龍は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ELSE
			PRINTFORMW 「んっ～！！！」
			PRINTFORMDW 龍は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ENDIF
	ENDIF
ENDIF
RETURN 1

