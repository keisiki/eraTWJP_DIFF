@M_KOJO_SUCCESS_COM_K140_300
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K140_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます。また、削除した場合は自動で口上色が付きません。
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K140_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K140_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
CALL M_KOJO_COLOR_K140
IF LOCAL
	;怒り
	IF CFLAG:318 == 1
			PRINTFORMW 「気安いな、俗物め！」
	;会話不能
	ELSEIF TCVAR:302
		IF RAND:5 == 0
			PRINTFORMW 「もう少し話の種を持っているべきだな」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「そう萎縮せずもっと気軽の喋って良いぞ」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「どうした？緊張してるのかな？」
		ELSEIF RAND:2 == 0
			PRINTFORMW （ふむ……やはり典の方がおしゃべりだな……）
		ELSE
			PRINTFORMW （天狗と人間では話も合わないものなのかな）
		ENDIF
	;ぐでぐで
	ELSEIF TALENT:恋慕 && BASE:酒気 > (MAXBASE:酒気 / 10) * 8
		IF RAND:5 == 0
			PRINTFORMW 「%CALLNAME:MASTER%はさぁ……もっと私に甘えるべきだと思うの」
			PRINTFORMW 龍は酔っ払いながら絡んでくる。
		ELSEIF RAND:4 == 0
			PRINTFORMW 「ねえ%CALLNAME:MASTER%になら何されてもいいよ……%UNICODE(0x2665) *1%
			PRINTFORMW 龍は酔いながら絡んでくる。
		ELSEIF RAND:3 == 0
			PRINTFORMW 「古来酒は魔を避けるもの、私が蟒蛇じゃないことは証明されてしまったな%UNICODE(0x2665) *1%」
			PRINTFORMW 龍は顔を上気させながら話しかけてくる。
			PRINTFORMW 「それとも%CALLNAME:MASTER%と居るから酔ったのかな……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「%CALLNAME:MASTER%……呼んでみただけよ」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%……」
			PRINTFORMW 「全く読みにくい名前で素敵だな♪%CALLNAME:MASTER%！」
		ENDIF
	;酔いどれ
	ELSEIF TALENT:恋慕 && BASE:酒気 > (MAXBASE:酒気 / 10) * 5
		IF RAND:5 == 0
			PRINTFORMW 「ふふふ……私が人間と吞んでこんな気分になるとはね」
			PRINTFORMW 「酔っているのよ、君と呑むことにね♪」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「%CALLNAME:MASTER%は天狗になりたいと思ったことはある？」
			PRINTFORMW 龍は酔いながら絡んでくる。
			PRINTFORMW 「調子に乗るという意味ではないが……私ならかなえられるよ」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「……どうかした？」
			PRINTFORMW 「吞んでいるところをあまり見ないで欲しい……恥ずかしいから」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「%CALLNAME:MASTER%……いい名前ね%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「そんなに見つめるな、興奮してしまう%UNICODE(0x2665) *1%」
		ENDIF
	;ほろ酔い
	ELSEIF TALENT:恋慕 && BASE:酒気 >= 1
		IF RAND:5 == 0
			PRINTFORMW 「人間と飲む経験はあまりないが……いいものだな」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「少々酔っているが気にすることはないよ」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「ん……少し酔ったな」
			PRINTFORMW 「いざという時には介抱してよ？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「酔いが回ると何をするかわからないからな……覚悟してよ？」
		ELSE
			PRINTFORMW 「君も飲める口か？天狗社会は飲めない者には人権が無いからな」
		ENDIF
	;ぐでぐで
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 8
		IF RAND:5 == 0
			PRINTFORMW 「天狗社会も所詮は秩序の範疇、例えば君にでも……」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「所詮自由主義経済も放っておけば破綻するのだからな……」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「全くどいつもこいつも馬鹿しかいない……あるいは盛りの付いた猿か……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「典は可愛いよな、羨ましい」
		ELSE
			PRINTFORMW 「少し飲み過ぎた……随分飲んだな……」
		ENDIF
	;酔いどれ
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 5
		IF RAND:5 == 0
			PRINTFORMW 「……んっ……」
			PRINTFORMW 「君も飲むといい、私が酌をしてやろうか？」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「こんな美人と一緒にいるのにつれないやつだな君は」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「無礼講といえど礼儀はあった方がいいんだけどね……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「射命丸の奴ももう少し真面目になればなぁ」
		ELSE
			PRINTFORMW 「姫海棠はもう少し落ち着いてくれないと私の頭が持たん……」
		ENDIF
	;ほろ酔い
	ELSEIF BASE:酒気 >= 1
		IF RAND:5 == 0
			PRINTFORMW 「ふむふむ……酒は嫌いかな？」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「あまり羽目を外すなよ？どこで天狗が見ているかもわからんからな」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「山の神の酒も色々あるが、守矢神社のあれは……詮索はやめておくか」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「外の世界の酒というのも飲んでみたいね」
		ELSE
			PRINTFORMW 「酒造の一つでも作れば天狗社会に革命をもたらせるかな？」
		ENDIF
	;風呂場
	ELSEIF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
		IF RAND:3 == 0
			PRINTFORMW 「ん……いい湯だな%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「烏の行水？馬鹿言え、君と逢引きするのに体を洗わないといけないだろう？」
		ELSE
			PRINTFORMW 「こうして人と湯につかるのも悪くはないな！」
		ENDIF
	ELSE
		SELECTCASE RAND:4
		CASE 0
			IF DAY:2 == 1
				IF RAND:3 == 0
					PRINTFORMW 「春は皆陽気になって財布の紐も緩む、となればビジネスチャンスだな」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「桜の美しい季節だな、皆で花見をするか！」
				ELSE
					PRINTFORMW 「こういう季節こそ、桜で一杯やるのが風流だな」
				ENDIF
			ELSEIF DAY:2 == 2
				IF RAND:3 == 0
					PRINTFORMW 「こんなに暑いと開放的になってしまう……皆で水泳大会でもするか！」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「夏場は暑くないのかって？これでも風通しの良い素材なんだ、それに下は薄着にしてあるからな」
				ELSE
					PRINTFORMW 「水浴びの気持ちいい季節だ、典と一緒に渓流にでも出かけるかな」
				ENDIF
			ELSEIF DAY:2 == 3
				IF RAND:3 == 0
					PRINTFORMW 「天狗と言えば秋だが君は私のことを考えてくれたのかな？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「紅葉の秋と言えば、紅葉狩りツアーというのを考えているんだがウケるかな？」
				ELSE
					PRINTFORMW 「秋の味覚も商売になるからな、勝手に山の作物を取るんじゃないぞ？」
				ENDIF
			ELSEIF DAY:2 == 4
				IF RAND:3 == 0
					PRINTFORMW 「寒い季節は皆で鍋でも食べるに限るな！」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「へっくちっ！！！うう……くしゃみが出てしまった……寒暖差が激しいな」
				ELSE
					PRINTFORMW 「鴉天狗の羽毛布団？……考えておくよ」
				ENDIF
			ENDIF
		CASEELSE
			IF TALENT:恋人
				IF RAND:5 == 0
					PRINTFORMW 「君とこうしていると私も人肌を求めていたのだと感じるわね%UNICODE(0x2665) *1%」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「山の神々にも君との仲をからかわれてしまったよ、子はいつになるかなんて失礼だと思わないかな？」
					PRINTFORMW 龍はそう言いながらそっと%CALLNAME:MASTER%の手に触れてきた。
				ELSEIF RAND:3 == 0
					PRINTFORMW 「もっと人間とのビジネスを考えなければな」
					PRINTFORMW 「そうなると君の助力が必要になってくるから、頼りにしているよ」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「君との関係はビジネスじゃないさ、こうして会話しているだけでも楽しいもんだ」
				ELSE
					PRINTFORMW 「今度は山でデートでもしない？」
					PRINTFORMW 「他の天狗に写真を撮られると面倒？気にすることはない、そういう輩は……」
					PRINTFORMW 彼女はそう言いながら三脚でポンポンと肩を叩いている。
				ENDIF
			ELSEIF TALENT:恋慕
				IF RAND:5 == 0
					PRINTFORMW 「君も新聞を書いてみないか？何、面白い物が書けたら私が推してやろう！」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「典も可愛いが、君も中々だぞ？」
					PRINTFORMW 「……天狗らしいこと？そうでもないさ、お世辞でもなんでもないよ」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「レッセフェールも所詮は幻想の産物、誰かの手が入ることを前提に市場を動かすのさ」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「どうあがいても天狗社会も幻想郷も狭いからね、市場を統制するなんて容易い物さ」
				ELSE
					PRINTFORMW 「典はいつも私のために暗躍しているけど、君にもちょっかいをかけるかもしれないわね」
					PRINTFORMW 「私が君と懇意にしているなら……どうだろうか？」
				ENDIF
			ELSEIF TALENT:思慕
				IF RAND:5 == 0
					PRINTFORMW 「天狗装束を着てほしい？」
					PRINTFORMW 「……考えておくよ」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「最近は忙しいし肩が凝って仕方がない……あまり変なところを見るなよ？」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「天狗社会は縦社会、気苦労が絶えないから君と会話するのが私の癒しになるのよね」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「天狗の卵？……なるほどしかしそんなに大きくはないよ」
				ELSE
					PRINTFORMW 「典はよく働いてくれるんだがね、私も平気で前線に出なければならないことをするから疲れてしまうんだ」
				ENDIF
			ELSEIF CFLAG:2 >= 500
				IF RAND:5 == 0
					PRINTFORMW 「守矢神社は山に活気をもたらしてくれるいい商売相手だよ」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「山の風紀が乱れている？まあその方が活気づくからね、それでも粛清は必要になるかな？」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「人里でも色々ビジネスをしている者がいるからね、参考になるわ」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「人間の市場は色々参考になるよ、私たち天狗や妖怪は寿命が長いからどうしても保守的になりがちだからね」
				ELSE
					PRINTFORMW 「千亦はすぐ市場を開きたがるからね、我々にとっても好都合さ」
				ENDIF
			ELSE
				IF RAND:5 == 0
					PRINTFORMW 「どうかしたかな？」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「ふむ、君と話すのは意外と楽しいな！」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「人間も色々いるが……君は一際奇妙だな」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「あまり私と話していると紙面を飾ることになるよ？」
				ELSE
					PRINTFORMW 「たまには人間と話すのも楽しいものだな」
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
