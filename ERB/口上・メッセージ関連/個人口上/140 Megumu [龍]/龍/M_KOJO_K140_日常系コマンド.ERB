			CASE 3
				PRINTFORMW 「ん……盛ったのね……%UNICODE(0x2665) *1%」
			CASE 4
				PRINTFORMW 「大天狗にもそういう気分はあるんだよ%UNICODE(0x2665) *1%」
			CASE 5
			CASE 6
				PRINTFORMW 「ん……嫌じゃないけど……まだ覚悟が……！」
			CASE 7
				PRINTFORMW 「あまりそういうのは好きではないな」
			CASE 8
				PRINTFORMW 「大天狗を抱こうとは愚か者め！」
		ENDSELECT
	ELSE
		IF TALENT:恋人
			IF RAND:3 == 0
				PRINTFORMW 「よしよしいっぱい私が癒してあげよう%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「どうした？随分甘えてくるじゃないか%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「君に求められているというだけで興奮してしまうよ%UNICODE(0x2665) *1%」
			ENDIF
		ELSEIF TALENT:恋慕
			IF RAND:3 == 0
				PRINTFORMW 「君にこんなことをされては抵抗もできないじゃないか%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「こらこらあまりせっかちだとこっちも興奮してしまうぞ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「ん？若い雄は力強くてよろしい%UNICODE(0x2665) *1%」
			ENDIF
		ELSEIF TALENT:思慕
			IF RAND:3 == 0
				PRINTFORMW 「烏は押し倒されると抵抗できないんだぞ……%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「ん……優しくしてね%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「君になら何されてもいいよ……%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「君も男の子だね%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「そういう目で見られていたというのは悪くないな」
			ELSE
				PRINTFORMW 「ん、どうした？嫌なことでもあったのかな？」
			ENDIF
		ENDIF
	ENDIF

ENDIF
RETURN 1
