;==================================================
;144,放尿
;==================================================
@M_KOJO_MESSAGE_COM_K9_144
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K9_144_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K9_144_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「いいっ！？お、おおおおおおしっ……こ……が、み、見たいですって！？」
		PRINTFORML 「ちょ、ちょっとそれはさすがにどうかと思うの……」
		PRINTFORMDW 露骨にがっかりして見せると、夢美は動揺しはじめた
		PRINTFORML 「そ、そんなにがっかりしなくても……だって、その、は、排泄行為よ？み、見せるようなものじゃないでしょ？」
		PRINTFORMDW 見たかったなー。見たいなー。誰か見せてくれないかなー
		PRINTFORML 「ああ、もう、分かったわよ！見せてあげるわよ！だからそんなみっともなく落ち込まないでよ！」
		PRINTFORML 「もう、ほんと変態なんだから……」
		;洋式トイレかそうでないかでMSG分岐するよ
		; WC(14, 40, 236, 316, 322, 406, 418, 824, 906, 917)
		;洋式そうなところ(40,316,322,406,418,824,906,917)
		IF CFLAG:MASTER == 40 || CFLAG:MASTER == 316 || CFLAG:MASTER == 322 || CFLAG:MASTER == 406 || CFLAG:MASTER == 418 || CFLAG:MASTER == 824 || CFLAG:MASTER == 906 || CFLAG:MASTER == 917
			PRINTFORMDW 夢美は放尿の準備を済ませると、便器に腰を下ろした
			PRINTFORMDW 対面した状態で眺める%CALLNAME:MASTER%の目には、夢美の股間がしっかりと焼き付いた
		ELSE
			PRINTFORMDW 夢美はしゃがみ込んで放尿の準備をし始めた……
			PRINTFORMDW 後ろから眺めればひくひくと動く菊穴が、前から眺めれば夢美の股間が、%CALLNAME:MASTER%の目に飛び込んでくる……
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORML 「そ、それじゃあ、出すわね……\/\/\/\/」
		PRINTFORMDW 頬を染めながら夢美は放尿を始めた。
		PRINTFORML 「んんっ……あっ%CALLNAME:MASTER%見てっ、おしっこ出る、でるぅっ……\/\/\/」
		PRINTFORMDW 赤面した夢美の尿道口からおしっこがビューッと勢いよく出てくる。
		PRINTFORML 「ふぁぁ……%UNICODE(0x2764)%」

		;トイレなら水に当たってじょぼじょぼ音が出るからね
		IF CFLAG:MASTER == 14 || CFLAG:MASTER == 40 || CFLAG:MASTER == 236 || CFLAG:MASTER == 316 || CFLAG:MASTER == 322 || CFLAG:MASTER == 406 || CFLAG:MASTER == 418 || CFLAG:MASTER == 824 || CFLAG:MASTER == 906 || CFLAG:MASTER == 917
			PRINTFORMDW 便器の中に溜まった水におしっこが当たり、じょぼじょぼという音が響く。
		ELSE
			PRINTFORMDW 放たれたおしっこは、水たまりを作りながらびちびちと音を立てている。
		ENDIF

		PRINTFORML 「やぁ……そんなにじっくり見ないでよ……\/\/\/」
		PRINTFORMDW やがて排尿が終わると、夢美は熱い吐息を吐いて、ぶるりと震えた。
		PRINTFORMDW よく見ると陰毛にも飛び散っているようで、少し尿で濡れているのが分かる
		PRINTFORML 「んんっ……はぁ……%UNICODE(0x2764)%」

		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORML 「じゃあ……その、するわね……\/\/\/」
		PRINTFORML 「んん……んっ……」
		PRINTFORMDW 夢美の尿道口からおしっこがビューッと勢いよく出てくる。

		;トイレなら水に当たってじょぼじょぼ音が出るからね
		IF CFLAG:MASTER == 14 || CFLAG:MASTER == 40 || CFLAG:MASTER == 236 || CFLAG:MASTER == 316 || CFLAG:MASTER == 322 || CFLAG:MASTER == 406 || CFLAG:MASTER == 418 || CFLAG:MASTER == 824 || CFLAG:MASTER == 906 || CFLAG:MASTER == 917
			PRINTFORMDW 便器の中に溜まった水におしっこが当たり、じょぼじょぼという音が響く。
		ELSE
			PRINTFORMDW 放たれたおしっこは、水たまりを作りながらびちびちと音を立てている。
		ENDIF

		PRINTFORML 「やぁ……見られてる、人におしっこするとこ見られてるぅ……\/\/\/」
		PRINTFORMDW やがて排尿が終わると、夢美はぶるりと震えた。
		PRINTFORMDW よく見ると陰毛にも飛び散っているようで、少し尿で濡れているのが分かる
		PRINTFORML 「んっ……ふぅ……\/\/\/」

		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「……んっ」
		PRINTFORMDW 夢美の尿道口からおしっこがビューッと勢いよく出てくる。

		;トイレなら水に当たってじょぼじょぼ音が出るからね
		IF CFLAG:MASTER == 14 || CFLAG:MASTER == 40 || CFLAG:MASTER == 236 || CFLAG:MASTER == 316 || CFLAG:MASTER == 322 || CFLAG:MASTER == 406 || CFLAG:MASTER == 418 || CFLAG:MASTER == 824 || CFLAG:MASTER == 906 || CFLAG:MASTER == 917
			PRINTFORMDW 便器の中に溜まった水におしっこが当たり、じょぼじょぼという音が響く。
		ELSE
			PRINTFORMDW 放たれたおしっこは、水たまりを作りながらびちびちと音を立てている。
		ENDIF

		PRINTFORMW 「そんなまじまじと見るんじゃないわよ……」
		PRINTFORMDW やがて排尿が終わると、夢美はぶるりと震えた。
		PRINTFORMDW よく見ると陰毛にも飛び散っているようで、少し尿で濡れているのが分かる
		PRINTFORML 「んんっ……ふぅ」

		RETURN 1
	ENDIF

	;この見せたがりめ！
	IF ABL:14 > 5
		PRINTFORML 「ねえ%CALLNAME:MASTER%……見られながらおしっこするの、癖になっちゃいそう……\/\/\/」
		PRINTFORMDW 「変態」と言うと夢美は恍惚の表情を浮かべてぶるりと震えた。
		PRINTFORML 「やだぁ言わないで……\/\/\/」
	ENDIF

ENDIF
RETURN 1

