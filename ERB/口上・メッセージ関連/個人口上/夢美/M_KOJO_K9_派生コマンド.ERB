;==================================================
; 子宮口関数
;==================================================
@M_KOJO_MESSAGE_SIKYUKO_K9()
;ポルチオ性感持ち(EXP:57 > 50)か,綻び状態(EXP:57 > 25)か、それ以外か
IF TALENT:78 == 1
	SELECTCASE RAND:4
		CASE 0
			CALL HPH_PRINT,@"「ああっそこっ、そこぉっHPH　そこされるのすきぃっHPH」","W"
		CASE 1
			CALL HPH_PRINT,@"「しきゅうのぉっ、いりぐち、もっと、コンコンしてぇっHPH」","W"
		CASE 2
			CALL HPH_PRINT,@"「あうっHPH　そこっ、わたしのしきゅう、ノックされてっHPH　よろこんでるっHPH」","W"
		CASE 3
			CALL HPH_PRINT,@"「あひっHPH　ばかになるっHPH、しきゅう、こづかれ、てぇっHPH」","W"
	ENDSELECT
ELSEIF EXP:57 < 50 && EXP:57 > 25
	CALL HPH_PRINT,@"「あはぁっ、あぁっ！？　うそ、や、な、なんか、きもちぃ、よくぅっHPH」","W"
	CALL HPH_PRINT,@"「だめだめっ、それいじょうされたら……ばかにぃ、なっ、ちゃうッHPH」","W"
ELSE
	PRINTFORML 「ぁぐっ……っあ……そ、そんなところ……あっ、痛っ……ぃ、じゃない……」
ENDIF


