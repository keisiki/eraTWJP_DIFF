;==================================================
;6,挿入Ｇスポ責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K9_6
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K9_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K9_6_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「……どうしたの？終わり……だったりする？」
		PRINTFORMDW %CALLNAME:MASTER%の動きが止まったことを夢美が訝しむ。
		PRINTFORMDW もちろんこんなもので終わりではない、%CALLNAME:MASTER%は少し腰の角度を変え、
		PRINTFORMDW 夢美の膣内にある"そこ"を狙い澄まして抽送を再開した。
		CALL HPH_PRINT,@"「んんぅーーー～～～ッHPH！？　っぁ、やめっ、それっ、すご、いぃ……っHPH！？」","W"
		PRINTFORMDW 夢美は初めての快感に大きく体を震わせた。
		RETURN 1
	ENDIF

	;基本セット
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;前回射精した
		IF TCVAR:102
			;恋慕
			IF TALENT:恋慕
				CALL HPH_PRINT,@"「あっ、いいっ、もっとぬりぬりしてぇっHPH」","W"
				CALL HPH_PRINT,@"「わたしがっ、%CALLNAME:MASTER%のっ、オンナだって、からだじゅうっ、きざみこんでぇっHPH」","W"
				RETURN 1
			;愛欲Orセフレ
			ELSEIF TALENT:愛欲 || TALENT:セフレ
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あはっHPH　いいっ、そこっ、もっとぬりぬりしてぇHPH」","W"
						CALL HPH_PRINT,@"「これっ、%CALLNAME:MASTER%専用のっ、あな、だって、いわれてる、みたい、でぇHPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「やだっHPH　そんなところっ、マー、キング……しなくて、もぉっHPH」","W"
						CALL HPH_PRINT,@"「わたしのっ、おまんこ、はぁ、どこにもっ、い、イかない、わ、よぉッHPH」","W"
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				CALL HPH_PRINT,@"「んぅっ、%CALLNAME:MASTER%ってば、あひっ、いま、出したばっか、じゃ、ぁッHPH」","W"
				PRINTFORMDW 夢美の膣壁に精液を塗り込むように、%CALLNAME:MASTER%は腰の動きを変えていく
				CALL HPH_PRINT,@"「んぁあっ、ちょっ…どこ、に、あうっ、塗っ……てぇっHPH」","W"
				RETURN 1
			ENDIF
		ELSE
			IF TALENT:3
				CALL HPH_PRINT,@"「んひぃっ、すきっ、だいすきっHPH　そこっ、こすられるのスキっHPH」","W"
				CALL HPH_PRINT,@"「もっとぉっ、%CALLNAME:MASTER%っ、もっとシてぇっHPH」","W"
				RETURN 1
			ELSEIF TALENT:184 || TALENT:185
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あはっHPH　いいっ、そこっ、もっとこすこすしてぇHPH」","W"
						CALL HPH_PRINT,@"「これっ、よすぎてぇっHPH　やみつき、にっ、なっちゃい、そぉHPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「やだっHPH　そこだめだからぁHPH　すぐ、イっちゃう、からぁHPH」","W"
						PRINTFORMDW そうか、と一旦腰の動きを止めた%CALLNAME:MASTER%に、すぐさま夢美は本音をぶちまける
						CALL HPH_PRINT,@"「うそHPH　うそだからぁHPH　ソレやめないでイかせてぇっHPH」","W"
				ENDSELECT
				RETURN 1
			ELSE
				PRINTFORML 「あはぁっ！、そこ、感じすぎ、ちゃう、からっ！？\/\/\/」
				CALL HPH_PRINT,@"「らめぇ、そこ、ばっかりぃっ、され、る、とぉ、すぐ、い、いく、からっ、ぁHPH」","W"
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	
	;時姦中
	;IF FLAG:70
	;	PRINTFORMW 
	;	RETURN 1
	;恋慕
	;ELSEIF TALENT:恋慕
	;	PRINTFORMW 
	;	RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	;ELSE
	;	PRINTFORMW 
	;	RETURN 1
	;ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K9_{SELECTCOM}_1
RETURN 1

