		ELSEIF TCVAR:102
			;子作りOKしているか(CFLAG:14==2),危険日以外OKか(CFLAG:14==1),ピル服用済か(TCVAR:109),それ以外
			IF CFLAG:14 == 2
				CALL HPH_PRINT,@"「きてきてっHPH　もっと奥まで塗り込んで、かき回してぇっHPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
				IF CFLAG:900 == 7 && CFLAG:902 == 0
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%のせーえきでぇっ、ゆめみをはらませてぇっHPH」","W"
				ENDIF
			ELSEIF CFLAG:14 == 1
				CALL HPH_PRINT,@"「あはぁっHPH　もっとおくに、熱いの入ってくるぅっHPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSEIF TCVAR:109
				CALL HPH_PRINT,@"「あぁっ、んぅっ、奥まで入ってきてるぅ……HPH　わたしのナカ、いっぱいにされてる……\/\/\/」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSE
				CALL HPH_PRINT,@"「ぅくっ……ちょっ、いきなりナカに出すとか無遠慮っ、す、ぎぃっ……！？HPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ENDIF
			RETURN 1
		ELSE
			;子作りOKしているか(CFLAG:14==2),危険日以外OKか(CFLAG:14==1),ピル服用済か(TCVAR:109),それ以外
			IF CFLAG:14 == 2
				CALL HPH_PRINT,@"「ふぁぁぁっHPH　%CALLNAME:MASTER%のがきてるHPH　しきゅうまでとどいてるっHPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSEIF CFLAG:14 == 1
				CALL HPH_PRINT,@"「あぁっ、いいわよっHPH　きょうは、だいじょうぶっ……な、ひ、だからぁっHPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSEIF TCVAR:109 == 1
				CALL HPH_PRINT,@"「んあぁっ、ちょっそこはっ、いくらっ、ピル、のんでる、からっ、てぇっ？！HPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSE
				CALL HPH_PRINT,@"「ぅあっ、ちょっとぉっ、%CALLNAME:MASTER%っどこ、つい、てぇっHPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;時姦中
	;IF FLAG:70
	;	PRINTFORMW 
	;	RETURN 1
	;恋慕
	;ELSEIF TALENT:恋慕
	;	PRINTFORMW 
	;	RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	;ELSE
	;	PRINTFORMW 
	;	RETURN 1
	;ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K9_{SELECTCOM}_1
