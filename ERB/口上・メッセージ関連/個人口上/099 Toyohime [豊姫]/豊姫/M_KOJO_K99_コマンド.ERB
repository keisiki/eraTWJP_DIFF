@M_KOJO_MESSAGE_COM_K99_180_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「そんな入口ばっかり、切ないわ……HPH」","L"
				CALL HPH_PRINT,@"「塗ってよ……ナカも……HPH」","L"
				SIF !CFLAG:99:1998
					PRINTFORMDW %CALLNAME:TARGET%が陰唇を割り開く。そこはもうローションを塗る必要がない程しとどに濡れていた
			CASE 1
				CALL HPH_PRINT,@"「ふふっHPH シてあげましょうか……？HPH」","L"
				CALL HPH_PRINT,@"「こうやって……お風呂でするみたいに……HPH」","L"
				SIF !CFLAG:99:1998
					PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%に抱き付くと、ローション塗れの躰を擦り付けてきた……
			CASE 2
				CALL HPH_PRINT,@"「ヌルヌル、癖になってきたかも……HPH」","L"
				IF !CFLAG:99:1998
					PRINTFORMDL %CALLNAME:TARGET%はリラックスした様子で%CALLNAME:MASTER%に身を任せている
					PRINTFORMDW %CALLNAME:MASTER%はボトル一本丸ごと使って、%CALLNAME:TARGET%の全身に満遍なくローションを塗り付けた
				ENDIF
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「あんっ♪ ヌルヌルにされちゃった……♪」
				IF !CFLAG:99:1998
					PRINTFORMDL ローションを塗られた%CALLNAME:TARGET%の肌が妖しく濡れ光る
					PRINTFORMDW 煽情的な光景に、%CALLNAME:MASTER%はすぐにでも%CALLNAME:TARGET%を抱きたい衝動に駆られた
				ENDIF
			CASE 1
				PRINTFORML 「私だけじゃなくて、貴方のも……♪」
				IF !CFLAG:99:1998
					PRINTFORMDL %CALLNAME:TARGET%は手の平にローションをたっぷりと垂らすと、
					PRINTFORMDW 楽しそうに%CALLNAME:MASTER%の躰にローションを塗り付けた……
				ENDIF
			CASE 2
				PRINTFORML 「ねぇ……%CALLNAME:MASTER%……」
				PRINTFORML 「ここも、塗って……？」
				SIF !CFLAG:99:1998
					PRINTFORMDW %CALLNAME:TARGET%が脚を開く。%CALLNAME:MASTER%は頷くと、その淫らな秘裂にたっぷりとローションを塗り付けた
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

