@M_KOJO_MESSAGE_COM_K99_605_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;天候で分岐
	SELECTCASE TIME:5
		;晴れ
		CASE 0,1
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「少し寄り道？ ふふっ、いいわよ」
					PRINTFORML 「天気も良いし、お散歩には丁度良いから」
					PRINTFORMDW 二人は少しだけ遠回りをして風景を楽しんだ
				CASE 1
					PRINTFORML 「何か面白いものでも見つけた？」
					PRINTFORMDL 急にルートを外れた%CALLNAME:MASTER%に
					PRINTFORMDW %CALLNAME:TARGET%は興味津々といった様子で付いて行った
				CASE 2
					PRINTFORML 「こう、ポカポカした日は……」
					PRINTFORML 「ふぁ……なんだか眠くなるのよねぇ……」
					PRINTFORML 「……あそこに丁度良い木陰もあるし、少し休憩していかない？」
					PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%に付き合って木陰で休憩することにした……
					;１時間経過
					TIME += 60
			ENDSELECT
		;曇り・霧
		CASE 2,3,7
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「急に降り出さないと良いけど……」
					PRINTFORMDL %CALLNAME:TARGET%は天気を気にしている
					PRINTFORMDW 早めに寄り道を切り上げた方が良さそうだ
				CASE 1
					PRINTFORML 「ここ？ 良い景色が見れるという場所は」
					PRINTFORML 「……んー、何も見えないわね」
					PRINTFORMW 「また今度天気の良い日に来ましょう」
				CASE 2
					PRINTFORML 「……あら？」
					PRINTFORMDL %CALLNAME:TARGET%が不意に空を見上げる
					PRINTFORMDL %CALLNAME:MASTER%も釣られて空を見上げると
					PRINTFORMDW 顔にポツリ、ポツリと雨粒が落ちてきた
					PRINTFORMW 「うーん、急いだ方がよさそうね。傘の用意は良い？」
					;雨になる
					TIME:5 = 4
			ENDSELECT
		;雨・雪
		CASE 4,5,6,8,9,10,11,12,13
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「こんな天気なのに寄り道？」
					PRINTFORML 「うーん……止めた方が良いと思うけど」
					PRINTFORMDW そう言いながらも%CALLNAME:TARGET%は付いて来てくれた
				CASE 1
					PRINTFORML 「はぁ……服が濡れちゃった……」
					PRINTFORMDL %CALLNAME:MASTER%と%CALLNAME:TARGET%は朽ち果てた小屋の軒下で休憩している
					PRINTFORML 「……へ……へ……へくしゅっ」
					PRINTFORML 「ぐすっ……風邪を引かないと良いけど……」
					PRINTFORMDW %CALLNAME:TARGET%には悪いことをしたかもしれない……
					;３０分経過
					TIME += 30
				CASE 2
					PRINTFORMDL 寄り道をしている間に天候が悪化してきたので偶然見つけた洞窟で休憩していると……
					PRINTFORML 「あ、%CALLNAME:MASTER%。見て」
					PRINTFORML 「晴れてきたわ」
					PRINTFORMDL %CALLNAME:MASTER%が洞窟から出てみると
					PRINTFORMDL 先程までの天候が嘘のように晴れ、空に七色の虹が掛かっていた
					PRINTFORML 「ふふっ、晴れてくれてよかったわ」
					PRINTFORMW 「それじゃあ、デートの続きをしましょうか♪」
					;晴れて虹になる
					TIME:5 = 0
					TIME:7 = 1
					;１時間経過
					TIME += 60
			ENDSELECT
	ENDSELECT
ENDIF
RESETCOLOR
RETURN 1

