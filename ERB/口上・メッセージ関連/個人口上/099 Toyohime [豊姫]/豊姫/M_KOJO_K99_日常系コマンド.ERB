IF LOCAL
	;豊姫とデート中限定
	IF FLAG:デート相手 == 99
		PRINTFORML 
		SELECTCASE CFLAG:MASTER:現在位置
			CASE 連れ込み宿
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「……ふふっ」
						PRINTFORML 「そうね。少し休憩していきましょうか」
						PRINTFORMDW %CALLNAME:MASTER%と%CALLNAME:TARGET%は腕を組みながら連れ込み宿へと入った……
					CASE 1
						PRINTFORML 「この部屋みたいね」
						PRINTFORMDL 鍵に書かれた番号と照らし合わせる。確かにこの部屋のようだ
						PRINTFORMDW 鍵を開け部屋に入ると、簡素な部屋に布団が敷かれていた……
					CASE 2
						PRINTFORML 「ああ、%CALLNAME:MASTER%。待って」
						PRINTFORML 「ここは私に任せて頂戴」
						PRINTFORMDW %CALLNAME:TARGET%が代わりに料金を支払ってくれた
						MONEY += 3000
				ENDSELECT
			CASE ラブホ
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「%CALLNAME:MASTER%……疲れちゃったわ……」
						PRINTFORML 「ねぇ、ここで少し休んでいきましょう……？」
						PRINTFORMDL %CALLNAME:TARGET%がわざとらしく密着し目の前の建物を指差す
						PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%の肩を抱くと、他のカップルの後に続いて建物の中へと入っていった……
					CASE 1
						PRINTFORML 「シャワーもあるのね。先に浴びてこようかしら」
						PRINTFORML 「それとも……一緒に入りたい？」
						PRINTFORMDW %CALLNAME:MASTER%が『待ってる』と言うと、%CALLNAME:TARGET%は少し残念そうな顔をした
					CASE 2
						PRINTFORML 「色々なものが売ってるのねぇ……」
						PRINTFORML 「ねぇ%CALLNAME:MASTER%、これ使ってみましょう？」
						PRINTFORMDW いつの間に買ったのだろう。%CALLNAME:TARGET%は手にローターを持っていた
						CALL GET_ITEM("ローター", 1, 2)
				ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
