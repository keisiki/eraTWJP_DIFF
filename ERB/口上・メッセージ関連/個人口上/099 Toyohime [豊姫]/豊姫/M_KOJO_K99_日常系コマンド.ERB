#DIM 正解
#DIM 経験値
#DIM 制限時間
#DIM 倍率
#DIM FEINT
#DIM イカサマ
VARSET LOCAL
イカサマ = 0
制限時間 = 2000 - (ABL:99:戦闘能力 - ABL:MASTER:戦闘能力) * 100

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特訓デイリーからお借りしました
	PRINTFORML 「玉琢かざれば器を成さず。人学ばざれば道を知らず」
	PRINTFORML 「宝石も才能も、磨かなければ光り輝くことはないということ……」
	PRINTFORMDL %CALLNAME:TARGET%が手に持った扇子を%CALLNAME:MASTER%へと突き付ける
	PRINTFORML 「さあ、持てる力を全て出し切って」
	PRINTFORMW 「死ぬ気で掛かってきなさい。地上の人間らしくね」
	RESETCOLOR
	PRINTFORML 
	CALL ASK_YN("稽古開始","やっぱり止める")
	IF RESULT
		CALL M_KOJO_COLOR_K99
		PRINTFORML 「あら？ やめるの？」
		PRINTFORML 「それなら一緒にお茶でもしましょうか」
		PRINTFORMDW 先程までの気迫が嘘のように消え、%CALLNAME:TARGET%はすっかりいつもの調子に戻っていた
		RESETCOLOR
	ELSE
		正解 = RAND:4
		経験値 = MIN(ABL:99:戦闘能力 * 5, 5)
		SELECTCASE RAND:20
			CASE IS <= 11
			;普通に
				LOCALS:0 = 上
				LOCALS:1 = 右
				LOCALS:2 = 左
				LOCALS:3 = 下
				倍率 = 100
			CASE 12 TO 15
			;ローマ字
				LOCALS:0 = UE
				LOCALS:1 = MIGI
				LOCALS:2 = HIDARI
				LOCALS:3 = SHITA
				倍率 = 160
			;英語
			CASE 16,17,18
				LOCALS:0 = UP
				LOCALS:1 = RIGHT
				LOCALS:2 = LEFT
				LOCALS:3 = DOWN
				倍率 = 120
			;スペイン語
			CASE 19
				LOCALS:0 = encima de
				LOCALS:1 = a la derecha
				LOCALS:2 = a la izquierda
				LOCALS:3 = abajo de
				倍率 = 300
		ENDSELECT
		;あなたの戦闘能力が高いと５分の一の確率でフェイントをかけてくる
		IF ABL:MASTER:戦闘能力 >= 5 && !RAND:5
			FEINT = 1
			制限時間 += 400
			倍率 += 80
		ELSE
			FEINT = 0
		ENDIF
		CALL FISHER_YATES_SHAFFLE(4)
		PRINTFORMW %CALLNAME:TARGET%の攻撃を避けよう（クリックで開始）
		PRINTFORM %CALLNAME:TARGET%の
		SETCOLOR C_YELLOW
		IF FEINT
			PRINTFORM %LOCALS:(RAND:4)%
			RESETCOLOR
			PRINTFORM と見せかけて%LOCALS:(3 - 正解)%
		ELSE
			PRINTFORM %LOCALS:(3 - 正解)%
			RESETCOLOR
		ENDIF
		PRINTFORML からの攻撃！
		$LOOP_IKASAMA
		FOR LOCAL:10,0,4
			LOCAL = SHAFFLE_ARRAY:(LOCAL:10)
			PRINTFORML [{LOCAL}] - %LOCALS:LOCAL%に避ける
		NEXT
		SIF !イカサマ && BASE:MASTER:TSP > 500
			PRINTFORML [{4}] - 時間停止
		TINPUT 制限時間, -1, 1
		IF RESULT == 正解
			PRINTFORML %CALLNAME:MASTER%は攻撃を回避した！
			CALL M_KOJO_COLOR_K99
			IF !イカサマ
				PRINTFORMDL ……………
				PRINTFORMDL ………
				PRINTFORMDW …
				PRINTFORML 「ふぅ……こんなところかしら」
				PRINTFORMDL すっかり息の上がった%CALLNAME:MASTER%に対して、%CALLNAME:TARGET%は平然としている
				PRINTFORML 「さ、疲れたでしょう？」
				PRINTFORMW 「稽古はこのくらいにして、一休みしましょうか」
			ELSE
				PRINTFORML 「ぁ、きゃっ！？」
				PRINTFORMDL %CALLNAME:MASTER%は時間を止めて攻撃を回避すると隙を突いて%CALLNAME:TARGET%を押し倒した
				PRINTFORML 「はぁ……負けちゃった。強いのねぇ」
				PRINTFORML （それにしても、この能力……どこかで……）
				PRINTFORMDW 衣服に付いた汚れを払う%CALLNAME:TARGET%は訝し気に%CALLNAME:MASTER%を見ていた
			ENDIF
			RESETCOLOR
			BASE:MASTER:体力 -= 100 * (1 + ABL:99:戦闘能力)
			BASE:MASTER:気力 -= 100 * (1 + ABL:99:戦闘能力)
			経験値 = 経験値 * 倍率 / 100
			EXP:MASTER:戦闘経験 += 経験値
			EXP:99:戦闘経験 += 経験値 * 2
			CFLAG:99:好感度 += 経験値 * 2
			CFLAG:99:信頼度 += 経験値 * 2
			PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:MASTER%）
			PRINTFORML 戦闘経験 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			PRINTFORML 好感度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			PRINTFORML 信頼度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
		ELSEIF RESULT == 4 && !イカサマ && BASE:MASTER:TSP > 500
			イカサマ = 1
			倍率 /= 5
			PRINTFORMW %CALLNAME:MASTER%は時間停止能力で攻撃を見極めた！
			GOTO LOOP_IKASAMA
		ELSE
			CALL M_KOJO_COLOR_K99
			PRINTFORMDL 失敗した……
			PRINTFORML 「大丈夫ー？ 怪我してないー？」
			PRINTFORMDL 派手に吹き飛ばされた%CALLNAME:MASTER%の元に%CALLNAME:TARGET%が歩いてくる
			PRINTFORML 「ん……大した怪我は無さそうね」
			PRINTFORML 「どう？ 起きられそう？」
			PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%に手を貸してもらいながら、ふらつく躰をなんとか起こした……
			RESETCOLOR
			BASE:MASTER:体力 -= 500 * (1 + ABL:99:戦闘能力)
			BASE:MASTER:気力 -= 500 * (1 + ABL:99:戦闘能力)
			PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:TARGET%）
			EXP:99:戦闘経験 += 経験値
			CFLAG:99:好感度 += 経験値 / 2
			CFLAG:99:信頼度 += 経験値 / 2
			PRINTFORML 好感度 ＋{経験値}（%CALLNAME:TARGET%）
			PRINTFORML 信頼度 ＋{経験値}（%CALLNAME:TARGET%）
		ENDIF
	ENDIF
ENDIF
RETURN 1
