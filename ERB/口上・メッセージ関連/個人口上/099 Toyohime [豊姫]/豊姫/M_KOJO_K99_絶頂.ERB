					CALL HPH_PRINT,@"「赤ちゃん、デキるといいわね……HPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%はまるでそこに我が子がいるかのように、
					PRINTFORMDW 精液で満たされた子宮の辺りを優しく撫でている……
				CASE 1
					CALL HPH_PRINT,@"「待って、このままでいて……HPH」","L"
					CALL HPH_PRINT,@"「今抜いたら、貴方のがこぼれちゃう……HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%は本気で%CALLNAME:MASTER%の子を孕むつもりのようだ……
				CASE 2
					CALL HPH_PRINT,@"「はぁ……ふぅ……HPH」","L"
					CALL HPH_PRINT,@"「……凄かった……HPH」","L"
					PRINTFORMDW 蕩けるようなセックスに、%CALLNAME:TARGET%は大満足の様子だ……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「はぁぁ……貴方の、熱い……HPH」","L"
					CALL HPH_PRINT,@"「私のナカ……沁み込んでくる……HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%の精を子宮に受けながら、心の底から幸せそうに息を吐いた……
				CASE 1
					CALL HPH_PRINT,@"「――気持ち良かったか……？HPH」","L"
					CALL HPH_PRINT,@"「くすっHPH ええ、貴方と同じくらい……HPH」","L"
					PRINTFORMDW それを証明するかのように、%CALLNAME:TARGET%の膣はキュウキュウと%CALLNAME:MASTER%のペニスを締め付けている……
				CASE 2
					CALL HPH_PRINT,@"「んっHPH まだ足りないみたい……HPH」","L"
					CALL HPH_PRINT,@"「貴方も……私も……HPH」","L"
					PRINTFORMDW 未だ硬度を保ったままのペニスを、%CALLNAME:TARGET%はキュッと締め付けた……
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;妊娠中
		IF TALENT:妊娠
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「もう……射精しすぎよ……HPH」","L"
					CALL HPH_PRINT,@"「赤ちゃん、溺れちゃうじゃない……HPH」","L"
					PRINTFORMDW 既に子が宿っているにも関わらず、%CALLNAME:TARGET%の子宮は完全に%CALLNAME:MASTER%の精液で満たされた……
				CASE 1
					CALL HPH_PRINT,@"「たくさん注ぎ込んでくれるのは嬉しいけど……HPH」","L"
					CALL HPH_PRINT,@"「――ん、撫でてあげて……HPH」","L"
					PRINTFORMDW ビックリさせてしまったかもしれない。%CALLNAME:MASTER%は優しく%CALLNAME:TARGET%のお腹を撫でた
				CASE 2
					CALL HPH_PRINT,@"「んっHPH 赤ちゃん、ビックリしちゃったみたい……HPH」","L"
					PRINTFORMDL 少し激しくしすぎたか。%CALLNAME:MASTER%は『ごめん』と謝りながら%CALLNAME:TARGET%のお腹を撫でた
					CALL HPH_PRINT,@"「ふぅ……HPH 大丈夫よ、続けましょ……HPH」","W"
			ENDSELECT
		;危険日生解禁済
		ELSEIF CFLAG:生解禁 >= 2
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「凄い……まだ射精てる……HPH」","L"
					CALL HPH_PRINT,@"「そんなに私を妊娠させたいの……？HPH」","L"
					PRINTFORMDW %CALLNAME:MASTER%が頷くと、%CALLNAME:TARGET%の膣がキュッと締まった
				CASE 1
					CALL HPH_PRINT,@"「ぁぁ……HPH 私のナカ、満たされて……HPH」","L"
					CALL HPH_PRINT,@"「子作り……癖になっちゃいそう……HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%と繋がったまま、ウットリと下腹部を撫でている……
				CASE 2
					CALL HPH_PRINT,@"「もっと奥、突き入れて……HPH」","L"
					CALL HPH_PRINT,@"「ちゃんと……赤ちゃんデキるように……HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%は一滴も精液を逃すまいと、キツく膣を締め付けている……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ぁぁ……はぁ……はぁ……HPH」","L"
					PRINTFORMDL 一つの区切りを迎え、二人は心地良い疲労感に身を委ねる
					PRINTFORMDW 汗だくのまま見つめ合い、そして唇を重ねた
				CASE 1
					PRINTFORMDL 全てを注ぎ込んでも、%CALLNAME:MASTER%のペニスはまだ硬度を保っている
					CALL HPH_PRINT,@"「あんっHPH おちんちん、まだ硬い……HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%はキュッと膣を締めると、腰を揺すって次を求めてきた
				CASE 2
					CALL HPH_PRINT,@"「ふぅ……激しいのね……HPH」","L"
					CALL HPH_PRINT,@"「でも……気持ち良かった……HPH」","L"
					PRINTFORMDW 二人は深く繋がったまま、暫し余韻に浸った……
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

