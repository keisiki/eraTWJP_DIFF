							PRINTFORMDW ドクンッドクンッと送り込まれてくる熱に%CALLNAME:TARGET%は吐く息を震わせた
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「んあっHPH はぁぁ……穢れが、私のナカに……っHPH」","L"
						CALL HPH_PRINT,@"「……ふぅ。ふふっ、気持ち良かった……？HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:TARGET%がしなだれかかり、うっとりとした表情で囁く
							PRINTFORMDW %CALLNAME:MASTER%が頷くと、%CALLNAME:TARGET%は嬉しそうに口元を緩めた
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「ぅ、んっHPH そう、そこよ……HPH」","L"
						CALL HPH_PRINT,@"「ほら、ぴゅっぴゅってして……HPH んんっHPH 偉いわ……HPH」","L"
