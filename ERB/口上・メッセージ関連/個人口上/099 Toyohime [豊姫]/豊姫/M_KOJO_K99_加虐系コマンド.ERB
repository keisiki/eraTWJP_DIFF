								ENDIF
							CASE 1
								CALL HPH_PRINT,@"「これ……きっと痣になっちゃうわ……HPH」","L"
								CALL HPH_PRINT,@"「ああ……依姫になんて説明したら……HPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL 欲望の為に自ら望んで縄に縛られたと説明した時、愛する妹から受ける冷めた視線……
									PRINTFORMDL 軽蔑されるだろうか。罵倒されるだろうか。口も聞いてくれなくなるだろうか
									PRINTFORMDW どちらに転んでも、今の%CALLNAME:TARGET%にとってはご褒美でしかなかった
								ENDIF
							CASE 2
								CALL HPH_PRINT,@"「これも、一つの愛のカタチなのね……HPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL %CALLNAME:MASTER%に縛られ、転がされた%CALLNAME:TARGET%は
									PRINTFORMDW 何かを期待するようにこちらを見つめている……
								ENDIF
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「誰かが縛られるのを見るのは好きだけど……」
								PRINTFORML 「自分が縛られるのは……ちょっと……」
								IF !CFLAG:99:1998
									PRINTFORMDL 縛られる側の気持ちを知ってもらおう
									PRINTFORMDW %CALLNAME:MASTER%は気持ちキツめに%CALLNAME:TARGET%を縄で縛り上げた
								ENDIF
							CASE 1
								PRINTFORML 「ぁ、くっ……縄が、食い込んで……っ」
								IF !CFLAG:99:1998
									PRINTFORMDL %CALLNAME:TARGET%が動けないようにしっかりと拘束し、座らせる
									PRINTFORMDW 縛られ慣れていないのか、躰は強張っていた
								ENDIF
							CASE 2
								PRINTFORML 「も、もうちょっと緩くできない……？」
								PRINTFORML 「その、跡になっちゃうから……」
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%はあまり気乗りしていないようだ……
						ENDSELECT
				ENDSELECT
			ENDIF
		;解いた時
		ELSE
			;マゾっ気で分岐
			SELECTCASE ABL:マゾっ気
				;マゾっ気２以上
				CASE IS > 1
					CALL HPH_PRINT,@"「次は貴方のことも縛ってあげましょうか……？HPH」","L"
					CALL HPH_PRINT,@"「ほら……遠慮しないで……HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW %CALLNAME:TARGET%は純粋な善意で%CALLNAME:MASTER%を縛ろうとしている。丁重にお断りした
				;マゾっ気２未満
				CASEELSE
					PRINTFORML 「はぁ……この跡、消えるかしら……」
					SIF !CFLAG:99:1998
						PRINTFORMDW %CALLNAME:TARGET%は肌についた縄の跡を手で摩っている
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

