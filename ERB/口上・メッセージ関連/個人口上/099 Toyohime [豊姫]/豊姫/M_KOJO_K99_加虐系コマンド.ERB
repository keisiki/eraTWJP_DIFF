;==================================================
;104,アイマスク
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K99_104
RETURN 0

@M_KOJO_MESSAGE_COM_K99_104
CALL M_KOJO_MESSAGE_COM_K99_104_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_104_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMDL 
		RETURN 1
	;通常
	ELSE
		;装着
		IF TEQUIP:アイマスク
			;恋慕
			IF TALENT:恋慕
				;マゾっ気で分岐
				SELECTCASE ABL:マゾっ気
					;マゾっ気２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「あ……待って、言わないで……HPH」","L"
								CALL HPH_PRINT,@"「何をされるか分からない方がワクワクするから……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW 自発的にアイマスクを装着した%CALLNAME:TARGET%は興奮した様子で次に行われる行為を待っている……
							CASE 1
								CALL HPH_PRINT,@"「これ……何も見えなくて……HPH」","L"
								CALL HPH_PRINT,@"「凄く……興奮しちゃう……HPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL %CALLNAME:TARGET%は何をされるか分からない恐怖に息を荒げている
									PRINTFORMDL %CALLNAME:MASTER%が耳元でわざと大きな音を立てて脅かしてやると
									PRINTFORMDW %CALLNAME:TARGET%はビクビクッと躰を跳ねさせ、股から興奮の蜜を滴らせた……
								ENDIF
							CASE 2
								SIF !CFLAG:99:1998
									PRINTFORMDL %CALLNAME:MASTER%はアイマスクを装着した%CALLNAME:TARGET%を抱き締めると、彼女の首筋をねっとりと舐め上げた
								CALL HPH_PRINT,@"「ひゃんっHPH ん……ふぅ……っHPH」","L"
								CALL HPH_PRINT,@"「見えないと……感覚が鋭くなって……HPH」","L"
								CALL HPH_PRINT,@"「はぁぁ……HPH おかしくなっちゃいそう……HPH」","W"
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「私は…貴方の顔を見ながらするほうが好きかな……」
								SIF !CFLAG:99:1998
									PRINTFORMDW アイマスクを装着した%CALLNAME:TARGET%は少しだけ不満そうにしている
							CASE 1
								PRINTFORML 「……ねぇ、やっぱり外しても良い？」
								PRINTFORML 「貴方の顔が見えないのは……少し怖いわ」
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%は怯えてしまっている。その姿が%CALLNAME:MASTER%の嗜虐心を煽った
							CASE 2
								PRINTFORML 「これで良いのよね。%CALLNAME:MASTER%」
								PRINTFORML 「……%CALLNAME:MASTER%？」
								PRINTFORML 「ねぇ、どうして黙っているの？」
								SIF !CFLAG:99:1998
									PRINTFORMDW 暫く黙って様子を見ていたが、%CALLNAME:TARGET%は実にそそる反応をしてくれた
						ENDSELECT
				ENDSELECT
			;それ以外
			ELSE
				;マゾっ気で分岐
				SELECTCASE ABL:マゾっ気
					;マゾっ気２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「アイマスクを外して、もし貴方が居なくなってたら……HPH」","L"
								CALL HPH_PRINT,@"「……ああっHPH それって凄く興奮しちゃう……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%は妄想を膨らませて股を濡らしている。本当に放置してやろうか……
							CASE 1
								CALL HPH_PRINT,@"「さて、次は何をされるのかしら……HPH」","L"
								CALL HPH_PRINT,@"「うふふっHPH ワクワクするわ……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%は次に行われる行為に期待を膨らませている……
							CASE 2
								CALL HPH_PRINT,@"「ん……ちょっと待っててね……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:MASTER%がアイマスクを付けるように指示すると、%CALLNAME:TARGET%は素直に従った
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「これを付ければ良いのね？」
								PRINTFORML 「……ん、真っ暗で何も見えないわ」
								PRINTFORMW 「ねぇ、まだそこに居る？」
							CASE 1
								PRINTFORML 「ふふっ、真っ暗ね。幼い頃に依姫と探検した日のことを思い出すわ」
								PRINTFORML 「真っ暗な部屋に閉じ込められて……あの子、わんわん泣いてたっけ」
								SIF !CFLAG:99:1998
									PRINTFORMDW 思い出話も程々に、%CALLNAME:MASTER%は行為を再開した
							CASE 2
								PRINTFORML 「これを付けると、眠くなっちゃいそう……」
								PRINTFORML 「ふぁ……もし寝てしまったら起こしてね……」
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%は既に眠そうにしている
						ENDSELECT
				ENDSELECT
			ENDIF
		;解除
		ELSE
			;マゾっ気で分岐
			SELECTCASE ABL:マゾっ気
				;マゾっ気２以上
				CASE IS > 1
					CALL HPH_PRINT,@"「顔を見ながらするのも良いけど……HPH」","L"
					CALL HPH_PRINT,@"「偶には、こういうのも悪くないわね……HPH」","W"
				;マゾっ気２未満
				CASEELSE
					PRINTFORML 「んっ、眩しい……」
					SIF !CFLAG:99:1998
						PRINTFORMDW %CALLNAME:TARGET%は目をシパシパと瞬かせている
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

