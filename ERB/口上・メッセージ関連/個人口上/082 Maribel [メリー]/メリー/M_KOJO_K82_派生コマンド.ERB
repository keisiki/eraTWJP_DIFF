;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K82_1
CALL M_KOJO_MESSAGE_SCOM_K82_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K82_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「お互いに舐めあうなんて道化芝居よ」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%」
				PRINTFORMW メリーは%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「もっと舐めてっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん%UNICODE(0x2665) *1%あっ%UNICODE(0x2665) *1%ザーメン臭っ%UNICODE(0x2665) *1%」
				PRINTFORMW メリーは%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%ん～っ%UNICODE(0x2665) *1%」
				PRINTFORMW メリーは%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「とても濃いカウパーの臭い……好き%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「ん？」
				PRINTFORMW メリーは%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「もう出ちゃいそうなの%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……カウパー溢れてる……%UNICODE(0x2665) *1%」
				PRINTFORMW メリーは%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ駄目っ%UNICODE(0x2665) *1%」
				PRINTFORMW メリーは%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「ん……%UNICODE(0x2665) *1%臭くて好き……%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「慣れたもんでしょ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「あっ%UNICODE(0x2665) *1%れろっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「んん～」
				PRINTFORMW 「上手いでしょ、舌使い%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んっ……どうかしら%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んんっ……もっと舐めてっ%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん……んんっ……%UNICODE(0x2665) *1%」
			PRINTFORMW 「下手なのはお互い様ね♪」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……んっ……んんっ……」
		ELSE
			PRINTFORMW 「……んっ……ん～」
		ENDIF
	ENDIF
ENDIF
