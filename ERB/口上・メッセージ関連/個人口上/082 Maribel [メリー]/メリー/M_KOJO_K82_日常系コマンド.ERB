@M_KOJO_MESSAGE_COM_K82_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怒り
	IF CFLAG:318 == 1
		PRINTFORMW 「……煩い」
		PRINTFORMW メリーは怒気をはらんだ声で返事した。
	;会話不能
	ELSEIF TCVAR:302
		PRINTFORMW 「……そうね、そうかもしれないわ」
		PRINTFORMW メリーは押し黙ってしまった。
	;ミニマップ時のみ
	;条件をメインマップ時にできなくもないけど面倒
	;現在位置が墓
	ELSEIF CFLAG:MASTER:現在位置 == 150
		IF RAND:5 == 0
			PRINTFORMW 「なんて罰当たりなのかしら……こっちの話よ」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「ここでも桜が見えるといいんだけどね」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「変なものが見えちゃいそう、気味悪いわ」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あまり長居するとこじゃないわ、行きましょう」
		ELSE
			PRINTFORMW 「このお寺、キマイラがいるんだっけ？」
		ENDIF
	;現在位置が竹林入り口
	ELSEIF CFLAG:MASTER:現在位置 == 410
		IF RAND:5 == 0
			PRINTFORMW 「夢で見たわ」
			PRINTFORMW 「……本当に夢だったのかしら」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「何だか怖いわ、護ってくれるかしら？%CALLNAME:MASTER%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「夢の中だとしても、化け物からは逃げないといけないの」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「筍は地中の物が旬なのよ、知ってた？」
		ELSE
			PRINTFORMW 「全身火だるまの女の子って知ってる？見たことあるのよ、そうここで」
		ENDIF
	;現在位置が彼岸
	ELSEIF CFLAG:MASTER:現在位置 == 640
		IF RAND:5 == 0
			PRINTFORMW 「彼岸花って気持ち悪いよね」
			PRINTFORMW 「私子供のころからあの花嫌いなのよ」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「私は―――多分地獄に行きそうね」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「蓮子は天国に行けるのかしら？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「閻魔様には会いたくないわね……褒められるようなことはしてないもの」
		ELSE
			PRINTFORMW 「渡ってしまったら2度と帰ることができないのよね」
		ENDIF
	;風呂場
	ELSEIF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
		IF RAND:3 == 0
			PRINTFORMW 「極楽ね、あんまり気持ちいいと茹っちゃいそう」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「こういう所、誰にも見られたくはないわ、ゆっくりしていきましょう」
		ELSE
			PRINTFORMW 「……折角混浴してるんだから、もっと近づいてもいいわよ」
		ENDIF
	ELSE
		SELECTCASE RAND:4
		CASE 0
			IF DAY:2 == 1
				IF RAND:3 == 0
					PRINTFORMW 「麗らかな陽気ね、桜が見たいわ」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「ねえ知ってる？桜の下には死体が埋まってるのよ？」
				ELSE
					PRINTFORMW 「春の陽気に当てられて踊ってしまいそうだわ」
				ENDIF
			ELSEIF DAY:2 == 2
				IF RAND:3 == 0
					PRINTFORMW 「やっぱり夏は暑いわね……もっと脱いだ方がいいかも……」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「海に行きたいけど生憎とここには三途の川しかないらしいわ」
				ELSE
					PRINTFORMW 「汗びっしょりよ……あまり見ないでね」
				ENDIF
			ELSEIF DAY:2 == 3
				IF RAND:3 == 0
					PRINTFORMW 「紅葉が綺麗ね、神様のお陰かしら？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「美味しい物の多い季節だわ！何か食べに行きましょう！」
				ELSE
					PRINTFORMW 「ふんふん、いい匂いがするわね、天然物のお芋の匂いかしら？」
				ENDIF
			ELSEIF DAY:2 == 4
				IF RAND:3 == 0
					PRINTFORMW 「寒いのは苦手なのよね」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「白銀の銀世界とまではいかなくても幻想的ね」
				ELSE
					PRINTFORMW 「雪だるまでも作ってみましょうか？」
				ENDIF
			ENDIF
		CASEELSE
			IF TALENT:恋人
				IF RAND:5 == 0
					PRINTFORMW 「%CALLNAME:MASTER%の色が、私にも移っちゃったみたいね」
					PRINTFORMW 他人から見れば汚れかもしれないけど―――
				ELSEIF RAND:4 == 0
					PRINTFORMW 「%CALLNAME:MASTER%と一緒にいると落ち着くの、なぜかしら」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「この目で見える物が怖いこともあったけど、%CALLNAME:MASTER%がいるのなら怖くないわ」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「%CALLNAME:MASTER%は死なないわよね？死んで別れるなんて嫌よ私」
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%がいる場所が、私のサナトリウムね」
				ENDIF
			ELSEIF TALENT:恋慕
				IF RAND:5 == 0
					PRINTFORMW 「情報が神を殺すのなら、ここは神々の世界ね、情報で神が作られるんだもの」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「%CALLNAME:MASTER%は気軽に結界を踏み越えられるのね、羨ましいわ」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「%CALLNAME:MASTER%は夢の中でも私を見つけてくれるかしら？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「アガサクリスQ？ペンネームね、どんな人なのかしら？」
				ELSE
					PRINTFORMW 「私の目に見える物が全て真実なら紛れもなく貴方の存在も真実よ」
				ENDIF
			ELSEIF TALENT:思慕
				IF RAND:5 == 0
					PRINTFORMW 「うん、次の同人誌の内容どうしようかなって」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「見ているものが違ってもコミュニケーションは成り立つのよ？私と貴方でそうなようにね」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「性質の違う量子世界を行き渡ることができても排除されてしまうのよね」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「ここはどこもかしこも妖怪だらけ、私の方が異物なのかしら？」
				ELSE
					PRINTFORMW 「蓮子は元気だからいいけど、私はバテちゃうわ」
				ENDIF
			ELSEIF CFLAG:2 >= 500
				IF RAND:5 == 0
					PRINTFORMW 「他者との境界は他者と関わり続けることで曖昧になってくるのよ」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「ここではキマイラに襲われる心配も無いわね……あるの？」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「ここは色々なものが見えるけど、気分が悪くならないわね」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「夢の中と現実とどちらが本物なのかしら？」
				ELSE
					PRINTFORMW 「こちらの世界だと旧型酒が普通なのよね」
				ENDIF
			ELSE
				IF RAND:5 == 0
					PRINTFORMW 「思えば霊能サークルなのにそれを証明できなければ不良サークルって失礼よね」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「本当は結界を暴くのは禁止されているんだけどね、見える物は仕方ないじゃない？」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「華奢に見える？これでも墓石を回すぐらいはできるのよ？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「霊障があると気分が悪くなるよね……」
				ELSE
					PRINTFORMW 「美味しいお酒の店知らない？」
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
