;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K130_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K130_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K130_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「ん……私のも舐めなさい%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「ん～～～%UNICODE(0x2665) *1%」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「ほら、早く雄汁を飲ませなさい%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……%UNICODE(0x2665) *1%……早く出してください%UNICODE(0x2665) *1%」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「臭くて濃くていい味ですよ……%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「ん～もう出そうなのですか？とんだ雑魚ちんぽですね」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……んっ」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ……れろっ」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「ん……%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665) *1%」
				PRINTFORMW 「……早く飲ませなさい……%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「臭くてたまらない味ですね%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「ん～%UNICODE(0x2665) *1%れろっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「ん～」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW （こうして雄の性器を舐めていると自分が雌なのを感じる……）
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……ちゅっ」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ……」
				PRINTFORMW 八千慧は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん……ちょっとちんぽ動かさないでくださいよ……咥えられません」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……れろっ……慣れてませんよ？」
		ELSE
			PRINTFORMW 「……んん……臭い……」
			PRINTFORMW 八千慧はどこかぎこちなく陰茎を口に含んだ。
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K130_{SELECTCOM}_1
RETURN 1

