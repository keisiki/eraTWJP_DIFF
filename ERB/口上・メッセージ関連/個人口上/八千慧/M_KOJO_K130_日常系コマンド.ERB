@M_KOJO_SUCCESS_COM_K130_300
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K130_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます。また、削除した場合は自動で口上色が付きません。
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K130_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K130_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怒り
	IF CFLAG:318 == 1
		PRINTFORMW 「うるさいですね、黙ってもらえませんかね」
	;会話不能
	ELSEIF TCVAR:302
		IF RAND:5 == 0
			PRINTFORMW （……何かしゃべってもらえませんかね）
		ELSEIF RAND:4 == 0
			PRINTFORMW （……気まずいなぁ……）
		ELSEIF RAND:3 == 0
			PRINTFORMW （……話題がないですね）
		ELSEIF RAND:2 == 0
			PRINTFORMW （……間が空きましたね）
		ELSE
			PRINTFORMW （何か……話すことは……）
		ENDIF
	;ぐでぐで
	ELSEIF TALENT:恋慕 && BASE:酒気 > (MAXBASE:酒気 / 10) * 8
		IF RAND:5 == 0
			PRINTFORMW 「%CALLNAME:MASTER%～」
			PRINTFORMW 八千慧は酔っ払いながら絡んでくる。
		ELSEIF RAND:4 == 0
			PRINTFORMW 「ねえ%CALLNAME:MASTER%……」
			PRINTFORMW 八千慧は酔いながら絡んでくる。
			PRINTFORMW 「……何でもないですよ」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「%CALLNAME:MASTER%」
			PRINTFORMW 八千慧は顔を上気させながら呼びかけてくる。
			PRINTFORMW 「呼んだだけですよ……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「%CALLNAME:MASTER%～どこ見てるんですかー？」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%……」
		ENDIF
	;酔いどれ
	ELSEIF TALENT:恋慕 && BASE:酒気 > (MAXBASE:酒気 / 10) * 5
		IF RAND:5 == 0
			PRINTFORMW 「%CALLNAME:MASTER%……」
			PRINTFORMW 「……酔いつぶれたら介抱してくださいね」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「%CALLNAME:MASTER%」
			PRINTFORMW 八千慧は酔いながら絡んでくる。
			PRINTFORMW 「……呼んだだけですよ？勘違いしないでくださいね」
			
		ELSEIF RAND:3 == 0
			PRINTFORMW 「少し良い過ぎましたね」
			PRINTFORMW 「悪くはありませんが」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「ん……」
			PRINTFORMW 「%CALLNAME:MASTER%は呑んでますか？」
		ENDIF
	;ほろ酔い
	ELSEIF TALENT:恋慕 && BASE:酒気 >= 1
		IF RAND:5 == 0
			PRINTFORMW 「んん……♪」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「……%CALLNAME:MASTER%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「ん……ん……」
			PRINTFORMW 「まだ酔っていませんよ？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んん？何ですかもう……」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%、こういうのも楽しいですね」
		ENDIF
	;ぐでぐで
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 8
		IF RAND:5 == 0
			PRINTFORMW 「どいつもこいつも馬鹿ばかり……」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「驪駒も碌なことをしませんね」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「なんだか酔ってきました……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「少し飲み過ぎましたかね……」
		ELSE
			PRINTFORMW 「……頭痛い」
		ENDIF
	;酔いどれ
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 5
		IF RAND:5 == 0
			PRINTFORMW 「……んっ……」
			PRINTFORMW 「酔ってきた……」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「……」
			PRINTFORMW 「お水……頂戴……」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「全くあの神様気取りも面倒なことを……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……亀じゃないですよ……」
		ELSE
			PRINTFORMW 「……少し酔ってきましたかね」
		ENDIF
	;ほろ酔い
	ELSEIF BASE:酒気 >= 1
		IF RAND:5 == 0
			PRINTFORMW 「……いい気分ですね♪」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「～♪」
			PRINTFORMW 八千慧は鼻歌を小さく歌っている。
		ELSEIF RAND:3 == 0
			PRINTFORMW 「少し気分がよくなってきました」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「こういう時、変なことを考えてしまうんですよね」
		ELSE
			PRINTFORMW 「まだまだ飲めますよ？」
		ENDIF
	;風呂場
	ELSEIF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
		IF RAND:3 == 0
			PRINTFORMW 「湯浴みも好きですが、泳ぐのも好きですよ」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「湯船に浸かっていると何もかも忘れられるんですよ……だから風呂は好きなんです」
		ELSE
			PRINTFORMW 「ああ……いつまでも浸かっていたいですね……」
		ENDIF
	ELSEIF TALENT:恋人
		IF RAND:5 == 0
			PRINTFORMW 「我々が神を望んでも何も得られないのに、どうして人間が望めば神が降りるのでしょう？」
			PRINTFORMW 「理不尽ですよね、まあどうでもいい話ですけど」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「%CALLNAME:MASTER%は私と話をするのは楽しいですか？」
			PRINTFORMW 「私は楽しいですね、%CALLNAME:MASTER%と話をできるのが」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「%CALLNAME:MASTER%のことは首輪を着けたいぐらい好きですよ？」
			PRINTFORMW 「……私もそういうことをされたいってところは否定できませんね」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「人間も案外面白いですね、こうやってコミュニケーションを取るというのはあまり経験がないもので」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%が居れば我々が天下を取るのも容易いことですね」
			PRINTFORMW 「……他言無用ですよ？人に言えば……殺します%UNICODE(0x2665) *1%」
			PRINTFORMW 八千慧はからからと笑いながら冗談ですと後付けした。
		ENDIF
	ELSEIF TALENT:恋慕
		IF RAND:5 == 0
			PRINTFORMW 「人間というのは面白いですね、%CALLNAME:MASTER%が面白いだけかもしれませんが」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「人の話を聴くというのも楽しいですね」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「驪駒も頭が良ければ魅力的なのですがね」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「%CALLNAME:MASTER%は私と驪駒どっちが好きですか？」
			PRINTFORMW 「……聞いただけです、忘れてください」
		ELSE
			PRINTFORMW 「あの埴輪軍団にも困ったものです」
			PRINTFORMW 「いざというときには頼りにしてますよ、%CALLNAME:MASTER%」
		ENDIF
	ELSEIF TALENT:思慕
		IF RAND:5 == 0
			PRINTFORMW 「地上も案外面白い所でしたね」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「%CALLNAME:MASTER%も驪駒のように筋肉だけで生きては駄目ですよ？」
			PRINTFORMW 「頭を使わないと、いいように使われますからね」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「空を飛べるかですって？」
			PRINTFORMW 「……手足を引っ込めて回転しながら飛べますよ」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「数は力です、だから我々は徒党を組んでいるのですよ」
		ELSE
			PRINTFORMW 「……畜生界も悪くはないのですが」
			PRINTFORMW 「%CALLNAME:MASTER%はお気に召しませんでしたかね」
		ENDIF
	ELSEIF CFLAG:2 >= 500
		IF RAND:5 == 0
			PRINTFORMW 「あの巫女も所詮は人間に過ぎませんでしたね」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「あの神様も所詮は人を奴隷にするだけなのにね」
			PRINTFORMW 「なら私に使い潰される方が世のため人のためでしょうに」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「厄介ごとがあるのなら私が解決しますよ？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……%CALLNAME:MASTER%の話はいつだって興味深いですね」
		ELSE
			PRINTFORMW 「地上へ出ても我々には地盤が無いというのにね」
		ENDIF
	ELSE
		IF RAND:5 == 0
			PRINTFORMW 「何ですか？」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「面白い話をしますね、%CALLNAME:MASTER%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「勁牙組という連中にそそのかされてはいけませんよ？」
			PRINTFORMW 「あいつらは頭が悪くてすぐ暴力に訴えてろくでもない連中ですから」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「畜生界も案外狭いものですね、地上は広いというのに」
		ELSE
			PRINTFORMW 「何かあればすぐ連絡してくださいね」
		ENDIF
	ENDIF
ENDIF
RETURN 1
