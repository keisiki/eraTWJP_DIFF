;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇共通関数
;==================================================
@REISEN_ENCOUNTER
IF TALENT:恋慕
	IF !RAND:5
		PRINTFORMDL どうやら%CALLNAME:TARGET%はまだこちらに気付いていないようだ
		PRINTFORMDL %CALLNAME:MASTER%は背後からそっと近付くと、%TEXTR("ギュッとレイセンを抱き締めた/うなじに息を吹きかけた/『ワッ』と大きな声で驚かした/")%
		PRINTFORML 「ひゃああっ？！！ あ、%MASTERNAME:100%…？」 
		PRINTFORML 「もう、急に驚かすなんて酷いよっ」 
		PRINTFORMDW %CALLNAME:MASTER%は『ごめんごめん』と謝りながら、次はいつやろうかと考えを巡らせていた
	ELSE
		PRINTFORML 「あ、%MASTERNAME:100%！」
		PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%に気付くと、頬を染め嬉しそうに駆け寄ってきた
		PRINTFORMDL その様があまりにも可愛らしかったので、%CALLNAME:MASTER%はペットにするように優しく頭を撫でてやった
		PRINTFORMW 「あぅ…くすぐったいよぉ…」 
	ENDIF
ELSEIF TALENT:思慕
	SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「あ、%MASTERNAME:100%。元気してます？」
			PRINTFORMDW %CALLNAME:MASTER%を見かけた%CALLNAME:TARGET%が気さくに挨拶してきた
		CASE 1
			PRINTFORML 「%MASTERNAME:100%、来てくれたんですか？」
			PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%を視止めると、パッと顔を輝かせた
			PRINTFORMDL %CALLNAME:MASTER%は、『そんなに会いたかった？』と彼女を揶揄った……
			PRINTFORMW 「そ、そういうわけじゃあないですけど……でも、嬉しいです」 
	ENDSELECT
ELSE
	PRINTFORM 「あ、%MASTERNAME:100% 
	;TIME:2で朝昼晩を選択
	SELECTCASE TIME:2
	;朝
	CASE 1,2
		PRINTFORML おはようございます」
	;昼
	CASE 3,4
		PRINTFORML こんにちは」
	;夜
	CASE 5,6,7
		PRINTFORML こんばんわ」
	ENDSELECT
	PRINTFORMDW %CALLNAME:TARGET%はこちらに気付くと、ぺこりとお辞儀をしてきた
ENDIF

;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K100_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	IF !TCVAR:100:今日会ったか
		;-------------------------------------------------
		;小分岐
		;-------------------------------------------------
		;MASTERが入ってきた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 1
			CALL REISEN_ENCOUNTER
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが入ってきた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 2
			CALL REISEN_ENCOUNTER
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが風呂に入ってきて一緒に入る
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 3
			IF TALENT:恋慕
				PRINTFORML 「あ、%MASTERNAME:100%。お風呂入ってたの？」
				PRINTFORMDL %CALLNAME:MASTER%が湯舟でくつろいでいると、%CALLNAME:TARGET%が入ってきた
				PRINTFORMDL 『すぐに上がるから』%CALLNAME:MASTER%はそう言い立ち上がろうとするが……
				PRINTFORML 「背中流してあげるね♪」
				PRINTFORMDW 結局、二人で入ることになってしまった
			ELSE
				PRINTFORML 「あ…その……一緒に入っても、いいですか？」
				PRINTFORMDL まさか%CALLNAME:TARGET%がそんなことを言ってくるとは……
				PRINTFORMDW %CALLNAME:MASTER%は意外に思いながらも、彼女の為にスペースを空けた
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが風呂に入ってきて出て行った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 4
			PRINTFORML 「きゃあっ！？す、すみませんっ！」
			PRINTFORMDL %CALLNAME:MASTER%が呼び止めるより早く%CALLNAME:TARGET%は戸を閉めてしまった
			PRINTFORMDW ……誰かが風呂に入っているのに気が付かないとは、よほど疲れていたのだろうか
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが風呂に入ってきて追い出された
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 5
			PRINTFORML 「あ、あの～」
			PRINTFORMDL %CALLNAME:MASTER%が風呂でくつろいでいると、戸の向こうから%CALLNAME:TARGET%の声が聞こえてきた
			PRINTFORML 「後がつかえてるので…その、早めにお願いします」
			PRINTFORMDW どうやら待たせてしまっていたようだ。しょうがないので、%CALLNAME:MASTER%はさっさと風呂から出た
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;外出先で遭遇した
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 6
			CALL REISEN_ENCOUNTER
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとデート中に遭遇した
		;ARG:1にデート相手
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 7
			PRINTFORML 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MAIN_MAP上で一日最初の挨拶
		;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 8
			PRINTFORML 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;外出先で一日最初の挨拶
		;ARG:1にデート相手、ARG:1が0の場合は非デート
		;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 9
			;ARG:1とデート中
			IF ARG:1
			;一人で外出
			ELSE
			ENDIF
			PRINTFORML 
			RETURN 1
		ENDIF
		;その他
		PRINTFORML  
	ENDIF
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K100_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF TALENT:恋慕
				PRINTFORML 「あ、おはよう。もう少ししたら起こそうと思ってたんだけど…はい、お水」
				PRINTFORMDL 目を覚ますと、%CALLNAME:TARGET%は既に身支度を整えていた
				PRINTFORMDW %CALLNAME:MASTER%は渡された水を一気に呷ると、布団から這い出た……
			ELSE
				PRINTFORML 「あ、おはようございます。もう少ししたら起こそうと思っていたんですけど…」
				PRINTFORMDW 目を覚ますと、%CALLNAME:TARGET%は既に身支度を整えていた
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF TALENT:恋慕
				PRINTFORML 「あ、おはよう」
				PRINTFORMDL 目を覚ますと、%CALLNAME:TARGET%と目が合った
				;月在住時の初対面繋がりで。別の場所から月に移住した場合は知らん
				IF GET_MAPID(CFLAG:MASTER:現在位置) == MAIN_MAP && MAIN_MAP == 10
					PRINTFORML 「お着替え、ここに置いておくね」
					PRINTFORMDW 甲斐甲斐しく世話を焼いてくれる彼女を%CALLNAME:MASTER%は有難いなぁと思った
				ELSE
					PRINTFORMDL ……なぜ彼女が自分の部屋にいるのだろうか 
					PRINTFORML 「えっとね、その…%MASTERNAME:100%に会いたくなって、きちゃった」
					PRINTFORMW 「……迷惑…だったかな…？」
				ENDIF
			ELSE
				PRINTFORML 「あ、おはようございます」
				PRINTFORMDL 目を覚ますと、%CALLNAME:TARGET%と目が合った
				;上に同じ
				IF GET_MAPID(CFLAG:MASTER:現在位置) == MAIN_MAP && MAIN_MAP == 10
					PRINTFORML 「お着替え、ここに置いておきますね」
					PRINTFORMDW 甲斐甲斐しく世話を焼いてくれる彼女を%CALLNAME:MASTER%は有難いなぁと思った
				ELSE
					PRINTFORMDL ……なぜ彼女が自分の部屋にいるのだろうか 
					PRINTFORML 「…えっと、その…道に迷っちゃって…彷徨っていたらここに」
					PRINTFORMDW そういうことにしておこう
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「あ、おはよう。もう少ししたら起こそうと思ってたんだけど…はい、お水」
				PRINTFORMDL 目を覚ますと、%CALLNAME:TARGET%は既に身支度を整えていた
				PRINTFORMDW %CALLNAME:MASTER%は渡された水を一気に呷ると、布団から這い出た……
			CASE 1
				PRINTFORMDL %CALLNAME:MASTER%が目を覚ますと、%CALLNAME:TARGET%が机に向かって何かをしていた
				PRINTFORML 「……ぁ、起きた？ごめんね、すぐに片付けちゃうから」
				PRINTFORMDW どうやら銃のメンテナンスをしていたようだ
			CASE 2
				PRINTFORMDL %CALLNAME:MASTER%が目を覚ますと、目の前に%CALLNAME:TARGET%の顔があった
				PRINTFORML 「……おはよう♪」
				PRINTFORMDW どうやら寝顔を観察されていたようだ……
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「……くぅ…」
		PRINTFORMDW %CALLNAME:MASTER%が目を覚ますと、%CALLNAME:TARGET%はまだ寝息を立てていた
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;共通
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					;トランペットを持ってる
					IF ITEM:33 == 1
						PRINTFORMDL %CALLNAME:TARGET%は涎を口の端から垂らしぐっすりと眠っている
						PRINTFORMDL ふと悪戯心が湧いた%CALLNAME:MASTER%は、彼女の横で起床ラッパを吹いてみた
						PRINTFORMW 「ッ！………って、あれ…？」
					ELSE
						PRINTFORML 「……んん…ぁ…おはよう…%MASTERNAME:100%…」
						PRINTFORMW 「…えへへ、起きて最初に見るのが%MASTERNAME:100%の顔って…なんだかいいね」
					ENDIF
				CASE 1
					PRINTFORML 「……んーっ…はぁ…おはよー…」
					PRINTFORMDW 寝惚け眼の%CALLNAME:TARGET%に、%CALLNAME:MASTER%は顔を洗ってくるように促した……
				CASE 2
					PRINTFORML 「ふぁぁ…」
					PRINTFORMDL %CALLNAME:MASTER%が座って本を読んでいると、ようやく%CALLNAME:TARGET%が目を覚ました
					PRINTFORMDW いつまでもパジャマでぽけーっとしている彼女に、%CALLNAME:MASTER%は早く着替えるよう促した……
					IF TALENT:109:恋慕
						PRINTFORML 「……んー…夢の中に誰か出てきて…よく眠れなかったんだよね…」
						PRINTFORMDW ……あいつめ……
					ENDIF
			ENDSELECT
		ELSE
			PRINTFORML 「………」
			PRINTFORMDL 目を覚ました%CALLNAME:TARGET%は、ぽけーっとしている
			PRINTFORMDL よく眠れなかったのだろうか。%CALLNAME:MASTER%は彼女に水を差しだした
			PRINTFORML 「…あ、ありがとうございます……って、あれ？どうしてここに…」
			PRINTFORMDW ……どうやらまだ寝惚けているようだ
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K100_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰る（定時になって寝る）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会話というよりは寝言？）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;イタズラ中
	;記入チェック（=0, 非表示、1, 表示）
	;LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG == 4
		;PRINTFORMW 
		;RETURN 1
	;ENDIF
	;-------------------------------------------------
	;衰弱中（恋慕付き拠点内キャラがMAIN_MAPで衰弱状態になった場合か、お招き部屋で相手が衰弱した場合に発生
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（部屋まで運ばれる）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが自室まで戻ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;そのまま着替えて寝た
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 0
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部屋から追い出された
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとのＨを止めさせて寝た
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;非在住TARGETがMAIN_MAPで休憩
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;前のターンから一緒の部屋にいる状態で相手が寝る（お招き部屋含む（
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;移動時すれちがい
;==================================================
@M_KOJO_EVENT_K100_4(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;立ち止まった
