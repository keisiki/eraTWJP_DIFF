@M_KOJO_MESSAGE_COM_K100_411_1

#DIM 正解
#DIM 経験値
#DIM 制限時間
#DIM 倍率
#DIM FEINT
#DIM イカサマ
VARSET LOCAL
イカサマ = 0
制限時間 = 2000 - (ABL:100:戦闘能力 - ABL:MASTER:戦闘能力) * 100

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;３分の１の確率で特別な稽古
	IF !RAND:3
		;特訓デイリーからお借りしました
		CALL M_KOJO_COLOR_K100
		PRINTFORMDL %CALLNAME:MASTER%と%CALLNAME:TARGET%は戦場で生き残る為の稽古をすることにした
		PRINTFORML 「ええっ、撃ってこいって……そんなの危ない\@ !TALENT:恋慕 ? です # \@よ……」
		PRINTFORMDW %CALLNAME:TARGET%はあまり乗り気ではないようだが……
		RESETCOLOR
		PRINTFORML 
		CALL ASK_YN("かまわん！やれ","やっぱり止める")
		IF RESULT
			CALL M_KOJO_COLOR_K100
			PRINTFORML 「ほっ……」
			PRINTFORMDW %CALLNAME:MASTER%が危ないことを止めてくれたので%CALLNAME:TARGET%はホッとしている
			RESETCOLOR
		ELSE
			正解 = RAND:4
			経験値 = MIN(ABL:100:戦闘能力 * 5, 5)
			SELECTCASE RAND:20
				;漢字
				CASE IS <= 11
					LOCALS:0 = 上
					LOCALS:1 = 右
					LOCALS:2 = 左
					LOCALS:3 = 下
					倍率 = 100
				;ひらがな
				CASE 12 TO 15
					LOCALS:0 = うえ
					LOCALS:1 = みぎ
					LOCALS:2 = ひだり
					LOCALS:3 = した
					倍率 = 100
				;カタカナ
				CASE 16,17,18
					LOCALS:0 = ウエ
					LOCALS:1 = ミギ
					LOCALS:2 = ヒダリ
					LOCALS:3 = シタ
					倍率 = 100
				;スワヒリ語
				CASE 19
					LOCALS:0 = juu
					LOCALS:1 = kulia
					LOCALS:2 = kushoto
					LOCALS:3 = chini
					倍率 = 320
			ENDSELECT
			;あなたの戦闘能力が高いと５分の一の確率でフェイントをかけてくる
			IF ABL:MASTER:戦闘能力 >= 5 && !RAND:5
				FEINT = 1
				制限時間 += 400
				倍率 += 80
			ELSE
				FEINT = 0
			ENDIF
			CALL FISHER_YATES_SHAFFLE(4)
			PRINTFORMW %CALLNAME:TARGET%の攻撃を避けよう（クリックで開始）
			PRINTFORM %CALLNAME:TARGET%の
			SETCOLOR C_YELLOW
			IF FEINT
				PRINTFORM %LOCALS:(RAND:4)%
				RESETCOLOR
				PRINTFORM と見せかけて%LOCALS:(3 - 正解)%
			ELSE
				PRINTFORM %LOCALS:(3 - 正解)%
				RESETCOLOR
			ENDIF
			PRINTFORML からの銃撃！
			$LOOP_IKASAMA
			FOR LOCAL:10,0,4
				LOCAL = SHAFFLE_ARRAY:(LOCAL:10)
				PRINTFORML [{LOCAL}] - %LOCALS:LOCAL%に避ける
			NEXT
			SIF !イカサマ && BASE:MASTER:TSP > 500
				PRINTFORML [{4}] - 時間停止
			TINPUT 制限時間, -1, 1
			IF RESULT == 正解
				CALL M_KOJO_COLOR_K100
				IF イカサマ
					PRINTFORMDL %CALLNAME:MASTER%はズルをして攻撃を回避した
				ELSE
					PRINTFORMDL %CALLNAME:MASTER%は銃撃を回避した！
				ENDIF
				PRINTFORML 「す、すごい……本当に当てるつもりで撃ったのに」
				PRINTFORMDL %CALLNAME:TARGET%はとてつもなく物騒なことを言っている
				PRINTFORMDW ……もしかして嫌われているのだろうか
				RESETCOLOR
				BASE:MASTER:体力 -= 200 * (1 + ABL:100:戦闘能力)
				経験値 = 経験値 * 倍率 / 100
				EXP:MASTER:戦闘経験 += 経験値
				EXP:100:戦闘経験 += 経験値 * 2
				CFLAG:100:好感度 += 経験値 * 2
				CFLAG:100:信頼度 += 経験値 * 2
				PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:MASTER%）
				PRINTFORML 戦闘経験 ＋{経験値 * 2}（%CALLNAME:TARGET%）
				PRINTFORML 好感度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
				PRINTFORML 信頼度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			ELSEIF RESULT == 4 && !イカサマ && BASE:MASTER:TSP > 500
				イカサマ = 1
				倍率 /= 5
				PRINTFORMW %CALLNAME:MASTER%は時間停止能力で射線を見極めた！
				GOTO LOOP_IKASAMA
			ELSE
				SETCOLOR C_RED
				FONTBOLD
				PRINTFORML 
				PRINTFORML パァン
				PRINTFORMW 
				FONTREGULAR
				RESETCOLOR
				CALL M_KOJO_COLOR_K100
				PRINTFORML 「きゃああああっ！！？」
				PRINTFORMDL %CALLNAME:TARGET%からの攻撃を避けることができなかった
				PRINTFORMDL 灼けるような痛みに%CALLNAME:MASTER%は立つことすらままならない……頭痛がする……は…吐き気もだ……
				PRINTFORML 「ち、血が！ お、応急手当を！」
				PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%の命を助けようと必死になって手当をしている
				PRINTFORMDW 後は彼女に任せよう。%CALLNAME:MASTER%は目を閉じると、そのまま意識を失った……
				RESETCOLOR
				BASE:MASTER:体力 -= 1000 * (1 + ABL:100:戦闘能力)
				PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:TARGET%）
				EXP:100:戦闘経験 += 経験値
				CFLAG:100:好感度 += 経験値 / 2
				CFLAG:100:信頼度 += 経験値 / 2
				PRINTFORML 好感度 ＋{経験値}（%CALLNAME:TARGET%）
				PRINTFORML 信頼度 ＋{経験値}（%CALLNAME:TARGET%）
			ENDIF
		ENDIF
	;通常
	ELSE
		CALL M_KOJO_COLOR_K100
		PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の稽古に付き合うことにした
		PRINTFORMDW ………
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「ぜぇッ、はぁッ、もう限界～～ッ！」
				PRINTFORMDW %CALLNAME:TARGET%と走り込みを行った
			CASE 1
				PRINTFORML 「やーたー」
				PRINTFORMDL %CALLNAME:TARGET%と組み手を行った
				PRINTFORMDW どことなく掛け声が適当な気がするのは気のせいだろうか…
			CASE 2
				PRINTFORML 「腕が曲がってないって……ぅうう……これ以上は……ッ」
				PRINTFORMDW %CALLNAME:TARGET%は腕立て伏せをしている
			CASE 3
				;Q-E SPAM
				PRINTFORML 「えっと、敵と撃ち合う時は上半身を左右に動かしながら……」
				PRINTFORMDW %CALLNAME:TARGET%は何やら怪しげな動きをしている
			CASE 4
				PRINTFORML 「%TEXTR("やった！当たった！/ぁぅ……外れたぁ……/")%」
				PRINTFORMDW %CALLNAME:TARGET%の射撃訓練を見守った
		ENDSELECT
		RESETCOLOR
	ENDIF
ENDIF
RETURN 1
