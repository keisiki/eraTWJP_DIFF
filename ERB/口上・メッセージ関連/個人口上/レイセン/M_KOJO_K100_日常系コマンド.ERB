;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K100_305
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K100_305_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K100_305_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;あなたが女
	IF TALENT:MASTER:性別 != 2
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「はい、どうぞ」
					PRINTFORMDL %CALLNAME:MASTER%が膝枕を求めると、%CALLNAME:TARGET%は快く膝を貸してくれた
					PRINTFORMW 「疲れちゃった？ 起こしてあげるから、寝ていいよ」
				CASE 1
					PRINTFORML 「ふぁぁ……なんだか私まで眠くなってきたかも……」
					PRINTFORML 「後で……交代してね…………zzz」
					PRINTFORMDW %CALLNAME:MASTER%は膝枕されながら、%CALLNAME:TARGET%が涎を垂らさないか気が気ではなかった
				CASE 2
					PRINTFORML 「……%MASTERNAME:100%？」
					PRINTFORML 「………」
					PRINTFORML 「…寝てるから、いいよね」
					CALL HPH_PRINT,@"「ん……HPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%は前屈みになり、そっと%CALLNAME:MASTER%の額へ口付けした……
					;選択肢
					CALL ASK_M("起きる", 1, "寝たふり", 1, )
					;判定
					SELECTCASE RESULT
						;起きる
						CASE 0
							PRINTFORML 「ふえっ？！ お、おぉ、起きてたの？！」
							PRINTFORML 「これは、えと、あの、その……」
							PRINTFORMW 「………はぅ」
						;寝たふり
						CASE 1
							PRINTFORMW 「……えへへ」
					ENDSELECT
			ENDSELECT
		;思慕
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ゆっくりくつろいでくださいね」
					PRINTFORMDL %CALLNAME:TARGET%の膝枕は寝心地が良く、気を抜けば眠ってしまいそうな程だ……
					PRINTFORMW 「……ふふ、よしよし……」
				CASE 1
					PRINTFORML 「膝枕が大好きなんですね」
					PRINTFORMDL %CALLNAME:TARGET%は慈愛に満ちた笑みを浮かべながら%CALLNAME:MASTER%の頭を撫でている
					PRINTFORMW 「なんだか私、%MASTERNAME:100%のお母さんになった気分です」
				CASE 2
					PRINTFORMDL 頭を撫でる%CALLNAME:TARGET%の手はとても温かい
					PRINTFORML 「昔、こうしてもらったことを思い出したんです」
					PRINTFORMW 「迷惑……でしたか？」
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「は、はい。私の膝で良かったら……」
					PRINTFORMDL %CALLNAME:TARGET%はスカートを整え、%CALLNAME:MASTER%へと膝を差し出した
					PRINTFORMW 「そんなに寝心地の良いものでもないと思いますけど……どうぞ」
				CASE 1
					PRINTFORML 「こういうことって、あまりしたことがなくて……」
					PRINTFORML 「その、首とか痛くないですか？」
					PRINTFORMW 「大丈夫？……よかった」
				CASE 2
					PRINTFORML 「あの……大丈夫ですか……？」
					PRINTFORMDL %CALLNAME:TARGET%は心配そうに%CALLNAME:MASTER%の顔を覗き込んでいる
					PRINTFORMDL どうやら、具合が悪いのではと勘違いしているようだ……
					PRINTFORMW 「体調が悪いのなら、お医者様に……疲れただけ？それなら良いんですけど……」
			ENDSELECT
		ENDIF
	;あなたが男
	ELSE
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「次は私の番だからね？」
					PRINTFORMDL %CALLNAME:TARGET%はそう言いながら%CALLNAME:MASTER%の頭を撫でている
					PRINTFORMW （こんな毎日が、ずっと続けばいいなぁ…）
				CASE 1
					CALL HPH_PRINT,@"「頬擦りしちゃダメだよ、もうっ…HPH」","L"
					PRINTFORMDL すべすべの太腿を思う存分堪能する%CALLNAME:MASTER%
					PRINTFORMDL %CALLNAME:TARGET%は困ったような表情を浮かべながらも、好きなようにさせていた
					CALL HPH_PRINT,@"「んぅぅ…くすぐったい…HPH」","W"
				CASE 2
					PRINTFORML 「そうだ、耳掃除してあげようか？」
					PRINTFORML 「それとも…このまま寝ちゃう？」
					PRINTFORMDW どちらも魅力的な提案だと思いながら、%CALLNAME:MASTER%は膝枕を楽しんだ
			ENDSELECT
		;思慕
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ちょっと待ってくださいね。…はい、いいですよ」
					PRINTFORMDL %CALLNAME:TARGET%は座り直しスカートを整えると膝を差し出してきた
					PRINTFORMDL %CALLNAME:MASTER%が頭を乗せると、優しく髪を撫でられる…
					PRINTFORMW 「ふふ、甘えん坊なんですね」
				CASE 1
					PRINTFORML 「あぅ…悪戯しちゃダメですよぉ…」
					PRINTFORMDL %CALLNAME:TARGET%に膝枕させながら、%CALLNAME:MASTER%は両手を伸ばし彼女の耳をモフる
					PRINTFORMW （まるで子供みたい…でも、不思議と嫌じゃないかも…）
				CASE 2
					PRINTFORML 「～♪」
					PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%に膝枕をしながら鼻歌を歌っている
					PRINTFORMDW それが子守唄代わりとなり、暫しウトウトしてしまった
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「その…どうですか？」
					PRINTFORMDL %CALLNAME:TARGET%の膝枕は極上の寝心地だ
					PRINTFORMDW 思わず頬擦りしそうになるのを%CALLNAME:MASTER%はぐっと堪えた…
				CASE 1
					PRINTFORML 「こういった事、あまりしたことなくて…」
					PRINTFORMDL %CALLNAME:TARGET%は居心地悪そうに身を縮こまらせている
					PRINTFORMW 「…落ち着く？…ならいいんですけど…」
				CASE 2
					PRINTFORML 「は、はい。どうぞ…」
					PRINTFORMDL 差し出された膝に頭を乗せると、%CALLNAME:TARGET%は恐る恐る頭を撫でてきた
					PRINTFORMW （…ちょっと可愛いかも…）
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
