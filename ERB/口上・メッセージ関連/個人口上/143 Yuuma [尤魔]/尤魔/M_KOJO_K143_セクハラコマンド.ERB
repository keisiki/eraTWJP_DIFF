;==================================================
;310,尻を撫でる
;TFLAG:193(1=不快(TALENT:臆病なら涙目) 2&&3=恥ずかしがる(TALENT:男嫌い兼MASTERが男なら、身をすくめる) 4=されるがまま)
;CFLAG:うふふ==2&&TCVAR:20(パンツ無し顔面騎乗(70=クンニ強制 71=フェラチオ強制 72=アナル奉仕) 35=スカート内見せつけ 52=素股)
;CFLAG:うふふ==2&&TEQUIP:シックスナイン(パンツありTCVAR:20(70～72)はこちら)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_SUCCESS_COM_K143_310
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K143_310
CALL M_KOJO_MESSAGE_COM_K143_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
	ELSE
		IF FIRSTTIME(SELECTCOM)
			;ねだるな、勝ち取れ！
			IF CFLAG:好感度 <= 200
				IF CFLAG:1000 == 1
					PRINTFORML 「ほう…♪」
					PRINTFORML 「泣く子も黙る剛欲同盟の長の尻を臆面もなく撫でるとはなぁ…」
					PRINTL 
					PRINTFORML 「いいぞ、欲望に素直な奴は嫌いじゃない」
					PRINTFORML 「私を慕う奴はいても、私に助平を働こうなんて奴はいなかったしな」
					PRINTFORML 「そのクソ度胸と助平心に免じて好きなだけ触らせてやるよ」
					PRINTFORML 「私もそういうことは大好きだからな」
					PRINTL 
					CFLAG:1002 = 2
				ELSE
					PRINTFORML 「おいおい…」
					PRINTFORML 「ひょっとして、それがお前の欲望ってやつか？」
					PRINTL 
					PRINTFORML 「まあいい、欲望を抑え込むやつなんかよりよほど好感が持てる」
					PRINTFORML 「それに、地上の欲を取り込んだ私には、お前の気持ちも分からんでもないしな」
					PRINTFORML 「そのクソ度胸と助平心に免じて触るのを許してやろう」
					PRINTL 
					CFLAG:1002 = 1
				ENDIF
			ELSE
				IF CFLAG:1000 == 1
					PRINTFORML 「ほう…♪」
					PRINTFORML 「泣く子も黙る剛欲同盟の長の尻を撫でるとはなぁ…」
					PRINTL 
					PRINTFORML 「まあ、尻の一つや二つ、触られたところでどうということはないさ」
					PRINTFORML 「どうせなら、恐る恐る触るくらいなら、劣情剥き出しにして触ってほしいもんだな」
					PRINTL 
					CFLAG:1002 = 1
				ELSE
					PRINTFORML 「おいおい…」
					PRINTFORML 「ひょっとして、それがお前の欲望ってやつか？」
					PRINTL 
					PRINTFORML 「恐れより劣情が勝ったのか、単に私の怖さを知らない阿呆か…」
					PRINTFORML 「いずれにせよ、触った相手が私でよかったな」
					PRINTFORML 「鬼傑のか勁牙の尻でも触ろうもんなら、あっという間に八つ裂きにされていたかもしれないぞ」
					PRINTL 
				ENDIF
			ENDIF
		;2回目以降
		ELSE
			IF K143C_セクハラに貪欲 == 2
				
			ELSEIF K143C_セクハラに貪欲 == 1
			ELSE
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
;==================================================
;311,抱き付く
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 60=ディープキス 73=ぱふぱふ 74=乳合わせ 77=乳を吸わせる ＶＡ性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_SUCCESS_COM_K143_311
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K143_311
CALL M_KOJO_MESSAGE_COM_K143_311_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CHK_DATENOW(CFLAG:LOCAL:デート中)
		IF K143C_鬼傑組 == 1
			IF TALENT:[[八千慧]]:恋人
			ELSE
				IF TALENT:恋慕 || TALENT:恋人
				ELSE
				ENDIF
			ENDIF
		ELSEIF K143C_勁牙組 == 1
			IF TALENT:[[早鬼]]:恋人
			ELSE
				IF TALENT:恋慕 || TALENT:恋人
				ELSE
				ENDIF
			ENDIF
		ELSEIF K143C_剛欲同盟 == 1
			IF TALENT:[[八千慧]]:恋人
				IF TALENT:恋慕
					SELECTCASE RAND:6
					CASE 0
						PRINTFORML 「私の気持ちを知りながら、吉弔と繋がって、こんなことをするんだからな…」
						PRINTFORML 「お前は、まさに畜生だよ」
						PRINTFORML 「そして、そんな奴こそ私にふさわしい」
					CASE 1
						PRINTFORML 「吉弔と比べて抱き心地はどうだ？」
						PRINTFORML 「私の方がいい女だろう？　今から乗り換えても全然かまわんぞ？」
					CASE 2
						PRINTFORML 「吉弔という女がいながら、お盛んなことだな…」
						PRINTFORML 「いや、そんなお前に懸想する私も同類だな。今は、この時間を大事にしようじゃないか」
					CASE 3
						PRINTFORML 「…なあ、吉弔のことなんか捨てて……」
					CASE 4
						PRINTFORML 「どさくさに紛れて尻を触ろうとするんじゃない」
						PRINTFORML 「吉弔と別れたら触らせてやるぞ？　どうする？　クックック」
					CASE 5
						PRINTFORML 「吉弔は奥手そうだからな。こういうこと、許してくれないだろう？」
						PRINTFORML 「何？　ああ見えてすごく可愛いところがある？　チッ、あいつ二人きりだとデレるタイプか…」
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「お前な…いつか刺されるぞ」
						PRINTFORML 「多分、いろんなところから」
					CASE 1
						PRINTFORML 「お前、ウチに所属して、ライバル組織の長と恋仲になって、その上私にまで抱き着いてくるのか…」
						PRINTFORML 「なんというか…お前、すごいよ」
					CASE 2
						PRINTFORML 「お前…驪駒の恋人なんだろ？」
						PRINTFORML 「こういうことしていいのか…というか、私が複雑な気持ちなんだが…」
					ENDSELECT
				ENDIF
			ELSEIF TALENT:[[早鬼]]:恋人
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「お前な…いつか刺されるぞ」
						PRINTFORML 「多分、いろんなところから」
					CASE 1
						PRINTFORML 「お前、ウチに所属して、ライバル組織の長と恋仲になって、その上私にまで抱き着いてくるのか…」
						PRINTFORML 「なんというか…お前、すごいよ」
					CASE 2
						PRINTFORML 「お前…驪駒の恋人なんだろ？」
						PRINTFORML 「こういうことしていいのか…というか、私が複雑な気持ちなんだが…」
					ENDSELECT
					PRINTW 
			ELSEIF TALENT:[[袿姫]]:恋人
			ELSE
				IF TALENT:恋慕 || TALENT:恋人
					IF TALENT:恋人
						PRINTFORML 「まったく、恋人同士だというのに大っぴらにくっ付けないのも考えものだな」
						PRINTFORML 「堂々と抱きしめていられるのもこんな時くらいだ。もう少し、こうしていて…」
						PRINTW 
					ELSE
						PRINTFORML 「お忍びでないと、こういったことも気楽にできんからな」
						PRINTFORML 「このまま二人で、どこか遠くへ行ってしまうか？」
						PRINTFORML 「…なんてな。一度は担がれた神輿だ。同盟がなくなるまでは私が同盟長を全うせんとな」
						PRINTW 
					ENDIF
				ELSE
					SELECTCASE RAND:6
					CASE 0
						PRINTFORML 「お前、結構大胆な奴だな…」
						PRINTFORML 「一応、私も女なんだぞ…」
					CASE 1
						PRINTFORML 「おいおい、気が早い奴だな」
						PRINTFORML 「私たちはそういう関係じゃあないだろう？」
					CASE 2
						PRINTFORML 「おい、お前…」
						PRINTFORML 「…まあ、いい。今はデート中だ。特別に許してやる」
					CASE 3
						PRINTFORML 「抱きしめ返してこいって、注文の多い奴だな…」
						PRINTFORML 「…ほら、これでいいか？」
					CASE 4
						PRINTFORML 「お前っ、断りもなく…」
						PRINTFORML 「チッ、私はそこまで安い女じゃないぞ」
					CASE 5
						PRINTFORML 「デートだからって遠慮がなさすぎだろう」
						PRINTFORML 「…今日だけ、特別だからな」
					ENDSELECT
					PRINTW 
				ENDIF
			ENDIF
		;所属なし
		ELSE
			IF TALENT:[[八千慧]]:恋人
			ELSEIF TALENT:[[早鬼]]:恋人
			ELSEIF TALENT:[[袿姫]]:恋人
			ELSE
				IF TALENT:恋慕 || TALENT:恋人
				ELSE
				ENDIF
			ENDIF
		ENDIF
	ELSEIF CFLAG:睡眠
	ELSE
		IF BASE:MASTER:勃起 >= 1000 && CFLAG:TARGET:溜まってる度 > 200 && TALENT:機嫌 > -1 && CFLAG:ブチギレ == 0
			CALL 勃起処理
		ELSE
		ENDIF
	ENDIF

ENDIF
RETURN 1
