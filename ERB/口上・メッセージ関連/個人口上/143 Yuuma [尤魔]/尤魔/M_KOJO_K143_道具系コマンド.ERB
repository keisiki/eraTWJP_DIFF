;==================================================
;50,ゴム装着
;==================================================
@M_KOJO_MESSAGE_COM_K143_50
CALL M_KOJO_MESSAGE_COM_K143_50_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_50_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		SELECTCASE TALENT:MASTER:形状
		CASE 6,10;巨根、馬並み
			PRINTFORML 「マジかよ…ほとんど先端しか着けられないんだが」
			PRINTFORML 「途中で外れたらどうすんだ、これ…」
		CASE 1,2;粗チン
			PRINTFORML 「お前…コンドームの方がちっとデカいんじゃないか？」
			PRINTFORML 「膣内で抜けると後で面倒なんだがな…」
		CASEELSE
			PRINTFORML 「自分で付けるのが面倒だぁ？　お前、それくらいしっかりやれよな…」
			PRINTFORML 「…ほら、これでいいか？」
		ENDSELECT
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:恋人
		IF K143C_妊娠レベル <= 7
			PRINTFORML 「あまり皮肉なことはしてくれるなよ…」
			PRINTFORML 自身の不妊体質のことを思い、尤魔の表情に影が差した
			PRINTW 
		ELSEIF K143C_妊娠レベル >= 8
			PRINTFORML 「せっかく不妊が解消できたのに…付けちゃうのか？」
			PRINTFORML 尤魔は愛する%CALLNAME:MASTER%から子種を授かれると思っていたばかりに、寂しそうな表情を浮かべている
			PRINTW 
		ELSE
			IF CFLAG:生理周期 == 7
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「確かに今日は危険日だが…女の体を気遣うなんてな。見直したぞ」
				CASE 1
					IF K143C_鬼傑組 == 1 ||  K143C_勁牙組 == 1
						PRINTFORML 「くくっ。ライバル組織の長を孕ませるのは気が引けるか？」
						PRINTFORML 「欲望のままに犯せばいいものを。意外と律儀だな、お前は」
						PRINTW 
					ELSEIF K143C_剛欲同盟 == 1
						PRINTFORML 「くくっ。さすがに親を孕ませるのは気が引けるか？」
						PRINTFORML 「別に私はお前ならいいんだがなあ…」
						PRINTW 
					ELSE
						PRINTFORML 「なんだ、私を孕ませてくれるんじゃないのか」
						PRINTFORML 「お前との間の子どもなら、産むことも吝かじゃないぞ。まあ、部下どもはうるさいだろうがな」
						PRINTW 
					ENDIF
				ENDSELECT
			ELSEIF CFLAG:生理周期 == 1 || CFLAG:生理周期 == 2
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「簡単にできるとも思えんが…まあ、避妊は大事だよな」
					PRINTFORML 「それに、こっちのほうが純粋にセックスを楽しめるというものだ。くっくっく」
					PRINTW 
				CASE 1
					PRINTFORML 「いいのか？　待ちに待った安全日だぞ？」
					PRINTFORML 「それとも、万が一が怖いか？　お前、真面目な奴だなあ。まあ、それが悪いとは言わんが」
					PRINTW 
				CASE 2
					IF K143C_鬼傑組 == 1 ||  K143C_勁牙組 == 1
						PRINTFORML 「くくっ。ライバル組織の長を孕ませるのは気が引けるか？」
						PRINTFORML 「欲望のままに犯せばいいものを。意外と律儀だな、お前は」
						PRINTW 
					ELSEIF K143C_剛欲同盟 == 1
						PRINTFORML 「くくっ。さすがに親を孕ませるのは気が引けるか？」
						PRINTFORML 「別に私はお前ならいいんだがなあ…」
						PRINTFORML 「それに、今日はできにくい日だ。遠慮しなくていいんだぞ？」
						PRINTW 
					ELSE
						PRINTFORML 「なんだ、私を孕ませてくれるんじゃないのか」
						PRINTFORML 「お前との間の子どもなら、産むことも吝かじゃないぞ。まあ、部下どもはうるさいだろうがな」
						PRINTW 
					ENDIF
				ENDSELECT
			ELSE
				PRINTFORML 「今日は危険日というわけでもないが…まあ、万が一ということもあるしな」
				PRINTFORML 「身重になっては色々と不都合だ。今は、純粋にセックスを楽しもうじゃないか、なあ」
				PRINTW 
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF K143C_性に貪欲 > 0
			PRINTFORML 「私相手に避妊しようとするなんて正気か？」
			PRINTFORML 「こんな無粋なものを着ける奴の気が知れん。それとも、こっちの方が興奮するのか？」
			PRINTFORML 「なるほど、純粋にセックスを楽しんでいる気分に浸れるわけか…面白い解釈だ」
			PRINTW 
		ELSE
			PRINTFORML 「私が人間の種で孕むとも思えんが、万が一ということもあり得る」
			PRINTFORML 「それに、身重になると色々不都合だしな。悪いが避妊してもらうぞ」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1