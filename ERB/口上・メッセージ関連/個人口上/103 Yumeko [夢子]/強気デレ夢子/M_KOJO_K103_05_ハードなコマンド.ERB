;==================================================
;150,胸の谷間を犯す
;==================================================
@M_KOJO_MESSAGE_COM_K103_150
;CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K103_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K103_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;谷間潤滑チェック
	LOCAL:2 = M_KOJO_K103_B_WET()
	;地の文を移動させているため口上色を変更
	SIF FLAG:口上色
		CALL M_KOJO_COLOR_K103
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
		PRINTFORML 「ちょ、ちょっとちょっと、胸をそんな乱暴にっ！」
		PRINTFORMW 「あっ、あんっ、もうっ！！」
		PRINTFORMW 「ひぎっ…ち、乳首を摘ままないでっ…あぅっ%M_KOJO_K103_HEART()%」
		PRINTFORML
		RETURN 1
	ENDIF
	;地の文 意図に沿わないので持ってきた
	IF SELECTCOM == PREVCOM
		PRINTFORMD %CALLNAME:PLAYER%は
		SIF FLAG:70 == 1
			PRINTFORMD 時間を止めた
		SIF CFLAG:MASTER:イタズラ == 2
			PRINTFORMD 眠っている
		PRINTFORMD %CALLNAME:TARGET%の%OPPAI_DESCRIPTION(TARGET,1)%
		IF RAND:2
			IF RAND:3 == 0
				PRINTFORMD の乳輪を親指でくすぐりながら
			ELSEIF RAND:2
				PRINTFORMD の%NIPPLE_DESCRIPTION(TARGET,1)%を指で扱きながら
			ELSE
				PRINTFORMD を揉みしだきながら
			ENDIF
		ELSE
			IF RAND:4 == 0
				PRINTFORMD を内側に寄せながら
			ELSEIF RAND:3 == 0
				PRINTFORMD を密着させながら
			ELSEIF RAND:2
				PRINTFORMD を締め付けながら
			ELSE
				PRINTFORMD を圧迫しながら
			ENDIF
		ENDIF

		IF EXP:PLAYER:Ｖ性交経験 + EXP:PLAYER:Ａ性交経験 && RAND:2
			IF RAND:3 == 0
				PRINTD 性交でもするかのように
			ELSEIF RAND:2
				PRINTFORMD 乳を孕ませようと
			ELSE
				PRINTD まるで交尾をしているかの如く
			ENDIF
		ENDIF
		SIF RAND:2
			PRINTFORMD 体力の続くまま
		IF ABL:PLAYER:サドっ気 && RAND:3 == 0
			PRINTFORMDL 荒々しく腰を動かし
		ELSEIF RAND:2
			PRINTFORMDL 腰を激しく動かし
		ELSE
			PRINTFORMDL 腰を前後に動かし
		ENDIF
		
		IF RAND:3
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET)%を
		ELSE
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET)%の谷間を
		ENDIF
		
		IF RAND:2 == 0
			PRINTFORMDL 犯し続けている…
		ELSE
			PRINTFORMDL 犯し続けた…
		ENDIF
	ELSE
		PRINTFORMD %CALLNAME:PLAYER%は
		SIF FLAG:70 == 1
			PRINTFORMD 時間を止めた
		SIF CFLAG:MASTER:イタズラ == 2
			PRINTFORMD 眠っている
		PRINTFORMD %CALLNAME:TARGET%の
		;貧乳
		IF TALENT:バストサイズ == -1
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%に%TINKO(PLAYER)%を擦りつけ
		;巨乳
		ELSEIF TALENT:バストサイズ == 1
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%に%TINKO(PLAYER)%をはさませて擦り上げ
		;爆乳
		ELSEIF TALENT:バストサイズ == 2
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%に%TINKO(PLAYER)%をはさませて擦り上げ
		;超乳
		ELSEIF TALENT:バストサイズ == 3
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%に%TINKO(PLAYER)%を突っ込み擦り上げ
		ELSE
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%に%TINKO(PLAYER)%をはさませて擦り上げ
		ENDIF
		PRINTDL る…

		PRINTFORMD %CALLNAME:PLAYER%は%CALLNAME:TARGET%の%OPPAI_DESCRIPTION(TARGET)%
		IF RAND:2
			IF RAND:3 == 0
				PRINTFORMD の乳輪を親指でくすぐりながら
			ELSEIF RAND:2
				PRINTFORMD の%NIPPLE_DESCRIPTION(TARGET,1)%を指で扱きながら
			ELSE
				PRINTFORMD を揉みしだきながら
			ENDIF
		ELSE
			IF RAND:4 == 0
				PRINTFORMD を内側に寄せながら
			ELSEIF RAND:3 == 0
				PRINTFORMD を密着させながら
			ELSEIF RAND:2
				PRINTFORMD を締め付けながら
			ELSE
				PRINTFORMD を圧迫しながら
			ENDIF
		ENDIF

		IF EXP:PLAYER:Ｖ性交経験 + EXP:PLAYER:Ａ性交経験 && RAND:2
			IF RAND:3 == 0
				PRINTD 性交でもするかのように
			ELSEIF RAND:2
				PRINTFORMD 乳を孕ませようと
			ELSE
				PRINTD まるで交尾をしているかの如く
			ENDIF
		ENDIF

		IF ABL:PLAYER:サドっ気 && RAND:3 == 0
			PRINTFORMDW 荒々しく腰を動かした…
		ELSEIF RAND:2
			PRINTFORMDW 腰を激しく動かした…
		ELSE
			PRINTFORMDW 腰を前後に動かした…
		ENDIF

		IF !FLAG:70 && CFLAG:MASTER:イタズラ != 2
			PRINTFORMD %CALLNAME:TARGET%は胸の谷間を犯され
			;恋慕か親愛（愛欲の有無でちょっと分岐）
			IF TALENT:愛欲
				PRINTFORMDL \@ TALENT:恋慕 ? うっとりとした # 蕩けた \@表情をしている…
			ELSE
				PRINTFORMDL ている…
			ENDIF
			IF ABL:PLAYER:技巧 > 2
			;プレイヤーの技巧3以上
				;胸で分岐させてサイズはSELECTCASEで分けてみる。大きさは違ってもその先に突起はあるんだよ
				SELECTCASE TALENT:バストサイズ
				;きょにゅー以上
				CASE IS > 0
					IF RAND:2 == 0
						PRINTFORMDL むっちりと詰まった肉と肉、%OPPAI_DESCRIPTION(TARGET,1)%の間に%TINKO(PLAYER)%を挟み込み、扱きあげる。
						PRINTDL しっとりと包み込んで、柔らかくどこまでも硬い肉棒を受け入れて
						PRINTDL 沈んでいくような心地よさは、まるで肉の海に蕩けるような感覚だ…
					ELSE
						PRINTFORMDL その%OPPAI_DESCRIPTION(TARGET,1)%に%TINKO(PLAYER)%を押し付け、たっぷりと%OPPAI_DESCRIPTION(TARGET)%の重圧を加えた中に、
						PRINTFORMDL %CALLNAME:PLAYER%の%TINKO(PLAYER)%を突きこむ。
						PRINTFORMDL %CALLNAME:TARGET%の肉が柔らかく圧倒的な量感で触れていく心地に、%CALLNAME:PLAYER%は飲み込まれていく…
					ENDIF
						SIF LOCAL:2 >= 3 && LOCAL:2 < 5
							PRINTFORMDL 程よく濡れた%TINKO(PLAYER)%は、胸に打ち付ける度にぬぷぬぷ、ぱちゅぱちゅと音を立てる。
						SIF LOCAL:2 >= 3
							PRINTFORMDL ぬらぬらと濡れた%TINKO(PLAYER)%は、胸に打ち付ける度にじゅぷじゅぷ、ばちゅばちゅと音を立てる。
					PRINTDL ぬめる肉と肉は押しこめば押しこむほど硬いペニスをどこまでも受け入れ、沈み込むほど柔らかい…
					SIF EX:Ｂ絶頂 * 10000 + PALAM:快Ｂ + PALAM:欲情 >= 10000
						PRINTFORMDL その中で桃色付いた%OPPAI_DESCRIPTION(TARGET,1)%の先端、%NIPPLE_DESCRIPTION(TARGET,1)%が時折、コリコリと竿やカリを擦ってくる…
				;ひんぬー以下
				CASE IS < 0
					PRINTFORMDL 物理的なハンデを経験と技で補おうと%OPPAI_DESCRIPTION(TARGET,1)%を使い%TINKO(PLAYER)%に奉仕している。
					SIF EX:Ｂ絶頂 * 10000 + PALAM:快Ｂ + PALAM:欲情 >= 10000
						PRINTFORMDL 上気した胸板の先にある%NIPPLE_DESCRIPTION(TARGET,1)%で竿やカリを擦るように丁寧に誘導してくる。
				;ふにゅぅ
				CASEELSE
					PRINTFORMDL %OPPAI_DESCRIPTION(TARGET,1)%で%TINKO(PLAYER)%を挟み、扱き上げる。
					SIF EX:Ｂ絶頂 * 10000 + PALAM:快Ｂ + PALAM:欲情 >= 10000
						PRINTFORMDL 時折汗ばんだ%OPPAI_DESCRIPTION(TARGET,1)%の頂き、%NIPPLE_DESCRIPTION(TARGET,1)%に竿やカリを擦れさせては甘い声をあげる。
				ENDSELECT
			ELSEIF ABL:PLAYER:技巧 <= 2
			;胸2以下
				;貧乳
				IF TALENT:バストサイズ == -1
					PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%で懸命に%TINKO(PLAYER)%を
				;巨乳
				ELSEIF TALENT:バストサイズ == 1
					PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%で%TINKO(PLAYER)%を包み込んで
				;爆乳、超乳
				ELSEIF TALENT:バストサイズ >= 2
					PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%で%TINKO(PLAYER)%を包み隠すようにして
				ELSE
					PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%で%TINKO(PLAYER)%をはさんで
				ENDIF
				PRINTDL 擦りあげている…
			ELSE

			ENDIF
		ENDIF
	ENDIF
	IF FLAG:70 == 1 || CFLAG:MASTER:イタズラ == 2
		RETURN 1
	ENDIF
	PRINTDW 
	;基本セット
	;恋慕
	IF TALENT:恋慕
		SELECTCASE M_KOJO_K103_RANDNR(4,150)
		CASE 1
			PRINTFORMW 「あっ、もう！ そんなにがっつかなくてもいいから！」
			PRINTL
			PRINTFORMDL %CALLNAME:TARGET%はそう言いながらも脇を締めて、%OPPAI_DESCRIPTION(TARGET,1)%を内側にぐっと寄せてくれた
			PRINTDW 密着度が上がり、快感が強くなって頭の芯が痺れる感覚が頭に上ってくる
			PRINTL
			PRINTFORMW 「ふふ、気持ちよさそうな顔…%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「乳首掴んで揺すられるとっ、私も、ひぎっ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「%CALLNAME:MASTER%の%M_KOJO_K103_GET("ペニス")%が私の胸の中で暴れてる…っ！」
			PRINTL
			PRINTFORMDL %CALLNAME:TARGET%が身を縮こまらせると、%OPPAI_DESCRIPTION(TARGET,1)%の圧がぎゅっと増した
			PRINTFORMDW 気持ち良さが増したお返しに、掴んでいる%OPPAI_DESCRIPTION(TARGET,1)%の乳首をぐりぐりとこねる
			PRINTL
			PRINTFORMW 「あっ、ああんっ%M_KOJO_K103_HEART()% 駄目だってばぁ%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「ん…んんっ……%M_KOJO_K103_HEART()% ふふっ…おっぱい気持ちいい？」
			PRINTFORMW 「すっごい必死で、だらしない顔してる」
			PRINTFORMW 「あっ、ちょっとっ、乳首を捏ねないでっ%M_KOJO_K103_HEART(2)%」
		CASEELSE
			PRINTFORMW 「ん、んぁ…%M_KOJO_K103_HEART()% ど、どう？ 気持ちいい？」
			PRINTFORMW 「こうしたら、…どう？」
			PRINTL
			PRINTFORMDL %CALLNAME:TARGET%はそう言いながら手で胸の横を掴んで、
			PRINTFORMDW %OPPAI_DESCRIPTION(TARGET,1)%をこちらの腰の動きに合わせて揺すった
			PRINTFORMDW 勢いよく当たる%TINKO(PLAYER)%の根元が、パチン！パチン！と音を立てている
			PRINTL
			PRINTFORMW 「いつでもっ、イって、いいからねっ%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		SELECTCASE M_KOJO_K103_RANDNR(4,150)
		CASE 1
			PRINTFORMW 「あっ、もう！ そんなにがっつかないでっ！」
			PRINTL
			PRINTFORMDL %CALLNAME:TARGET%が身をわずかに捩ると、%OPPAI_DESCRIPTION(TARGET,1)%の圧が増した
			PRINTFORMDW 気持ち良さが増したお返しに、掴んでいる%OPPAI_DESCRIPTION(TARGET,1)%の乳首をぐりぐりとこねる
			PRINTL
			PRINTFORMW 「あっ、ちょっとっ…ああんっ%M_KOJO_K103_HEART()%」
		CASE 2
