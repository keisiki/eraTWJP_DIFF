;MESSAGECHECK関数は、以下のような戻り値を返す
;RETURN 0;通常地の文と口上を表示
;RETURN 1;口上のみ表示
;RETURN 2;通常地の文のみ表示
;RETURN 3;どちらも表示しない
;以下原文
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K98_80
RETURN 0

@M_KOJO_MESSAGE_COM_K98_80
CALL M_KOJO_MESSAGE_COM_K98_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K98_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;奉仕精神が１０以上または恋人
		IF ABL:奉仕精神 > 9 || TALENT:恋人
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「はい、このまま――んぁっHPH」","L"
						PRINTFORMDL 肉棒の先端から迸る精液に%CALLNAME:TARGET%は咄嗟に目を瞑る
						PRINTFORMDW 手を放してしまった%CALLNAME:TARGET%に代わって、あなたは自らペニスを扱き彼女の顔中に精液を撒き散らした
					CASE 1
						CALL HPH_PRINT,@"「ここ、ですね……HPH あむ……ふ……っHPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%はあなたの股に顔を埋め、玉を食みながら竿を扱いている
						CALL HPH_PRINT,@"「んぷ、ちゅう……HPH はぁ……ちゅ……っHPH」","L"
						PRINTFORMDW 時々上目遣いで見上げる彼女は、まるで「早く射精して」と催促しているようだった
					CASE 2
						CALL HPH_PRINT,@"「凄い、ヌルヌルと滑って……HPH」","L"
						CALL HPH_PRINT,@"「はぁ……HPH 臭いも、こんなに……HPH」","L"
						PRINTFORMDW %CALLNAME:TARGET%はウットリと、射精寸前のペニスを扱いている……
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「硬くて、熱い……HPH」","L"
						CALL HPH_PRINT,@"「これが、私の中に……HPH」","L"
						PRINTFORMDW %CALLNAME:TARGET%は、まるで恋人の頬を撫でるようにあなたのペニスを撫で擦っている……
					CASE 1
						CALL HPH_PRINT,@"「んっ……気持ち良いですか……？HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%は上目遣いであなたを見つめながらペニスを扱いている
						PRINTFORMDW あなたは返事の代わりに、%CALLNAME:TARGET%の頭を優しく撫でた
					CASE 2
						CALL HPH_PRINT,@"「すぅ……はぁ……っHPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%は手で竿を扱きながら、至近距離でペニスの臭いを嗅いでいる
						PRINTFORMDW きっと無意識の行動なのだろう。だが、彼女の見てはいけない部分を見た気がしてとても興奮した
				ENDSELECT
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「――もう射精そう？ えっと、どうすれば？」
						PRINTFORMDL もっと強く、激しく扱いて欲しい。そうあなたはお願いした
						PRINTFORMW 「もっと強く……あ、跳ねて……っ！？」
					CASE 1
						PRINTFORML 「――そのままって、へ、あっ！？」
						PRINTFORMDL %CALLNAME:TARGET%の手の中でペニスが跳ね、先端から精液を迸らせる
						PRINTFORMDW 勢いよく飛び散ったそれが、ドロリと糸を引いて彼女の顔から重そうに垂れ落ちた
					CASE 2
						PRINTFORMDL あなたは%CALLNAME:TARGET%にペニスを握らせると、そのまま射精した
						PRINTFORML 「あっ！……凄い、こんなに……」
						PRINTFORMDW %CALLNAME:TARGET%はしげしげと、手に付着した精液を見つめている……
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML （こんなにも穢らわしいものなのに……）
						PRINTFORML （どうして……目が離せない……）
						PRINTFORMDW %CALLNAME:TARGET%の視線は、赤黒く膨張した竿の先端に注がれている……
					CASE 1
						PRINTFORML 「この、溢れているものを止めてください」
						PRINTFORML 「ヌルヌルして、滑ってしまって……」
						PRINTFORMDW %CALLNAME:TARGET%が手を動かす度、ニチュニチュと淫らな音が鳴る……
					CASE 2
						PRINTFORML 「――もう少し変化を……こうでしょうか……？」
						PRINTFORMDL %CALLNAME:TARGET%はあなたの反応を見ながら不規則に手を動かしている
						PRINTFORMDW 気持ち良い。そう言ってやると、%CALLNAME:TARGET%は恥ずかしそうに目を逸らした
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

