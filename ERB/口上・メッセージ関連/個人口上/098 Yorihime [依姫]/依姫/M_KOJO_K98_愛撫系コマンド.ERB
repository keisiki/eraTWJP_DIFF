@M_KOJO_MESSAGE_COM_K98_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;膣内射精直後
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER && TCVAR:102
		;膣射中毒が５以上の場合に２分の１で表示
		IF ABL:膣射中毒 > 4 && !RAND:2
			PRINTFORML （……もったいない……）
			PRINTFORML （って私、なにを考えて……っ）
			PRINTFORMDW 自分がとっくに膣射中毒になっていることに、%CALLNAME:TARGET%は気付いていない……
		;それ以外
		ELSE
			CALL HPH_PRINT,@"「はぁ……はぁ……っHPH ぁん……んん……っHPH」","L"
			PRINTFORMDL コポコポと精液を零す%CALLNAME:TARGET%の膣を、あなたは労わるように舐めほぐす
			PRINTFORMDW 二人の愛が入り混じった体液は、なんともいえない味がした……
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		;Ｃ感覚が１０以上
		IF ABL:Ｃ感覚 > 9
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「もっと、ぁっHPH いぃんっHPH あぁあっHPH HPH HPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%の膣に口を付け、わざと下品な音を立て蜜を啜る
					PRINTFORMDW 「いや」とも「やめて」とも言わず、%CALLNAME:TARGET%は「もっと」とあなたを求め続けた
				CASE 1
					CALL HPH_PRINT,@"「すご、ぁっHPH ナカ、ぁんっHPH までぇっHPH HPH HPH」","L"
					PRINTFORMDL ザラザラとした舌が膣内を我が物顔で這いまわる
					PRINTFORMDW %CALLNAME:TARGET%は腰を跳ねさせて、何度も何度も絶頂している……
				CASE 2
					CALL HPH_PRINT,@"「あぁんっHPH きもち、いっHPH いぃっHPH HPH HPH」","L"
					PRINTFORMDL あなたの舌技に%CALLNAME:TARGET%はすっかり虜になっている
					PRINTFORMDW ぷっくりとした陰核を甘噛みすると、%CALLNAME:TARGET%は嬌声を上げて絶頂した
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「も、舐めな……っHPH ぃあぁんっHPH」","L"
					PRINTFORMDL 絶頂した%CALLNAME:TARGET%の膣口がパクパクと開閉する
					PRINTFORMDW あなたは息がかかるほどの距離で、その様を観察した
				CASE 1
					CALL HPH_PRINT,@"「やぁっHPH ひ、くぅっHPH んぅんんっHPH」","L"
					CALL HPH_PRINT,@"（私……舐められて、イッて……っHPH）","L"
					PRINTFORMDW 絶頂の波が引くと、%CALLNAME:TARGET%はクタリと脱力した……
				CASE 2
					CALL HPH_PRINT,@"「ま、待ってっHPH それ以上、は、あぁっHPH」","L"
					PRINTFORMDL 逃げる腰を押さえつけ、グショグショに濡れた秘唇にむしゃぶりつく
					PRINTFORMDW %CALLNAME:TARGET%が達しても、あなたは舐めるのを止めなかった
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;Ｃ感覚が１０以上
	ELSEIF ABL:Ｃ感覚 > 9
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「ひぁんっHPH あぁっHPH んふぅ、あぁっHPH」","L"
				PRINTFORMDL %CALLNAME:TARGET%は脚であなたの頭を挟み、グイグイと秘所に押し付けている
				PRINTFORMDW 無意識の行動なのだろうが、苦しいので手加減して欲しい……
			CASE 1
				CALL HPH_PRINT,@"「くふんっHPH んくぅっHPH あっHPH あんっHPH」","L"
				PRINTFORMDL 最初は我慢しようとするが、すぐに口から喘ぎ声が漏れてしまう
				PRINTFORMDW 努力は認めるが、ここまで開発されてしまってはもう無理だろう……
			CASE 2
				CALL HPH_PRINT,@"「んんっHPH 美味しい、なんてHPH そんなこと……っHPH 」","L"
				PRINTFORMDL 本当の事だと、そう言うかのようにあなたは%CALLNAME:TARGET%の淫汁を吸い上げた
				CALL HPH_PRINT,@"「ぃひんっHPH あぁあっHPH それ、ダメぇっHPH HPH HPH」","W"
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMDL あなたは%CALLNAME:TARGET%の股に顔を埋め、こんこんと湧く蜜を味わっている
				CALL HPH_PRINT,@"「音を……んっHPH 立てないで……っHPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%は抗議をするように、脚であなたの頭をキュッと挟んだ
			CASE 1
				CALL HPH_PRINT,@"「そ、そこばかり、ぁんっHPH 舐めるのは……っHPH」","L"
				PRINTFORMDL %CALLNAME:TARGET%はあなたの頭を手で押さえている
				PRINTFORMDW が、その力は弱々しい……
			CASE 2
				CALL HPH_PRINT,@"「ひゃっHPH し、舌、ぁっHPH あっHPH」","L"
				PRINTFORMDL 舌の侵入に%CALLNAME:TARGET%が驚き、脚を閉じようとする
				PRINTFORMDW あなたはそれを掴んで止めると、秘唇に口を付け汁を啜った
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
