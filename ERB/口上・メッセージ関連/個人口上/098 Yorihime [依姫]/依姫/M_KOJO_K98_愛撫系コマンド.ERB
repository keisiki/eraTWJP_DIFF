@M_KOJO_MESSAGE_COM_K98_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;ＶorＡに挿入中
	IF LOCAL:1 && (TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER)
		;依姫が絶頂
		IF NOWEX:Ｃ絶頂 || NOWEX:Ｖ絶頂 || NOWEX:Ａ絶頂 || NOWEX:Ｂ絶頂 || NOWEX:Ｍ絶頂
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ぃ、くっHPH あぁあぁぁあっHPH HPH HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%が達したのと同時に、あなたも腰を突き入れ射精した
						CALL HPH_PRINT,@"「ぁぁ……射精て……っHPH はぁぁ……っHPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「ぃ、いまはっHPH あぁあんっHPH HPH HPH」","L"
						PRINTFORMDL 絶頂にわななく%CALLNAME:TARGET%の粘膜を、反り返る肉棒でグチュグチュと擦り上げる
						PRINTFORMDW ずっと我慢していた分、あなたの容赦のないピストンで%CALLNAME:TARGET%を責め立てた
					CASE 2
						CALL HPH_PRINT,@"「くはぁっHPH あぁっHPH んあぁあっHPH HPH HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%は舌を、あなたは腰を突き出し、同時に絶頂した
						CALL HPH_PRINT,@"「はぁ……はぁ……っHPH ん、ちゅ……っHPH」","L"
						PRINTFORMDW 絶頂の余韻が引くと、二人は言葉を交わさずただ唇を重ね合わせた……
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ふぁ、あっHPH はぁああっHPH HPH HPH」","L"
						PRINTFORMDL 背を逸らし、躰を震わせて%CALLNAME:TARGET%が絶頂する
						PRINTFORMDW 挿入されたあなたのモノが、%CALLNAME:TARGET%のナカでキツく締め付けられる……
					CASE 1
						CALL HPH_PRINT,@"「へぁ、あんっHPH ひぅ、んんんっHPH HPH HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%が絶頂し、キュウゥッと肉棒が締め付けられる
						PRINTFORMDW このままだとイッてしまいそうだ。あなたは緩やかに動かしていた腰の動きを止めた
					CASE 2
						PRINTFORMDL %CALLNAME:TARGET%が絶頂するタイミングを見計らって、あなたはズグッと腰を突き入れた
						CALL HPH_PRINT,@"「ぅあっHPH あぁっ！？HPH いや、あぁあっHPH HPH HPH」","L"
						PRINTFORMDW %CALLNAME:TARGET%は収まることのない絶頂感にビクビクと躰を震わせている……
				ENDSELECT
			ENDIF
		;それ以外
		ELSE
			;膣orアナル技術が３以上
			IF ABL:膣 > 2 || ABL:アナル > 2
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ひぁ、んぅっHPH っぁ、はぁっHPH」","L"
						PRINTFORMDL あなたの手が触れる度に、%CALLNAME:TARGET%のナカが悦び締まる
						PRINTFORMDW 溶け合うような感覚の中、二人は肉欲のままに絡み合う……
					CASE 1
						CALL HPH_PRINT,@"「そんな、深っHPH っんはぁHPH」","L"
						PRINTFORMDL そう言う%CALLNAME:TARGET%だが、腰を押し付けてきているのは彼女の方だ
						CALL HPH_PRINT,@"「ゃっHPH だめ、ぇっHPH あぁんっHPH」","W"
					CASE 2
						CALL HPH_PRINT,@"「んくふぅっHPH ぁ、おっHPH 奥、ぅんっHPH」","L"
						PRINTFORMDL あなたは%CALLNAME:TARGET%の求めに応じ、腰を深くまで突き入れた
						CALL HPH_PRINT,@"「くはぁっHPH ふぁ、あっHPH あぁんっHPH」","W"
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ひぁ……ん……っHPH ぁっHPH んはぁ……っHPH」","L"
						PRINTFORMDL 一定のペースで腰を遣いながら、%CALLNAME:TARGET%の肌に手を這わせる
						PRINTFORMDW 中も外も愛撫され、%CALLNAME:TARGET%の躰はすっかり熱くなっている……
					CASE 1
						CALL HPH_PRINT,@"「はぁ……んぅ……っHPH っ、くぅ……っHPH」","L"
						PRINTFORMDL 切ない喘ぎを漏らす%CALLNAME:TARGET%に顔を近付け、唇を奪う
						CALL HPH_PRINT,@"「っぷぁ……ちゅ……ふぅ……ん……っHPH」","L"
						PRINTFORMDW %CALLNAME:TARGET%は目を閉じ、あなたと舌を絡ませている……
					CASE 2
						CALL HPH_PRINT,@"「ふぁ、ぁぁ……っHPH ぃ、っん……っHPH」","L"
						PRINTFORMDL 子種を求めるように肉穴が収縮し、あなたも思わず腰が動いてしまう
						PRINTFORMDW 意識を逸らす為、あなたは%CALLNAME:TARGET%の揺れる乳房に手を伸ばした
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;依姫が絶頂
	ELSEIF NOWEX:Ｃ絶頂 || NOWEX:Ｖ絶頂 || NOWEX:Ａ絶頂 || NOWEX:Ｂ絶頂 || NOWEX:Ｍ絶頂
		;ＣorＢorＭ感覚が１０以上
		IF ABL:Ｃ感覚 > 9 || ABL:Ｂ感覚 > 9 || ABL:Ｍ感覚 > 9
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ちょ、もうイッてHPH ひぁんっHPH」","L"
					PRINTFORMDL あなたは%CALLNAME:TARGET%をイかせるため、ひたすらに愛撫を続ける……
					CALL HPH_PRINT,@"「やっHPH またっHPH またっHPH イッ、くぅぅっHPH HPH HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「いいっぁっHPH あ、あぁぁっHPH ――あはぁ……っHPH」","L"
					PRINTFORMDL キュウッと脚を閉じて%CALLNAME:TARGET%が絶頂に達する
					PRINTFORMDW 余韻で%CALLNAME:TARGET%が幸せそうにしている間に、あなたは指についた愛液を舐め取った
				CASE 2
					CALL HPH_PRINT,@"「ぁ、くぅんんぅっHPH HPH HPH ――っは、はぁ……っHPH」","L"
					PRINTFORMDL 『イッたのか？』とあなたは%CALLNAME:TARGET%の耳元で囁いた
					CALL HPH_PRINT,@"「はぁぁ……はぁ……ぇ……？HPH」","L"
					PRINTFORMDW よほど絶頂が深かったのか、%CALLNAME:TARGET%はあなたの言葉を聞いていなかったようだ……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ぁ、待っHPH ――っぃ、んんんっHPH」","L"
					PRINTFORMDL ビクビクと躰を震わせて%CALLNAME:TARGET%が達する
					PRINTFORMDW あなたは愛撫の手を止めると、絶頂の波が収まるまで%CALLNAME:TARGET%の頭を撫で続けた
				CASE 1
					CALL HPH_PRINT,@"「やぁ、ぅんっHPH くぅ、ぁ、あぁあっHPH」","L"
					PRINTFORMDL 絶頂に達した%CALLNAME:TARGET%が嬌声を辺りに響かせる
					CALL HPH_PRINT,@"（私、声……っHPH 恥ずかしい……っHPH）","W"
				CASE 2
					CALL HPH_PRINT,@"「そこ、あぁっHPH だめ、ぇっHPH」","L"
					PRINTFORMDL ここだな、とあなたはその部分を重点的に責め立てた
					CALL HPH_PRINT,@"「ダメ、てっHPH あっHPH いやぁっHPH」","W"
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;ＣorＢorＭ感覚のどれかが１０以上
		IF ABL:Ｃ感覚 > 9 || ABL:Ｂ感覚 > 9 || ABL:Ｍ感覚 > 9
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「っ、ふぁっHPH あっHPH んぁっHPH」","L"
					PRINTFORMDL どこをどう触っても、%CALLNAME:TARGET%は敏感に反応してくれる
					PRINTFORMDW あなたは%CALLNAME:TARGET%がすぐイかないように、加減をしながら愛撫を続けた
				CASE 1
					CALL HPH_PRINT,@"「はぁ……ぁん……っHPH っん、くぅ……っHPH」","L"
					PRINTFORMDL 敏感なトコロはあえて避け、焦らすような愛撫を続ける
					CALL HPH_PRINT,@"（言えない……でも、あぁ……あと少しなのに……っHPH）","W"
				CASE 2
					CALL HPH_PRINT,@"「んんっHPH んふぅ、ちゅ……ぢゅる……っHPH」","L"
					PRINTFORMDL 口寂しそうにしていた%CALLNAME:TARGET%の唇を奪い、ねっとりと舌を絡ませる
					PRINTFORMDW そのまま愛撫を再開すると、%CALLNAME:TARGET%は喉の奥から堪えきれない喘ぎを漏らした
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ん……く……っHPH ふぁ……んん……っHPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%の胸の頂にキスをしながら、秘所を優しく指で擦る
					PRINTFORMDW %CALLNAME:TARGET%は、むず痒そうに身を捩っている……
				CASE 1
					CALL HPH_PRINT,@"（ただ……彼が触れているだけなのに……っHPH）","L"
					CALL HPH_PRINT,@"（どうして……こんなに痺れるの……っHPH）","L"
					PRINTFORMDW %TEXTR("陰核/乳首/")%を指で摘まみ上げると、%CALLNAME:TARGET%はビクンと躰を震わせた
				CASE 2
					CALL HPH_PRINT,@"「っ、んん……っHPH っあHPH はぁ……ん……っHPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%は何かを耐えるように眉を八の字に歪めている
					PRINTFORMDW もっと素直になって欲しい。そう思いながらあなたは愛撫を続けていった
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
