;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_SCOM_K98_1
RETURN 0

@M_KOJO_MESSAGE_SCOM_K98_1
CALL M_KOJO_MESSAGE_SCOM_K98_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K98_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		CALL HPH_PRINT,@"「どうして……こんな体勢で……っHPH」","L"
		PRINTFORMDL こういうことだと、あなたは%CALLNAME:TARGET%の秘所を舐め上げた
		CALL HPH_PRINT,@"「――っっHPH ふぅ……私も、舐めろと……そういうことですか……っHPH」","L"
		PRINTFORMDL %CALLNAME:TARGET%の目の前にある、硬く反り返った雄の性器
		PRINTFORMDW 早くしろと言わんばかりに脈動するそれに、%CALLNAME:TARGET%は恐る恐る舌を這わせた
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;奉仕精神が１０以上または恋人
		IF ABL:奉仕精神 > 9 || TALENT:恋人
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ずず……ぢゅる……っHPH はぁ……んふぅ……っHPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%は舌や唇、喉までも使ってあなたのペニスを気持ち良くさせている
						PRINTFORMDW 我慢できず射精すると、%CALLNAME:TARGET%はウットリと目尻を下げつつ、口内に溜まっていく精液を飲み込んでいった
					CASE 1
						CALL HPH_PRINT,@"「んふむ……ふっ……HPH ん……ん……っHPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%は一言も発さず、ただ陶然とペニスをしゃぶり続けている
						PRINTFORMDW あなたが射精しても、%CALLNAME:TARGET%は僅かに息を乱しただけですぐに喉を鳴らしはじめた
					CASE 2
						PRINTFORMDL あなたが射精を予告すると、%CALLNAME:TARGET%はしっかりと奥までペニスを咥え込んだ
						CALL HPH_PRINT,@"「――んん、ん、ぶ……ふむ、ん……っHPH」","L"
						PRINTFORMDW %CALLNAME:TARGET%は吐き出される精液をこぼさないよう、きゅっと口を窄めている……
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「んん……っHPH」","L"
						PRINTFORMDL あなたが息継ぎのために口を離すと、%CALLNAME:TARGET%が不満そうに秘所を押し付けてきた
						PRINTFORMDW あなたは『ごめんごめん』と尻を撫でてやると、再び%CALLNAME:TARGET%の秘所を舌で舐め上げた
					CASE 1
						CALL HPH_PRINT,@"「ん、んぅっHPH ぁぁ……舌が……っHPH」","L"
						PRINTFORMDL 膣に舌を挿入すると、%CALLNAME:TARGET%は悦びに腰を震わせた
						PRINTFORMDW あなたは%CALLNAME:TARGET%の腰を抱き竦めると、優しく撫でるように、舌の腹を膣壁に擦り付けた……
					CASE 2
						CALL HPH_PRINT,@"「では……今度は、私が下に……HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%が下に、あなたが上になり舐め合いを再開する
						PRINTFORMDW %CALLNAME:TARGET%はまるで乳を飲む子牛のように、あなたのペニスに吸い付いている……
				ENDSELECT
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「くぷ……ちゅ……っHPH っ、ぅんんHPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%の膣に舌を侵入させ、ザラザラとした舌の腹で何度も粘膜を擦り上げる
						PRINTFORMDW %CALLNAME:TARGET%は快楽を少しでも逃がそうと、強くあなたの脚を掴んでいる……
					CASE 1
						CALL HPH_PRINT,@"「はぁ……む……っHPH んむ、ぅん……っHPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%は中ほどまでペニスを咥えると、顔を上下に揺すりだした
						PRINTFORMDW あなたは下半身が熱くなっていく感覚を感じながら、負けじと%CALLNAME:TARGET%の秘裂に舌を這わせた……
					CASE 2
						CALL HPH_PRINT,@"「――もっと……舐めろ、って……っHPH」","L"
						CALL HPH_PRINT,@"「そうして欲しいなら……貴方こそ、舐めるのを……っHPH」","L"
						PRINTFORMDL なんだ物足りなかったのか。と、あなたは%CALLNAME:TARGET%の膣に舌をねじ込んだ
						CALL HPH_PRINT,@"「な、ちがっHPH んぅっHPH くふぁっHPH はぁんっHPH」","W"
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ん、ん……っHPH ふぅ……んぷ……っHPH」","L"
						PRINTFORMDL 互いの前に性器を晒し、それを舐め合うという行為
						PRINTFORMDW %CALLNAME:TARGET%はまだ恥ずかしいのか、腰が若干引き気味だ……
					CASE 1
						CALL HPH_PRINT,@"「ちゅ……ぷぁっHPH ちょっと……大人しく……っHPH」","L"
						PRINTFORMDL 温かい舌が這い回る感触にどうしても腰が動いてしまう
						PRINTFORMDW あなたは%CALLNAME:TARGET%に謝ると、目先の秘裂を舐めることに意識を集中させた
					CASE 2
						CALL HPH_PRINT,@"「ふぅ……んふ……っHPH んぷ……ちゅ……っHPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%はペニスの先のほうを咥え、唇で亀頭を扱いている
						PRINTFORMDW 奥まで咥えるのは抵抗があるからそうしているのだろうが、これが意外と気持ち良い……
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
