@M_KOJO_EVENT_K98_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		TCVAR:今日会ったか = 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			;恋慕
			IF TALENT:恋慕
				GOTO YORIHIME_OHAYO
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			;恋慕
			IF TALENT:恋慕
				GOTO YORIHIME_OHAYO
			ENDIF
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		$YORIHIME_OHAYO
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「おはようございます」
				PRINTFORML 「さあ、早く着替えを済ませてください」
				PRINTFORMDW あなたは%CALLNAME:TARGET%に急かされながら朝の準備を終えた
			CASE 1
				PRINTFORML 「どうぞ。目が覚めますよ」
				PRINTFORMDL あなたは礼を言って、%CALLNAME:TARGET%からコップを受け取った
				PRINTFORMDW それを一気に呷る。冷たい水がぼやけた頭をシャキッと引き締めた
			CASE 2
				PRINTFORML 「寝坊ですよ。%CALLNAME:MASTER%」
				PRINTFORMDL ふぁ～あ、と大きなあくびをしながらボリボリとお腹を掻く
				PRINTFORMDW それを%CALLNAME:TARGET%がだらしないと叱る。いつもの日常だ
		ENDSELECT
		TCVAR:今日会ったか = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMDL あなたは布団を掛け直すと%CALLNAME:TARGET%の頭を優しく撫でた
				PRINTFORML 「ん……ぅ……」
				PRINTFORMDW %CALLNAME:TARGET%は嫌そうに寝返りを打つと、再び寝息を立て始めた……
			CASE 1
				PRINTFORML 「すぅ……すぅ……」
				PRINTFORMDL %CALLNAME:TARGET%は静かに寝息を立てている
				PRINTFORMDW 布団から抜け出したあなたは%CALLNAME:TARGET%を起こさないように１日の準備を始めた
			CASE 2
				PRINTFORML 「……%CALLNAME:MASTER%……」
				PRINTFORMDL 名前を呼ばれたので振り返ると%CALLNAME:TARGET%はまだ寝息を立てていた
				PRINTFORMDW 夢でも見ているのだろうか。%CALLNAME:TARGET%の寝顔はとても幸せそうだ
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		TCVAR:今日会ったか = 1
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;共通
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「おはようございます……」
					PRINTFORMDL %CALLNAME:TARGET%はなんだか寝足りなさそうだ
					PRINTFORMDW あなたはコップに冷たいお茶を注ぐと、彼女に差し出した
				CASE 1
					PRINTFORML 「…………」
					PRINTFORMDL まだ頭が寝ぼけているのか、%CALLNAME:TARGET%はボーッと床を眺めている
					PRINTFORML 「……よし、と」
					PRINTFORMDW だがそれも数秒で、頬を叩くとすぐに支度を始めた
				CASE 2
					PRINTFORML 「おはようございます」
					PRINTFORML 「……あの、%CALLNAME:MASTER%？」
					PRINTFORMDL 『続けて？』とあなたは%CALLNAME:TARGET%に着替えを催促した
					PRINTFORMW 「もう……」
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「……お見苦しいところをお見せしました……」
					PRINTFORMDL %CALLNAME:TARGET%はとても気まずそうにしている
					PRINTFORMDW 可愛い寝顔が見れたので、こちらとしてはとても良かった
				CASE 1
					PRINTFORML 「…………」
					PRINTFORMDL 暫くしてから戸を開けると、%CALLNAME:TARGET%が布団の上で気まずそうに正座していた
					PRINTFORMDW なんというか、真面目すぎる……
				CASE 2
					PRINTFORML 「あの、このことはお姉様には……」
					PRINTFORMDL 別に言いふらしたりしないから安心して欲しい
					PRINTFORMDW そう言うと、%CALLNAME:TARGET%はほっと息を吐いた
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

