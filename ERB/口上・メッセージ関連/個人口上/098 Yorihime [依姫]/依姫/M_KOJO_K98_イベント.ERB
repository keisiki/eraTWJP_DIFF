;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K98_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		$YORIHIME_ENCOUNTER
		IF !TCVAR:[[依姫]]:今日会ったか
			;恋人
			IF TALENT:恋人
				;TIME:2で朝昼晩を選択
				;笑顔絵を表示（オプションでオンにしていないと表示されない）
				CALL PRINT_FACE, [[依姫]], "笑顔", "服", "", ""
				SELECTCASE TIME:2
					;朝
					CASE 1,2
						PRINTFORML 「おはようございます。%CALLNAME:MASTER%」
						PRINTFORML 「さあ、どうぞこちらに。朝食の用意ができていますよ」
						PRINTFORMDW そんなつもりはなかったが、朝食をご馳走になってしまった
					;昼～夕
					CASE 3,4,5
						PRINTFORML 「お疲れ様です。%CALLNAME:MASTER%」
						PRINTFORML 「少し待っていてください。これを片付けたら……」
						PRINTFORMDW あなたは座って%CALLNAME:TARGET%の仕事が終わるのを待った
					;夜
					CASE 6,7
						PRINTFORML 「――――あっ」
						PRINTFORML 「%CALLNAME:MASTER%。今日はもういらっしゃらないかと……」
						PRINTFORMDW どことなく暗い表情をしていた%CALLNAME:TARGET%だが、あなたを見つけるとすぐに明るい表情になった
				ENDSELECT
			;恋慕
			ELSEIF TALENT:恋慕
				PRINTFORML 「%CALLNAME:MASTER%、お待ちしていましたよ」
				PRINTFORMDL 挨拶をすると、%CALLNAME:TARGET%がこちらまで来てくれた
				PRINTFORMDW どことなく嬉しそうに見えるのは気のせいだろうか？
			;思慕
			ELSEIF TALENT:思慕
				PRINTFORML 「次の稽古は……おや」
				PRINTFORML 「%CALLNAME:MASTER%、何かご用ですか？」
				PRINTFORMDL %CALLNAME:TARGET%がこちらに振り返り、笑みを作る
				PRINTFORMDW %CALLNAME:TARGET%と話していた玉兎はペコリとお辞儀をすると、足早に去っていった
			;それ以外
			ELSE
				PRINTFORML 「ああ、いらっしゃったのですね」
				PRINTFORML 「お姉様が貴方のことを探していましたよ」
				PRINTFORMDW %CALLNAME:TARGET%はそれだけ言うと、自分の仕事に戻った
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF !TCVAR:[[依姫]]:今日会ったか
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 「ここにいらっしゃったのですね」
				PRINTFORMDL %CALLNAME:TARGET%はあなたを見留めると小走りで駆け寄った
				PRINTFORMDW 『侍女みたいだ』とあなたが言うと、%CALLNAME:TARGET%は首を傾げた
			;思慕
			ELSEIF TALENT:思慕
				PRINTFORML 「%CALLNAME:MASTER%？ 今日はいらっしゃらないかと」
				PRINTFORMDL 難しい顔をしていた%CALLNAME:TARGET%だが、あなたを見留めるとすぐにそれも和らいだ
				PRINTFORMDW %CALLNAME:MASTER%は軽く手を上げて、%CALLNAME:TARGET%に挨拶を返した
			;それ以外
			ELSE
				PRINTFORMDL あなたは%CALLNAME:TARGET%に『おはよう』と挨拶した
				PRINTFORML 「おはようございます」
				PRINTFORMDW %CALLNAME:TARGET%はそっけなくあなたに挨拶を返した
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		;恋慕
		IF TALENT:恋慕
			PRINTFORMDL 一糸まとわぬ姿の%CALLNAME:TARGET%に、あなたは思わず見とれてしまう
			PRINTFORML 「……%CALLNAME:MASTER%？ 入ってもいいですか？」
			PRINTFORMDW あなたは頷くと、%CALLNAME:TARGET%のスペースを空けた
		;それ以外
		ELSE
			PRINTFORML 「だ、誰も一緒に入りたいだなんて言ってません」
			PRINTFORML 「わ、私は！ お姉様に貴方の背中を流してこいと言われたから……」
			PRINTFORMDW 分かった分かった。そう言うようにあなたは%CALLNAME:TARGET%に手招きをした
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 「あ……」
			PRINTFORMDL 全裸の二人の目が合った瞬間、温かいお風呂の空気が一瞬で凍り付く
			PRINTFORMDW 『えっと』あなたがそう言った瞬間、%CALLNAME:TARGET%は顔を真っ赤にして戸を閉めた
		;それ以外
		ELSE
			PRINTFORML 「か、鍵くらい掛けておいてくださいっ！」
			PRINTFORMDL %CALLNAME:TARGET%がバタンと戸を閉める
			PRINTFORMDW 脱衣所の時点で分かりそうなものだが……
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		;恋慕
		IF TALENT:恋慕
			PRINTFORMDL ……仕方ない。と、あなたは浴槽から立ち上がった
			PRINTFORML 「っ！？ ま、前を隠しなさいっ！」
			PRINTFORMDW %CALLNAME:TARGET%は顔を真っ赤にして慌てている。これでおあいこだ
		;それ以外
		ELSE
			PRINTFORML 「……出ていきなさい」
			PRINTFORMDL 胸と局部を手で隠した%CALLNAME:TARGET%が、あなたをキッと睨みつける
			PRINTFORMDW あなたは風呂桶で局部を隠しながら、スゴスゴと中腰で退散した
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		GOTO YORIHIME_ENCOUNTER
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;既に会っている場合は非表示
		IF !TCVAR:[[依姫]]:今日会ったか
			;霊夢とデート
			IF FLAG:デート相手 == 1
				PRINTFORMDL 霊夢が手を引くのでその場を後にしようとすると……
				PRINTFORML 「――%CALLNAME:MASTER%？ と、そこにいるのは……」
				PRINTFORMDW 霊夢は『面倒臭いのに気付かれた』とでも言いたげな表情をしている……
			;永琳とデート
			ELSEIF FLAG:デート相手 == 72
				PRINTFORML 「えっ！？」
				PRINTFORML （八意様が……どうして……）
				PRINTFORMDW %CALLNAME:TARGET%は思わず落とした書類を集めながら、チラチラと二人を見ている……
			;豊姫とデート
			ELSEIF FLAG:デート相手 == 99
				PRINTFORML 「お姉様。いえ、問題は起きていません」
				PRINTFORML 「……いや、起きているといえば起きているのですが……」
				PRINTFORMDW あなたと腕を組む豊姫を見て、%CALLNAME:TARGET%は「はぁ…」と溜息を吐いた
			;鈴仙orレイセンとデート
			ELSEIF FLAG:デート相手 == 52 || FLAG:デート相手 == 100
				PRINTFORML 「はぁ……義務も果たさずに遊んでいるなんて」
				PRINTFORML 「また稽古をつけてあげる必要がありそうですね」
				PRINTFORMDW \@ FLAG:デート相手 == 52 ? 鈴仙 # レイセン \@はブルブルと震えている。誘った手前、ちょっと申し訳なかった
			;清蘭or鈴瑚とデート
			ELSEIF FLAG:デート相手 == 107 || FLAG:デート相手 == 108
				PRINTFORML 「……最近、兎達の規律が緩んでいるとは思っていたけど」
				PRINTFORML 「はぁ……まさか貴女たちまでなんて」
				PRINTFORMDW %CALLNAME:TARGET%が疲れた様子で目頭を押さえる。\@ FLAG:デート相手 == 107 ? 清蘭 # 鈴瑚 \@は何故か口笛を吹いている……
			;純狐orヘカーティアとデート
			ELSEIF FLAG:デート相手 == 112 || FLAG:デート相手 == 113
				PRINTFORML 「なっ……」
				PRINTFORML （どうして……%CALLNAME:MASTER%が『アレ』と……）
				PRINTFORMDW %CALLNAME:TARGET%は驚き、目を見開いている……
			;それ以外
			ELSE
				;恋慕
				IF TALENT:恋慕
					CALL PRINT_FACE, [[依姫]], "通常", "服", "", ""
					PRINTFORML 「あんなに仲良く……」
					CALL PRINT_FACE, [[豊姫]], "笑顔", "服", "", ""
					CALL M_KOJO_COLOR_K99
					PRINTFORML 「羨ましいわねぇ……」
					CALL PRINT_FACE, [[依姫]], "怒り", "服", "", ""
					CALL M_KOJO_COLOR_K98
					PRINTFORML 「いえ、別に……」
					PRINTFORMDW 他人とデートをするあなたを見て、%CALLNAME:TARGET%はモヤモヤとしたものを感じていた
					RESETCOLOR
				;それ以外
				ELSE
					PRINTFORML 「…………」
					PRINTFORMDW %CALLNAME:TARGET%は我関せずを貫いている
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
	;ARG:1にデート相手、ARG:1が0の場合は非デート
	;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1とデート中
		IF ARG:1
		;一人で外出
		ELSE
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

