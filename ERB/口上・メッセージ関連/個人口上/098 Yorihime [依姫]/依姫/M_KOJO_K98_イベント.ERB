	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF ARG:1
			;デート中に眠くて帰って寝る（拠点キャラのみ）
			PRINTFORMW 
		ELSEIF CFLAG:うふふ
			;うふふ中に眠くて自室に帰って寝る（拠点キャラのみ）
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
