		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（～は眠っている）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
