			FISH_EXP:(TFLAG:193) ++
		ENDIF
		;説明を流す
		SELECTCASE TFLAG:193
			CASE 1 TO 20, 22 TO 60
				DRAWLINE
				PRINTFORML %LOCALS%
				CALL FISHING_EXPLANATION(TFLAG:193)
			CASE IS > 61
				DRAWLINE
				PRINTFORML %LOCALS%
				PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
		GOTO YORIHIME_FISH
	ENDIF
