;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K98_605
RETURN 0

@M_KOJO_MESSAGE_COM_K98_605
;寄り道するにはTRAIN_MESSAGEが無い
CALL M_KOJO_MESSAGE_COM_K98_605_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K98_605_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;恋慕
	IF TALENT:恋慕
		SELECTCASE RAND:5
			CASE 0
				PRINTFORMDL 寄り道をしていると、綺麗な毛並みの猫が足元に擦り寄ってきた
				PRINTFORML （か、可愛いっ……でも持って帰るのは……）
				PRINTFORMDL %CALLNAME:TARGET%はしゃがんで猫の頭を撫でている
				PRINTFORMDW 人慣れしているようだ。地域猫というやつだろうか
			CASE 1
				PRINTFORMDL 寄り道の途中で迷子の子供を見つけた
				PRINTFORML 「ほら泣かないで。男の子でしょう？」
				PRINTFORMDL %CALLNAME:TARGET%がそう言うと、男の子はぐしぐしと鼻水まみれの顔を擦った
				PRINTFORMDW とりあえず、人里まで送り届けよう……
			CASE 2
				PRINTFORMDL 少し寄り道をして二人で景色を眺めた
				PRINTFORML 「叶わないこと、なのかもしれませんが……」
				PRINTFORML 「いつかお姉様や八意様、あなたも入れてみんなで……」
				PRINTFORMDW 幻想郷の眺めは、今日もとても良かった
			CASE 3
				PRINTFORMDW %CALLNAME:TARGET%は寄り道の途中で見つけた犬を可愛がっている
				PRINTFORMW 「ちょ、こらっ！ そこは……っ」
				PRINTFORMDL 犬は依姫を押し倒すと、なんとスカートの中に顔を突っ込んで匂いを嗅ぎ始めた！
				PRINTFORMDW けしからんヤツだ！ と犬を見ると、なんとオスではなくメスの犬だった
				PRINTFORMW 「ひぁっ！ ふ、ふふっ！ やめなさいったらっ！」
				PRINTFORMDL 犬は相変わらずフンフンと匂いを嗅いでいる
				PRINTFORMDW %CALLNAME:TARGET%も嫌がっていないようだし、まあいいか……
			CASE 4
				;１日１回だけストーキング中の豊姫と遭遇
				IF !TCVAR:[[依姫]]:350
					PRINTFORMDW 寄り道の途中、なぜか豊姫とすれ違った
					;笑顔絵を表示（オプションでオンにしていないと表示されない）
					CALL PRINT_FACE, [[豊姫]], "笑顔", "服", "", ""
					CALL M_KOJO_COLOR_K99
					PRINTFORMW 「あら偶然。二人とも、デートは楽しんでる？」
					;顔絵を表示（オプションでオンにしていないと表示されない）
					CALL PRINT_FACE, [[依姫]], "通常", "服", "", ""
					CALL M_KOJO_COLOR_K98
					PRINTFORMW 「ええまあ……」
					PRINTFORMW 「ってお姉様。兎達の稽古を見てくれているはずじゃ？」
					CALL PRINT_FACE, [[豊姫]], "通常", "服", "", ""
					CALL M_KOJO_COLOR_K99
					PRINTFORMW 「そうだったかしら？ そんな約束もしたような……」
					CALL PRINT_FACE, [[依姫]], "怒り", "服", "", ""
					CALL M_KOJO_COLOR_K98
					PRINTFORMW 「ついさっきの話ですよ……もう……」
					PRINTFORMDL 豊姫は%CALLNAME:TARGET%の説教を「はいはい」と適当に聞き流している
					PRINTFORMDW 本当に困ったお姉ちゃんだ……
					TCVAR:[[依姫]]:350 = 1
					RESETCOLOR
				;以降は下を読み込む
				ELSE
					GOTO YORIHIME_YORIMICHI
				ENDIF
		ENDSELECT
	;それ以外
	ELSE
		$YORIHIME_YORIMICHI
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「地上は穢れに満ちていて、同時に騒々しくもある。貴方は何も感じないのですか？」
				PRINTFORMDL 辺りを散策していると、風に乗って色々な音が聞こえてくる
				PRINTFORMDW あなたは何も感じないが、%CALLNAME:TARGET%はどこか落ち着かない様子だ
			CASE 1
				PRINTFORML 「ところで、一体どこまで……」
				PRINTFORMDL 特にこれといった目的はないが、少しでも長くデートを楽しみたい
				PRINTFORMDW あなたは適当にはぐらかしながら、%CALLNAME:TARGET%と連れ立って周囲を散策した
			CASE 2
				PRINTFORML 「――寄り道？ どこへ行くのです？」
				PRINTFORMDL ちょっとそこまで。とあなたは%CALLNAME:TARGET%と連れ立って歩いた
				PRINTFORMDW ……特にめぼしいものはなかった
			CASE 3
				PRINTFORML 「――？ 何か気になることでも？」
				PRINTFORMDL 気になると言えば、隣を歩く%CALLNAME:TARGET%のことはとても気になる
				PRINTFORMDW 彼女はこのデートを楽しんでくれているだろうか……
			CASE 4
				PRINTFORML 「――刀は重くないか？」
				PRINTFORML 「慣れていますから、それほどでも」
				PRINTFORMDW あなたと%CALLNAME:TARGET%は歩きながら色々なことを話した……
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

