			CALL PRINT_FACE, [[依姫]], "発情", "服", "", ""
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「な……ぇ……？」
					PRINTFORMDL いま自分が何をされたのか
					PRINTFORMDW それを理解すると同時に、%CALLNAME:TARGET%の顔がボンッと赤くなった
				CASE 1
					PRINTFORML 「あっ、ぅ……」
					PRINTFORML 「こんな……恥ずかしくないのですか……？」
					PRINTFORMDW %CALLNAME:TARGET%は恥ずかしそうに額をおさえている……
				CASE 2
					PRINTFORML 「ぁ……そこ、ですか……」
					PRINTFORMDL ホッとしたような、少し物足りないような
					PRINTFORMDW あなたが首を傾げると、%CALLNAME:TARGET%は恥ずかしそうに顔を背けた
			ENDSELECT
		;それ以外
		ELSE
			;発情顔絵を表示（オプションでオンにしていないと表示されない）
			CALL PRINT_FACE, [[依姫]], "発情", "服", "", ""
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL %CALLNAME:TARGET%の頭に手を回し、額に軽くキスをする
