#DIM 正解
#DIM 経験値
#DIM 制限時間
#DIM 倍率
#DIM FEINT
#DIM イカサマ
VARSET LOCAL
イカサマ = 0
制限時間 = 2000 - (ABL:[[依姫]]:戦闘能力 - ABL:MASTER:戦闘能力) * 100

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特訓デイリーからお借りしました
	CALL M_KOJO_COLOR_K98
	PRINTFORMDL 木剣を手にしたあなたと依姫が相対する
	PRINTFORML 「稽古だからといって気を抜かないように」
	PRINTFORML 「実戦だと思って、本気でかかってらっしゃい」
	PRINTFORMDW %CALLNAME:TARGET%も本気のようだ。さて……
	RESETCOLOR
	PRINTFORML 
	CALL ASK_YN("稽古開始！","逃げる！")
	IF RESULT
		CALL M_KOJO_COLOR_K98
		PRINTFORML 「えぇっ！？」
		PRINTFORMDL 逃げるが勝ち。つまり勝者は自分だ
		PRINTFORMDW あなたは唖然とする%CALLNAME:TARGET%を放置して、全力疾走で逃げ去った……
		RESETCOLOR
	ELSE
		正解 = RAND:4
		経験値 = MIN(ABL:[[依姫]]:戦闘能力 * 5, 5)
		SELECTCASE RAND:20
			CASE IS <= 11
			;普通に
				LOCALS:0 = 上
				LOCALS:1 = 右
				LOCALS:2 = 左
				LOCALS:3 = 下
				倍率 = 100
			CASE 12 TO 15
			;ローマ字
				LOCALS:0 = UE
				LOCALS:1 = MIGI
				LOCALS:2 = HIDARI
				LOCALS:3 = SHITA
				倍率 = 160
			;英語
			CASE 16,17,18
				LOCALS:0 = UP
				LOCALS:1 = RIGHT
				LOCALS:2 = LEFT
				LOCALS:3 = DOWN
				倍率 = 120
			;スペイン語
			CASE 19
				LOCALS:0 = encima de
				LOCALS:1 = a la derecha
				LOCALS:2 = a la izquierda
				LOCALS:3 = abajo de
				倍率 = 300
		ENDSELECT
		;あなたの戦闘能力が高いと５分の一の確率でフェイントをかけてくる
		IF ABL:MASTER:戦闘能力 >= 5 && !RAND:5
			FEINT = 1
			制限時間 += 400
			倍率 += 80
		ELSE
			FEINT = 0
		ENDIF
		CALL FISHER_YATES_SHAFFLE(4)
		PRINTFORMW %CALLNAME:TARGET%の攻撃を避けよう（クリックで開始）
		PRINTFORM %CALLNAME:TARGET%の
		SETCOLOR C_YELLOW
		IF FEINT
			PRINTFORM %LOCALS:(RAND:4)%
			RESETCOLOR
			PRINTFORM と見せかけて%LOCALS:(3 - 正解)%
		ELSE
			PRINTFORM %LOCALS:(3 - 正解)%
			RESETCOLOR
		ENDIF
		PRINTFORML からの攻撃！
		$LOOP_IKASAMA
		FOR LOCAL:10,0,4
			LOCAL = SHAFFLE_ARRAY:(LOCAL:10)
			PRINTFORML [{LOCAL}] - %LOCALS:LOCAL%に避ける
		NEXT
		SIF !イカサマ && BASE:MASTER:TSP > 500
			PRINTFORML [{4}] - 時間停止
		TINPUT 制限時間, -1, 1
		IF RESULT == 正解
			CALL M_KOJO_COLOR_K98
			IF !イカサマ
				PRINTFORMDL %CALLNAME:MASTER%は攻撃を回避した！
				PRINTFORMDL ……………
				PRINTFORMDL ………
				PRINTFORMDW …
				PRINTFORML 「ありがとうございました」
				PRINTFORMDL 稽古を終え、向かい合って一礼する
				PRINTFORMDL 汗だくのあなたに対して、澄まし顔の%CALLNAME:TARGET%は汗ひとつかいていない
				;うどんげっしょーネタ
				PRINTFORML 「筋は悪くありませんが、いくつか足りないものがありますね」
				PRINTFORML 「力に技に、速さにやる気に……」
				PRINTFORMDW %CALLNAME:TARGET%が指折り数える。とにかく、全部足りないことが分かった……
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は攻撃を回避しようとした……
				PRINTFORML 「――貴方も不思議な術を使うのですね」
				PRINTFORMDL 時間を止め、行動を先読みし――
				PRINTFORMDL 後ろを取った筈なのに、あなたは地に伏せていた
				PRINTFORML 「まあ、手品の種が分かっているのなら造作もないことですが……」
				PRINTFORMDL %CALLNAME:TARGET%の出した名前。それはあなたと同じ能力を持つメイドの名だった
				PRINTFORMDW 戦う前から勝負は決まっていたらしい。次からは正々堂々とやる必要がありそうだ……
				BASE:MASTER:体力 -= 1000
			ENDIF
			RESETCOLOR
			BASE:MASTER:体力 -= 100 * (1 + ABL:[[依姫]]:戦闘能力)
			BASE:MASTER:気力 -= 100 * (1 + ABL:[[依姫]]:戦闘能力)
			経験値 = 経験値 * 倍率 / 100
			EXP:MASTER:戦闘経験 += 経験値
			EXP:[[依姫]]:戦闘経験 += 経験値 * 2
			CFLAG:[[依姫]]:好感度 += 経験値 * 2
			CFLAG:[[依姫]]:信頼度 += 経験値 * 2
			PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:MASTER%）
			PRINTFORML 戦闘経験 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			PRINTFORML 好感度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			PRINTFORML 信頼度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
		ELSEIF RESULT == 4 && !イカサマ && BASE:MASTER:TSP > 500
			イカサマ = 1
			倍率 /= 5
			PRINTFORMW %CALLNAME:MASTER%は時間停止能力で攻撃を見極めた！
			GOTO LOOP_IKASAMA
		ELSE
			CALL M_KOJO_COLOR_K98
			PRINTFORMDW 失敗した……
			PRINTFORMDL 息を切らし跪くあなたに%CALLNAME:TARGET%が木剣を突き付ける
			PRINTFORMDL 降参だ。あなたは木剣を捨てると、仰向けに倒れ込んだ
			PRINTFORML 「休んでいる暇はありませんよ。次は走り込み。休憩はその後です」
			PRINTFORMDW 鬼だ、鬼がいる。あなたは悲鳴を上げる兎達の気持ちが少しだけ分かった気がした
			RESETCOLOR
			BASE:MASTER:体力 -= 500 * (1 + ABL:[[依姫]]:戦闘能力)
			BASE:MASTER:気力 -= 500 * (1 + ABL:[[依姫]]:戦闘能力)
			PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:TARGET%）
			EXP:[[依姫]]:戦闘経験 += 経験値
			CFLAG:[[依姫]]:好感度 += 経験値 / 2
			CFLAG:[[依姫]]:信頼度 += 経験値 / 2
			PRINTFORML 好感度 ＋{経験値}（%CALLNAME:TARGET%）
			PRINTFORML 信頼度 ＋{経験値}（%CALLNAME:TARGET%）
		ENDIF
	ENDIF
ENDIF
RETURN 1
