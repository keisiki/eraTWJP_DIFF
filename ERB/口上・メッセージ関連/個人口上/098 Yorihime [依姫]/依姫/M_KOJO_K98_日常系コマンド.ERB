@M_KOJO_MESSAGE_COM_K98_368_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE Activity_Type:TARGET
		CASE 0
			;▼考え事［なに考えてるの］
			PRINTFORMDL あなたは%CALLNAME:TARGET%に何か悩み事かと尋ねた
			PRINTFORMDW 
			;恋慕
			IF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「実は、今度の宴の幹事を任されてしまって……」
						PRINTFORML 「はぁ……本当はお姉様がやるはずだったのに」
						PRINTFORMDW のらりくらりと豊姫が逃げるので、結局%CALLNAME:TARGET%がやる羽目になったようだ……
					CASE 1
						PRINTFORML 「%TEXTR("兎達の稽古/月の防衛/お姉様の体重/")%について、少し……」
						PRINTFORMDL あなたは%CALLNAME:TARGET%に悩みを相談された
						PRINTFORMDW 自分なりの意見を伝えると、%CALLNAME:TARGET%は納得したように頷いた
					CASE 2
						PRINTFORML 「悩み、というか……」
						PRINTFORML 「……私って、兎達に嫌われてるんでしょうか？」
						PRINTFORMDW 何があったかは知らないが、そんなことはないと%CALLNAME:TARGET%を慰めた
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「いえ、別に大したことでは」
						PRINTFORML 「――気になる？ だから大したことではないと言っているでしょう」
						PRINTFORMDW 何か悩んでいるようだが%CALLNAME:TARGET%は教えてくれなかった
					CASE 1
						PRINTFORML 「貴方に言う必要はないと思いますが」
						PRINTFORML 「……大したことではありません。本当です」
						PRINTFORMDW %CALLNAME:TARGET%にはぐらかされてしまった……
					CASE 2
						PRINTFORML 「月の防衛、兎達の稽古、お姉様の栄養管理」
						PRINTFORML 「それに貴方の監視。ええ、確かに悩み事はあります。それも山ほどに」
						PRINTFORMDW 「だから大人しくしていてください」と%CALLNAME:TARGET%に釘を刺されてしまった
				ENDSELECT
			ENDIF
		CASE 2
			;▼食事［一緒に食事する］
			PRINTFORMDL あなたは%CALLNAME:TARGET%と一緒に食事を取ることにした
			PRINTFORMDW 
			;恋慕
			IF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMDL %CALLNAME:TARGET%は皿の端っこにピーマンをよけている。もしかして嫌いなのだろうか
						PRINTFORML 「いや、食べますよ？ 食べますけど……」
						PRINTFORML 「今はまだ、その時ではないというか……」
						PRINTFORMDW あなたがじっと見つめると、%CALLNAME:TARGET%は渋々よけていたピーマンを食べた……
					CASE 1
						PRINTFORMDL 『おかわり』そう言って、あなたは茶碗を%CALLNAME:TARGET%に差し出した
						PRINTFORML 「どれくらいですか？ ――はいはい、大盛りですね」
						PRINTFORMDW %CALLNAME:TARGET%は慣れた様子で、茶碗に白米を盛りつけた……
					CASE 2
						PRINTFORMDL %CALLNAME:TARGET%の食べているものが凄く美味しそうだったので、あなたは横から箸でかっさらって食べた
						PRINTFORML 「ぇ、あぁあっ！？」
						PRINTFORMDW 『見た目ほど美味しくない』あなたがそう言うと、%CALLNAME:TARGET%に恨みがましい目で睨まれた
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「あむ……ん……」
						PRINTFORML 「……？ 食べないのですか？」
						PRINTFORMDW %CALLNAME:TARGET%と目が合う。食べる所作が美しいので、つい眺めてしまった……
					CASE 1
						PRINTFORMDL 言葉は交わさず、黙々と料理を食べ進める
						PRINTFORML 「ふぅ……ごちそうさまでした」
						PRINTFORMDW 『ごちそうさまでした』あなたは%CALLNAME:TARGET%と同時に手を合わせた
					CASE 2
						PRINTFORML 「はぁ……まったくもう……」
						PRINTFORMDL %CALLNAME:TARGET%の方をちらりと見る。なんだかおかずの種類が少ないような……
						PRINTFORML 「お姉様ったら、どうして毎回私の好物を……」
						PRINTFORMDL %CALLNAME:TARGET%はブツブツと文句を言いながら箸をすすめている
						PRINTFORMDW 豊姫におかずをつまみ食いされてしまったらしい。あなたは少しだけ%CALLNAME:TARGET%に同情した
				ENDSELECT
			ENDIF
		CASE 4
			;▼掃除［掃除を手伝う］
			PRINTFORMDL あなたは%CALLNAME:TARGET%の掃除を手伝うことにした
			PRINTFORMDW 
			;恋慕
			IF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「まずはゴミ出しをお願いします」
						PRINTFORML 「それが終わったら、このバケツに水を……」
						PRINTFORMDW あなたは%CALLNAME:TARGET%の指示に従ってテキパキと掃除を進めた
					CASE 1
						PRINTFORML 「私はここを片付けますから、貴方は……」
						PRINTFORMDL %CALLNAME:TARGET%の指示に従って掃除を進めていく
						PRINTFORMDW 途中、高いところの掃除に苦戦している%CALLNAME:TARGET%を手伝ったりした
					CASE 2
						PRINTFORMDL もっとこう、神様を降ろしてバーッと片付けられないものだろうか
						PRINTFORML 「名案ですね。じゃあ『伊豆能売』よ！ 私に代わって埃を掃え！」
						PRINTFORMDL %CALLNAME:TARGET%が手を上げると、神楽鈴……ではなくハタキを持った巫女が現れた
						PRINTFORMDW いや、冗談だったんだが……いいのかな……
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「ちょっと届きそうもないわね……仕方ない」
						PRINTFORMDL %CALLNAME:TARGET%は刀の鞘にハタキをくくりつけると、それで埃をはたき始めた
						PRINTFORMDW 台とか用意すればいいのにと思ったが、あなたは敢えて何も言わなかった
					CASE 1
						PRINTFORML 「私がゴミを集めるので、貴方は分別の方をお願いします」
						PRINTFORMDL あなたは頷くと、%CALLNAME:TARGET%が集めたゴミを丁寧に仕分けした
						PRINTFORMDW ……けっこう疲れた
					CASE 2
						PRINTFORML 「ここはもう大丈夫です。貴方は別の場所を」
						PRINTFORMDL 協力したおかげでスムーズに掃除を進めることができた
						PRINTFORMDW やはり、汚れているところを綺麗にするのは気持ちが良い
				ENDSELECT
			ENDIF
		CASE 5
			;▼運動［一緒に運動する］
			PRINTFORMDL あなたは%CALLNAME:TARGET%に声を掛けて一緒に運動することにした
			PRINTFORMDW 
			;恋慕
			IF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「２９７……２９８……２９９……っ！」
						PRINTFORMDL あなたは腕立てをする%CALLNAME:TARGET%の背中に乗っている
						PRINTFORMDW ここまで一切フォームが崩れていないのは、やはり普段の彼女の努力の賜物か
					CASE 1
						PRINTFORMDL あなたは%CALLNAME:TARGET%とフルマラソンの距離を走り切った
						PRINTFORML 「お疲れ様です。ではトレーニングを始めましょう」
						PRINTFORMDW 『え？』と顔を上げると、%CALLNAME:TARGET%はもう腕立て伏せを始めていた……
					CASE 2
						PRINTFORMDL あなたは%CALLNAME:TARGET%に護身術の手ほどきを受けた
						PRINTFORML 「相手がこう来たら……こうして、こうっ！」
						PRINTFORMDW %CALLNAME:TARGET%があなたにアームロックを決める。もしこれが本気だったら腕を折られていたかもしれない……
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「はっ……はっ……」
						PRINTFORMDL %CALLNAME:TARGET%が軽くランニングするというのであなたはそれに付き合った
						PRINTFORMDW ……ところで、あと何キロ走ればいいんだろう……
					CASE 1
						PRINTFORML 「じゃあ、この岩をあそこまで押してきてください」
						PRINTFORMDL いや無理だろ……
						PRINTFORMW 「私はできますけど……」
					CASE 2
						PRINTFORML 「まずは基本の腕立てから」
						PRINTFORML 「回数は……とりあえず３００回から始めましょうか」
						PRINTFORMDW %CALLNAME:TARGET%と一緒に腕立てをすることになった……が、地獄だった……
				ENDSELECT
			ENDIF
		CASE 6
			;▼読書［本の感想を聞く］
			PRINTFORMDL あなたは%CALLNAME:TARGET%に本の感想を尋ねた
			PRINTFORMDW 
			;恋慕
			IF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「まあ悪くはありませんね……ところで」
						PRINTFORML 「……この主人公って、この後どうなっちゃうんですか！？」
						PRINTFORMDW そう言う%CALLNAME:TARGET%に、あなたは敢えて意味深に笑ってみせた
					CASE 1
						;笑顔絵を表示（オプションでオンにしていないと表示されない）
						CALL PRINT_FACE, [[依姫]], "笑顔", "服", "", ""
						PRINTFORML 「ふふっ、ふふふっ。か、感想は後でいいますからっ」
						PRINTFORMDL %CALLNAME:TARGET%は熱心にギャグ漫画を読んでいる
						PRINTFORMDW 最近、漫画ばかり読んでいるような……
					CASE 2
						PRINTFORML 「兎達の間で流行っているというので、レイセンから借りたのですが……」
						PRINTFORML 「うーん……このキャラクター、少し目が大きすぎやしませんか？」
						PRINTFORMDW %CALLNAME:TARGET%が読んでいたのは、女児が読むような漫画本だった……
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORM 「……
						CALL COLORMESSAGE(@"（ぺらり）",C_WHITE,0,1)
						PRINTFORML ……」
						PRINTFORMDL 集中しているのか返事が返ってこない
						PRINTFORMDW そんなに面白いのだろうか。後で読んでみよう
					CASE 1
						PRINTFORML 「面白いか、面白くないかという理由で本は読みません」
						PRINTFORML 「ためになるか、ならないかです」
						PRINTFORMDW ためになったかとあなたが聞くと、%CALLNAME:TARGET%は首を横に振った
					CASE 2
						PRINTFORML 「気になるのでしたら、どうぞ」
						PRINTFORMDL 丁度読み終わったのか、読んでいた本を%CALLNAME:TARGET%が差し出してきた
						PRINTFORMDW 試しに読んでみる。難しすぎて内容が頭に入ってこなかった
				ENDSELECT
			ENDIF
		CASE 7
			;▼料理［料理を手伝う］→［料理を分けてもらう］
			;料理を手伝う
			IF TFLAG:193 == 2
				PRINTFORMDL あなたは%CALLNAME:TARGET%の料理を手伝った
				PRINTFORMDW 
				;恋慕
				IF TALENT:恋慕
					SELECTCASE RAND:3
						CASE 0
							PRINTFORMDL %CALLNAME:TARGET%に頼まれ食材を切る。一人分にしては量が多いような……
							PRINTFORML 「私とお姉様、あと貴方をいれて三人分です」
							PRINTFORMDW なるほどと頷く。そんな会話をしている内に食材のカットも終わった
						CASE 1
							PRINTFORML 「いたっ……すみません、絆創膏を……」
							PRINTFORMDL %CALLNAME:TARGET%の手を取り傷を確認する。血は出ているが浅そうだ
							PRINTFORML 「%CALLNAME:MASTER%？ ――えっ！？」
							PRINTFORMDW これくらいなら舐めれば治る。あなたは%CALLNAME:TARGET%の指を咥えると、労わるように傷口を舐めた
						CASE 2
							PRINTFORML 「今日は少し味付けを変えてみようかと……」
							PRINTFORMDL あなたは%CALLNAME:TARGET%と雑談しながら料理を手伝った
							PRINTFORMDW 味見と称してつまみ食いしていたら、けっこうお腹も膨れた
					ENDSELECT
				;それ以外
				ELSE
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「でしたら、お皿の用意をお願いします」
							PRINTFORMDL あなたは%CALLNAME:TARGET%に頷くと、食器棚のほうに向かった
							PRINTFORMDW 作りすぎたというので、あなたは少しだけ料理を分けてもらった
						CASE 1
							PRINTFORML 「――ちょっと分けて欲しい？ それは貴方の頑張り次第ですね」
							PRINTFORMDL 働かざる者食うべからず。シンプルで分かりやすい
							PRINTFORMDW あなたは腕を捲ると、張り切って%CALLNAME:TARGET%の料理を手伝った
						CASE 2
							PRINTFORMDL あなたは食材をカットしている最中、うっかり指を切ってしまった
							PRINTFORML 「見せてください。……ん、見た目ほど酷くはなさそうね。いま手当を……」
							PRINTFORMDL %CALLNAME:TARGET%が救急箱を取りに台所を離れる
							PRINTFORMDW 指を舐めて癒してくれる展開を期待していたが、現実はそう甘くはなかった
					ENDSELECT
				ENDIF
			;料理を分けてもらう
			ELSE
				PRINTFORMDL あなたは%CALLNAME:TARGET%の作った料理を分けてもらった
				PRINTFORMDW 
				;恋慕
				IF TALENT:恋慕
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「丁度良かった。貴方に味見をお願いしたくて……」
							PRINTFORMDL 味付けを変えてみたというのであなたは試食係になった
							PRINTFORMDW 良いも悪いも、%CALLNAME:TARGET%はしっかりとメモに取っている……
						CASE 1
							PRINTFORML 「そう言われると思っていたので、取り分けておきましたよ」
							PRINTFORMDL 料理中、ずっとソワソワしていたのがバレていたらしい
							PRINTFORMDW あなたは礼を言って、ありがたく%CALLNAME:TARGET%の手料理をいただいた
						CASE 2
							PRINTFORML 「お皿はそのままにしておいてください。私が片付けます」
							PRINTFORMDL 料理を分けてもらっておいて何もしないのは申し訳ない
							PRINTFORMDW あなたが机掃除を願い出ると、%CALLNAME:TARGET%が絞った雑巾を持ってきてくれた
					ENDSELECT
				;それ以外
				ELSE
					SELECTCASE RAND:3
