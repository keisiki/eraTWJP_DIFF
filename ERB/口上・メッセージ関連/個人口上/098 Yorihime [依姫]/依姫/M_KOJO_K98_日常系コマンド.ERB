@M_KOJO_MESSAGE_SCOM_K98_60_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;恋慕
	IF TALENT:恋慕
		;判定
		SELECTCASE TFLAG:193
			;大成功
			CASE 1
				PRINTFORML 「――%CALLNAME:MASTER%？ %CALLNAME:MASTER%？」
				PRINTFORMDL %CALLNAME:TARGET%に肩を揺すられパチッと目を覚ます
				PRINTFORMDW ウトウトしている間に耳掃除が終わっていたようだ……
			;成功
			CASE 0
				PRINTFORML 「……～♪」
				PRINTFORMDL %CALLNAME:TARGET%は耳かきを動かしながら何やら鼻歌を唄っている
				PRINTFORMDW 好きな曲なのだろうか？ 何やら懐かしい感じがする……
			;失敗
			CASE -1
				PRINTFORML 「ちょ、ちょっと！ 動いたら……あっ」
				PRINTFORMDL うっかり手を滑らせた%CALLNAME:TARGET%が、あなたの耳に耳かきを突き刺してしまう
				PRINTFORMDW シーンと静かになったあなたを見て、%CALLNAME:TARGET%は辺りを見回した後、何食わぬ顔で耳掃除を再開した
		ENDSELECT
	;それ以外
	ELSE
		;判定
		SELECTCASE TFLAG:193
			;大成功
			CASE 1
				PRINTFORML 「――お姉様にもしているのか？」
				PRINTFORML 「そうですね、頼まれれば……」
				PRINTFORMDW %CALLNAME:TARGET%の耳掃除が気持ち良くて、あなたはついウトウトしてしまった
			;成功
			CASE 0
				PRINTFORML 「こら、大人しくしていなさい」
				PRINTFORMDL %CALLNAME:TARGET%はあなたの頭を軽く叩くと、耳掃除を再開した
				PRINTFORMDW 頬擦りできないのは残念だが、これはこれでいいものだ
			;失敗
			CASE -1
				PRINTFORML 「あっ……」
				PRINTFORMDL なにが『あっ』なのだろう
				PRINTFORMDW %CALLNAME:TARGET%が何も言わないので、あなたはとても不安になった
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
