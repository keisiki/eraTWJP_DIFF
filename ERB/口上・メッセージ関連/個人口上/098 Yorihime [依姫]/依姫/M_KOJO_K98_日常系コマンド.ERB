@M_KOJO_MESSAGE_COM_K98_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;会話不能
	IF TCVAR:302
		;顔絵を表示（オプションでオンにしていないと表示されない）
		;お風呂
		IF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
			CALL PRINT_FACE, [[依姫]], "通常", "裸", "", ""
		;それ以外
		ELSE
			CALL PRINT_FACE, [[依姫]], "通常", "服", "", ""
		ENDIF
		;親密５以上
		IF ABL:親密 > 4
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「その……」
					PRINTFORMDL %CALLNAME:TARGET%は何か話題を振ろうとしている
					PRINTFORMDW が、結局気まずい空気になってしまった……
				CASE 1
					PRINTFORML 「――好きな食べ物、ですか？」
					PRINTFORML 「ぁ……桃は、よく好んで食べますけど……」
					PRINTFORMDW 気まずい空気をなんとかしようとしたが、余計に気まずくなってしまった……
				CASE 2
					PRINTFORML 「少し……喉が渇きましたね」
					PRINTFORMDL そうだね。とあなたは%CALLNAME:TARGET%に相槌を返す
					PRINTFORMDW ……何故か、%CALLNAME:TARGET%にため息をつかれてしまった……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「話は終わりましたか？」
					PRINTFORMDW 地上での体験を色々と話してみたが、依姫は興味を持てなかったようだ……
				CASE 1
					PRINTFORML 「…………」
					PRINTFORMDW %CALLNAME:TARGET%は話すあなたに目もくれず手持ちの古書型端末を弄っている……
				CASE 2
					PRINTFORML 「――気まずい？ そう感じているのは貴方だけでは？」
					PRINTFORMDL じゃあ依姫は楽しんでくれている……というわけではなさそうだ
					PRINTFORMDW きっと、どうでもいいのだろう……
			ENDSELECT
		ENDIF
	;通常
	ELSE
		;恋人
		IF TALENT:恋人
			SELECTCASE RAND:10
				CASE 0
					PRINTFORML 「貴方は私が護ります。必ず、この身に代えても」
					PRINTFORMDL 恋人になった%CALLNAME:TARGET%は改めてそうあなたに宣言した
					PRINTFORMDW 誰に言われたからとかではなく、これが彼女の意志なのだろう
				CASE 1
					PRINTFORML 「少しはお姉様にもちゃんとして欲しいものです」
					PRINTFORML 「……そうしたら、貴方に寄り添っていられる時間がもっと増えるのに」
					PRINTFORMDW %CALLNAME:TARGET%はそう言うと、甘えるようにあなたの肩にもたれかかった
				CASE 2
					PRINTFORML 「――旦那様と呼んで欲しい？」
					PRINTFORML 「ええ、いいですよ。旦那様♪」
					PRINTFORMDL 屈託のない笑顔で%CALLNAME:TARGET%がそう言う
					PRINTFORMDW 恥ずかしがらせようと思ったのだが、恥ずかしい気持ちになったのはあなたの方だった
