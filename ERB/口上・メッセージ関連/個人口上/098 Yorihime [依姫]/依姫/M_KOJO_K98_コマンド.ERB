;特殊コマンド
;=================================================
;-------------------------------------------------
;180,ローション
;-------------------------------------------------
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K98_180
RETURN 0

@M_KOJO_MESSAGE_COM_K98_180
CALL M_KOJO_MESSAGE_COM_K98_180_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K98_180_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		GOTO YORIHIME_LUBRICANT
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;発情顔絵を表示（オプションでオンにしていないと表示されない）
		IF !TEQUIP:0 && !TEQUIP:1
			CALL PRINT_FACE, [[依姫]], "発情", "裸", "", ""
		ELSE
			CALL PRINT_FACE, [[依姫]], "発情", "服", "", ""
		ENDIF
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「んん……ん……っHPH」","L"
					CALL HPH_PRINT,@"（凄く……触り方、厭らしい……HPH）","L"
					PRINTFORMDL たっぷりと垂らしたローションを%CALLNAME:TARGET%の躰に塗り広げていく
					PRINTFORMDW 這い回る手の感触に、%CALLNAME:TARGET%は小さく鼻息を漏らしている……
				CASE 1
					CALL HPH_PRINT,@"「そんなところまで……くぅっHPH 塗らなくても……んっHPH」","L"
					PRINTFORMDL 脇の下やお尻の割れ目にまであなたはローションを塗りたくっていく
					PRINTFORMDW %CALLNAME:TARGET%は恥ずかしいのか、顔を紅潮させている……
				CASE 2
					CALL HPH_PRINT,@"「ま、まだやるのですか……？HPH」","L"
					PRINTFORMDL 手の平にたっぷりとローションを垂らし、%CALLNAME:TARGET%の躰に塗り付けていく
					PRINTFORMDW もちろん大事なところは念入りに。あなたは%CALLNAME:TARGET%の躰をローションまみれにした
			ENDSELECT
		;それ以外
		ELSE
			$YORIHIME_LUBRICANT
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL あなたは自分にも塗って欲しいと%CALLNAME:TARGET%にローションのボトルを手渡した
					PRINTFORML 「分かりました……」
					PRINTFORMDW %CALLNAME:TARGET%は手の平にローションを垂らすと、塗り付けるようにあなたの肌を手で撫でた……
				CASE 1
					PRINTFORML 「んっ……」
					PRINTFORMDL ローションを垂らすと%CALLNAME:TARGET%が小さく声を上げた
					PRINTFORMDW 懐で温めておいたが、まだ冷たかったかもしれない
				CASE 2
					PRINTFORMDL まるまる一本を使って%CALLNAME:TARGET%の躰をローションまみれにしてやった
					PRINTFORML 「……後のことを考えると頭が痛いですね」
					PRINTFORMDW それは言わないお約束だ
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

