;ハードな調教コマンド
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;140,イラマチオ
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K98_140
RETURN 0

@M_KOJO_MESSAGE_COM_K98_140
CALL M_KOJO_MESSAGE_COM_K98_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K98_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;奉仕精神が１０以上
	IF ABL:奉仕精神 > 9
		;このコマンドであなたが射精
		IF NOWEX:MASTER:射精
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL 射精の瞬間、あなたは%CALLNAME:TARGET%を孕ませるつもりで腰を突き入れた
					CALL HPH_PRINT,@"「ぇ゛ぶ゛っHPH んぇむ……げふ……っHPH」","L"
					PRINTFORMDW あまりの量に飲み切れなかったのか、むせた拍子に%CALLNAME:TARGET%の鼻から精液が飛び出してきた……
				CASE 1
					CALL HPH_PRINT,@"「ぅ゛っHPH ん゛っHPH んはぁっHPH んぷっHPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%の頭をガッシリと掴み、射精に向けて激しく腰を打ち付ける
					PRINTFORMDW 涙と鼻水、そして奉仕することができる悦びで、%CALLNAME:TARGET%の表情はすっかり崩れ切っている……
				CASE 2
					PRINTFORMDL あなたは何度も腰を打ち付けた後、腰を突き出し奥の奥までペニスをねじ込んだ
					CALL HPH_PRINT,@"「ぉ゛こ゛っHPH ぐっHPH んん、ぶっHPH」","L"
					PRINTFORMDW 零したらお仕置きだとあなたが言うと、%CALLNAME:TARGET%は涙を流し頷いた……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「んぢゅる、んっHPH んぶっHPH ふっHPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%は顔を動かし、乱暴に突き入れられたペニスをしゃぶっている……
					CALL HPH_PRINT,@"（彼に……悦んでもらえるなら……っHPH）","W"
				CASE 1
					CALL HPH_PRINT,@"「ごふっHPH んーっ……ふーっ……HPH ぢゅる……っHPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%は乱暴に突き込まれたペニスを咥えなおすと、
					PRINTFORMDW 口を窄ませ、ゆっくりと顔を前後に揺すりだした……
				CASE 2
					PRINTFORMDL あなたはオナホのように%CALLNAME:TARGET%の喉を犯した後、ペニスを抜き%CALLNAME:TARGET%に口を開かせた
					CALL HPH_PRINT,@"「ふぁ……はぁ……ぁ……っHPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%はダラダラと涎を垂らしながら、物欲しそうに喉をヒクつかせている……
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;このコマンドであなたが射精
		IF NOWEX:MASTER:射精
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「――んむぶふぅっっ！？？」
					PRINTFORMDL あなたは%CALLNAME:TARGET%の喉奥までペニスを突き入れ、そのまま気持ち良く射精した
					PRINTFORMDW 大量の精液が%CALLNAME:TARGET%の食道を通り、胃へと注がれていく……
				CASE 1
					PRINTFORML 「ぶっ――――」
					PRINTFORMDL 乱暴にペニスを突き入れ、そのまま何も言わずに射精する
					PRINTFORMDW 欲望という穢れを、あなたは一滴残らず%CALLNAME:TARGET%の胃に注ぎ込んだ……
				CASE 2
					PRINTFORMDL ゴリッ…とペニスを喉奥まで押し込み、数回ピストンしてから射精する
					PRINTFORML 「っ……っ……」
					PRINTFORMDW %CALLNAME:TARGET%は涙と鼻水で顔をグチャグチャにしたまま、白目を剥いている……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ごふっ！？ んも、むぅあっ！」
					PRINTFORMDL %CALLNAME:TARGET%の頭をしっかりと掴み、何度も腰を打ち付け喉奥を犯す
					PRINTFORMDW %CALLNAME:TARGET%は苦しそうにあなたの腰を叩いている……
				CASE 1
					PRINTFORML 「っ、え゛……ぉえ゛……ぇ゛……っ」
					PRINTFORMDL ビタンと打ち付けた腰を、ゆっくり、ゆっくりと引いていく
					PRINTFORMDW えずく度に締まる喉の感触に、あなたは恍惚と息を漏らした
				CASE 2
					PRINTFORML （息、が……できな……っ）
					PRINTFORMDL %CALLNAME:TARGET%が意識を失う寸前で、あなたはズルンとペニスを引き抜いた
					PRINTFORMDW %CALLNAME:TARGET%は喉を手で押さえながら、激しく咽込んでいる……
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

