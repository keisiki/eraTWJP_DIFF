			ENDSELECT
		ENDIF
	ELSE
		;サディスティック
		IF M_KOJO_K70_SADISTIC(70)
			;カウンター[手淫]から
			SELECTCASE RAND:4
			CASE 1
				PRINTFORMW 「シコシコしてほしいですかぁ？ わたしのおててでイきたいんですかぁ？ イきたいならイっていいんですよぉ❤」
				PRINTFORMW 「ほらっ❤ しこしこっ❤ いっぱいぴゅっぴゅしましょうねー❤ ぴゅっ❤ ぴゅっ❤」
			CASE 2
				PRINTFORMW 「んふふ…❤ きもちいいですかぁ？ %CALLNAME:MASTER%さん、かわいいです❤」
				PRINTFORMW 「あら、イきそうですか？ いいですよ❤ ぴゅっぴゅしてください❤ ぴゅっ❤ ぴゅっ❤」
			CASE 3
				PRINTFORMW 「あぁっ❤ %CALLNAME:MASTER%さんがイきそうなのがわかりますっ❤」
				PRINTFORMW 「では、シコシコしてあげるのでイってくださいっ❤ ぴゅっ❤ ぴゅーっ❤」
			CASEELSE
				PRINTFORML 「%CALLNAME:MASTER%さんのおちんちんすごい…っ❤ ギンギンです…❤」
				PRINTFORML 「わたしがシコシコして、かいほうしてあげますね❤ しこしこ❤ ぴゅーっ❤」
			ENDSELECT
		ELSEIF ABL:[[小鈴]]:指 < 3
			SELECTCASE RAND:4
			CASE 1
				PRINTFORMW 「手でしてほしいんですかぁ？ わかりました」
			CASE 2
				PRINTFORMW 「手でするんですね……はい……」
			CASE 3
				PRINTFORMW 「手でしごくんですね、いいですよ」
			CASEELSE
				PRINTFORMW 「手でしごいてあげます…❤」
			ENDSELECT
			SELECTCASE RAND:4
			CASE 1
				PRINTFORMW 「んっ、んしょ……こ、こんな感じでしょうか……」
			CASE 2
				PRINTFORMW 「んしょ……っ！ き、きもちいいですかぁ？」
			CASE 3
				PRINTFORMW 「やさしく……よいしょ、よいしょ……ど、どうでしょうか？」
			CASEELSE
				PRINTFORMW 「んっ、んっ……どうでしょう…きもちよくできてますか…？」
			ENDSELECT
		ELSE
			;一部[身を任せる]から
			SELECTCASE RAND:4
			CASE 1
				PRINTFORMW 「手でするんですね、わかりましたっ！」
			CASE 2
				PRINTFORMW 「手でしてほしいですか？ はいっ❤」
			CASE 3
				PRINTFORMW 「ふふ、シコシコしてほしいんですねっ❤ いいですよっ！」
			CASEELSE
				PRINTFORMW 「いいですよ、わたしがシコシコしてあげますっ❤」
			ENDSELECT
			SELECTCASE RAND:4
			CASE 1
				PRINTFORMW 「きもちいいですかぁ？ %CALLNAME:MASTER%さん、かわいいです❤」
			CASE 2
				;二重の意味で…？
				PRINTFORMW 「どうですか？ きもちいーですか？ きもちよかったら、いってください❤」
			CASE 3
				PRINTFORMW 「%CALLNAME:MASTER%さん……きもちよさそう❤ いいですよ……わたしに身をあずけて…❤」
			CASEELSE
				PRINTFORMW 「%CALLNAME:MASTER%さんのおちんちん、すごぉい…❤ どんどん大きくなります…❤」
