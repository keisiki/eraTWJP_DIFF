;鈴奈庵判定
;1=鈴奈庵,2=自分部屋（引っ越した後も考慮）,3=部屋追い出し
@M_KOJO_K70_SUZUNAAN(ARG)
#FUNCTION
IF MAIN_MAP == 2
	IF ARG == P213鈴奈庵
		RETURNF 1
	ELSEIF ARG == P226小鈴私室
		;連れ出す関数ができてました＆特定条件で連れ出せるので対応しました
		IF COM_351_CONDITION(70)
			RETURNF 2
		;不法侵入,恋慕でも思慕でもなく阿求と一緒でもなく連れ出すこともできない,要するに時止めて侵入した（一応阿求と一緒に入って阿求が出て行くパターンもある）
		ELSEIF !TALENT:[[小鈴]]:恋慕 && !TALENT:[[小鈴]]:思慕 && CFLAG:MASTER:現在位置 != CFLAG:[[阿求]]:現在位置 && COM_351_CONDITION([[小鈴]]) == 0
			RETURNF 3
		ELSE
			RETURNF 2
		ENDIF
	ENDIF
ELSE
	IF ARG == 商家町
		RETURNF 1
	ELSEIF CFLAG:[[小鈴]]:現在位置 == OMANEKIBEYA() || CFLAG:[[小鈴]]:現在位置 == CFLAG:[[小鈴]]:初期位置
		RETURNF 2
	ENDIF
ENDIF

