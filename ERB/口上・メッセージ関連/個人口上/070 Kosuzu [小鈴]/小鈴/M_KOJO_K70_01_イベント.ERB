		ELSEIF CFLAG:70:職種 == 40
			;特に要らない気がした
			;WAIT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;仕事終了
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;小鈴の仕事って活動時間外に終わるからデート誘えないのよねー
	IF LOCAL:1 && ARG == 3
		;店番
		IF CFLAG:70:職種 == 47
