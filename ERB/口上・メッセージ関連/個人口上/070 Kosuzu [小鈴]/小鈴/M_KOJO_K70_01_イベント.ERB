	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;店番
		IF CFLAG:70:職種 == 47
			SELECTCASE RAND:10
			CASE 1
				PRINTFORML 「%M_KOJO_K70_AISATSU()%～」
			CASE 2
				PRINTFORML 「えーと、こちらの本はですね――」
			CASE 3
				PRINTFORML 「いらっしゃいませー！」
			CASE 4
				PRINTFORML 「ご返却ありがとうございます～」
			CASE 5
				PRINTFORML 「そこに無ければ無いですね」
