	IF STRCOUNT(SENSE, "(妖魔本)")
		PRINTFORMW 「%CALLNAME:MASTER%さんに貰ったこの妖魔本……」
		PRINTFORMW 「私には判る……表紙を見ただけでこれがハズレであることが……」
		PRINTFORMW 「で、でも、妖魔本には違いないんだし！ %CALLNAME:MASTER%さんには感謝しなくちゃね！」
		;日記用のフラグ
		TCVAR:[[小鈴]]:381 = 1
	ELSEIF STRCOUNT(SENSE, "(鈴)")
		;プレゼントをあげた女の子が実はこんな風に思ってると思うと悲しくなる
		PRINTFORMW 「%CALLNAME:MASTER%さんから鈴を頂いたけど、う～ん……」
		PRINTFORMW 「どうせならもっとオシャレなのが良かったなぁ」
	ELSEIF STRCOUNT(SENSE, "(書物)") && !STRCOUNT(SENSE, "(手帳)")
		PRINTFORMW 「%CALLNAME:MASTER%さんに貰ったこの本……」
		PRINTFORMW 「一応読んでみるけど……表紙を見ただけであんまり面白そうじゃないのが判っちゃったなぁ」
	ELSEIF STRCOUNT(SENSE, "(眼鏡)")
		PRINTFORMW 「%CALLNAME:MASTER%さんに貰ったこの眼鏡、どうしよう」
		PRINTFORMW 「まぁ多少は使わないと%CALLNAME:MASTER%さんに悪いよねぇ……」
	ENDIF
	IF STRCOUNT(SENSE, "(/カード|レアカード)")
		PRINTFORMW 「そうだ、これ栞に使おうっと」
	ELSE
		SELECTCASE CFLAG:[[小鈴]]:1074 % 5
		CASE 1
			PRINTFORMW 「とりあえずここに置いておこう」
		CASE 2
			PRINTFORMW 「このへんに飾っておこうかな」
		CASE 3
			PRINTFORMW 「ここにしまっておけばいいかな」
		CASE 4
			PRINTFORMW 「う～ん、どこにしまっておこう……」
		CASEELSE
			PRINTFORMW 「ここに置いても商品だとは思われないよね」
		ENDSELECT
		IF CFLAG:[[小鈴]]:1074 >= 3
			;順当にいけば3から
			SELECTCASE CFLAG:[[小鈴]]:1074 % 4
			CASE 1
				PRINTFORMW 「気づいたらこんなに貰っちゃってる……」
			CASE 2
				PRINTFORMW 「あ、ここにはもう置けないか……」
			CASE 3
				PRINTFORMW 「貰い物も増えてきたなぁ」
			CASEELSE
				PRINTFORMW 「ふふ、%CALLNAME:MASTER%さんの物が充実してきたなぁ」
			ENDSELECT
		ENDIF
	ENDIF
	;保存した回数をカウント
	CFLAG:[[小鈴]]:1074 ++
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の終わりに今日貰った贈り物をこっそり質に入れる（400点未満）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 5
	IF TALENT:[[小鈴]]:恋人
		PRINTFORML 「はぁ……%CALLNAME:MASTER%さん、私のこともっと判ってくれてると思ってたけど」
		PRINTFORMW 「これはさすがにちょっと……」
	ELSEIF TALENT:[[小鈴]]:恋慕
		PRINTFORMW 「う～む……%CALLNAME:MASTER%さん、実はセンス良くないのかも……」
	ELSE
		PRINTFORMW 「%CALLNAME:MASTER%さんってなんというか、センス悪い人なのかも……」
	ENDIF
	PRINTFORMW 「仕方ないけど、お母さんに頼んでこっそり処分してもらおう……」
	RETURN 1
ENDIF
RETURN 1

;==================================================
;ムシバトル
;ムシバトルの口上は少し特殊で、PRINTを使うのではなく、
;台詞一行のみをRESULTSに入れてください
;ムシの名前は %MUSHI_NAME% で表示できます
;ムシの名前の最長はアレキサンドラトリバネアゲハの全角14文字です
;文字数制限があります。はみ出ないように気をつけてください
;ARGは残りムシ数か順位です
;==================================================
@M_KOJO_MESSAGE_COM_K70_MUSHI_BATTLE(ARGS, ARG)
#DIM 残りムシ数
#DIM 順位
SELECTCASE ARGS
CASE "試合前"
	;-------------------------------------------------
	;キャラセレクト時,ARGは使えません
	;全角50文字くらいまで
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「えっ…私ですか……？」
	ENDIF
CASE "戦場に出した"
	;-------------------------------------------------
	;"【（キャラ）】は《（ムシ）》を戦場に出した！」"の直後
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「じゃあ、この子で」
	ENDIF
CASE "攻撃時"
	;-------------------------------------------------
	;"【（キャラ）】《（ムシ）》の『（攻撃名）』！」"の直前
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「がんばれー！ %MUSHI_NAME%！」
	ENDIF
CASE "倒れた時"
	;-------------------------------------------------
	;"【（キャラ）】《（ムシ）》は倒れた！！"の直後
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「あっ、やられちゃった」
	ENDIF
CASE "敗退時"
	;-------------------------------------------------
	;"【（キャラ）】敗退！！"の直後
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「あれ、もう終わり…？」
	ENDIF
CASE "タイマン：勝ち"
	;-------------------------------------------------
	;順位は1位
	;全角20文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「負けちゃいました～」
	ENDIF
CASE "タイマン：負け"
	;-------------------------------------------------
	;順位は2位
	;全角20文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「あれ、私の勝ちですか…？」
	ENDIF
CASE "バトルロイヤル：優勝"
	;-------------------------------------------------
	;順位は1位
	;全角30文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「まさか勝っちゃうなんて……」
	ENDIF
CASE "バトルロイヤル：２位以下"
	;-------------------------------------------------
	;順位は2～4位
	;全角30文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「残念ですね～」
	ENDIF
CASE "チーム戦：勝ち"
	;-------------------------------------------------
	;順位は1位（やられていても）
	;全角32文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「勝ったんですか？ やった～」
	ENDIF
CASE "チーム戦：負け"
	;-------------------------------------------------
	;順位は2位
	;全角32文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	順位 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「あらら、残念です」
	ENDIF
ENDSELECT
SIF RESULTS != ""
	RETURN 1

