@M_KOJO_MESSAGE_COM_K70_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K70_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K70_300_1
#DIM GUEST, 2
#DIMS GUESTS
#DIM POINT
#DIM デート会話, 120
#DIM 会話日
#DIM 会話回数
;日が変わってたらフラグリセット
SIF !TCVAR:70:352
	VARSET デート会話
IF 会話日 != DAY
	会話回数 = 0
	会話日 = DAY
ENDIF
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	RESETCOLOR
	会話回数 ++
	;ゲストキャラ判定
	CALL M_KOJO_K70_GUEST("会話")
	GUEST = RESULT
	GUEST:1 = RESULT:1
	GUESTS = %RESULTS%
	;ここに口上書いて下さい
	IF ALCOHOL_FACE(70) == "酔いどれ" || ALCOHOL_FACE(70) == "ぐでぐで"
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%に話し掛けてみたが、%CALLNAME:TARGET%は呂律が回っておらず何を言っているかも聴き取れない
	ELSEIF TCVAR:[[小鈴]]:発情
		PRINTFORML %CALLNAME:PLAYER%は様子のおかしい%CALLNAME:TARGET%に話し掛けてみた
	;会話不能の地の文は恋慕でない場合は省略できず、恋慕の時は表示されないので、ここではどちらの場合も同じように表示されるようにする
	ELSEIF TCVAR:70:会話不能
		IF TALENT:70:恋慕
			PRINTFORML %CALLNAME:TARGET%に話しかけようとしたが言葉が見つからない
			PRINTFORMW あたりに気まずい空気が流れた…
		ENDIF
	ELSEIF GUESTS != ""
		CLEARLINE 2
		PRINTFORML 三人で会話
		DRAWLINE
		;文章上だけであってソース等は変わりません,優先度の問題で多少齟齬は出るけど面倒なので妥協
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%と%GUESTS%を交えて三人で話をした
	ELSE
		;デート会話
		IF TALENT:70:恋慕 && FLAG:デート相手 == 70 && DATENAME_SPOT() != "道中" && !デート会話:(CFLAG:70:現在位置 / 10)
			CLEARLINE 2
			PRINTFORML %DATENAME_SPOT()%でデート
			DRAWLINE
		ENDIF
		PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%と
		IF TALENT:70:恋慕
			PRINTDATA
				DATAFORM 隣り合って
				DATAFORM 少し照れながら
				DATAFORM 微笑みを交わしながら
				DATAFORM 和やかな雰囲気で
				DATAFORM 少し熱くなりながら
				DATAFORM じゃれ合いながら
			ENDDATA
		ELSE
			PRINTDATA
				DATAFORM くだらない
				DATAFORM たわいのない
				DATAFORM 世間
				DATAFORM 平和な
				DATAFORM よくある
				DATAFORM ちょっとした
				DATAFORM 可笑しな
			ENDDATA
		ENDIF
		IF TFLAG:SELECTCOMの分岐 > 0
			PRINTFORML 話をして盛り上がった
		ELSEIF TFLAG:SELECTCOMの分岐 < 0
			PRINTFORML 話をしていたが話が途切れてしまった……
		ELSE
			PRINTFORML 話をした
		ENDIF
	ENDIF
	PRINTW 
	CALL M_KOJO_COLOR_K70
	;添い寝中
	IF CFLAG:[[小鈴]]:添い寝中
		IF TALENT:[[小鈴]]:恋慕
			SELECTCASE RAND:8
			CASE 1
				PRINTFORMW 「んん～……このまま……ぐっすり……」
			CASE 2
				PRINTFORMW 「またあした……おはなししましょうね……」
			CASE 3
				PRINTFORMW 「んん、むにゃむにゃ……」
			CASE 4
				PRINTFORMW 「ふふ……きもちいいですね……」
			CASE 5
				PRINTFORMW 「ふにゃぁ～……あったかぁぃ……」
			CASE 6
				PRINTFORMW 「えへぇ……もっとくっつきましょ…❤」
			CASE 7
				PRINTFORMW 「ふぁ……あんしんしますぅ……」
			CASEELSE
				PRINTFORMW 「ほぁ～…ねむいですぅ～……」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「んん～……もうねましょうよぉ～…」
			CASE 2
				PRINTFORMW 「んん、むにゃむにゃ……」
			CASEELSE
				PRINTFORMW 「はい……はい……」
			ENDSELECT
		ENDIF
	;酔ってる
	ELSEIF ALCOHOL_FACE(70) == "酔いどれ" || ALCOHOL_FACE(70) == "ぐでぐで"
		CALL M_KOJO_K70_DRUNK("会話")
	;発情中
	ELSEIF TCVAR:[[小鈴]]:発情
		CALL M_KOJO_K70_HATSUJO("会話")
	;自己紹介
	ELSEIF CFLAG:70:1004 == 0
		PRINTFORMDL %CALLNAME:MASTER%は軽く自己紹介をした
		PRINTW 
		PRINTFORMW 「へー、見かけない人だなとは思ってたんですけど、そうだったんですかぁ」
		PRINTFORMW 「幻想郷にそんな人が暮らしてるだなんて知らなかったです」
		CFLAG:70:1004 = 1
	;不機嫌
	ELSEIF M_KOJO_K70_FUKIGEN([[小鈴]])
		IF TALENT:70:恋慕
			;あなたが怒らせた
			IF !TCVAR:[[小鈴]]:380
				SELECTCASE RAND:5
				CASE 1
					PRINTFORMW 「%CALLNAME:MASTER%さん……どうして……」
				CASE 2
					PRINTFORMW 「む～、幻滅です……」
				CASE 3
					PRINTFORMW 「はぁ……見損ないました……」
				CASE 4
					PRINTFORMW 「%CALLNAME:MASTER%さんがそんな人だったとは……ショックです」
				CASEELSE
					PRINTFORMW 「%CALLNAME:MASTER%さん、私いま怒ってるんです…！」
				ENDSELECT
			ELSE
				SELECTCASE RAND:5
				CASE 1
					PRINTFORMW 「う～ん……」
				CASE 2
					PRINTFORMW 「えーっと……はぁ……」
				CASE 3
					PRINTFORMW 「え？ はい！ すみません、聞いてませんでした……」
				CASE 4
					PRINTFORMW 「すみません、今日はなんだか……」
				CASEELSE
					PRINTFORMW 「うーん、ごめんなさい、今ちょっと考え事が……」
				ENDSELECT
			ENDIF
		ELSE
			;あなたが怒らせた
			IF !TCVAR:[[小鈴]]:380
				SELECTCASE RAND:5
				CASE 1
					PRINTFORMW 「はぁ～……」
				CASE 2
					PRINTFORMW 「むー……！」
				CASE 3
					PRINTFORMW 「今は話しかけないでください！」
				CASE 4
					PRINTFORMW 「%CALLNAME:MASTER%さんに話す事は何もないです！」
				CASEELSE
					PRINTFORMW 「なんですか？ 私いま気が立ってるので！」
				ENDSELECT
			ELSE
				SELECTCASE RAND:5
				CASE 1
					PRINTFORMW 「うーん……」
				CASE 2
					PRINTFORMW 「え？ あぁ……はい……」
				CASE 3
					PRINTFORMW 「ふーん……」
				CASE 4
					PRINTFORMW 「すみません、今はちょっと……」
				CASEELSE
					PRINTFORMW 「はぁ…そうですかー……」
				ENDSELECT
			ENDIF
		ENDIF
	;会話不能
	ELSEIF TCVAR:70:会話不能
		IF TALENT:70:恋慕 || TALENT:70:思慕
			SELECTCASE RAND:8
			CASE 1
				PRINTFORMW 「すみません……私、本の話くらいしかできなくて……」
			CASE 2
				PRINTFORMW 「あはは、無理に話そうとしなくても大丈夫ですよ」
			CASE 3
				PRINTFORMW 「あ、そうだ！ ……あれ？ 何を話そうとしたんだっけ…？」
			CASE 4
				PRINTFORMW 「今日見た夢ですか？ えっと、見ませんでした……」
			CASE 5
