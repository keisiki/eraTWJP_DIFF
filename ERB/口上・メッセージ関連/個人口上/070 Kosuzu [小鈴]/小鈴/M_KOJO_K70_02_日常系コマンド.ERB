		CALL M_KOJO_K70_TALKSET("状況会話", 37)
[SKIPSTART]
	;
	ELSEIF !GETBIT(TCVAR:[[小鈴]]:359, ) && 
		CALL M_KOJO_K70_TALKSET("状況会話", )
[SKIPEND]
	;恋人
