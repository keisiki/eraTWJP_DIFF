@M_KOJO_MESSAGE_COM_K70_354_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K70
	;恋慕前提だっけ？
	IF TALENT:70:恋慕
		SELECTCASE RAND:10
		CASE 1
			IF CFLAG:MASTER:初期位置 == CFLAG:[[小鈴]]:初期位置 
				PRINTFORML 「ぁ……%CALLNAME:MASTER%さん……」
				PRINTFORML 「どうぞ…❤ おふとんあたためておきました…❤」
				SIF GET_MONTH() == "夏の月"
					PRINTFORML 「あ、あつかったらごめんなさい……」
				WAIT
			ELSE
				PRINTFORML 「ん、%CALLNAME:MASTER%さん……」
				PRINTFORMW 「一緒に寝ましょ…❤」
			ENDIF
		CASE 2
			IF CFLAG:MASTER:初期位置 == CFLAG:[[小鈴]]:初期位置 
				PRINTFORML 「ふぁ……%CALLNAME:MASTER%さん……」
				PRINTFORMW 「さみしかったですぅ～…❤」
			ELSE
				PRINTFORML 「ん、%CALLNAME:MASTER%さん……」
				PRINTFORMW 「一緒に寝てくれるんですかぁ…うれしい…❤」
			ENDIF
		CASE 3
			IF GET_MONTH() == "冬の月"
				PRINTFORML 「はわぁ…❤」
				PRINTFORMW 「%CALLNAME:MASTER%さんがいてくれるとポカポカです～❤」
			ELSE
				PRINTFORML 「あぁ……❤」
				PRINTFORMW 「そんなにくっついたらあついですよぉ❤」
			ENDIF
		CASE 4
			IF GET_MONTH() == "冬の月"
				PRINTFORML 「はやくきてください～っ！」
				PRINTFORMW 「さむいです～～！」
			ELSE
				PRINTFORMW 「ふふ…❤ お布団が無くても温まれそうです❤」
			ENDIF
		CASE 5
			IF GET_MONTH() == "夏の月"
				PRINTFORML 「ん、%CALLNAME:MASTER%さん……」
				PRINTFORMW 「ちょっと……あついです…❤」
			ELSE
				PRINTFORML 「ん、%CALLNAME:MASTER%さん……」
				PRINTFORMW 「ふぅ……あったかい…❤」
			ENDIF
		CASE 6
			PRINTFORMW 「ふふ…❤ %CALLNAME:MASTER%さんと一緒のお布団…❤ しあわせ❤」
		CASE 7
			PRINTFORML 「あの……%CALLNAME:MASTER%さん…❤」
			PRINTFORMW 「だ、だっこしてもいいですか…❤」
		CASE 8
			PRINTFORML 「あ…❤ %CALLNAME:MASTER%さん……」
			PRINTFORMW 「もっと近づきましょっ…❤」
		CASE 9
			PRINTFORML 「%CALLNAME:MASTER%さん……どうぞ…❤」
			PRINTFORMW 「最高のお布団です…❤」
		CASEELSE
			PRINTFORML 「%CALLNAME:MASTER%さん……あの……」
			PRINTFORMW 「ぎゅ～ってしながら……寝ましょ❤」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
