;352,告白する
;FLAG:甲斐性無(1=失敗)
;TFLAG:193 (1=成功 0=失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K70_352
CALL M_KOJO_MESSAGE_COM_K70_352_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K70_352_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K70
	;した
	IF TFLAG:SELECTCOMの分岐
		;初めて
		IF FIRSTTIME("告白")
			IF TALENT:70:恋慕
				PRINTFORMW 「うう……うれしいですぅ～～っ！」
				PRINTL 
				PRINTFORMDL 　ぎゅっ
				PRINTFORMDL 小鈴は歓喜に泣きじゃくりながら%CALLNAME:MASTER%を抱きしめた
				PRINTW 
				PRINTFORMW 「わたしも%CALLNAME:MASTER%さんの事が好きでした……！」
				PRINTFORMW 「でも、口に出すのが怖くて……関係が崩れてしまうんじゃないかと思って……」
				PRINTFORMW 「だからすごく嬉しいです……今、とっても幸せです……❤」
				;日記を書く
				CALL M_KOJO_DIARYSETTING_K70(43)
			ELSE
				PRINTFORMW 「えっ……えぇーーっ！？」
				PRINTFORMW 「ほ、ほんとですか……？」
				PRINTFORMW 「そんな…と、突然言われても……でも……」
				PRINTFORMW 「でも……、い、いいですよ……%CALLNAME:MASTER%さんとなら……」
				PRINTFORMW 「不安はありますけど、きっと%CALLNAME:MASTER%さんなら私を大事にしてくれるでしょうし……」
				PRINTFORMW 「私も%CALLNAME:MASTER%さんのこと、好きになれると思います…だから……」
				;日記を書く
				CALL M_KOJO_DIARYSETTING_K70(44)
			ENDIF
		;一度フった,都合のいい奴じゃ！
		ELSE
			;告白口上と同じ,こちらは恋慕のみしかない、はず
			PRINTFORMW 「ほ、ほんとですか……？ い、いいんですか……？」
			PRINTFORMW 「う、嬉しい……良かったぁ……」
			PRINTFORMW 「私、嫌われたんじゃないかと……ずっと怖くて……」
			PRINTFORMW 「でも、希望を捨てずにいて良かったです……%CALLNAME:MASTER%さんが受け入れてくれて……本当に良かった……」
			;日記を書く
			CALL M_KOJO_DIARYSETTING_K70(45)
		ENDIF
		IF TALENT:70:恋慕
			PRINTL 
			PRINTFORMDL 恋が成就した二人は静かに寄り添い、甘い空気に包まれてどちらかともなく口づけを交わした……
			PRINTW 
			EXP:70:キス経験 += 1
			EXP:70:Ｍ経験 += 1
			EXP:MASTER:Ｍ経験 += 1
			EXP:MASTER:キス経験 += 1
			IF FIRSTTIME("ファーストキス")
				CALL SET_FIRSTKISS(70)
				;［告白する］の条件が二人きり
				IF M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 1
					CSTR:70:ファーストキス喪失履歴 = %PRINT_DATE_F(DAY)%、誰も居なくなった静かな鈴奈庵で恋い焦がれていた%CALLNAME:MASTER%の告白を受け、幸せなムードの中で契りのファーストキスを捧げた
				ELSEIF M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 2
					CSTR:70:ファーストキス喪失履歴 = %PRINT_DATE_F(DAY)%、自分の部屋で恋い焦がれていた%CALLNAME:MASTER%の告白を受け、幸せなムードの中で契りのファーストキスを捧げた
				ELSEIF FLAG:デート相手 == 70
					CSTR:70:ファーストキス喪失履歴 = %PRINT_DATE_F(DAY)%、%DATENAME_PLACE()%でデート中、%DATENAME_PLACE()%で恋い焦がれていた%CALLNAME:MASTER%の告白を受け、幸せなムードの中で契りのファーストキスを捧げた
				ELSE
					CSTR:70:ファーストキス喪失履歴 = %PRINT_DATE_F(DAY)%、%NAME_FROM_PLACE(CFLAG:70:現在位置)%で恋い焦がれていた%CALLNAME:MASTER%の告白を受け、幸せなムードの中で契りのファーストキスを捧げた
				ENDIF
			ENDIF
			CALL M_KOJO_COLOR_K70
			PRINTFORMW 「ふふ……❤ これから先、何があるか判りませんけど――」
			PRINTFORMW 「ずっと一緒にいましょうね♪ %CALLNAME:MASTER%さん❤」
		ENDIF
	;言葉に詰まる
	ELSE
		PRINTFORML 「？」
		PRINTFORMW 「%CALLNAME:MASTER%さん、どうしました？」
	ENDIF
ENDIF
RETURN 1
