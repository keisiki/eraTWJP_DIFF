				PRINTFORML 「あ、あの……コレ……料理……なんです……ね……」
				PRINTFORMW 「い……、いただき……ます……」
			;軽食
			ELSEIF DISH_TYPE == "軽食"
				PRINTFORML 「あら、私にもいただけるんですか？ ありがとうございます～」
				PRINTFORMW 「では、いただきますね」
			;デザート
			ELSEIF DISH_TYPE == "デザート"
				PRINTFORML 「わぁ！ いただけるんですかぁ！？ ありがとうございます～♪」
				PRINTFORMW 「では、遠慮なく……いただきます♪」
			;温かい
			ELSEIF STRCOUNT(DISH_TASTE, "温かい")
				PRINTFORML 「あら、私にもいただけるんですか？ ありがとうございます～」
				IF GET_MONTH() == "夏の月"
					PRINTFORML 「わっ！ 熱い！ 汗かいちゃいそうです……」
				ELSEIF GET_MONTH() == "冬の月"
					PRINTFORML 「わぁ～、温かい！ コレを食べたらポカポカですね～」
				ENDIF
				PRINTFORMW 「では、いただきます」
			;冷たい
			ELSEIF STRCOUNT(DISH_TASTE, "冷たい")
				PRINTFORML 「あら、私にもいただけるんですか？ ありがとうございます～」
				IF GET_MONTH() == "夏の月"
					PRINTFORML 「わぁ～！ 冷たくて夏にはいいですねぇ～」
				ELSEIF GET_MONTH() == "冬の月"
					PRINTFORML 「うっ！ 冷たい！ 身体冷やしちゃいそうです……」
				ENDIF
				PRINTFORMW 「では、いただきます」
			ELSE
				PRINTFORML 「あら、お食事いただけるんですか？ ありがとうございます～」
				PRINTFORMW 「では、いただきますね」
			ENDIF
		ENDSELECT
	ENDIF
	;味の評価
	PRINTL 
	;阿求と作った
	SIF DISH_ASSI == [[阿求]]
		PRINTFORML 「えっ、この料理阿求と作ったんですか？ ふぅん……」
	;ドッキリ味
	IF STRCOUNT(DISH_TASTE, "ドッキリ味/") || STRCOUNT(DISH_TASTE, "未知の味/")
		PRINTFORML 「う…っ！ なんでしょうこれ……？」
		PRINTFORMW 「なんか変な味がしませんか…？」
		IF TCVAR:[[小鈴]]:料理評価値 >= 700
			PRINTFORMW 「んー…でも不思議と美味しいですねぇ」
		ELSEIF TCVAR:[[小鈴]]:料理評価値 >= 500
			PRINTFORMW 「んん……不思議と悪くない味ですねぇ」
		ELSE
			PRINTFORMW 「うぐ……お、お腹が痛くなってきました……」
		ENDIF
	;低評価
	ELSEIF TCVAR:[[小鈴]]:料理評価値 < 700
		;辛い
		IF STRCOUNT(DISH_TASTE, "辛い")
			PRINTFORMW 「ひぅ…っ！ か、からいれふぅ～…！」
		;阿求と作った
		ELSEIF DISH_ASSI == [[阿求]]
			;悪い
			IF TCVAR:[[小鈴]]:料理評価値 < 500
				PRINTFORMW 「げー……阿求なんかに作らせるから……」
			;……まあまあだ。
			ELSEIF TCVAR:[[小鈴]]:料理評価値 < 600
				PRINTFORML 「ふ～ん……まぁまぁですかね～」
				PRINTFORMW 「阿求って料理うまいんですか？」
			;おいしい
			ELSE
				PRINTFORML 「ん～！ 美味しいです～」
				PRINTFORMW 「阿求もなかなかやるじゃな～い」
			ENDIF
		ELSE
			IF TCVAR:[[小鈴]]:料理評価値 < 500
				PRINTFORMW 「んー……この味付けは私にはちょっと……」
			;……まあまあだ。
			ELSEIF TCVAR:[[小鈴]]:料理評価値 < 600
				PRINTFORMW 「ふんふん、まぁまぁですかね～」
			;おいしい
			ELSE
				PRINTFORMW 「美味しいですね～！」
			ENDIF
		ENDIF
	;高評価
	ELSE
		;味チェック
		LOCALS = 
		WHILE LOCALS == ""
			SELECTCASE RAND:13
			CASE 1
				SIF STRCOUNT(DISH_TASTE, "甘い")
					LOCALS = 甘い
			CASE 2
				SIF STRCOUNT(DISH_TASTE, "和食")
					LOCALS = 和食
			CASE 3
				SIF STRCOUNT(DISH_TASTE, "麺類")
					LOCALS = 麺類
			CASE 4
				SIF STRCOUNT(DISH_TASTE, "すっぱい")
					LOCALS = すっぱい
			CASE 5
				SIF STRCOUNT(DISH_TASTE, "あっさり")
					LOCALS = あっさり
			CASE 6
				SIF STRCOUNT(DISH_TASTE, "淡白")
					LOCALS = 淡白
			CASE 7
				SIF STRCOUNT(DISH_TASTE, "上品")
					LOCALS = 上品
			CASE 8
				SIF STRCOUNT(DISH_TASTE, "もちもち")
					LOCALS = もちもち
			CASE 9
				SIF STRCOUNT(DISH_TASTE, "外界風")
					LOCALS = 外界風
			;ここから先は好きな味ではない
			CASE 10
				SIF STRCOUNT(DISH_TASTE, "サクサク")
					LOCALS = サクサク
			CASE 11
				SIF STRCOUNT(DISH_TASTE, "ぷるぷる")
					LOCALS = ぷるぷる
			CASE 12
				SIF STRCOUNT(DISH_TASTE, "不思議")
					LOCALS = 不思議
			CASEELSE
				LOCALS = その他
			ENDSELECT
		WEND
		SELECTCASE LOCALS
		CASE "甘い"
			PRINTFORMW 「ん～～♪ あまくておいひぃ～♪」
		CASE "和食"
			PRINTFORMW 「やっぱり和食が好きですね～」
		CASE "麺類"
			PRINTFORMW 「ずずっ…！ 私、麺類は好きなんですよ～」
		CASE "すっぱい"
			PRINTFORMW 「んー！ すっぱくて美味しいです～」
		CASE "あっさり"
			PRINTFORMW 「ん～～！ さっぱりしていて美味しいです～！」
		CASE "淡白"
			PRINTFORMW 「ん～～！ なめらかな味わいがいいですね～！」
		CASE "上品"
			PRINTFORMW 「んー…！ 上品な味わいが良いですね～！」
		CASE "もちもち"
			PRINTFORMW 「ん～～♪ もちもちしてておいひぃ～♪」
		CASE "外界風"
			PRINTFORMW 「これが外界の味…！ 美味しいですっ！」
		CASE "サクサク"
			PRINTFORMW 「サクサクした食感が良いですね～！」
		CASE "ぷるぷる"
			PRINTFORMW 「ふふ♪ ぷるぷるしててかわいいですね♪」
		CASE "不思議"
			PRINTFORMW 「ん～？ 不思議な味わいですけど、美味しいです！」
		CASEELSE
			;すごくおいしい
			IF TCVAR:[[小鈴]]:料理評価値 < 850
				PRINTFORMW 「コレすごく美味しいです～！」
			;大好き
			ELSE
				PRINTFORMW 「私コレ大好きかも～！」
			ENDIF
		ENDSELECT
		;阿求と作った
		IF DISH_ASSI == [[阿求]]
			PRINTFORML 「阿求ってこんなに料理できるんだ～」
			IF TALENT:[[小鈴]]:恋慕
				PRINTFORMW 「それとも、%CALLNAME:MASTER%さんのおかげですかね？」
			ELSE
				PRINTFORMW 「ちょっと見直しちゃったな♪」
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

