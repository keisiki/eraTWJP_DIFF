@M_KOJO_K70_MANZOKU(ARG)
#FUNCTION
;SIF BASE:ARG:体力 <= MAXBASE:ARG:体力 / 2
;	RETURNF 0
LOCAL = EX:ARG:Ｃ絶頂 + EX:ARG:Ｖ絶頂 + EX:ARG:Ａ絶頂 + EX:ARG:Ｂ絶頂 + EX:ARG:Ｍ絶頂
IF CFLAG:ARG:溜まってる度 <= 100
	RETURNF 1
ELSEIF CFLAG:ARG:溜まってる度 < 300 && LOCAL >= 20
	RETURNF 1
ELSEIF CFLAG:ARG:溜まってる度 < 500 && LOCAL >= 40
	RETURNF 1
ELSEIF CFLAG:ARG:溜まってる度 < 800 && LOCAL >= 60
	RETURNF 1
ELSEIF LOCAL >= 80
	RETURNF 1
ENDIF

