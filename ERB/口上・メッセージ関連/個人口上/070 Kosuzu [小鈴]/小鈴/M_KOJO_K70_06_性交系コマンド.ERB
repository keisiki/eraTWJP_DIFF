;性交系コマンド
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;中出し（挿入中に射精するとき愛撫,胸愛撫および性交系コマンドを実行した場合,キスは別途）
;==================================================
@M_KOJO_K70_NAKADASHI
;一応暴発防止
SIF !M_KOJO_K70_UFUFU()
	RETURN 
SIF !NOWEX:MASTER:射精
	RETURN
SIF FLAG:時間停止
	RETURN
IF TEQUIP:MASTER:コンドーム || TCVAR:[[小鈴]]:ピル 
	;だいしゅきホールドを呼ぶ
	CALL M_KOJO_K70_DAISHUKI_HOLD("コンドーム")
;後背位（立ち位）,背面座位
ELSEIF TEQUIP:70:体位 == 2 || TEQUIP:70:体位 == 5
	PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：挿入", "❤ ", 3)%%M_KOJO_K70_AUTO("喘ぎ：急", "❤")% おっ❤」
	IF CFLAG:[[小鈴]]:生解禁 == 2 && !TALENT:[[小鈴]]:妊娠
		CALL M_KOJO_K70_NAKADASHISET("後背位_子作り")
	ELSE
		CALL M_KOJO_K70_NAKADASHISET("後背位")
	ENDIF
;騎乗位
ELSEIF TEQUIP:70:体位 == 3
	;＞素股,素股→騎乗位の分岐って昔YMかJ+で見て滾った
	IF PREVCOM == 83
		CALL M_KOJO_K70_NAKADASHISET("騎乗位_素股")
	ELSE
		PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：挿入", "❤ ", 3)%%M_KOJO_K70_AUTO("喘ぎ：急", "❤")% %M_KOJO_K70_AUTO("喘ぎ：挿入", "❤")%」
		IF CFLAG:[[小鈴]]:生解禁 == 2 && !TALENT:[[小鈴]]:妊娠
			CALL M_KOJO_K70_NAKADASHISET("騎乗位_子作り")
		ELSE
			CALL M_KOJO_K70_NAKADASHISET("騎乗位")
		ENDIF
	ENDIF
;その他の体位,正常位（立ち位）,対面座位,体位リセット時（押し倒され中）
ELSE
	;だいしゅきホールドを呼ぶ
	CALL M_KOJO_K70_DAISHUKI_HOLD("中出し")
ENDIF
RETURN 1

;==================================================
;だいしゅきホールド
;紆余曲折を経て半分中出し口上化してしまったうえに関数が分かれた
;めんどくさいだけなので参考にしないほうがいいです
;==================================================
@M_KOJO_K70_DAISHUKI_HOLD(ARGS)
IF TEQUIP:MASTER:コンドーム || TCVAR:MASTER:コンドーム || ARGS == "コンドーム" || TCVAR:[[小鈴]]:ピル 
	SELECTCASE RAND:3
	CASE 1
		PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：挿入", "❤")% かんじますっ❤ んっ❤ %CALLNAME:MASTER%さんとひとつにっ❤ もっとっ❤」
	CASE 2
		PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：挿入", "❤")% %CALLNAME:MASTER%さんっ❤ あついっ❤ %M_KOJO_K70_AUTO("喘ぎ：挿入", "❤")% いっぱいっ❤ だしてっ❤ %M_KOJO_K70_AUTO("喘ぎ：挿入", "❤")%」
	CASEELSE
		PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：挿入", "❤")% %CALLNAME:MASTER%さんっ❤ すきですっ❤ しゅきっ❤ %M_KOJO_K70_AUTO("喘ぎ：挿入", "❤")%」
	ENDSELECT
ELSE
	;カウンター中のコマンド選択時は既に表示されているので省略
	SIF !GROUPMATCH(ARGS, "キスする", "胸愛撫", "クリ愛撫")
		PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：挿入", "❤ ", 3)%%M_KOJO_K70_AUTO("喘ぎ：急", "❤")% %M_KOJO_K70_AUTO("喘ぎ：挿入", "❤")%」
	;カウンター中は体位不明なので汎用のみ,正常位（立ち位）と対面座位は共通
	IF CFLAG:[[小鈴]]:うふふ == 2
		IF CFLAG:[[小鈴]]:生解禁 == 2 && !TALENT:[[小鈴]]:妊娠
			CALL M_KOJO_K70_NAKADASHISET("汎用_子作り", ARGS == "カウンター")
		ELSE
			CALL M_KOJO_K70_NAKADASHISET("汎用", ARGS == "カウンター")
		ENDIF
	ELSE
		IF CFLAG:[[小鈴]]:生解禁 == 2 && !TALENT:[[小鈴]]:妊娠
			CALL M_KOJO_K70_NAKADASHISET("正常位_子作り")
		ELSE
			CALL M_KOJO_K70_NAKADASHISET("正常位")
		ENDIF
	ENDIF
ENDIF
RETURN 1

