;==================================================
;9,ダブルフェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K70_9
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K70_9_1
;戻り値とTARGETを保存、TARGETを二人目に一時的に交代して二人目口上を呼び出した後、TARGETをもどして保存した戻り値でRETURNする
;二人目はいらない場合はここで処理
;二人目
LOCAL = MASTER_POSE(4,1,1)
LOCAL:1 = RESULT
LOCAL:2 = TARGET
TARGET = LOCAL
TRYCALLFORM M_KOJO_MESSAGE_SCOM_K{TARGET}_9_2
TARGET = LOCAL:2
RETURN LOCAL:1

;一人目口上
@M_KOJO_MESSAGE_SCOM_K70_9_1(ARG)
#DIM CHARA
IF !ARG
	CHARA = MASTER_POSE(4,1,1)
ELSE
	CHARA = ARG
ENDIF
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K70_UFUFU()
;-------------------------------------------------
IF LOCAL
	IF CHARA == [[阿求]]
		SELECTCASE M_KOJO_K70_BALANCE([[小鈴]], [[阿求]])
		CASE "独占", "信頼"
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「阿求、一緒に%CALLNAME:MASTER%さんにご奉仕しましょっ❤」
			CASEELSE
				PRINTFORMW 「ふふ❤ 阿求とはんぶんこね❤」
			ENDSELECT
		CASE "悲恋", "揶揄"
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「しょ、しょうぶよ阿求！ どっちか%CALLNAME:MASTER%さんを気持ちよくさせられるか…！」
			CASEELSE
				PRINTFORMW 「んんぅ……阿求ばっかりずるいわ……！ わたしだって……！」
			ENDSELECT
		CASEELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「阿求より気持ちよくしてあげますよっ！」
			CASEELSE
				PRINTFORMW 「阿求にはあげないんだから…っ！ %M_KOJO_K70_AUTO("おくち：前", "❤")%」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「んんぅ……%CALLNAME:CHARA%さんばっかりずるいです……！ わたしだって……！」
		CASEELSE
			PRINTFORMW 「んーっ！ %CALLNAME:CHARA%さん、わたしにもください……っ！」
		ENDSELECT
	ENDIF
	;ここから[フェラチオ]に転送
	CALL M_KOJO_MESSAGE_COM_K70_81_1
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K70_{SELECTCOM}_1
RETURN 1

