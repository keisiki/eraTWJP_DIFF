RETURN LOCAL:1

;一人目口上
@M_KOJO_MESSAGE_SCOM_K70_9_1
#DIM CHARA
CHARA = MASTER_POSE(4,1,1)
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = CFLAG:70:1006
;-------------------------------------------------
IF LOCAL
	IF CHARA == 80
		SELECTCASE M_KOJO_K70_BALANCE(70, 80)
		CASE 8, 5
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「阿求、一緒に%CALLNAME:MASTER%さんにご奉仕しましょっ%UNICODE(0x2764)%」
			CASEELSE
				PRINTFORMW 「ふふ%UNICODE(0x2764)% 阿求とはんぶんこね%UNICODE(0x2764)%」
			ENDSELECT
		CASE 7, 6
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「しょ、しょうぶよ阿求！ どっちか%CALLNAME:MASTER%さんを気持ちよくさせられるか…！」
			CASEELSE
				PRINTFORMW 「んんぅ……阿求ばっかりずるいわ……！ わたしだって……！」
			ENDSELECT
		CASE 4, 3
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「阿求より気持ちよくしてあげますよっ！」
			CASEELSE
				PRINTFORMW 「阿求にはあげないんだから…っ！ あむぅ%UNICODE(0x2764)%」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「んんぅ……%CALLNAME:CHARA%さんばっかりずるいです……！ わたしだって……！」
		CASEELSE
			PRINTFORMW 「んーっ！ %CALLNAME:CHARA%さん、わたしにもください……っ！」
		ENDSELECT
	ENDIF
	;サディスティックになる
	IF NOWEX:MASTER:射精
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「んぐっ……ぐっ……でほうでふか？ いいえふよ、いっぱいあひてくあはいっ%UNICODE(0x2764)%」
			PRINTFORMW 「んっ%UNICODE(0x2764)% ぐぷっ%UNICODE(0x2764)% んっ%UNICODE(0x2764)% んっ%UNICODE(0x2764)% んっ%UNICODE(0x2764)% ん%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「%CALLNAME:MASTER%さん、こんなにふくらまして……%UNICODE(0x2764)% わたしがすいとってあげますよぉ%UNICODE(0x2764)%」
			PRINTFORMW 「ちゅっ%UNICODE(0x2764)% ちゅぷっ%UNICODE(0x2764)% んぐっ%UNICODE(0x2764)% ぐぷっ%UNICODE(0x2764)% えろえろ…%UNICODE(0x2764)% ぐぽっ%UNICODE(0x2764)% ぐちゅっ%UNICODE(0x2764)%」
		CASEELSE
			PRINTFORMW 「ちゅぷっ%UNICODE(0x2764)% んぐっ%UNICODE(0x2764)% んっ%UNICODE(0x2764)% いひほうえふか？ ぷぁ…%UNICODE(0x2764)% わかりました…%UNICODE(0x2764)%」
			PRINTFORMW 「%ONEDARI_MASTER_TINKO()%みるくいっぱいください%UNICODE(0x2764)% んぷっ%UNICODE(0x2764)% ぐぷっ%UNICODE(0x2764)% ぐぽっ%UNICODE(0x2764)% 」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「ちゅっ%UNICODE(0x2764)% ぺろぉ…%UNICODE(0x2764)% あむっ%UNICODE(0x2764)% れろ%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「はぁむ…%UNICODE(0x2764)% ぐぷっ%UNICODE(0x2764)% ぐちゅっ%UNICODE(0x2764)% はぁ%UNICODE(0x2764)% んぷっ%UNICODE(0x2764)%」
		CASEELSE
			PRINTFORMW 「おうえふかぁ〜？ ひもひいえふかぁ〜？ んぷっ%UNICODE(0x2764)% くちゅっ%UNICODE(0x2764)% ぐぷっ%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
ENDIF
