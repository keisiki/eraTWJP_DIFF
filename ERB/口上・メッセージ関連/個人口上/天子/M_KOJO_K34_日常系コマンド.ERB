;==================================================
;302,スキンシップ
;==================================================
@M_KOJO_MESSAGE_COM_K34_302
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K34_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K34_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;背中を流す
	IF BATHROOM(CFLAG:現在位置)
			PRINTFORMW 「%CALLNAME:MASTER%　背中、流して」
			PRINTFORMW 天子のお願いに応じて%CALLNAME:MASTER%は彼女の背中に手を伸ばした。
			PRINTFORMW 手のひらに泡を盛り、綺麗な柔肌を直接揉み込むように指を動かした。
			PRINTFORMW 「んー、気持ち良いわ～♪」
			PRINTFORMW 「洗いっこしましょう？ それっ！」
			PRINTFORMW その後天子に自分の身体を洗ってもらった。
	ELSE
	IF TALENT:恋人
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 
				PRINTFORML 天子は体を傾けてきた 
				PRINTFORMW 「いいじゃない、落ち着くのよ」
				
			CASE 2
				PRINTFORML 
				PRINTFORMW 「きゃあ！全くもう…どこ触ってんのよ」
				PRINTFORMW 「私だってこうしちゃうんだからっ」
				PRINTFORMW こうして互いの乳繰り合いが終わるのはそれから十分程後だった
			CASE 1
				PRINTFORML 
				PRINTFORMW 「くぅ…くぅ……」
				PRINTFORMW 天子はあなたの腕に抱かれたまま寝息を立てている
				PRINTFORMW あなたが頬をそっと撫でてやると天子は気持ち良さそうに身じろぎした
			CASE 0
				PRINTFORML 
				PRINTFORMW 「あはは！　くすぐったいったら！」
		ENDSELECT
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 
				PRINTFORMW 天子の頬を突付いてみた
				PRINTFORMW 「何？私を突付いても何も出ないわよ？」
			CASE 2
				PRINTFORML 
				PRINTFORMW 天子は%CALLNAME:MASTER%の腕に抱かれている
				PRINTFORMW 「ああ、眠くなるわ…」。
			CASE 1
				PRINTFORML 
				PRINTFORML 天子に抱きついてみた
				PRINTFORML 「ちょ、ちょっと…急にはやめてよ」 
				PRINTFORMW 言葉とは裏腹に天子はにやけている
			CASE 0
				PRINTFORML 
				PRINTFORMW 「ちょ、ちょっと…スキンシップを図るといっても…」
				PRINTFORMW 「これはやりすぎじゃあ……」
				PRINTFORMW そうは言っているが、抵抗の意思が見えないのは気のせいだろうか…
		ENDSELECT
	ELSEIF TALENT:思慕
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 
				PRINTFORMW 「まあ、それくらいならいいけど…」
				PRINTFORMW 「ひっ！　…へっ、ヘンなとこ触らないでよ」
			CASE 2
				PRINTFORML 
				PRINTFORMW 「ねえ、もうちょっと優しく…」
			CASE 1
				PRINTFORML 
				PRINTFORMW 「ん、なんか変な感じ…くすぐったいかも」
			CASE 0
				PRINTFORML 
				PRINTFORMW 「んふふ、なんだかくすぐったいわ」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
			CASE 1
				PRINTFORML 
				PRINTFORMW 「ちょっと、馴れ馴れしいわよ」
			CASE 0
				PRINTFORML 
				PRINTFORMW 「撫でるくらいだったらいいわ」
		ENDSELECT
	ENDIF
ENDIF
ENDIF
RETURN 1
