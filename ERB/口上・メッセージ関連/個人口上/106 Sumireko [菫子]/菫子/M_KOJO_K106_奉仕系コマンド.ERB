;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K106_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K106_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K106_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋人
			PRINTFORMW 「手でしろって？」
			PRINTFORMW 「…まぁ、いいけど」
			PRINTFORMW 菫子はたどたどしい手付きで%CALLNAME:MASTER%の肉棒に手を這わせた
			PRINTFORMW 「うっ…、何というか、逞しいわね…%UNICODE(0x2764)%」
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋人
	ELSEIF TALENT:恋人
		IF RAND:3 == 0
			PRINTFORMW 「もっと強いほうがいいのかしら」
			PRINTFORMW 「どう、%CALLNAME:MASTER%…痛くない？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「超能力コキっていうのを思いついたんだけど」
			PRINTFORMW 「やらなくていい？ でしょうね」
		ELSE
			PRINTFORMW 「何だか愛おしく思えてきたわ…」
			PRINTFORMW 「ほーらがんばれ%UNICODE(0x2764)% がんばれ%UNICODE(0x2764)%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

