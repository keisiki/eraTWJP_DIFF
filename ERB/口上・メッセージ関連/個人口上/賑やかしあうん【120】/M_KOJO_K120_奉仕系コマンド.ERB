	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「………………………………」
		PRINTFORMW あうんは一切の反応を返さず、%CALLNAME:MASTER%の肉棒を深く咥え込んだ。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORML 物言わぬあうんの口内に肉棒を突き入れた……。
		PRINTFORMW どうしてかあうんの舌が動いているような気がする……。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「じゅるっ……じゅるるるっ………………」
		PRINTFORMW あうんは憑りつかれたように%CALLNAME:MASTER%の肉棒を貪っている。
		IF NOWEX:PLAYER:射精
			CALL HPH_PRINT,@"「………………………HPH」","L"
			PRINTFORMW あうんの真っ黒に濁った瞳が、征服感に歪んだ。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

