;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K120_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K120_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K120_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「……手で、すればいいんですか？」
		PRINTFORMW 「いいですよ。%CALLNAME:MASTER%さんがそうして欲しいなら。」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW %CALLNAME:MASTER%は動かないあうんの顔に気の済むまで肉棒を擦り付けた……。
		RETURN 1
	ELSE
		;この後射精する(射精寸前の描写)
		IF NOWEX:PLAYER:射精
			PRINTFORML 「……………………」
			PRINTFORMW あうんは普段の媚びを一切忘れた無表情で射精寸前の肉棒を凝視している……。
		ELSE
			PRINTFORML 「………………あっ、何か言いました？」
			PRINTFORMW 無言で手淫を続けるあうんの目は、どろりとした劣情で濁っている……。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

