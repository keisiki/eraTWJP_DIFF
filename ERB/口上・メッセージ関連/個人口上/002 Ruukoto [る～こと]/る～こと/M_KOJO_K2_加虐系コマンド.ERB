;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K2_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$SPANKING_LOOP
	IF ( TALENT:恋慕 || TALENT:愛欲 || TALENT:思慕 || TALENT:セフレ ) && K2_BASIC_CONDITION_IN_MATING()
		IF ( ABL:マゾっ気 >= 4 || ( TALENT:マゾ && ABL:マゾっ気 >= 2 ) ) && ( !TALENT:サド || ABL:欲望 >= 20 )
			SELECTCASE RAND:4
			CASE 1
				IF CFLAG:1001 && !RAND:3
					IF TALENT:恋慕 || TALENT:恋人
						CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%さまに処女を捧げずにごめんなさいっHPH」","L"
						CALL K2_HPH_PRINT,@"「いろんな人のおちんぽ味わった中古ロボットをゆるしてくださいっHPH」","W"
					ELSEIF ABL:清掃技能 + ABL:料理技能 < 4
						CALL K2_HPH_PRINT,@"「お仕事おぼえずに、おまんこすることばかりおぼえた破廉恥メイドにお仕置きしてくださいHPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「んっ、はぁんっHPH 誰とでもおまんこする淫乱メイドでごめんなさいっHPH」","W"
					ENDIF
				ELSE
					GOTO SPANKING_LOOP
				ENDIF
			CASE 2
				CALL K2_HPH_PRINT,@"「あはぁHPH だめメイドにもっとお仕置きしてくださいっHPH」","W"
			CASE 3
				CALL K2_HPH_PRINT,@"「んふぅっHPH はぁHPH 痛いのすごいですHPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「あひぃHPH かはっ、んっHPH んはぁ、はぁ… もう…おしまいですか？HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「うぅ…… おしりぺんぺん痛いです」
			CASEELSE
				PRINTFORMW 「んくっ…… おしりジンジンします」
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「くぅっ…… 痛いですっ、やめてくださいっ」
		CASEELSE
			PRINTFORMW 「いたっ、んっ、はぁ、はぁ…… や、やめて、ください……」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

