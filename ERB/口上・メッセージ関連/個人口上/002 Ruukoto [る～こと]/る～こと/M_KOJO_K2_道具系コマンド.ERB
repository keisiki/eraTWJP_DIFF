;==================================================
;50,ゴム装着
;==================================================
@M_KOJO_MESSAGE_COM_K2_50
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_50_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_50_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K2_CONSCIOUS_AND_TOLERANT() && !TALENT:無知 && FIRSTTIME(SELECTCOM)
		IF !CFLAG:妊娠自覚状態 && !EXP:出産経験
			PRINTFORMW 「わたしはロボットなので妊娠しませんし、病気に関しても大丈夫ですが……」
			PRINTFORMDW それでもコンドームをつけようとする%CALLNAME:MASTER%を見て、%CALLNAME:TARGET%はいぶかしんでいる様子だ。
		ENDIF
		RETURN 1
	ENDIF
	
	;装着
	IF TEQUIP:MASTER:コンドーム
		IF K2_CONSCIOUS_AND_TOLERANT() && !TALENT:無知
			;育児中の子なし
			IF CFLAG:育児人数 == 0
				IF TALENT:恋慕 && CFLAG:生理周期 >= 3
					PRINTFORML 「あ……つけるんですね、それ………」
					PRINTFORMDW %CALLNAME:TARGET%はすこし残念そうな顔をしている。
				ELSEIF ( TALENT:恋慕 || TALENT:愛欲 ) && ABL:精液中毒 >= 5
					PRINTFORML 「おちんぽ汁ぴゅっぴゅして下さらないんですか？」
					PRINTFORMDW %CALLNAME:TARGET%はおあずけを食った犬のような表情だ。
				ENDIF
			ENDIF
		ENDIF
	;外す
	ELSE
	
	ENDIF
ENDIF
RETURN 1