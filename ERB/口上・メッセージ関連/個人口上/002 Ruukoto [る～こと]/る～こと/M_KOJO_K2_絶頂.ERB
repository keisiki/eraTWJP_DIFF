		ENDIF
		IF NOWEX:Ｂ絶頂
			;超強絶頂Ｂ
			IF NOWEX:Ｂ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K2_HPH_PRINT,@"「だめええ！ も、もうっHPH お胸が、すごいですっ！ んひっ、あっ、あーっHPH」","W"
				CASE 2
					CALL K2_HPH_PRINT,@"「んくぅっ、ちっ、乳首ぃっHPH ピリピリってぇっ！　しびれてますっHPH」","W"
				CASEELSE
					CALL K2_HPH_PRINT,@"「ふあぁっ、あぁっHPH おっぱいがっ、ジンジンしますっ、トんじゃいますっHPH」","W"
				ENDSELECT
			;強絶頂Ｂ
			ELSEIF NOWEX:Ｂ絶頂 == 2
				PRINTFORMW 「いいですっ、おっぱいっ、いぃ、んぁっ」
			;絶頂Ｂ
			ELSE
				PRINTFORMW 「ふあっ、あっ あふっ、んっ」
			ENDIF
		ENDIF
		[SKIPSTART]
		IF NOWEX:Ｍ絶頂
			;超強絶頂Ｍ
			IF NOWEX:Ｍ絶頂 == 3
				PRINTFORMW 
			;強絶頂Ｍ
			ELSEIF NOWEX:Ｍ絶頂 == 2
				PRINTFORMW 
			;絶頂Ｍ
