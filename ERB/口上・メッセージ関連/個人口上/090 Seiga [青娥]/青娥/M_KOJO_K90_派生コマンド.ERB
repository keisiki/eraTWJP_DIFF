;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K90_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K90_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K90_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「ん～～～%UNICODE(0x2665) *1%」
			PRINTFORMW 青娥は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			PRINTFORMW 「早く濃いのくださいまし%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「じゅる…%UNICODE(0x2665) *1%…ん…%UNICODE(0x2665) *1%…出して…%UNICODE(0x2665) *1%」
			PRINTFORMW 青娥は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
		ELSE
			PRINTFORMW 「…ちゅる…%UNICODE(0x2665) *1%」
			PRINTFORMW 青娥は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			PRINTFORMW 「…ん…れろ…ちゅっ…我慢しないで濃厚ザーメンいっぱいくださいまし%UNICODE(0x2665) *1%」
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…%UNICODE(0x2665) *1%…れろ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…これ…とっても美味しい…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「っ…じゅぶ…%UNICODE(0x2665) *1%…んっ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…我慢汁…%UNICODE(0x2665) *1%…だけじゃ足りないの…%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「じゅる…%UNICODE(0x2665) *1%…れろっ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「口の中ザーメンでいっぱいにしてくださいまし…%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「ん…美味し%UNICODE(0x2665) *1%」
			PRINTFORMW 青娥は勢い良く陰茎に吸い付いている。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んちゅっ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「れろっ%UNICODE(0x2665) *1%射精してくださいまし%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「口でイッてもいいですわよ%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「っ…ん…ちゅるる…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…濃厚な雄の味ですわね%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…れろ…チンカスち○ぽ…綺麗にしてあげますわ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「…ん…」
			PRINTFORMW 青娥はどこかぎこちなく陰茎を口に含んだ。
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K90_{SELECTCOM}_1
RETURN 1

