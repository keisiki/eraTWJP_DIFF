;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「恋人デートするわよ～%CALLNAME:MASTER%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ねえ、%CALLNAME:MASTER%……デートしましょ？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、暇じゃない？一緒にデートしましょっ♪」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、暇でしょ？デート行くわよ～」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「うふふっ！じゃあ行きましょっか……どこでもいいのよ%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「甘味を食べたい気分ね～ふふっ」
			ELSEIF TALENT:思慕
				PRINTFORMW 「では甘味でも食べに行きましょっか？」
			ELSE
				PRINTFORMW 「別にモテなあなくてもお話だけでもいいのよ～？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「もう、残念ねぇ。じゃあまた今度お願いね？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ふ～ん、私より遊びたい女の子がいるのかしら～」
			ELSE
				PRINTFORMW 「つれないわねぇ～……」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「%CALLNAME:MASTER%、お部屋デートしましょっ♪」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%、うちまで来ないかしら？いいお茶菓子入ったのよ～」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、お茶を酌みにおうちまで来ないかしら～？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「……っ%UNICODE(0x2665) *1%　ねぇ……何したい？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ふぅ～っ……ゆったりしましょうか？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「……何も面白いものがないし妖夢が来たら虐めましょっか？」
			ELSE
				PRINTFORMW 「誘っておいてあれだけど何もないから適当に過ごしなさ～い」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「いいわよ～、じゃあまた別の機会にね？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ふ～ん、じゃあまた今度誘うわ～」
			ELSEIF TALENT:思慕
				PRINTFORMW 「……別に変なことするつもりは無いのに～」
			ELSE
				PRINTFORMW 「女の子がお家まで誘ってるのにつれないわねぇ～……」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

