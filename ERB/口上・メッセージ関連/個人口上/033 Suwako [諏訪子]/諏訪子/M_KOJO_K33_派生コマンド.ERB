@M_KOJO_MESSAGE_SCOM_K33_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K33_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K33_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		CALL K33_TATARI_COUNTER(3)
		RETURN 1
	ELSEIF NOWEX:TARGET:Ｃ絶頂
		PRINTDATAW
			DATAFORM 「んッ%UNICODE(0x2764)%　ダメっ、イかされちゃうぅ%UNICODE(0x2764)%」
			DATAFORM 「ちゅ……んぶ……！　やぁ……先に、イっちゃ……ふああぁッ%UNICODE(0x2764)%」
			DATAFORM 「いいよっ、もっとはげしくしてぇ……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF NOWEX:MASTER:射精
		PRINTDATAW
			DATAFORM 「んちゅ、ちゅぶ……えへへ、イっちゃう？　いいよ、出して出してー♪」
			DATAFORM 「んー、手が止まっちゃってるよー？　イきそうなんだ……ふふ%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF NOWEX:MASTER:射精 && NOWEX:TARGET:Ｃ絶頂
		PRINTDATAW
			DATAFORM 「やぁ……も、イっちゃう……%UNICODE(0x2764)%　ね、いっしょに……ん、ちゅ……ふああッ%UNICODE(0x2764)%」
			DATAFORM 「んーッ、れろ……ふやッ%UNICODE(0x2764)%　もお、ガマンしないで、イっちゃってよぉ……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ
		PRINTDATAW
			DATAFORM 「ひゃあん%UNICODE(0x2764)%　ん……、負けてらんないねえ♪　んちゅ、んぐ……ちゅうう%UNICODE(0x2764)%」
			DATAFORM 「やん、どっちが先に、ッ、イっちゃうのかなー%UNICODE(0x2764)%」
			DATAFORM 「んッ、ちゅ……ッ、はあっ……気持ちいいよっ、もっとしてぇ……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSE
		PRINTFORMW 「ひゃあん%UNICODE(0x2764)%　ん……、負けてらんないねえ♪　んちゅ、んぐ……ちゅうう%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
ENDIF
