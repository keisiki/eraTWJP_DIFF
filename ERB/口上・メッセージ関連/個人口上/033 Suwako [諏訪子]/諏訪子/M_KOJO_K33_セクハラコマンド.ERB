					ENDIF
				;それ以外
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%の舌、いやらしくはいまわってる……%UNICODE(0x2764)%」
					RETURN 1
				ENDIF
			CASE 87, 89
				CALL INSERT_KOJO_K33(1, 0, 1)
			CASEELSE
				IF NOWEX:TARGET:Ｖ絶頂
					PRINTDATAW
						DATAFORM 「やぁ%UNICODE(0x2764)%……もう、イきそう、だから、とめないでぇ%UNICODE(0x2764)%」
						DATAFORM 「ふぁ……%CALLNAME:MASTER%だからかな、こんなに、感じてるの……ねぇ……わかる？」
						DATAFORM 「ああっ%UNICODE(0x2764)%　イかされちゃう、よぉ……♪」
					ENDDATA
				ELSE
					PRINTDATAW
						DATAFORM 「ひゃぅ……%CALLNAME:MASTER%　指、えっちぃよぉ%UNICODE(0x2764)%」
						DATAFORM 「いやらしい音、させないでよぉ……わざとでしょ、もぉ%UNICODE(0x2764)%」
						DATAFORM 「あっ……やだぁ……ぐちゃぐちゃになっちゃうよぉ……%UNICODE(0x2764)%」
					ENDDATA
				ENDIF
				RETURN 1
		ENDSELECT
	ELSE
		IF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ
			IF NOWEX:TARGET:Ｖ絶頂
				PRINTDATAW
					DATAFORM 「だめ%UNICODE(0x2764)%……もう、イっちゃ……ああっ%UNICODE(0x2764)%」
					DATAFORM 「ふぁ……%CALLNAME:MASTER%だからかな、こんなに、感じてるの……ねぇ……わかる？」
					DATAFORM 「ああっ%UNICODE(0x2764)%　こんなところで、イかされちゃう、よぉ……♪」
				ENDDATA
			ELSE
				PRINTDATAW
					DATAFORM 「ひゃぅ……　もぉ、こんなところでするなんて%UNICODE(0x2764)%」
					DATAFORM 「いやらしい音、させないでよぉ……わざとでしょ、もぉ%UNICODE(0x2764)%」
					DATAFORM 「あぅ……ぐちゃぐちゃにするつもりでしょぉ%UNICODE(0x2764)%」
				ENDDATA
			ENDIF
			RETURN 1
