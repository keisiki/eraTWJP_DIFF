				ENDDATA
		ENDSELECT
	ELSE
		IF (TALENT:[[諏訪子]]:恋慕 || TALENT:[[諏訪子]]:愛欲 || TALENT:[[諏訪子]]:セフレ) && (CFLAG:[[諏訪子]]:溜まってる度 >= 800 || TCVAR:[[諏訪子]]:発情 || PALAM:[[諏訪子]]:欲情 >= PALAMLV:5)
