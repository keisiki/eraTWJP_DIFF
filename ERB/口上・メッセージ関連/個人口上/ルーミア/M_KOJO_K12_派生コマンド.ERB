;==================================================
;7,挿入子宮口責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K12_7
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K12_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K12_7_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
PRINTFORML 
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
	;	PRINTFORMW 「」
	;	GOTO _SIKYUUKOU
	;	RETURN 1
	;ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;射精1ターン後。射精するときの口上はNOWEX:MASTER:射精
	ELSEIF TCVAR:102 && !NOWEX:MASTER:射精
		IF (TALENT:恋慕 || TALENT:恋人) && CFLAG:生解禁 > 0
			SELECTCASE RAND:5
				CASE 1
					CALL HPH_PRINT("「おちんちんで子宮に蓋しちゃってる…　そんなに孕ませたいの？　くすHPH」","L")
				CASE 2
					CALL HPH_PRINT("「今突っついちゃダメぇHPH　子宮が喜んじゃってるよぉ…HPH」","L")
				CASE 3
					CALL HPH_PRINT("「こんな奥ばっかり責められたら…　赤ちゃん欲しくなっちゃうよHPH」","L")
				CASE 4
					CALL HPH_PRINT(@"「えへへーHPH　%CALLNAME:MASTER%の美味しいわ。　おかわりちょーだいHPH」","L")
				CASEELSE
					CALL HPH_PRINT("「せーえきHPH　お腹の奥に押しこまれちゃう…HPH」","L")
			ENDSELECT
				
			SELECTCASE RAND:2
				CASE 1
					PRINTFORMW ルーミアの膣は%CALLNAME:MASTER%からさらに精液を搾り取ろうとペニスに絡みついている…
				CASEELSE
					PRINTFORMW ルーミアの子宮口は%CALLNAME:MASTER%の精液を吸い尽くそうと亀頭を咥えこんでいる…
			ENDSELECT
		ELSEIF CFLAG:既成事実 >= 1
			SELECTCASE RAND:5
				CASE 1
					PRINTFORMW 「今出したばっかりなのにっ…　元気すぎよぉ」
				CASE 2
					PRINTFORMW 「さっき出したのが溢れてきてる…　すっごくエッチね♪」
				CASE 3
					PRINTFORMW 「やーん、中でぐちゅぐちゅ言ってるぅ…」
				CASE 4
					PRINTFORMW 「一旦抜かないと、せーしが奥に入っちゃうよぉ」
				CASEELSE
					PRINTFORMW 「まだ出し足りないのぉ？　スケベぇ…」
			ENDSELECT
		ENDIF
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:恋人
		IF EXP:子宮口開発経験 >= 50
			SELECTCASE RAND:5
				CASE 1
					CALL HPH_PRINT(@"「そこグリグリされるの好きぃHPH　もっとやってぇHPH」","L")
				CASE 2
					CALL HPH_PRINT(@"「奥のお口で%CALLNAME:MASTER%のおちんちん食べちゃったHPH　このままいーっぱい出してねHPH」","L")
				CASE 3
					IF TALENT:妊娠
						CALL HPH_PRINT("「子宮開いちゃう…　赤ちゃんに精液かかっちゃうよぉ…HPH」","L")
					ELSE
						CALL HPH_PRINT("「子宮開いちゃう…　おちんちん入っちゃうよぉ…HPH」","L")
					ENDIF
				CASE 4
					CALL HPH_PRINT("「思いっきり押しこんでねHPH　子宮の奥まで届くくらいに…HPH」","L")
				CASEELSE
					CALL HPH_PRINT(@"「おまんこ全部%CALLNAME:MASTER%のものにされちゃったHPH」","L")
					CALL HPH_PRINT("「いっちばん奥でセックスするのって、気持ちいいねHPH」","L")
			ENDSELECT
				SELECTCASE RAND:4
					CASE 1
						PRINTFORMW ルーミアは快楽に染まった顔で%CALLNAME:MASTER%の突きこみを受け入れている…
					CASE 2
						PRINTFORMW ルーミアは甘い声をあげて%CALLNAME:MASTER%のペニスに蹂躙されている…
					CASE 3
						PRINTFORMW ルーミアの膣肉は最奥を突かれる度に%CALLNAME:MASTER%のペニスを優しく締めあげる…
					CASEELSE
						PRINTFORMW ルーミアの子宮口がペニスを歓迎するかのように柔らかく包み込んでくる…
				ENDSELECT
					
		ELSE
			SELECTCASE RAND:5
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%を全部受け止められたの、嬉しいよ…♪」
				CASE 2
					PRINTFORML 「ちょっときついねー。%CALLNAME:MASTER%は痛くない？」
				CASE 3
					PRINTFORML 「%CALLNAME:MASTER%のおっきいねー。おへそのとこまで来てるよ」
				CASE 4
					PRINTFORML 「%CALLNAME:MASTER%のでお腹ぱんぱん。まるで妊娠したみたい…♪」
				CASEELSE
					PRINTFORML 「一番奥はまだ痛いけど、ちょっとゾクゾクする感じ…」
			ENDSELECT
				PRINTFORMW ルーミアの膣肉は突きこまれたペニスをきつく締めつけている…
		ENDIF
		RETURN 1
	ELSEIF CFLAG:既成事実 >= 1
		SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 「お腹、苦しいよ…」
			CASE 2
				PRINTFORMW 「それ以上入らないってばー…」
			CASE 3
				PRINTFORMW 「やー、そこ痛いー」
			CASE 4
				PRINTFORMW 「ああ…　奥まで入れられちゃったよぅ…」
			CASEELSE
				PRINTFORMW 「赤ちゃんの部屋、潰れちゃう…！」
		ENDSELECT
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
;$_SIKYUUKOU
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_KX_{SELECTCOM}_1
RETURN 1

