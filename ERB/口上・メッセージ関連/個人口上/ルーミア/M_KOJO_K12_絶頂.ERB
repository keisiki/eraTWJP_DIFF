;-------------------------------------------------
;膣内
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TEQUIP:50 == MASTER
		PRINTFORML 
		;初めて
		IF CFLAG:1002 == 0
			IF CFLAG:既成事実 == 0
			ELSEIF CFLAG:1003 == 1
				PRINTFORML 「奥にどっくんどっくん出てるよ…　初めての膣内射精、どうだった？」
				CALL HPH_PRINT("「次はもうちょっと我慢して、一緒に気持ちよくなろうねーHPH」","W")
			ELSE
				PRINTFORML 「えっ、なに！？　お腹の中で、あっついのが出てる…」
				PRINTFORML 「…もしかして精液？　そっかー、これが射精なんだ…」
				PRINTFORMW ルーミアは感慨深げに脈動を続ける結合部を眺めている…
				PRINTFORML 
				IF (TALENT:恋慕 || TALENT:恋人) && CFLAG:生解禁 > 1
					PRINTFORML 「…これって、赤ちゃん作ってるんだよね」
					PRINTFORML 「%CALLNAME:MASTER%は子供欲しい？　私に産ませたい？」
					PRINTFORMW 「%CALLNAME:MASTER%が望むなら私は…　いいよ？」
				ELSEIF CFLAG:生解禁 >= 1
					PRINTFORML 「…子供できちゃったら、どうするの？　二人で育てる？」
					PRINTFORMW ルーミアは%CALLNAME:MASTER%の目をじっと見つめている…
				ELSEIF CFLAG:既成事実 >= 1
					PRINTFORML 「…って、ダメ！　中に出されたら妊娠しちゃう！」
					PRINTFORMW 我に返ったルーミアを押さえつけ、%CALLNAME:MASTER%は最後の一滴まで注ぎ込んだ…
				ENDIF
			ENDIF
			CFLAG:1002 = 1
		ELSEIF !TCVAR:12:ピル && !TALENT:妊娠
			IF TALENT:恋慕 || TALENT:恋人
				IF CFLAG:生解禁 > 1
					;乳首吸いで射精
					IF SELECTCOM == 10 || SELECTCOM == 11
						SELECTCASE RAND:5
							CASE 1
								PRINTFORML 「エッチな赤ちゃんね。ママを妊娠させたいのー？」
							CASE 2
								CALL HPH_PRINT("「この満たされてく感じ、好きぃ…HPH」","L")
							CASE 3
								CALL HPH_PRINT(@"「赤ちゃん欲しくなっちゃうよぉHPH　%CALLNAME:MASTER%…HPH」","L")
							CASE 4
								PRINTFORML 「おっぱい吸いながらびゅーびゅーって、甘えん坊さん♪」
							CASEELSE
								PRINTFORML 「おちんちんミルク、いっぱいもらっちゃった」
								IF TALENT:母乳体質
									CALL HPH_PRINT("「お返しに私のミルク、いっぱい飲んでねHPH」","L")
								ELSE
									CALL HPH_PRINT("「本当におっぱいが出るようになるかも…HPH」","L")
								ENDIF
						ENDSELECT
						PRINTFORMW ルーミアは%CALLNAME:MASTER%を優しく抱きしめながら射精を受け止めている…
					;子宮口責め
					ELSEIF SELECTCOM == 73
						IF CFLAG:生理周期 == 7
							SELECTCASE RAND:5
								CASE 1
									GOTO _NAKADASI10
								CASE 2
									CALL HPH_PRINT("「まだ抜いちゃだめー。子宮いっぱいになるまで放してあげない…HPH」","L")
								CASE 3
									CALL HPH_PRINT(@"「おなかの奥に届いてるよ…　%CALLNAME:MASTER%の赤ちゃんの素…HPH」","L")
								CASE 4
									CALL HPH_PRINT("「いつもより多いよ…　孕ませる気、満々だねーHPH」","L")
								CASEELSE
									CALL HPH_PRINT("「精液がどんどん溜まってく…　お腹膨らんじゃいそうHPH」","L")
							ENDSELECT
							PRINTFORMW ルーミアの子宮は%CALLNAME:MASTER%の精液を吸い出すように蠕動している…
						ELSE
							SELECTCASE RAND:5
								CASE 1
									GOTO _NAKADASI11
								CASE 2
									CALL HPH_PRINT("「もっと押しつけていいよー。ぜーんぶ飲んじゃうから…HPH」","L")
								CASE 3
									CALL HPH_PRINT("「奥にびゅくびゅく当たってるよぉ…HPH」","L")
								CASE 4
									CALL HPH_PRINT("「乱暴に射精されるのって、気持ちいい…HPH」","L")
								CASEELSE
									CALL HPH_PRINT("「ぎゅーって抱きしめて…　全部出して…HPH」","L")
							ENDSELECT
							PRINTFORMW ルーミアの子宮は%CALLNAME:MASTER%のペニスを咥えこんで離そうとしない…
						ENDIF
					ELSE
						IF CFLAG:生理周期 == 7
							$_NAKADASI10
							SELECTCASE RAND:6
								CASE 1
									CALL HPH_PRINT("「ぜーんぶ出し切った？　ふふっ、早くお母さんになりたいな…HPH」","L")
								CASE 2
									CALL HPH_PRINT("「これで妊娠できたかなー。念のため、もうちょっと欲しいなーHPH」","L")
								CASE 3
									CALL HPH_PRINT("「子供ができたら一緒に育てようね、パパHPH」","L")
								CASE 4
									CALL HPH_PRINT("「いーっぱい出してHPH　子供作ろHPH」","L")
								CASE 5
									CALL HPH_PRINT("「これでできちゃったかもねーHPH　へへへ…HPH」","L")
								CASEELSE
									GOTO _NAKADASI13
							ENDSELECT
							PRINTFORMW ルーミアは幸せそうに下腹部を撫でている…
						ELSE
							$_NAKADASI11
							SELECTCASE RAND:6
								CASE 1
									CALL HPH_PRINT("「お腹の中あったかくて気持ちいいよ…　えへへHPH」","L")
								CASE 2
									CALL HPH_PRINT("「精液でお腹いっぱいHPH　幸せー♪」","L")
								CASE 3
									CALL HPH_PRINT("「おちんちんと精液が暴れてるのHPH　すっごく気持ちいい…HPH」","L")
								CASE 4
									CALL HPH_PRINT("「膣内が精液でいっぱいHPH　やん、こぼれちゃう…」","L")
								CASE 5
									CALL HPH_PRINT("「全部搾りとってあげるHPH　いーっぱい射精してね…HPH」","L")
								CASEELSE
									GOTO _NAKADASI14
							ENDSELECT
							PRINTFORMW ルーミアは満足そうにお腹をさすっている…
						ENDIF
					ENDIF
				;危険日生許可なし
				ELSE
					IF SELECTCOM == 73
						$_NAKADASI13
						SELECTCASE RAND:4
							CASE 1
								CALL HPH_PRINT(@"「%CALLNAME:MASTER%の精液、全部子宮で食べちゃった…HPH」","W")
							CASE 2
								PRINTFORMW 「いっぱい種付けされちゃった…　くすくす♪」
							CASE 3
								PRINTFORMW 「射精したのに大きいままだねー。続けてやっちゃう？」
							CASEELSE
								PRINTFORMW 「奥にびゅーって出されるの、気持ちいいねー」
						ENDSELECT
					ELSE
						$_NAKADASI14
						SELECTCASE RAND:4
							CASE 1
								PRINTFORMW 「気持ちよかった？　もっと射精していーよ♪」
							CASE 2
								PRINTFORMW 「中に射精されるの、好きになってきちゃった」
							CASE 3
								PRINTFORMW 「こんなに中出しされたら、いつか孕んじゃうかも…　なんてね♪」
							CASEELSE
								PRINTFORMW 「あん、精液零れちゃった…　もう一回注いで♪」
						ENDSELECT
					ENDIF
				ENDIF
			ELSEIF CFLAG:既成事実 >= 1 && CFLAG:生解禁 > 0
				IF SELECTCOM == 73
					SELECTCASE RAND:4
						CASE 1
							PRINTFORMW 「そんな奥に出さないでー。妊娠しちゃったらどうするのよー」
						CASE 2
							PRINTFORMW 「お腹の奥で暴れてる…　気持ちいいけど、ちょっと怖い…」
						CASE 3
							PRINTFORMW 「好きな人とじゃないと子供作っちゃいけないんだよー？」
						CASEELSE
							PRINTFORMW 「やだぁ…　子宮の中まで入っちゃう…」
					ENDSELECT
				ELSE
					SELECTCASE RAND:4
						CASE 1
							PRINTFORMW 「もし当たっちゃったら責任取ってよー」
						CASE 2
							PRINTFORMW 「なんか変な感じねー。あ、垂れてきた…」
						CASE 3
							PRINTFORMW 「もー、子供欲しいんなら恋人としてよー」
						CASEELSE
							PRINTFORMW 「膣内でびくんびくんしてる…　イっちゃったんだ…」
					ENDSELECT
				ENDIF
			ELSEIF !CFLAG:生解禁
				IF CFLAG:生理周期 < 3
					IF CFLAG:既成事実 >= 1
						GOTO _NAKADASI12
					ELSE
						SELECTCASE RAND:3
							CASE 1
								PRINTFORMW 「無理やりなんて…　最低よ」
							CASE 2
								PRINTFORMW 「中で出しちゃだめー、気持ち悪いー」
							CASEELSE
								PRINTFORMW （今日は安全日…　だった、よね？）
						ENDSELECT
					ENDIF
				ELSE
					SELECTCASE RAND:3
						CASE 1
							PRINTFORMW 「精液いらない…　抜いてよぉ…」
						CASE 2
							PRINTFORMW 「なんで、こんなひどい事するの…？　%CALLNAME:MASTER%…」
						CASEELSE
							PRINTFORMW 「うう…　赤ちゃんできちゃうよぅ…」
					ENDSELECT
				ENDIF
			ENDIF
		ELSEIF TALENT:妊娠
			SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「ふう…　お腹苦しい…」
				CASE 2
					PRINTFORMW 「もうちょっと優しくしてほしかったなー」
				CASEELSE
					PRINTFORMW 「せめて外に出してよー。赤ちゃんがかわいそうじゃない」
			ENDSELECT
		ELSE
			;IF (TALENT:恋慕 || TALENT:恋人) && TALENT:育児中
			;	SELECTCASE RAND:3
			;		CASE 1
			;			PRINTFORMW 「」
			;		CASE 2
			;			PRINTFORMW 「」
			;		CASEELSE
			;			PRINTFORMW 「この子に手がかからなくなったら、弟か妹作ってあげたいねー」
			;	ENDSELECT
			;ピル中出し口上が思いつかなかったのでコピペ
			IF TALENT:恋慕 || TALENT:恋人
				SELECTCASE RAND:5
					CASE 1
						CALL HPH_PRINT("「お腹の中あったかくて気持ちいいよ…　えへへHPH」","L")
					CASE 2
						CALL HPH_PRINT("「精液でお腹いっぱいHPH　幸せー♪」","L")
					CASE 3
						CALL HPH_PRINT("「おちんちんと精液が暴れてるのHPH　すっごく気持ちいい…HPH」","L")
					CASE 4
						CALL HPH_PRINT("「膣内が精液でいっぱいHPH　やん、こぼれちゃう…」","L")
					CASEELSE
						CALL HPH_PRINT("「全部搾りとってあげるHPH　いーっぱい射精してね…HPH」","L")
				ENDSELECT
				PRINTFORMW ルーミアは満足そうにお腹をさすっている…
			ELSE
				$_NAKADASI12
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「男の人がイくときの顔って、面白いねー」
						PRINTFORMW 「もっかいやってあげようか？」
					CASE 2
						PRINTFORMW 「中出しは好きじゃないのよねー。後始末が面倒だし」
					CASEELSE
						PRINTFORMW 「どうせ妊娠しないんだし、好きなだけ出していいわよ」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		RETURN 1
	ENDIF
	RETURN 1
ENDIF
RETURN 1

