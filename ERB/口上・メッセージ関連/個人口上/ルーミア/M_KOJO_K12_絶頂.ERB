;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな変数などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:Ｃ絶頂 NOWEX:Ｖ絶頂 NOWEX:Ｂ絶頂 NOWEX:Ａ絶頂 NOWEX:Ｍ絶頂
;各部位の絶頂強度。まず0~3が割り当てられ、多重絶頂により最大16倍される。つまり単部位なら最大3、5重絶頂で最大48
;NOWEX:TotalEX
;多重絶頂補正を受ける前の各部位絶頂強度の合計値。1~15。このテンプレにおける分岐の基準。

;TEQUIP:50
;Ｖに誰の竿が入ってるか。TEQUIP:50 = MASTERであなたが挿入中となる。
;TEQUIP:51
;Ａに誰の竿が入ってるか。TEQUIP:51 = MASTERであなたが挿入中となる。
;NOWEX:射精
;射精量、最大3。たとえばIF NOWEX:MASTER:射精 > 1だと、あなたが大量射精以上した場合となる。
;SYNCED_ORGASM(TARGET)
;相手とあなたが同時に絶頂した場合真となる。絶頂の部位は問わない。

;-------------------------------------------------
;絶頂時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K12
#DIM TotalEX
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	IF TCVAR:発情
		IF TALENT:恋慕
			IF ABL:Ｂ感覚 >= 10
				SELECTCASE RAND:4
					CASE 1
						CALL HPH_PRINT("「んんっ…HPH　おっぱい出すの…気持ちいいよぉHPH」","W")
					CASE 2
						CALL HPH_PRINT("「びゅーびゅー…ってHPH　えへへぇHPH」","W")
					CASE 3
						CALL HPH_PRINT("「もっと搾って…HPH　ほら…まだまだ出るからぁHPH」","W")
					CASEELSE
						CALL HPH_PRINT("「きもちいーの…HPH　もっと出すのぉ…HPH」","W")
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						CALL HPH_PRINT("「やぁん…おっぱい止まらないよぉ…HPH」","W")
					CASE 2
						CALL HPH_PRINT("「はあぁ…HPH　おっぱい出ちゃった…」","W")
					CASEELSE
						CALL HPH_PRINT("「また、出ちゃう…っHPH」","W")
				ENDSELECT
			ENDIF
		;ELSEIF TALENT:思慕
		
		ELSE
			SELECTCASE RAND:3
				CASE 1
					CALL HPH_PRINT("「やだぁ…止まらないよぉ…HPH」","W")
				CASE 2
					CALL HPH_PRINT("「胸がムズムズして…ん、んんっHPH」","W")
				CASEELSE
					CALL HPH_PRINT("「見ないで…恥ずかしいよぉ…HPH」","W")
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:恋慕
			IF ABL:Ｂ感覚 >= 10
				SELECTCASE RAND:5
					CASE 1
						PRINTFORMW 「んう…おっぱい出すの、ちょっと気持ちいいかも…」
					CASE 2
						PRINTFORMW 「えへへ…こんなに出ちゃった。飲んでみる？」
					CASE 3
						PRINTFORMW 「いつでもお母さんになれるよー。ふふっ♪」
					CASE 4
						PRINTFORMW 「%CALLNAME:MASTER%の赤ちゃん、欲しいなー？」
					CASEELSE
						PRINTFORMW 「まだ張ってる気がするの。もう少し搾って欲しいなー」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「ちょっと苦しいけど、%CALLNAME:MASTER%がしてくれるなら…」
					CASE 2
						PRINTFORMW 「慣れてくれば楽に出せるようになるのかな…」
					CASEELSE
						PRINTFORMW 「どう？興奮したー？ふふ…♪」
				ENDSELECT
			ENDIF
		;ELSEIF TALENT:思慕
		
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「もう…早く止まってくれないかなぁ…」
				CASE 2
					PRINTFORMW 「胸は痛いし服は汚れるし…いい事無いわね」
				CASEELSE
					PRINTFORMW 「変な目でみないでよぉ…」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;-------------------------------------------------
;射精した
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	;口
	IF TCVAR:2 == 4
		IF TCVAR:発情
			IF TALENT:恋慕
				IF ABL:Ｃ感覚 >= 10
					SELECTCASE RAND:5
						CASE 1
							CALL HPH_PRINT("「私の精液好き？好きでしょHPH　今あげるから…受け取ってHPH」","W")
						CASE 2
							CALL HPH_PRINT(@"「%CALLNAME:MASTER%のお口好きぃHPH　もっと出したいよぉ…HPH」","W")
						CASE 3
							CALL HPH_PRINT("「吸って！せーえき吸ってHPH　あ…もう出ちゃうっHPH」","W")
						CASE 4
							CALL HPH_PRINT("「口の中に精液出すよHPH　いいよね？飲んで！全部飲んでHPH」","W")
						CASEELSE
							CALL HPH_PRINT(@"「もう一回舐めてHPH　%CALLNAME:MASTER%もお代わり欲しいでしょ？」","W")
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
						CASE 1
							CALL HPH_PRINT(@"「もう限界…！%CALLNAME:MASTER%のお口でイかせてぇっHPH」","W")
						CASE 2
							CALL HPH_PRINT("「先っぽペロペロされたら…もうイっちゃうよぉHPH」","W")
						CASEELSE
							CALL HPH_PRINT("「ああ…精液ごくごくされてるよぅHPH」","W")
					ENDSELECT
				ENDIF
			ELSEIF TALENT:思慕
					SELECTCASE RAND:3
						CASE 1
							CALL HPH_PRINT("「おちんちん気持ちよくって…我慢できないよ…HPH」","W")
						CASE 2
							CALL HPH_PRINT("「このままだと出しちゃうよ…？せーえき飲まされちゃうよ…HPH」","W")
						CASEELSE
							CALL HPH_PRINT("「気持ちいいの…止められないぃHPH」","W")
					ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						CALL HPH_PRINT("「もう出ちゃうからっ、口放してぇHPH」","W")
					CASE 2
						CALL HPH_PRINT("「今はイったばかりだから…舐めないでよぉ…HPH」","W")
					CASEELSE
						CALL HPH_PRINT("「うう…我慢できなかったよぉHPH」","W")
				ENDSELECT
			ENDIF
		ELSE
			;IF TALENT:恋慕
			;	SELECTCASE RAND:5
			;		CASE 1
			;			PRINTFORMW 「」
			;		CASE 2
			;			PRINTFORMW 「」
			;		CASE 3
			;			PRINTFORMW 「」
			;		CASE 4
			;			PRINTFORMW 「」
			;		CASEELSE
			;			PRINTFORMW 「」
			;	ENDSELECT
			IF TALENT:思慕
				SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「おいしくないでしょ？無理しないでいいよー」
					CASE 2
						PRINTFORMW 「口に出しちゃった…ごめんね」
					CASEELSE
						PRINTFORMW 「はぁ…おちんちん、すごかった…」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「それ、おいしくないでしょー？」
					CASE 2
						PRINTFORMW 「射精までさせるなんて…悪趣味」
					CASEELSE
						PRINTFORMW 「精液飲むなんて…変態」
				ENDSELECT
			ENDIF
		ENDIF
	ELSEIF TCVAR:2 == 1
	;PRINTFORMW 膣
	
	ELSE
	;PRINTFORMW 他
	
	ENDIF
	
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
;合計絶頂数で分岐
;これだけではどの部位が絶頂したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			PRINTFORMW 
		;中
		CASE 2 TO 4
			PRINTFORMW 
		;弱
		CASE 1
			PRINTFORMW 
	ENDSELECT
ENDIF

