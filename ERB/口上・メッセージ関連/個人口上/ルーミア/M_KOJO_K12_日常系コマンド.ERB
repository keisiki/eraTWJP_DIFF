@M_KOJO_MESSAGE_COM_K12_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K12_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K12_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:ブチギレ == 1 || TALENT:機嫌 < 0
		PRINTFORML 「むー…」
		PRINTFORMW ルーミアは%CALLNAME:MASTER%を睨んでいる…
	ELSEIF TFLAG:193 < 0
		SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 「うえっ！　ゲホッゴホッ」
			CASE 2
				PRINTFORMW 「これ…何入れたの？」
			CASE 3
				PRINTFORMW 「うえー…しぶいー」
			CASE 4
				PRINTFORMW 「%CALLNAME:MASTER%も飲んでみてよ、コレ…」
			CASEELSE
				PRINTFORMW 「このお茶きらいー」
		ENDSELECT
	ELSEIF TCVAR:発情
		;恋慕かつ危険日中出し許可
		IF TALENT:恋慕 && CFLAG:12:生解禁 == 2
			SELECTCASE RAND:5
				CASE 1
					CALL HPH_PRINT("「お茶よりー、別のものが飲みたいなーHPH」","L")
				CASE 2
					CALL HPH_PRINT("「ふふーHPH　これじゃ何か混ぜられててもわからないわねーHPH」","L")
				CASE 3
					CALL HPH_PRINT("「せっかくのチャンスなのにー。もったいなーいHPH」","L")
				CASE 4
					CALL HPH_PRINT("「お茶じゃあ静まらないよー。発散させないとHPH」","L")
				CASEELSE
					CALL HPH_PRINT("「のんびりしてると襲っちゃうよ…くすくすHPH」","L")
			ENDSELECT
			
					PRINTFORMW ルーミアは期待した目で見つめている…
		;ウフフ合意あり
		ELSEIF CFLAG:12:既成事実 >= 1
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「ここでお茶とかさー、空気読んでよー」
					PRINTFORMW ルーミアは不満そうだ
				CASE 2
					PRINTFORML ルーミアは湯飲みのふちに舌を這わせている…
					PRINTFORMW 「くすくす…想像しちゃった？」
				CASEELSE
					PRINTFORML 「休憩した後は…あっちで、ね？」
					PRINTFORMW 誘うような目で%CALLNAME:MASTER%を見つめている…
				;思いつかないので保留
				;CASE 4
				;	PRINTFORML 「」
				;	PRINTFORMW 
				;CASEELSE
				;	PRINTFORML 
				;	PRINTFORMW 
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 1
					PRINTFORML 「少しは気が静まるかも…」
				CASE 2
					PRINTFORML 「ふう…落ち着、かないなぁ。」
				CASE 3
					PRINTFORML 「どうせなら、水の方がいいなー」
				CASE 4
					PRINTFORML 「ちょっと楽になった…かも」
				CASEELSE
					PRINTFORML 「うう…味がわかんない…」
			ENDSELECT
				
					PRINTFORMW ルーミアは落ち着きなくお茶を啜った
		ENDIF
	ELSEIF TALENT:思慕
		SELECTCASE RAND:8
			CASE 1
				PRINTFORML 「お茶を淹れるのにもコツってあるの？」
				PRINTFORMW 「なんか%CALLNAME:MASTER%のと違うんだよねー」
			CASE 2
				PRINTFORML 「羊羹持ってきたんだ。一緒に食べよー」
				PRINTFORMW 「…んー。甘くておいしい…♪」
			CASE 3
				PRINTFORML 「%CALLNAME:MASTER%は紅い館で出てくるお茶は淹れないの？」
				PRINTFORMW 「あそこのケーキはおいしいんだよね。また食べたいなあ…」
			CASE 4
				PRINTFORML ルーミアは頬杖をついて%CALLNAME:MASTER%を眺めている…
				PRINTFORMW 「お茶を注ぐときのコポコポって音、なんだかわくわくしない？」
			CASE 5
				PRINTFORML 「おっ茶～、おっ茶～、まだかな～、まだかな～♪」
				PRINTFORMW …楽しそうに歌っている
			CASE 6
				PRINTFORML 「この間霊夢のお手伝いしたら、すごく苦いお茶貰ったんだけど」
				PRINTFORMW 「あれ何だったんだろ」
			CASE 7
				PRINTFORML 「お茶淹れるなら私も手伝ってあげる」
				PRINTFORMW 「おやつは草団子がいいなー」
			CASEELSE
				GOTO _OTYA3
		ENDSELECT
	ELSE
		$_OTYA3
		SELECTCASE RAND:10
			CASE 1
				PRINTFORMW 「んー。落ち着くー」
			CASE 2
				PRINTFORMW 「お茶請けはないのー？」
			CASE 3
				PRINTFORMW 「茶柱。縁起いーね」
			CASE 4
				PRINTFORMW 「ちょっと休憩ー」
			CASE 5
				PRINTFORMW 「お茶の匂いって落ち着くよね。なんでだろ」
			CASE 6
				PRINTFORMW 「お煎餅が欲しいなー」
			CASE 7
				PRINTFORMW 「うーん、まだ熱いかな…」
			CASE 8
				PRINTFORMW 「お代わりちょうだい。ぬるめで」
			CASE 9
				PRINTFORMW 「…ふう。まったりー」
			CASEELSE
				PRINTFORMW 「ありがとー。喉渇いてたの」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
