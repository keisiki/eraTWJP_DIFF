@M_KOJO_MESSAGE_PALAMCNG_A_K62_0(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;コンドーム
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	;ELSEIF GETBIT(TCVAR:62:50, 1)アナル用
	ELSEIF GETBIT(TCVAR:62:50, 1)
	CALLFORM M_KOJO_COLOR_K62
		IF ABL:膣射中毒
					PRINTFORMW
			IF TALENT:恋人
				;ゴムハメ嫌い。
				SELECTCASE RAND:10
					CASE 9
						PRINTFORML 「はぁ%UNICODE(0x2764)%　はぁ%UNICODE(0x2764)%　旦那さまはいじわるです……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 8
						PRINTFORML 「うぅ……っ%UNICODE(0x2764)%」
						PRINTFORMW
					CASE 7
						PRINTFORML 「どうして外して下さらなかったんですか……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 6
						PRINTFORML 「旦那さま、ちゃんと生でしてほしいです……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 5
						PRINTFORML 「ゴムに出すなんていじわるです……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 4
						PRINTFORML 「なんで外して下さらないんですか……っ%UNICODE(0x2764)%」
						PRINTFORML
					CASE 3
						PRINTFORML 「ちゃんと注いでもらえなかったから……%UNICODE(0x2764)%　輝夜の子宮がむずむずしてます……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 2
						PRINTFORML 「……もう%UNICODE(0x2764)%　……旦那さま、そういうものは外して下さい……っ%UNICODE(0x2764)%」
						PRINTFORML
					CASE 1
						PRINTFORML 「次はちゃんと中ですからね……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 0
						PRINTFORML 「……もう%UNICODE(0x2764)%　旦那さまはゴムで気持ちよくなって満足なんですか……っ？」
						PRINTFORML
				ENDSELECT
						PRINTFORML 輝夜は呼吸を整えると、うるうるとした目で精液の詰まったゴムを睨んでいる。
						PRINTFORML 自分ではなくゴムに注がれたことを何やら不満に思っているようだ。
					IF EX:膣内精液 < 1
						PRINTFORML まだ精液を注がれていない子宮が疼くのか、膝を擦り合わせ、欲求不満を溜め込んでいる。
						CFLAG:TARGET:溜まってる度  += 200
					ENDIF
						PRINTFORMW
			ELSE
				SELECTCASE RAND:10
					CASE 9
						PRINTFORML 「はぁ……%UNICODE(0x2764)%　はぁ……%UNICODE(0x2764)%　どうしてゴムなんて……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 8
						PRINTFORML 「生でしてくれたらいいのに……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 7
						PRINTFORML 「気にせず出してくれたらいいから……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 6
						PRINTFORML 「っ……%UNICODE(0x2764)%　ゴムに出されると変な気持ちになるからやめて……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 5
						PRINTFORML 「うぅ……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 4
						PRINTFORML 「なんでゴムに……っ%UNICODE(0x2764)%」
						PRINTFORML
					CASE 3
						PRINTFORML 「気にしなくて%UNICODE(0x2764)%　いいから……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 2
						PRINTFORML 「中にそのまま出してくれたらいいから……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 1
						PRINTFORML 「次はそのままして……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 0
						PRINTFORML 「うぅ、ゴムじゃなくて……っ%UNICODE(0x2764)%」
						PRINTFORML
				ENDSELECT		
					PRINTFORML 輝夜は中に出してくれなかったことを何やら不満そうにしている。
					IF EX:膣内精液 < 1
						PRINTFORML 精液を注がれていない子宮が疼くのか、膝を擦り合わせ、欲求不満を溜め込んでいる。
						CFLAG:TARGET:溜まってる度  += 100
					ENDIF
					PRINTFORMW
			ENDIF
		ELSE
					PRINTFORMW
				SELECTCASE RAND:4
					CASE 3
						PRINTFORML 「うぅ……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 2
						PRINTFORML 「はぁ%UNICODE(0x2764)%　はぁ%UNICODE(0x2764)%　ゴムに……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 1
						PRINTFORML 「っ……%UNICODE(0x2764)%」
						PRINTFORML
					CASE 0
						PRINTFORML 「どうして、ゴムに……%UNICODE(0x2764)%」
						PRINTFORML
				ENDSELECT
						PRINTFORML 輝夜は自分ではなくゴムに注がれたことを何やら不満に思っているようだ。
						PRINTFORMW
		ENDIF
	ENDIF
ENDIF
