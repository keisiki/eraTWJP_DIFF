				CALL HPH_PRINT,@"「はっ、はぁ、はぁーっ…HPH」","L"
				PRINTFORM 息も絶え絶えといった様子でありながらも、椛は
					SIF EQUIP:上半身上着１ || EQUIP:上半身上着２
						PRINTFORM 着衣越しに
				PRINTFORMW 白い下腹を撫でまわし\@ ABL:65:膣射中毒 > 3 ? 至福の表情を浮かべている… # 満足そうにため息をついている… \@
		ENDSELECT
	ELSEIF TALENT:恋慕
		;恋慕かつ発情
		IF TCVAR:発情
			;何度も射精
			IF TCVAR:350 > 3 && RAND:2 == 0
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「～～～～～ッHPH」","L"
						CALL HPH_PRINT,@"「もっとぉっ…HPHもっと、きてくださいぃっ…HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「また、きたぁっ……HPH」","L"
						PRINTFORM 「ぜんぶ、ぜんぶナカに注いで
							SIF !TCVAR:ピル && !TALENT:妊娠
								PRINTFORM 、孕ませて
						CALL HPH_PRINT,@"ぇっ…HPH」","W"
				ENDSELECT
			;キスもしてる
			ELSEIF CUP:TARGET:快Ｍ > 0 && RAND:2 == 0
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%TEXTR("くるっ、ぅ/はぁ、んいぃっ/")%…HPH%CALLNAME:MASTER%さんっ…！」","L"
						PRINTFORML 射精の瞬間、椛はあなたの唇に食いつこうかというほど激しく口づけして舌を絡めてくる。
						CALL HPH_PRINT,@"「%TEXTR("はぷっ/んむぅっ/")%…%K65_AEGI(,3)%…ふっ、んん～っHPH」","L"
						PRINTFORMW 椛はどくどくと注ぎ込まれる白濁の脈流に目を細めながら、なおも%CALLNAME:MASTER%を求めて熱烈なキスを続けていた…
					CASE 1
						CALL HPH_PRINT,@"「あっあぁっHPHはっ、あああああああ～～～～～っHPHHPH」","L"
						PRINTFORML 椛は背中を大きくのけぞらせながら、%CALLNAME:MASTER%の精液を受け止めている…
						PRINTFORMW
						SIF ABL:膣射中毒 > 3
							CALL HPH_PRINT,@"「わ、ぁっ…HPH%CALLNAME:MASTER%さんが、私のナカにじわぁって広がって……しあわせぇ…HPH」","L"
						PRINTFORML 「んっ……はぷ、ちゅる…ちゅ、ちゅううっ……」
						PRINTFORMW 蕩け切った表情を浮かべた椛は、%CALLNAME:MASTER%\@ TEQUIP:体位２ != 2 && TEQUIP:体位２ != 5 ? の頭を抱き寄せて # に熱っぽい視線を送って \@キスをねだった…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あはっ…HPH%CALLNAME:MASTER%さんのせーし、あったかぁい…HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「んんっ…HPHおなかのなかで、びゅーってしてるのわかっちゃいます…」","L"
						CALL HPH_PRINT,@"「えへへ、気持ちよかったですか？%CALLNAME:MASTER%さん…HPH」","W"
				ENDSELECT
			ENDIF
		;恋慕かつ非発情
		ELSE
			;何度も射精
			IF TCVAR:350 > 3 && RAND:2 == 0
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「～～～～～ッHPH」","L"
						CALL HPH_PRINT,@"「すごい、ですっ…まだ、こんなに、いっぱいぃ…HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「また、きたぁっ……HPH」","L"
						PRINTFORMW 「これ、ほんとにデキちゃいますよぉ…」
				ENDSELECT
			;キスもしてる
			ELSEIF CUP:TARGET:快Ｍ > 0 && RAND:2 == 0
			;	SELECTCASE RAND:2
			;		CASE 0
						CALL HPH_PRINT,@"「%TEXTR("はぷっ/んむぅっ/")%…ん、んん～っHPH」","L"
						PRINTFORML %CALLNAME:MASTER%は椛の小柄な身体を掻き抱いて桜色の唇に吸い付いた。
						PRINTFORML 抽送を続けていた腰の動きがぴたりと止まり、最後の瞬間を迎える。
						CALL HPH_PRINT,@"「んっ、ふ…%K65_AEGI(,3)%…ぷぁ…っHPH」","L"
						PRINTFORMW たっぷりと子種を注ぎ込んでから口を離すと、二人の口許が銀色の糸で繋がっていた…
			;		CASE 1
						
			;	ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「はぁっ…ナカで、あついのが…」
					CASE 1
						PRINTFORMW 「はぁっ…ぁ…%CALLNAME:MASTER%さん、大好きです…」
				ENDSELECT
			ENDIF
		ENDIF
	ELSE
		PRINTFORMW 「あっ、ナカに…」
	ENDIF
	RETURN 1
	;IF 充填率
ENDIF
RETURN 1

