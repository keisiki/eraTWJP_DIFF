;==================================================
;9,ダブルフェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K65_9
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K65_9_1
;戻り値とTARGETを保存、TARGETを二人目に一時的に交代して二人目口上を呼び出した後、TARGETをもどして保存した戻り値でRETURNする
;二人目はいらない場合はここで処理
;TCVAR:352 == 1のとき、二人目口上を呼ばない
IF TCVAR:352 == 0
	;二人目
	LOCAL = MASTER_POSE(4,1,1)
	LOCAL:1 = RESULT
	LOCAL:2 = TARGET
	TARGET = LOCAL
	TRYCALLFORM M_KOJO_MESSAGE_SCOM_K{TARGET}_9_2
	TARGET = LOCAL:2
	RETURN LOCAL:1
ELSE
	RETURN 1
ENDIF
;一人目口上
@M_KOJO_MESSAGE_SCOM_K65_9_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;二人目口上キャンセルフラグ設定
	TCVAR:352 = 0
	;はたて、映姫口上と同じくLOCALに二人目を入れてしまう
	LOCAL = MASTER_POSE(4,1,1)
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;二人目がはたて
	ELSEIF LOCAL == 42
		;二人目口上呼ばないフラグON
		TCVAR:352 = 1
		;はたて・椛ともにレズっ気がある
		;PRINTFORML {GETBIT(STAIN:口,汚れ_精液)}{GETBIT(STAIN:42:0,4)}　{GETBIT(STAIN:65:0,4)}
		IF ABL:42:レズっ気> 0 && ABL:65:レズっ気 > 0 && RAND:3 == 0
			;二人とも口に精液が付いてる
			;うまく動かんな
			IF GETBIT(STAIN:42:0,4) && GETBIT(STAIN:65:0,4) && RAND:2
				;SETCOLOR 0xA260BF
				CALL HPH_PRINT,@"『れるっ…椛ってば、口の周りが%MASTERNAME:42%のせーしでべっとべと…ん、ちゅっ』","L"
				;RESETCOLOR
				CALL HPH_PRINT,@"「はたてさんだって、ほっぺに付いちゃってますよ…ぺろ、ちゅ、ちゅく……」","L"
				PRINTFORMW
				PRINTFORML はたてと椛は交互にペニスを咥えながら、お互いの顔に飛び散った精液を舐めとっていく
				PRINTFORML 顔に残っていたモノがなくなると、今度は舌を絡めあって口内の白濁を分け合いはじめた。
				PRINTFORMW 
				PRINTFORML 「れる、ちゅ、ちゅむ……%CALLNAME:MASTER%さん…せーえき、もっとください……」
				;SETCOLOR 0xA260BF
				PRINTFORMW 『こうやって、仲良くはんぶんこするからぁ…』
				;RESETCOLOR
				STAIN:42:0 = 0
				STAIN:65:0 = 0
			ELSEIF ABL:65:精液中毒 > 3 && ABL:42:精液中毒 > 3 && RAND:2
				CALL HPH_PRINT,@"「ん……はたてさんのアソコ、\@ EX:42:膣内精液 >= 300 ? %CALLNAME:MASTER%さんのと混ざってとろっとろで # おちんちん舐めてびしょびしょに濡れちゃってま \@すね…」","L"
				PRINTFORML 椛の手がはたてのスベスベした太ももを撫で上げ、その付け根にある秘部を指先でくすぐる。
				PRINTFORML 『んむっ…椛だって、%MASTERNAME:42%にごほーしするだけでビッショビショにしてるくせに…』
				PRINTFORMW 二人の少女は%CALLNAME:MASTER%のペニスを競うように舐め上げながら、互いのワレメに指を這わせた…
				PRINTFORML
				PRINTFORML 『はむ、れるぅ、りゅるる…%MASTERNAME:42%聞こえる？椛のアソコからぐちゅぐちゅ音鳴っちゃってるの。やーらしー』
				PRINTFORML 「はたてさんだって、さっきから腰ピクピクさせちゃって…ぺろ、れるるる、ちゅりゅっ」
				PRINTFORMW 美しい少女たちが相互に秘裂をなぶり合いながらも、懸命にあなたのペニスへ奉仕を続ける…
				PRINTFORML
				PRINTFORML 「んぷ、ちゅる、くぷぁ…私たちのどっちかか、%CALLNAME:MASTER%さんか…誰が最初にイッちゃいますかね…？」
				PRINTFORMW 『ふふ、負けない、ンッ、だからぁ…ねろぉ、れちゅ…！』
				;SOURCE:快Ｃとか入れてもいいけどなんか処理が変になりそうなので保留
			ELSE
				CALL HPH_PRINT,@"「ちゅ、れるちゅ、ちゅるる……はふぁへ、ひゃん……HPH」","L"
				CALL HPH_PRINT,@"『れるれるっ、ぬりゅっ……もみひぃ……HPH』","L"
				PRINTFORMW
				PRINTFORML ほっそりとした指同士をキュッと絡め合わせるようにして繋がれる、椛とはたての手。
				PRINTFORML その柔らかな手のひらの間を、逞しく反り立ったペニスが前後している。
				PRINTFORML 二人の作った指オナホールに収まりきらなかった先端には、温かな舌先がねっとりとまとわりついてくる。
				PRINTFORML 尿道をなぞる舌を触れ合わせながら、彼女たちは%CALLNAME:MASTER%の先走りと互いの唾液を味わっていた。
				PRINTFORMW 
				CALL HPH_PRINT,@"『ちゅぷ、れりゅっ……HPH%MASTERNAME:42%ってばがっちがちー♪そんなに興奮してるんだ？』","L"
				CALL HPH_PRINT,@"「ん、りゅる…HPH…えへへ、私もはたてさんも、すっごく興奮してますから…おあいこですね……HPH」","L"
				PRINTFORMW 亀頭越しのディープキスで昂った瞳が、挑発的な色を帯びて%CALLNAME:MASTER%を見上げている…
			ENDIF
		;椛の舌技が高い
		ELSEIF ABL:65:舌 > 2 && RAND:2 == 0
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
					PRINTFORML 『それにしても椛、えっぐい咥えかたするわよねー…』
					PRINTFORML 『%MASTERNAME:42%が仕込んだんでしょ？うっわー…』
					IF ABL:42:精液中毒 > 3 && RAND:2
						PRINTFORML 「むぐっ…なんですか、急に…」
						PRINTFORMW
						PRINTFORML 「そういうはたてさんだって、おちんちん舐めておまんこビショビショにしてるくせに…」
						CALL HPH_PRINT,@"『ちゅ、ぢゅる…しょーがないじゃん…だって%MASTERNAME:42%の、美味しーんだもん…HPH』","L"
						CALL HPH_PRINT,@"「そ…それは…」","L"
						PRINTFORMW
						CALL HPH_PRINT,@"「…その通り、ですね…HPHはぁむ、ちゅるるるっ……HPH」","L"
						PRINTFORMW 二人の少女は陶酔しきった表情でペニスを舐めしゃぶり、じっくりと舌を這わせて粘膜を味わっている…
					ELSE
						PRINTFORMW
						PRINTFORML 「う…人聞きの悪い言い方しないでください」
						PRINTFORML 「私、%CALLNAME:MASTER%さんに気持ちよくなってもらいたくて頑張ったんですから…」
						PRINTFORMW
						PRINTFORML 『ぷっ……あはは！椛ってば、ほんとマジメで健気なのよねー！』
						PRINTFORML 「え、あ、なっ……！からかわれたんですか、私…！？」
						PRINTFORML 『ね、%MASTERNAME:42%？二人でペロペロしてもらったほーが、気持ちいいよね？あむ、ちゅるるっ♪』
						PRINTFORMW
						PRINTFORML 「うぅ……%CALLNAME:MASTER%さん、いっぱいペロペロしますから、私のこと慰めてください…あぷ、れるるっ…」
						PRINTFORMW 『ぁっ…！頭なでなではズルい！ねえ%MASTERNAME:42%、椛からイイ言葉引き出したんだから、はたてのことも褒めてよぉ…ちゅ、ちゅる…』
					ENDIF
				CASE 1
					PRINTFORML 『ん、ちゅむ、れるれるぅ……♪ここの笠になってる所を舐めてあげると気持ちいーんでしょ？』
					CALL HPH_PRINT,@"「はぷ、あむっHPH根っこのところをカプッと咥えて、舌先でグリグリしてあげるのもイイみたいですよ？」","L"
					PRINTFORML 『ふふ、椛もやるじゃない。れちゅ、れりゅりゅっ』
					PRINTFORMW
					PRINTFORML はたてと椛は%CALLNAME:MASTER%の弱点を教え合いながら、楽しそうにペニスを舐めまわしている。
					PRINTFORML 背筋のゾクゾクするような鋭敏な感覚を与えられ続け一層敏感になった粘膜は、
					PRINTFORML 少女たちの息遣いのこそばゆささえも快楽に変換して%CALLNAME:MASTER%を追い詰めていく。
					PRINTFORMW
					PRINTFORML 『%MASTERNAME:42%、だらしないカオしちゃってるー♪あは、かーわいー』
					PRINTFORML 「ふふ、おちんちんガッチガチにして、腰まで浮かせちゃって…もっともっと、私たちに夢中になってくださいね♪」
					PRINTFORMW 無邪気な笑顔と裏腹のねっとりとしたいやらしい奉仕が、%CALLNAME:MASTER%を否応なく昂らせた…
			ENDSELECT
		;はたて・条件なし
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 『ねえ椛、一緒にナメてあげよーよ？ほら、先っぽ譲ってあげるから』
					PRINTFORML 「え、と。はい……あぷ、はぷちゅっ…」
					PRINTFORML 『ふふ。一緒にこっちも刺激してあげちゃうからねー？』
					PRINTFORML はたてはペロッと突き出した舌の先で、睾丸をコロコロとねぶって弄びはじめた…
					PRINTFORMW
					PRINTFORML 『どーお？気持ちいいでしょー？ぺろぺろ、にゅるちゅ…』
					PRINTFORML 「はぷ、れるりゅ、ずちゅ…%CALLNAME:MASTER%さんの、いつもより、おっふぃい…♪」
					PRINTFORML 『ぺろ、ちゅれりゅっ…我慢しないで、いつでもイッていーからね？』
					CALL HPH_PRINT,@"「ふぁい…%CALLNAME:MASTER%さんの、いっふぁいくらはい…HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「%K65_AEGI("フェラ音",3)%…ん、それにしても、天狗二人でご奉仕するなんて…」","L"
					PRINTFORML 『ちゅ、ぢゅる、ちゅぷ…ぷぁ…ね、すっごいゼータクよね。%MASTERNAME:42%はもっとありがたがった方がいいわよ？』
					PRINTFORMW
					PRINTFORML %CALLNAME:MASTER%は礼を言いながら彼女たちの頭を撫でてやった。
					PRINTFORML 『んっ…♪ふふ、それ、正解かも』
					PRINTFORMW 「えへへ…正解のご褒美に、もっとサービスしてあげましょうか。ね、はたてさん」
			ENDSELECT
		ENDIF
	;汎用
	ELSE
		;IF TCVAR:発情
		;発情時口上の置き場
		;ELSE
			;これでMASTERが射精する
			IF NOWEX:MASTER:11
				;慣れてない
				IF EXP:精飲経験 < 20
					PRINTFORML 「で、でそう？」
					PRINTFORML 「わ、わっ…！！あぷっ……」
					PRINTFORMW %CALLNAME:LOCAL%に主導権を奪われそうになった椛は、慌てて%CALLNAME:MASTER%のペニスの先端に吸い付いた…
				;精液中毒高め
				ELSEIF ABL:精液中毒 > 3 && RAND:2
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「……！！」
							CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
							PRINTFORMW %CALLNAME:MASTER%の絶頂が近いことを感じ取った椛は、一層情熱的な奉仕を始めた…
						CASE 1
							CALL HPH_PRINT,@"「%K65_AEGI("フェラ音",2)%…%CALLNAME:MASTER%さん、イっちゃいそうなんですね…HPH」","L"
								PRINTFORML そう呟いた椛の耳が、うれしそうにピコンと跳ねる。
								PRINTFORMW
								PRINTFORML 「……あは、%CALLNAME:LOCAL%さんの鼓動が聞こえてきて、私もドキドキしちゃいます」
							IF TCVAR:発情 && ABL:LOCAL:精液中毒 > 3 && RAND:2
								PRINTFORML 「%CALLNAME:LOCAL%さんも、私も…%CALLNAME:MASTER%さんの精液を欲しがっちゃってますから…」
							ELSE
								PRINTFORML 「%CALLNAME:LOCAL%さんも、私も…%CALLNAME:MASTER%さんのおちんちんでドキドキが止まらなくなっちゃったので…」
							ENDIF
							CALL HPH_PRINT,@"「二人分、い～っぱいびゅるびゅるしてくださいねHPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
