;310,尻を撫でる
;TFLAG:193(1=不快(TALENT:臆病なら涙目) 2&&3=恥ずかしがる(TALENT:男嫌い兼MASTERが男なら、身をすくめる) 4=されるがまま)
;CFLAG:うふふ==2&&TCVAR:20(パンツ無し顔面騎乗(70=クンニ強制 71=フェラチオ強制 72=アナル奉仕) 35=スカート内見せつけ 52=素股)
;CFLAG:うふふ==2&&TEQUIP:シックスナイン(パンツありTCVAR:20(70～72)はこちら)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K65_310
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
	ELSE
		IF TCVAR:発情
			IF CFLAG:既成事実 & 合意_うふふ
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「ぁん、もう…」
						PRINTFORMW 椛は欲情に目を潤ませながらこちらを見つめている…
					CASE 1
						PRINTFORML 「はぁ、ふ、んんっ…」
						PRINTFORMW 小ぶりな尻を愛撫され、椛は熱い吐息を漏らしている…
					CASE 2
						CALL HPH_PRINT,@"「くぅ、ん…%CALLNAME:MASTER%さん…HPH」", "L"
						PRINTFORMW 椛は%CALLNAME:MASTER%にしなだれかかり、身体を擦り付けている…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「今はだめ、だめなんですっ…」
						PRINTFORMW 椛は目に涙をにじませ、ひたすら%CALLNAME:MASTER%の指に耐えている…
					CASE 1
						PRINTFORML 「ふぁあっ…！」
						PRINTFORMW 発情した椛は、小ぶりな尻を這い回る快感に耐えられず声を上げてしまった…
				ENDSELECT
			ENDIF
		;膝枕直後
		ELSEIF PREVCOM == 305
			PRINTFORMW 「ひゃっ…！そういう悪戯するなら、もう膝枕してあげませんよ…？」
		ELSEIF TALENT:恋慕 && RAND:4 > 0
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「きゅ、急になにするんですかっ」
					PRINTFORML 「えっ…い、嫌じゃないですけど…は、ぅ、んんっ…もう……」
					PRINTFORMW %CALLNAME:MASTER%は気の済むまでやわらかな椛の尻肉を堪能した。
				CASE 1
					PRINTFORML 「はぁ、ふ、んんっ…」
					PRINTFORMW 小ぶりな尻を愛撫され、椛は熱い吐息を漏らしている…
				CASE 2
					PRINTFORML 「そういう悪戯はやめてくださいよぉ…」
					PRINTFORML 椛は恥ずかしそうにちらちらと周りを見回したが、
					PRINTFORMW %CALLNAME:MASTER%の愛撫を拒むことはなかった…
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「やめてください！」
					PRINTFORMW 椛は%CALLNAME:MASTER%の手をはたき落とした。
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「あの、その…」
						CASE 1
							PRINTFORMW 「は、恥ずかしいですから…」
					ENDSELECT
				;されるがまま
				CASE 4
					PRINTFORML 「く、ふぅ…」
					PRINTFORMW 椛はわずかに身を捩り、許しを請うように%CALLNAME:MASTER%を見つめた…
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
;==================================================
;311,抱き付く
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 60=ディープキス 73=ぱふぱふ 74=乳合わせ 77=乳を吸わせる ＶＡ性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K65_311
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_311_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CHK_DATENOW(CFLAG:LOCAL:デート中)
	ELSEIF CFLAG:睡眠
	ELSE
		IF CFLAG:うふふ == 2
			SELECTCASE TCVAR:20
				;ディープキス
				CASE 60
					PRINTFORML 舌同士を絡ませあいながらぐっと抱きしめてやると、椛は嬉しそうに抱きしめ返してきた…
					CALL HPH_PRINT,@"「んふ、ちゅ、ちゅる、ちゅむ、ちゅぱ、ぷぁ……あの、もう一回…、ん、ちゅうう…HPH」", "W"
				;だいしゅきホールド
				CASE 86
					CALL HPH_PRINT,@"「っHPHあっHPHもっとっHPHもっとぉっHPH」", "L"
					PRINTFORMW 小柄な身体を抱きしめて狭い膣内を小突き回すと、椛は感極まったような叫びをあげて自ら腰を押し付けてきた…
				CASEELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「はぁっ…%CALLNAME:MASTER%さん、あったかい…」
						CASE 1
							PRINTFORMW 「もっと強く、抱きしめてください…」
					ENDSELECT
					
			ENDSELECT
		ELSEIF TALENT:恋人 && RAND:2
			IF TCVAR:発情
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI("控えめ",3)%…あの、%CALLNAME:MASTER%さん…HPH」","L"
						PRINTFORMW %CALLNAME:MASTER%に抱きしめられたまま、椛はもじもじと内腿を擦り合わせている…
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%…はぁっ…HPH」","L"
						PRINTFORMW 椛は%CALLNAME:MASTER%の背をひしっと抱きながら、やわらかな身体を擦り付けてくる…
						SIF RAND:3 == 0
							PRINTFORMW 彼女の匂いを残さんばかりの熱烈な抱擁は、さながらマーキングのようだ…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「ん…HPH」","L"
						PRINTFORMW 「ずっと、こうしていたいです…」
					CASE 1
						PRINTFORML 「ぅん…♪」
						CALL HPH_PRINT,@"「抱きしめてください…もっと、つよく…HPH」","W"
				ENDSELECT
			ENDIF
		ELSEIF TALENT:恋慕
			IF TCVAR:発情
				IF BASE:MASTER:勃起 > 999 && CFLAG:既成事実 > 0 && RAND:2 == 0
					PRINTFORML 「あは、%CALLNAME:MASTER%さんの…こんなになっちゃってる」
					CALL HPH_PRINT,@"「ね、どこか…人目のないところまでいきませんか…HPH」", "W"
				ELSE
					PRINTFORML （こうして抱きしめられるだけで、お腹の奥がきゅん、きゅんってしちゃう…）
					PRINTFORMW （…%CALLNAME:MASTER%さんが欲しくて、体中が疼いちゃってるんだ…）
				ENDIF
			ELSE
				IF BASE:MASTER:勃起 > 999 && CFLAG:既成事実 > 0 && RAND:2 == 0
					PRINTFORMW 「あの、%CALLNAME:MASTER%さん…当たって、ます…」
					SIF PALAM:欲情 >= PALAMLV:4
						PRINTFORMW （うぅ…私も変な気分になってきちゃった…）
				ELSE
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ん…HPH」", "L"
							PRINTFORMW 「%CALLNAME:MASTER%さん、いいにおいしますね…」
						CASE 1
							PRINTFORMW 「えへへ、%CALLNAME:MASTER%さん、あったかい…」
						CASE 2
							PRINTFORML 「%CALLNAME:MASTER%さん…」
							PRINTFORMW 椛は%CALLNAME:MASTER%の胸に頭を預けている…
					ENDSELECT
				ENDIF

			ENDIF
		ELSEIF TALENT:思慕
			IF TCVAR:発情
				PRINTFORML 「あ、あの…」
				PRINTFORMW 椛はもじもじと内腿を擦り合わせている…
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「うう…」
						PRINTFORMW 椛は頬を染め、されるがままになっている。
					CASE 1
						PRINTFORMW 椛はおずおずと%CALLNAME:MASTER%の背中に腕を回し、抱きしめ返してきた…
				ENDSELECT
			ENDIF
		;・非陥落時
		;親密>5 突然の事に驚いたのかじたばたと身体を揺らしている…
		ELSEIF ABL:親密 > 5
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「い、いきなり何するんですか…？」
					PRINTFORMW 急に抱き付かれ、椛は困惑している。
				CASE 1
					PRINTFORML 「うう…」
					PRINTFORMW 椛は反応に困っている様子だが、逃れようという意志はないらしい。
			ENDSELECT
		;親密<=5 大声を上げて%CALLNAME:MASTER%を突き飛ばした
		ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「きゃあ！？お、驚かせないでくださいっ」
			CASE 1
				PRINTFORML 「きゃあ！？」
				PRINTFORML 突然背後から抱き付かれ、驚いて暴れた椛の肘が%CALLNAME:MASTER%のみぞおちにクリーンヒットした…
				PRINTFORMW 「だ、大丈夫ですか！？」
		ENDSELECT
		ENDIF
	ENDIF

ENDIF
RETURN 1
;==================================================
;312,キスする
;TFLAG:193(99=拒絶(キス不可) 1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 50=手淫 60=キス 性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K65_312
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_312_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70	
	ELSEIF CFLAG:睡眠
	ELSEIF CFLAG:TARGET:添い寝中
		PRINTFORML 「んっ…」
		CALL HPH_PRINT,@"「えへへ…HPH」", "W"
	;拒絶
	ELSEIF TFLAG:193 == 99
		PRINTFORML 「ち、近いですよ…？」
		PRINTFORMW 椛は身を引いて、不審げに%CALLNAME:MASTER%を見つめている…
	ELSE
		;初めて
		IF FIRSTTIME(SELECTCOM)
			;デートキスイベント済み
			IF EXP:TARGET:キス経験 > 1
				PRINTFORML 「んっ…」
				PRINTFORML 「…%CALLNAME:MASTER%さんからキスしてくれるのは、これが初めてですね」
				PRINTFORMW 「えへへ、なんだか嬉しいです」
			ELSE
				PRINTFORML 「んっ…！？」
				PRINTFORML 「きゅ、急に何を…！？」
				PRINTFORML （あ、これって、ファーストキス…？）
				PRINTFORMW 椛は呆けたような表情で自分の唇に触れている。
			ENDIF
		ELSEIF CFLAG:うふふ == 2
			SELECTCASE TCVAR:20
				;だいしゅきホールド
				CASE 86
					CALL HPH_PRINT,@"「んっ！？んふぅっ…んちゅ、ちゅうううっHPH」","L"
					PRINTFORML きゅっと抱き着く彼女に応えて%CALLNAME:MASTER%が口づけをすると、
					PRINTFORMW 椛は感極まったように息を呑み、唇に吸い付いてきた…
				;キス
				CASE 60
					CALL HPH_PRINT,@"「んんっ、む、ふふ…ちゅ、ちゅうっHPHぺろっ、れるれるぅ…HPH」", "L"
					PRINTFORMW %CALLNAME:MASTER%がキスに応えると、椛はいっそう熱烈に舌を絡め合わせた…
				CASEELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「ちゅる、ちゅう、ちゅむ、れるぅ…」
							PRINTFORMW 椛は夢中になって%CALLNAME:MASTER%と口づけしている。
						CASE 1
							CALL HPH_PRINT,@"「んむ、ぷぁ…キスって、すっごく気持ちいいですね…HPHちゅ、ちゅぷ、ちゅむぅ…」", "W"
					ENDSELECT
			ENDSELECT
		ELSEIF TCVAR:発情
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「はぁ、%CALLNAME:MASTER%さんっ、ちゅ、ちゅうぅ…HPH」", "L"
					PRINTFORMW 椛は積極的に舌を絡めて、キスの愉悦に浸っている…
				CASE 1
					CALL HPH_PRINT,@"「ちゅる、んちゅ、ちゅ、れるぅ…HPH」", "L"
					PRINTFORMW （%CALLNAME:MASTER%さんとのキス、すごく気持ちいい…）
					SIF CFLAG:既成事実 & 合意_うふふ
						PRINTFORMW （が、我慢できなくなっちゃいそう…）
			ENDSELECT
		ELSEIF ABL:親密 >= 5
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「んむ、ちゅ…」
					PRINTFORM 椛は
					SIF TALENT:MASTER:体型 >= 0
						PRINTFORM つま先立ちになって
					PRINTFORMW %CALLNAME:MASTER%とのキスを愉しんでいる…
				CASE 1
					PRINTFORML 「ちゅっ…えへへ…」
					PRINTFORMW 椛はほのかに頬を染めて微笑んだ。
			ENDSELECT
		ELSE
			PRINTFORMW 「んむっ…！？…ぷぁっ」
			PRINTFORMW 「えっと、その…」
		ENDIF
	ENDIF

ENDIF
RETURN 1
;==================================================
;313,胸愛撫
;CFLAG:うふふ==2&&TCVAR:20(27=谷間を見せる 33=胸を揉ませる 53=パイズリ(小分岐は地の文EVENT_MESSAGE_COM_セクハラ.ERB参照) 73=ぱふぱふ 77=乳を吸わせる 性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;ABL:Ｂ感覚(2未満=性的には感じてない 5未満=多少感じてる 5以上=カイカン)
;TFLAG:400(1=デート先or地底のモブが反応する)
;乳のサイズでも変わる場合があるので、地の文EVENT_MESSAGE_COM_セクハラ.ERB参照
;台詞＋胸サイズごとのフレーバーテキストって形式が無難かしらね
;==================================================
@M_KOJO_MESSAGE_COM_K65_313
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_313_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_313_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	
	ELSEIF CFLAG:睡眠
	
	ELSEIF CFLAG:うふふ == 2
		IF TCVAR:発情
			SELECTCASE TCVAR:20
				;胸を揉ませる
				CASE 33
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ふっ、ぅ…あはっHPH」", "L"
						PRINTFORMW 椛は目を爛々とさせながら、あなたの手をより強く胸へと押し付けた。
					CASE 1
					;きょにゅー以上
						IF TALENT:バストサイズ > 0
							PRINTFORML 「えへ、胸には結構自信あるんです…」
							PRINTFORML 胸元に押し付けた%CALLNAME:MASTER%の手のひらごと自らの豊乳を揉みしだき、
							PRINTFORMW 椛は愉悦の表情を浮かべている…
						;ひんにゅー以下
						ELSEIF TALENT:バストサイズ < 0
							CALL HPH_PRINT,@"「はぁ…っHPHお、おっぱい、気持ちいい…ですっHPH」", "L"
							PRINTFORMW 欲情に尖った乳首を押しつぶすように、椛は%CALLNAME:MASTER%の手のひらを胸元へ押し付ける…
						;ふにゅー
						ELSE
							CALL HPH_PRINT,@"「%CALLNAME:MASTER%さん、もっと、つよく…っHPHHPH」", "L"
							PRINTFORMW 彼女の求めに応じてより強く胸を揉んでやると、椛は嬉しそうに身を震わせた。
						ENDIF
					CASE 3
						CALL HPH_PRINT,@"「はっぁHPH乳首っ、ちくびぃぃっHPH」", "L"
						PRINTFORMW 張り詰めた乳頭を爪の先で優しくカリカリとひっかかれ、椛は嬌声をあげて快楽に浸っている…
				ENDSELECT
				;パイズリ
				CASE 53
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ひゃうん！っはぁ、もっとぉ…っHPH」", "L"
							PRINTFORMW %CALLNAME:MASTER%に乳房を弄ばれ、椛は悩ましげな吐息を漏らしつつも乳奉仕を続けている。
						CASE 1
							CALL HPH_PRINT,@"「あはっ…HPHおっぱいに、%CALLNAME:MASTER%さんのにおい、つけてください…HPH」", "W"
						CASE 2
							PRINTFORML 「ぁん、もう、今は私が%CALLNAME:MASTER%さんを責めてるんですからぁ…」
							CALL HPH_PRINT,@"「あんまり、やんっHPHいたずらしないでください…」", "W"
					ENDSELECT
				;乳を吸わせる
				CASE 77
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「ふぁんっHPHそんなふうに乳首ちゅぅって吸われると、お腹の奥がきゅんってしちゃいます…HPH」", "W"
						CASE 1
							CALL HPH_PRINT,@"「きゃっHPHあ、ふぁんっHPHはぁっHPHはーっHPH%CALLNAME:MASTER%さん…っ」", "L"
							PRINTFORML 硬くしこった乳頭を口に含み舐め転がしてやると、椛は感極まったように%CALLNAME:MASTER%の頭を抱きしめた。
							PRINTFORMW 発情に火照った身体をぴくぴくと震わせながら、椛は%CALLNAME:MASTER%を一心に見つめていた…
					ENDSELECT
				;だいしゅきホールド
				CASE 86
					SELECTCASE BASE:PLAYER:2
						CASE 0 TO 2999
							;PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%と深く繋がったまま強く抱き合い、熱い口付けを交わした
							;PRINTFORML %CALLNAME:TARGET%は結合の時間をより長くしたいのか、ゆったりとしたストロークを開始した……
						CASE 3000 TO 9999
							;PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%と深く繋がったまま強く抱き合い、熱い口付けを交わした
							;PRINTFORML %CALLNAME:TARGET%は我慢できない様子で激しく腰を振り始めた
							;PRINTFORML 接合部からはとめどなく愛液が迸り、辺りにはグチュグチュと水音が響いている……
						CASEELSE
							SELECTCASE RAND:2
							CASE 0
								PRINTFORML 強烈な射精感に急き立てられるまま、%CALLNAME:MASTER%は椛の乳房を力いっぱい掴んだ。
								CALL HPH_PRINT,@"「はあっHPH%CALLNAME:MASTER%さん…HPH%CALLNAME:MASTER%さん…ッHPH」", "L"
								PRINTFORMW 発情しきった椛の身体はその痛みさえ快感として受け入れ、彼女はさらに高ぶっていく…
							CASE 1
								CALL HPH_PRINT,@"「きてっHPHきてえぇっHPHHPH%CALLNAME:MASTER%さんで私をいっぱいにしてえっHPH」", "L"
								PRINTFORMW 椛は乳頭を弄られる快感に翻弄されながらも、%CALLNAME:MASTER%の子種を求めて性器同士を密着させた…
						ENDSELECT
					ENDSELECT
				;その他
				CASEELSE
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「やっ、あは…HPH%CALLNAME:MASTER%さんの触り方、すっごくえっち…HPH」", "L"
							PRINTFORMW 椛は%CALLNAME:MASTER%に触れられるたびに熱い吐息を漏らしている。
						CASE 1
						;そういえばこの子[痛みに強い]ついてるのよね
							CALL HPH_PRINT,@"「えへ…もっと強く、押しつぶしちゃうくらいに揉んでも平気ですよ…HPH」", "L"
							CALL HPH_PRINT,@"「もっと昂ぶらせて、もっと溶け合って、もっと、もっと……HPH」", "W"
						CASE 2
							CALL HPH_PRINT,@"「ふぁんっHPHそんなふうに乳首こりこりされたら、お腹の奥がきゅんってしちゃいます…HPH」", "W"
					ENDSELECT
			ENDSELECT
		;うふふ(not発情)
		ELSE
			SELECTCASE TCVAR:20
				;胸を揉ませる
				CASE 33
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「やっ……乳首つねっちゃ…ひぅっHPH」", "W"
						CASE 1
