				ENDSELECT
			ENDIF
		ELSE
			IF TALENT:恋慕 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI(,3)%」", "L"
						PRINTFORML 「っ…あの、あんまり…見ないでください…」
						PRINTFORMW 「きっといま私、すっごくだらしない顔してますから…」
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "L"
						PRINTFORML 「ふふ。キスは\@ EXP:キス経験>20 ? 何度も # 何度か \@してるはずなのに、やっぱりいつもドキドキしちゃいます」
						CALL HPH_PRINT,@"「……はい。もう、一回…ん、%K65_AEGI("控えめ",2)%」","L"
						PRINTFORMW %CALLNAME:MASTER%と椛は抱きしめあい、互いの鼓動を感じながらゆっくりとキスをした…
