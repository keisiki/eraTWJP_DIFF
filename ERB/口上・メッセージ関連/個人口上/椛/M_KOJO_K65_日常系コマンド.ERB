@M_KOJO_MESSAGE_COM_K65_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:ブチギレ > 0
		PRINTFORML 「…どうも」
		PRINTFORMW 椛は押し黙ってあなたが淹れたお茶をすすっている。
	;仕事中
	ELSEIF CFLAG:行動 == 5
		SELECTCASE RAND:2
			CASE 0
			PRINTFORMW 「ありがとうございます。この仕事、結構喉が渇いてくるんですよね…」
			CASE 1
			PRINTFORML 「あ、ありがとうございます」
			PRINTFORMW 「せっかくですから、このあたりで一休みしましょうか」
		ENDSELECT
	ELSE
		;大成功
		IF TFLAG:193 == 1
			SELECTCASE RAND:2
				CASE 0
				PRINTFORML 「いい香りですね…」
				PRINTFORML 「こうして嗅いでいるだけでも幸せな気分になります」
				PRINTFORMW 椛はうっとりと目を閉じ、馥郁としたお茶の香りを楽しんでいる。
				CASE 1
				PRINTFORML 「お茶ですか？ありがとうございます」
				PRINTFORML 「……すごく美味しいです」
				PRINTFORMW 「淹れ方に何かコツがあるんでしょうか？」
			ENDSELECT
		;失敗
		ELSEIF TFLAG:193 == -1
		PRINTFORML 「お茶ですか？ありがとうございます」
		PRINTFORML 「……っぐ！？…けほけほっ…」
		PRINTFORMW 椛はお茶に口を付けた瞬間、むせてしまったようだ…
		;成功
		ELSE
			SELECTCASE RAND:2
				CASE 0
				PRINTFORMW 「お茶ですか？ありがとうございます」
				CASE 1
				PRINTFORML 「あ、すみません、いただきます…」
				PRINTFORMW 椛はうやうやしく湯呑みを受け取ると、お茶をすすり始めた。
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
