;==================================================
;621,釣りをする
;TFLAG:193(0=釣れない 1～4=魚 11～15=うまい魚 610=木材 131=守矢の守札 21=ローター 609=氷の鱗 606=龍の爪)
;TFLAG:194(1000以上ならTARGETが釣った)
;==================================================
@M_KOJO_MESSAGE_COM_K65_621
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_621_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;釣れなかった
	IF TFLAG:193 == 0
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「まあ、こういう時もありますよ」
				PRINTFORMW 「気長にいきましょう、%CALLNAME:MASTER%さん」
			CASE 1
				PRINTFORML 「ん、ちょっと風が出てきましたかね…」
				PRINTFORML 「…こうして、のんびりと浮きを眺めているのも良いものです」
				PRINTFORMW 椛と%CALLNAME:MASTER%は竿を並べ、水のせせらぎを聞きながらゆったりと時間を過ごした。
			CASE 2
				PRINTFORML 「………」
				PRINTFORML 「……」
				PRINTFORML 「…Zzz」
				PRINTFORMW 椛は竿を垂れながら櫓をこいでいる。器用なものだ。
			CASE 3
				;椛の初期能力は教養1
				PRINTFORML 「昔、とても釣りを好んだ名軍師がいたそうですね」
				PRINTFORMW 「私もこうやって釣り糸を垂れていれば、良い将棋の手が思いついたり…なんて。ふふっ」
		ENDSELECT
	;椛が釣った
	ELSEIF TFLAG:194 >= 1000
		SELECTCASE TFLAG:193
			;木材
			CASE 610
				PRINTFORMW 「うっ…手ごたえアリ、と思ったんですが…」
			;守矢の守札
			CASE 131
				PRINTFORML 「守矢神社の守札みたいですね」
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「…でも、なんでこんなところに？」
					CASE 1
						PRINTFORML 「何枚か集めるとくじ引きが出来るそうですが…」
						PRINTFORMW 「%CALLNAME:MASTER%さん、やったことあります？」
					CASE 2
						PRINTFORML 「1回、3000円…」
						PRINTFORMW 椛はどこか遠い目をして呟いた。
				ENDSELECT
			;ローター
			CASE 21
				PRINTFORML 「なんでしょう、このピンク色の…」
				PRINTFORML 「っ！？～～～～～～っ！！」
				PRINTFORML 椛は真っ赤になってローターを放り投げた。
				PRINTFORMW …勿体無いので拾っておいた。
			;素材
			CASE 609,606
				PRINTFORMW 「わ、なんだかキレイなものがかかりましたね」
			;その他の魚
			CASEELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「手ごたえありっ！そこだっ！」
					CASE 1
						PRINTFORMW 「あはっ、やりましたっ」
					CASE 2
						PRINTFORMW 「私から逃れられるとは思わないことです！」
				ENDSELECT
		ENDSELECT
	;あなたが釣った
	ELSE
		SELECTCASE TFLAG:193
			;木材
			CASE 610
				PRINTFORMW 「あはは…そういう時もありますよ。ね？」
			;守矢の守札
			CASE 131
				PRINTFORML 「守矢神社の守札みたいですね」
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「…でも、なんでこんなところに？」
					CASE 1
						PRINTFORML 「何枚か集めるとくじ引きが出来るそうですが…」
						PRINTFORMW 「%CALLNAME:MASTER%さん、やったことあります？」
					CASE 2
						PRINTFORMW 「…以前、あのくじ引きに大ハマリしちゃった天狗がいたそうで…」
				ENDSELECT
			;ローター
			CASE 21
				PRINTFORML 「何が釣れたんですか？見せてください…ッ！」
				PRINTFORMW %CALLNAME:MASTER%の手のひらに収まった楕円球を見た椛は、真っ赤になって視線を戻した。
			;素材
			CASE 609,606
				PRINTFORML 「わ、なんだかキレイなものがかかりましたね」
				PRINTFORMW 「何かに使えるんでしょうか…」
			;その他の魚
			CASEELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「わぁ…やりましたね！%CALLNAME:MASTER%さん！」
					CASE 1
						PRINTFORMW 「お見事です！」
					CASE 2
						PRINTFORML 「おおっ、いいですね！」
						PRINTFORMW 「せっかくなら、新鮮なうちに料理しちゃいませんか？私、お酒持ってきますよ！」
				ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
