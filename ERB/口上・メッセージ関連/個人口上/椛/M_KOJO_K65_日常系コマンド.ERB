					PRINTFORMW …時々視線がこちらの股間に向くのは気のせいだろうか。
			ENDSELECT
		;ELSEIF CFLAG:65:好感度 >= 1500
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「んん……」
					PRINTFORMW 椛は落ち着かない様子だ…
				CASE 1
					PRINTFORML 「……あっ…な、なんの話でしたっけ」
					PRINTFORMW 椛は上の空で%CALLNAME:MASTER%の話を聞いている…
			ENDSELECT
		ENDIF
	;添い寝中
	;ELSEIF CFLAG:添い寝中
	
	;デート中
	
	
	;大成功
	ELSEIF TFLAG:193 == 1
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「それでですね…」
				PRINTFORMW 椛は尻尾をぱたぱたと振りながら、楽しそうに言葉を続けた。
			CASE 1
				PRINTFORML 「あはははっ」
				PRINTFORMW 「その冗談、今度私も使っていいですか？」
		ENDSELECT
	
	;会話累積値が閾値を超えての失敗
	ELSEIF TCVAR:302
		PRINTFORML 「…えっと、そうなんですか」
		PRINTFORMW 同じ話を何度も聞かされ、椛は退屈そうにしている…
	
	;失敗
	ELSEIF TFLAG:193 < 0
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「あ、あはは…」
				PRINTFORMW 椛はお愛想で笑っている…
			CASE 1
				PRINTFORML 「……あ、えっと、その…」
				PRINTFORMW 椛は話題を探してあたふたしている。
		ENDSELECT
	
	;お仕事中 (1/2で普通のトークへ逃がす)
	ELSEIF CFLAG:行動 == 5 && RAND:2 == 0
		;おなかたぷたぷ
		IF EX:膣内精液 + EX:アナル内精液 >= 100
			;自覚あり
			IF TCVAR:350
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML （おなかの中…まだ、熱い…）
						PRINTFORMW ……椛は心ここにあらずと言った様子でたたずんでいる。
					CASE 1
						PRINTFORML （いつもよりも、人の視線が気になっちゃいますね……）
						PRINTFORML たっぷりと注ぎ込まれた精液の臭気を気にしているのか、
						PRINTFORMW 椛は%CALLNAME:MASTER%と会話する間も周囲をチラチラと確認していた…
					CASE 2
						PRINTFORML 「…ひどいです。今日は私、お仕事のある日だったのに…」
						PRINTFORML 椛は恨みがましい視線を%CALLNAME:MASTER%に向けている。
						PRINTFORMW ……%CALLNAME:MASTER%がじっと見つめ返すと、椛は真っ赤になってうつむいた。
				ENDSELECT
			;自覚なし
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「………」
						PRINTFORMW 椛は周囲を警戒しているようだ…
					CASE 1
						PRINTFORML 「……いつになく殺気だっている…？私が、ですか？」
						PRINTFORMW 「…すみません、すこし…んんっ…気になることが、あって…」
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE RAND:5
				CASE 0
				PRINTFORMW 「今日も異常なし、ですね」
				CASE 1
					PRINTFORML 「哨戒の仕事って結構手空きの時間が多いんですよね」
					PRINTFORMW 「こうしてお話する相手がいるのはありがたいです」
				CASE 2
					PRINTFORML 「哨戒中に千里眼でよそ見をすることですか…？」
					PRINTFORMW 「あ、ありません！ありませんよ！」
				CASE 3
					PRINTFORMW 「実のところ、悪意を持って山に来る人なんてほとんどいないんですよね」
				CASE 4
					PRINTFORMW 「%CALLNAME:MASTER%さんは山登りがお好きなんですか？」
			ENDSELECT
		ENDIF	
	;RANDで並列にする普通のトーク
	;5.お酒が入っていい気分トーク
	ELSEIF BASE:酒気 >= 400 && RAND:5 == 0
		IF CFLAG:既成事実 & 合意_うふふ && EXP:精液経験 >= 20 && RAND:2
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「そういえば、以前酒席で%CALLNAME:MASTER%さんの事が話題になったことがあったんですが…」
					PRINTFORMW 「…あっ」
					PRINTFORML 「………」
					PRINTFORML 「えと…私の同僚の白狼天狗が何かご迷惑をおかけしたりとか…してない、ですよね！あはは…」
					PRINTFORML 椛はなんとなく決まり悪そうに笑っている。
					PRINTFORMW …どんな事を話題にされていたのだろうか。
				CASE 1
					PRINTFORML 「ふぅ、ちょっと、酔っちゃいましたかね…」
					PRINTFORML 「%CALLNAME:MASTER%さんが相手だと、つい気が緩んじゃいます…」
					IF TALENT:恋人 && RAND:2
						PRINTFORMW 彼女は彼女なりに苦労しているのだろう…%CALLNAME:MASTER%は、椛の頭を優しく撫でてあげた。
						CALL HPH_PRINT,@"「んっ、んん…HPH…ふふ。きもちいい、です…」","L"
						PRINTFORMW 「もう少し、甘えさせてもらっても…いいですよね…？」
					ELSE
						PRINTFORMW 椛は頬を染めながら%CALLNAME:MASTER%に笑みを投げかけた。
					ENDIF
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「ふふ、結構いい気分になってきちゃいましたね…」
					PRINTFORMW 酒気でほんのり赤くなった椛が機嫌よさそうに笑っている。
				CASE 1
					;晴れた夜
					IF GROUPMATCH(TIME:5,0,1) && TIME:2 >= 5 && TIME:2 <= 7
						PRINTFORMW 「月見で一杯、なんていうのも風流でいいですね」
					ELSEIF GROUPMATCH(TIME:5,8,10,11)
						PRINTFORMW 「ううーん、やっぱり雪見酒は良いですねえ…」
					ELSE
						PRINTFORMW 「%CALLNAME:MASTER%さんとおしゃべりしながら一献傾けるというのも、いいですねえ…」
					ENDIF
			ENDSELECT
		ENDIF
	;4.お天気トーク
	ELSEIF RAND:4 == 0
		SELECTCASE TIME:5
			;晴れ,快晴
			CASE 0,1
				SELECTCASE RAND:2
					CASE 0
						IF TIME:2 >= 5 && TIME:2 <= 7
							PRINTFORMW 「星空を見上げるのも結構好きなんです」
						ELSE
							PRINTFORMW 「いいお天気だと気持ちも晴れてきますね」
						ENDIF
					CASE 1
						PRINTFORML 「ふわぁ…」
						PRINTFORML 椛は口許を押さえ、かわいらしく欠伸をした。
						PRINTFORMW 「あ、すみません、いいお天気なものでつい…」
				ENDSELECT
			;薄曇り
			CASE 2
				IF TIME:2 >= 5 && TIME:2 <= 7
					PRINTFORMW 「薄曇りの空に浮かぶ月、というのもいいですよね」
					SIF CFLAG:2 >= 1000
						PRINTFORMW 「…せっかくですし、月見酒なんてどうですか？」
				ELSE
					PRINTFORMW 「なんだか微妙な天気ですね…」
				ENDIF
			;曇り
			CASE 3
				SELECTCASE RAND:2
					CASE 0
						IF TIME:2 >= 5 && TIME:2 <= 7
							PRINTFORMW 「こう雲がかかっていると、夜空も見えませんね」
						ELSE
							PRINTFORMW 「すっかり曇っちゃってますねー」
						ENDIF
					CASE 1
						PRINTFORML 「降らないといいんですが…」
						PRINTFORMW 椛は曇り空を見上げている。
				ENDSELECT
			;雨、大雨、霧雨
			CASE 4,5,6
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「結構降ってますね…」
					CASE 1
						PRINTFORMW 「雨が降っていると、お出かけするのも面倒になっちゃいますね…」
					CASE 2
						IF DAY:5 == 5
							PRINTFORMW 「本当、桶をひっくり返したような雨です…」
						ELSE
							PRINTFORMW 「あんまり湿気が多いと尻尾が蒸れてきちゃうんです」
						ENDIF
				ENDSELECT
			;霧
			CASE 7
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「結構濃い霧が立っていますね」
						PRINTFORMW 「足元には十分気を付けてください、%CALLNAME:MASTER%さん」
					CASE 1
						PRINTFORML 「私ですか？これくらいの霧なら全然平気です」
						PRINTFORML 「白狼天狗の千里眼は伊達じゃありませんよ！」
						PRINTFORM 椛は
						SIF TALENT:バストサイズ > 0
							PRINTFORM 小柄な体躯に見合わないほどたわわに実った
						SIF TALENT:バストサイズ < 0
							PRINTFORM 小柄な体躯に似合いの控えめな
						SIF TALENT:バストサイズ == 0
							PRINTFORM 小柄な体格の割に発育のいい
						PRINTFORMW 胸を張った。
				ENDSELECT
			;雪、細雪、霧雪
			CASE 8,10,11
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「雪が降っていると、不思議と胸が高鳴るんです」
						PRINTFORMW 「…どうしてでしょうね」
					CASE 1
						PRINTFORML 「この降り具合、後で雪かきしなきゃいけませんね」
						PRINTFORMW 椛はなぜか楽しそうにしている…。
				ENDSELECT
			;吹雪
			CASE 9
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「雪が降っていると、不思議と胸が高鳴るんです」
						PRINTFORMW 「…どうしてでしょうね」
					CASE 1
						PRINTFORML 「吹雪いてますね…」
						PRINTFORMW 「遭難者が出たりしなければいいんですが」
				ENDSELECT
			;みぞれ
			CASE 12
				PRINTFORML 「雪混じりの雨ですか」
				PRINTFORMW 「ひと思いに雪になってくれませんかね…」
			;あられ
			CASE 13
				PRINTFORML 「うわあ、あられが降ってます…」
				PRINTFORMW 「今は外出しない方がいいですね…」
		ENDSELECT
	;3.季節トーク
	ELSEIF RAND:3 == 0
		SELECTCASE DAY:2
			;春
			CASE 1
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「春になると、山の色々なところで次々に花が咲くんです」
						PRINTFORML 「同じ花でも、生えている場所によって咲くタイミングが違ったりして…」
						PRINTFORMW 「哨戒中のささやかな楽しみですね」
					CASE 1
						PRINTFORMW 「暑くも寒くもない時期っていいですよね」
				ENDSELECT
			;夏
			CASE 2
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「麓の河童たちの所で、時々きゅうりをご馳走になるんです」
						PRINTFORMW 「沢で冷やしたきゅうりはまさしく絶品ですよ」
					CASE 1
						PRINTFORML 「暑いですねー…」
						PRINTFORMW 椛の尻尾は力なく垂れている。
				ENDSELECT
			;秋
			CASE 3
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「山が色づいてくると、いよいよ秋って感じですね」
						PRINTFORMW 「哨戒中によそ見…しません！してません！」
					CASE 1
						PRINTFORML 「秋は良いですね。過ごしやすいですし、何より食べ物が美味しくて」
						PRINTFORMW 「ふふ、今夜の肴は何にしましょうかね…」
				ENDSELECT
			;冬
			CASE 4
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「哨戒の仕事で慣れているとはいえ、やはり寒いものは寒いんですよね…」
					CASE 1
						PRINTFORMW 「乾燥してくると、尻尾に静電気がたまっちゃって…」
				ENDSELECT
		ENDSELECT
	;2.親密トーク
	ELSEIF RAND:2 == 0 && CFLAG:2 >=1000
		SELECTCASE RAND:5
			CASE 0
				IF BASE:MASTER:TSP >= 1000
					PRINTFORML 「そういえば、あなたって時々瞬間移動？してますよね…」
					PRINTFORML 「何か特殊な術を使ってるんですか？」
					PRINTFORMW …余計な詮索をされても面倒なので、適当にごまかした。
				ELSE
					PRINTFORMW 「正直に言うと、哨戒のお仕事ってすっごく暇なんですよね…」
			ENDIF
			CASE 1
				PRINTFORML 「正直に言うと、哨戒のお仕事ってすっごく暇なんです…」
				PRINTFORMW 「今後とも話し相手になってくださいね？」
			CASE 2
				PRINTFORML 「文さんですか？うーん、まあ、その…」
				PRINTFORMW 椛は言葉を濁している。
			CASE 3
				PRINTFORML 「にとりさんですか？はい、仲良くしてもらっていますよ」
				PRINTFORMW 「時々、とんでもない発明品のテストに付き合わされるのは困っちゃいますが…」
			CASE 4
				PRINTFORML 「はたてさんですか？ええと、まあ、何と言いますか…」
				PRINTFORMW 椛は言葉を濁している。
		ENDSELECT
	;1.通常トーク
	ELSE
		SELECTCASE RAND:6
			CASE 0
				PRINTFORML 「あっ！ちょっとすみません」
				PRINTFORML 椛は懐から手帳を取り出すと、急いで何か書付け始めた…
				PRINTFORMW 「いえ、将棋の新しいルールを考え付いたんです」
			CASE 1
				PRINTFORML 「河童たちとはよく将棋を指すんです」
				PRINTFORMW 「できるだけ長く楽しめるように、独自のルールを作っているんですよ」
			CASE 2
				PRINTFORML 「哨戒のお仕事は千里眼と鼻を活かせますから、
				PRINTFORMW 　私みたいな白狼天狗にはちょうど良いんですよね」
			CASE 3
				PRINTFORMW 「私だって天狗ですから、ちょっとした妖怪相手に遅れは取りませんよ！」
			CASE 4
				PRINTFORMW 「ここのところは大きな騒ぎもなくて、平和ですね」
			CASE 5
				PRINTFORMW 「以前山で大きな騒ぎがあったんですが…」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
