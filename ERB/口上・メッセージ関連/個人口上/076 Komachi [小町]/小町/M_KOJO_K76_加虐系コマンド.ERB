;加虐系コマンド
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K76_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K76_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K76_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF TEQUIP:Ｖセックス
			CALL HPH_PRINT,@"「あっひぃッHPH　ゆるしてっ、ゆるしておくれよぉっHPH　ひっ、ひィィィッHPH」","L"
			PRINTL 
			PRINTFORML 小町の熱く潤った肉襞を掻き分けながら、むしゃぶりつきたくなる尻肉を平手で叩く
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML そのたびに膣肉は急激に収縮し、媚びるかのように抽挿を繰り返す肉棒にまとわりついて離そうとしなかった…
			CASE 1
				PRINTFORML そのたびに膣孔と肉棒の隙間から本気汁がプシッ、プシッと飛び散り、小町が加虐行為に喜んでいることを示していた…
			ENDSELECT
			PRINTW 
		ELSE
			SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「ひぃっHPH　ひいいッHPHHPH」","L"
				CALL HPH_PRINT,@"「許してっHPH　許しておくれよおッHPH」","L"
				PRINTL 
				PRINTFORML 男ならば欲望の思うがままにチンポをねじ込みたくなる丸尻に、何度も平手を放つ
				PRINTFORML 衝撃を受けるたび、尻はプルン、プルンと弾み、媚びるかのように目の前でフリフリと揺れる…
				PRINTW 
			CASE 1
				CALL HPH_PRINT,@"「きゃんッHPH　きゃんッHPHHPH」","L"
				CALL HPH_PRINT,@"「あ、あたい、悦んでなんかあ…きゃんっHPH」","L"
				PRINTL 
				PRINTFORML 無意識なのか、挑発しているのか…デカ尻に責め苦を受けているはずの小町の声には明らかに艶が混じっている…
				PRINTFORML それを裏付けるように、成熟した雌穴からはトロリとした蜜がこぼれていた…
				PRINTW 
			ENDSELECT
		ENDIF
		RETURN 1
	ELSEIF ABL:マゾっ気 >= 3
		IF TEQUIP:Ｖセックス
			PRINTFORML 「きゃんっ！？」
			PRINTFORML きゅううんっ♥
			PRINTFORML たっぷりとした尻を叩かれた瞬間、小町の膣が急激に収縮し、%CALLNAME:MASTER%はあやうく射精しそうになってしまった…
			PRINTW 
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「きゃんっ、きゃんっ」
				PRINTFORML 「し、尻を叩かれて悦んでなんかぁ…」
				PRINTFORML そう言いながらも、小町のデカ尻は次を期待してフリフリと目の前で揺れている…
				PRINTW 
			CASE 1
				PRINTFORML 「ああっ、ひいいいッ」
				PRINTFORML 男なら誰もがむしゃぶりつきたいと思う尻に、平手の跡がいくつも折り重なっている
				PRINTFORML しかし、それが苦痛だけでないことを証明するかのように、膣孔から一筋の淫汁が流れ落ちていった…
				PRINTW 
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		PRINTFORML 「きゃんっ！？」
		PRINTFORML 「な、なにするんだよお。痛いのはよしとくれよ」
		PRINTW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

