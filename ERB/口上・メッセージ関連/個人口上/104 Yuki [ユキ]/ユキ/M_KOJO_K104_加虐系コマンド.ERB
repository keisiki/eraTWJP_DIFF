;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K104_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K104_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K104_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:イタズラ == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:泥酔 == 1
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		IF ABL:マゾっ気 < 2
			IF TALENT:恋慕 || TALENT:愛欲
				PRINTFORML 「いたっ！　待っ、%CALLNAME:MASTER%！？」
				PRINTFORMW 「ん～、%CALLNAME:MASTER%の趣味なら、良いけど……あんまり強くしないでね？」
			ELSE
				PRINTFORML 「いったーい！」
				PRINTFORMW 「そういう趣味は無いんだけど！　かんべんしてよ～」
			ENDIF
		ELSE
			IF TALENT:恋慕 || TALENT:愛欲
				PRINTFORML 「あはぁ%UNICODE(0x2665) *1%　いったぁ……%UNICODE(0x2665) *1%」
				PRINTFORMW 「……もっと、強くしても良いよぉ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORML 「んん%UNICODE(0x2665) *1%　こらっ、何叩いてんの！」
				PRINTFORMW 「まぁ……ちょっと気持ちいいし、べつに良いけどさ～」
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

