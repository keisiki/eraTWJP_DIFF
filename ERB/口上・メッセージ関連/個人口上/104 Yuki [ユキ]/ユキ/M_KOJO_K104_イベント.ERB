	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:愛欲
