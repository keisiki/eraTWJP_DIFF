;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K104_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF FIRSTTIME("告白")
			PRINTFORML 「%CALLNAME:MASTER%……%CALLNAME:MASTER%っ！」
			PRINTFORMW 「その～……我慢できなくなったから、言うね」
			PRINTFORML 
			PRINTFORMW 「わたし、%CALLNAME:MASTER%のことが好き」
			PRINTFORMW 「もっともっと仲良くなって、ずっと一緒にいられたらって……」
			PRINTFORMW 「まぁ、その～……恋人、みたいな」
			PRINTFORMW 「わたしのまわりだけでも、他にたくさん可愛い子いるから……なかなか難しいかもしれないけど」
			PRINTFORMW 「それでも……」
			PRINTFORML
			PRINTFORMW 「ね、%CALLNAME:MASTER%」
			PRINTFORMW 「わたしのことだけ見て……？」
		ELSE
			PRINTFORML 「ね～、わたしを振ったこと後悔してない？」
			PRINTFORMW 「今ならまだまだオッケーなんだけど……」
			PRINTFORMW 「どう？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;恋人の数え方があってるかはわからん
	IF LOCAL:1 && ARG == 1
		VARSET LOCAL
		FOR LOCAL, 1, CHARANUM
			SIF TALENT:LOCAL:恋人
				LOCAL:1 ++
		NEXT
		SELECTCASE LOCAL:1
		CASE 1
			PRINTFORML 「やったっ%UNICODE(0x2665) *1%　ありがと、%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
			PRINTFORML 「後で他の人が良いって言っても、もう駄目だからね！」
			PRINTFORMW 「大丈夫、退屈させないから%UNICODE(0x2665) *1%」
		CASE 2 TO 4
			PRINTFORMW 「やったっ%UNICODE(0x2665) *1%　ありがと、%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
			PRINTFORMW 「後で他の人が良いって言っても、もう駄目……って、え？　もう、恋人が他にいるの？」
			PRINTFORMW 「う～ん、わたしとしては一人だけの方が特別っぽくて憧れるけど……まぁ、いいわ！」
			PRINTFORMW 「お付き合いしていくなら、多少の文化の違いは受け入れていかないとねっ」
			PRINTFORMW 「何はともあれ、よろしくね%UNICODE(0x2665) *1%」
		CASE 5 TO 10
			PRINTFORMW 「やったっ%UNICODE(0x2665) *1%　ありがと、%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
			PRINTFORMW 「後で他の人が良いって言っても、もう駄目……って、え？　わたしで{LOCAL:1}人目？」
			PRINTFORMW 「そ、そうなんだ！　え～と、ちょっと想像と違ったけど……とりあえず、わかったわ」
			PRINTFORMW 「まあ、まあ……%CALLNAME:MASTER%を愛する者同士、多分仲良くできるよね」
			PRINTFORMW 「何はともあれ、恋人は恋人！　今後とも、よろしくね%UNICODE(0x2665) *1%」
		CASEELSE
			PRINTFORMW 「やったっ%UNICODE(0x2665) *1%　ありがと、%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
			PRINTFORMW 「後で他の人が良いって言っても、もう駄目……って、え？　もう、恋人が……{LOCAL:1}人！？」
			PRINTFORMW 「本当に！？　ちょっとした知り合いも恋人にカウントしてたりしない！？」
			PRINTFORMW 「えぇ～……告白成功して、ゴール！　って思ったのに……むしろスタートラインに立ったって感じね」
			PRINTFORMW 「う、埋もれないようにしないと……うぅぅ、気が抜けないわ～！」
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
			PRINTFORML 「う～っ、ダメ？　どうしてよ～」
			PRINTFORMW 「ねぇ、どういうのが好みなの？」
			PRINTFORMW 「これぐらいじゃ諦めないからね！」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K104_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORMW 「今日は楽しかったわ！　次も楽しみにしてるからねっ」
			PRINTFORMW 「……で、この後はどうする？……%UNICODE(0x2665) *1%」
		ELSEIF TALENT:思慕
			PRINTFORML 「%CALLNAME:MASTER%といるの、結構好きよ」
			PRINTFORMW 「また、どこか連れて行ってね%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORML 「やっぱり、魔界以外もなかなか新鮮で面白いわ！」
			PRINTFORMW 「他にも、どこか良いところ知ってたら教えてね」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「ん～？　わたし、別にもう怒ってないよっ」
		PRINTFORMW 「ちゃんと反省したなら、許してあげようじゃない」
		PRINTFORML 
		PRINTFORMW 「……反省、してるよね？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「わかった、わかったわ……ちょっとだけ許してあげる」
		PRINTFORMW 「ちょっとだけだからね？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「仲直りできると思ってたんだ」
		PRINTFORML 「まぁね～、わたしも他人と険悪なのは疲れるし」
		PRINTFORMW 「誠意に免じて、3割ほどは怒りを和らげてあげようじゃない」
		PRINTFORMW 「でも、まだまだ怒ってるからね！　7割くらい」
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K104_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF DAY:2 == 4 && DAY:3 == 14 && CFLAG:104:好感度 >= 1000 && !TCVAR:104:今日会ったか
			CALL EVENTDAY_GREETING_K104
		ELSEIF CFLAG:104:オナバレ == 1
			PRINTFORML 「あ、%CALLNAME:MASTER%……あのさ、この前の……」
			PRINTFORML 「あ～……何でもないわ」
			PRINTFORMW 「はぁ……」
		ELSEIF CFLAG:104:オナバレ
			IF (TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ)
				PRINTFORML 「%CALLNAME:MASTER%……%UNICODE(0x2665) *1%」
				PRINTFORMW 「次も、ちょうどよく来てくれたら……相手してあげるからね」
			ELSE
				PRINTFORML 「っ、%CALLNAME:MASTER%……」
				PRINTFORML 「この前は流されちゃったけど、次はこうはいかないから！」
				PRINTFORMW 「気を付けてよね！」
			ENDIF
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「あっ、%CALLNAME:MASTER%！」
			PRINTFORMW 「待ってたわ%UNICODE(0x2665) *1%」
		ELSEIF TALENT:思慕
			PRINTFORML 「あ、%CALLNAME:MASTER%！」
			PRINTFORMW 「今日は何してるの？」
		ELSE
			PRINTFORMW 「あら、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF DAY:2 == 4 && DAY:3 == 14 && CFLAG:104:好感度 >= 1000 && !TCVAR:104:今日会ったか
			CALL EVENTDAY_GREETING_K104
		ELSEIF CFLAG:104:オナバレ == 1
			PRINTFORML 「あ、%CALLNAME:MASTER%……あのさ、この前の……」
			PRINTFORML 「あ～……何でもないわ」
			PRINTFORMW 「はぁ……」
		ELSEIF CFLAG:104:オナバレ
			IF (TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ)
				PRINTFORML 「%CALLNAME:MASTER%……%UNICODE(0x2665) *1%」
				PRINTFORMW 「次も、ちょうどよく来てくれたら……相手してあげるからね」
			ELSE
				PRINTFORML 「っ、%CALLNAME:MASTER%……」
				PRINTFORML 「この前は流されちゃったけど、次はこうはいかないから！」
				PRINTFORMW 「気を付けてよね！」
			ENDIF
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「あっ、いたいた！」
			PRINTFORMW 「ね～、わたしの相手してよ」
		ELSEIF TALENT:思慕
			PRINTFORML 「あ、%CALLNAME:MASTER%……」
			PRINTFORMW 「あ～……暇なら、一緒にどこか行かない？」
		ELSE
			PRINTFORMW 「あら、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:泥酔 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「あっ、入ってたんだ？」
			PRINTFORML 「背中でも流してあげよっか～」
		ELSEIF TALENT:思慕
			PRINTFORML 「わっと、%CALLNAME:MASTER%」
			PRINTFORMW 「え～と、一緒に入っていい？」
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%じゃない」
			PRINTFORMW 「あはは、なんか照れ臭いね」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
