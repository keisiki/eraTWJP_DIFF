	ELSEIF CFLAG:添い寝中
		SELECTCASE RAND:5
		CASE 0,1
			PRINTFORMW 「あら……ちゅっ……」
			PRINTFORMW 「おやすみのちゅーかしら…ふふ」
		CASE 2,3
			PRINTFORMW 「ん……」
			PRINTFORMW 「優しいのね……」
		CASE 4
			PRINTFORMW 「ちゅっ……」
			PRINTFORMW 「よく眠れそうだわ……」
			PRINTFORMW 「ねぇ、もう一回して……」
		ENDSELECT
	;恋慕
	ELSE
		SELECTCASE EXP:キス経験
		CASE IS > 120
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「んんっ……！」
				PRINTFORMW 「あはっ…れろ……」
				PRINTFORMW 「温かくて気持ちいい……」
			CASE 1
				PRINTFORMW 「んあっ……」
				PRINTFORMW 「…ぷはぁ……」
				PRINTFORMW 「んふ……もっとして…」
			CASE 2
				PRINTFORMW 「あぅ……ちゅ……」
				PRINTFORMW 「ちゅっちゅ……」
				PRINTFORMW 「頭がぼーっとしちゃうわ…」
			ENDSELECT
		CASE IS > 70
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「んんっ……！」
				PRINTFORMW 「ちゅっ…れろ……」
				PRINTFORMW 「…ぷぁ……ふふ……激しいのね♪」
			CASE 1
				PRINTFORMW 「あはっ…れろ……ちゅ…」
				PRINTFORMW 「ん……もっとする…？」
			ENDSELECT

		CASE IS > 20
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「んぁ…ちゅ……」
				PRINTFORMW 「うふ……きもちぃ……」
			CASE 1
				PRINTFORMW 「あっ…んっ……」
				PRINTFORMW 「ちゅぅ……」
				PRINTFORMW 「ふぁ……温かいわ……」
			ENDSELECT
		CASEELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「んっ……」
				PRINTFORMW 「…ふぅ……」
				PRINTFORMW 「ま、まだ慣れなくて…ごめんなさい…」
			CASE 1
				PRINTFORMW 「あっ……ん……」
				PRINTFORMW 「もう、急にするんだから…」
			ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
