				GOTO COOKINGEND
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("軽食/"))
				GOTO OTEGARU
			ENDIF
		ENDSELECT
		
		PRINTL
		SELECTCASE RAND:5
		CASE 0
			PRINTFORMW 「ええっと、これを持って…あちっ！」
			PRINTFORMW 「うう…火は苦手……」
		CASE 1
			PRINTFORMW 「はかり？ああこれね」
