;301,お茶を淹れる
;==================================================
@M_KOJO_MESSAGE_COM_K63_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K63_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(301)
		PRINTFORMW 「まあ、お茶を淹れてくれたのね。ありがとう」
		SELECTCASE DAY:2
		;夏
		CASE 2
			PRINTFORMW 「蒸し暑い日に冷たいお茶は沁み渡るわ～」
			PRINTFORMW 「熱いお茶も飲めるかだって？」
		;夏じゃない
		CASEELSE
			PRINTFORMW 「熱いけど大丈夫かって？」
		ENDSELECT
			PRINTFORMW 「たしかに、野生の狼はみんな猫舌よ」
			PRINTFORMW 「だからって妖怪たる人狼が同じだと思ったら大間違い」
			PRINTFORMW 「でもなんで猫舌っていうのかしらね…ｽﾞｽﾞ……」

	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「さすが%CALLNAME:MASTER%、気が利くわね♪」
			PRINTFORMW 「ん～美味しい～♪」
		CASE 1
			PRINTFORMW 「あらありがとう♪いただきふぁす…」
			PRINTL ｽﾞｽﾞ…
			PRINTFORMW 「人間は火を使えるから猫舌じゃないのよね」
			PRINTFORMW 「だから自然から見れば人間の舌のがオカシイわけ」
			PRINTFORMW 「つまり人間舌ってわけね、私も、%CALLNAME:MASTER%も」
			PRINTFORMW 「あれ？でも私は妖怪だから妖怪舌ね」
		CASE 2
			PRINTFORMW 「きゃ～！ちょうど喉渇いてたのよ！ありがとう%CALLNAME:MASTER%♪」
			PRINTFORMW 「お茶は酔わないから、お酒より気軽に飲めていいわね」
			PRINTFORMW 「でもどっちも飲み過ぎは禁物よ」
			PRINTFORMW 「お茶の飲み過ぎは眠れなくなるわ！」
		CASE 3
			PRINTFORMW 「前から思っていたのだけれど……」
			PRINTFORMW 「このお茶いつ淹れたのかしら」
			PRINTFORMW 「%CALLNAME:MASTER%は時々不思議なことをするわよね」
		CASE 4
			PRINTFORMW 「%CALLNAME:MASTER%が淹れてくれたお茶、嬉しいわ♪」
			PRINTFORMW 「いつもありがとね♪」
		CASE 5
			SELECTCASE DAY:2
			CASE 1
				PRINTFORMW 「あら珍しい、抹茶なのね。ありがたくいただくわ」
				PRINTFORMW 「うっ、ちょっと苦いかも……」
			CASE 2
				PRINTFORMW 「夏はやっぱり麦茶よね」
				PRINTFORMW 「適度な水分補給を欠かさずに！」
			CASE 3
				PRINTFORMW 「あら今日は紅茶なのね。ありがたくいただくわ」
				PRINTFORMW 「うーん、味も良いけど、香りが素晴らしいわね」
			CASE 4
				PRINTFORMW 「わぁ～ありがとう！」
				PRINTFORMW 「あったかくて美味しいわ～！」
			ENDSELECT
		ENDSELECT

	;恋慕じゃない
	ELSE
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「あら、気が利くわね。いただくわ」
			PRINTFORMW 「ん～美味しい～」
		CASE 1
			PRINTFORMW 「あらありがとう。いただきます」
			PRINTL 
			PRINTFORMW 「人間以外の動物はみんな猫舌なのよ」
			PRINTFORMW 「化けた妖怪はどうなのか知らないわねぇ」
		CASE 2
			PRINTFORMW 「んー！ちょうど喉渇いてたのよ。ありがとう」
			PRINTFORMW 「お茶とお酒が美味しいというのはどの種族も共通認識ねぇ」
		CASE 3
			PRINTFORMW 「前から思っていたのだけれど……」
			PRINTFORMW 「このお茶いつ淹れたのかしら」
			PRINTFORMW 「さては妖術？！」
		CASE 4
			PRINTFORMW 「いつも悪いわね。感謝してるわよ」
			PRINTFORMW 「ほら、あなたの親切も一緒に五臓六腑に沁み渡るわ～」
		CASE 5
			SELECTCASE DAY:2
			CASE 1
				PRINTFORMW 「あら珍しい、抹茶なのね。ありがたくいただくわ」
				PRINTFORMW 「うっ、ちょっと苦いかも……」
			CASE 2
				PRINTFORMW 「夏はやっぱり麦茶よね」
				PRINTFORMW 「適度な水分補給を欠かさずに！」
			CASE 3
				PRINTFORMW 「あら今日は紅茶なのね。ありがたくいただくわ」
				PRINTFORMW 「うーん、味も良いけど、香りが素晴らしいわね」
			CASE 4
				PRINTFORMW 「わぁ～ありがとう！」
				PRINTFORMW 「あったかくて美味しいわ～！」
			ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
;==================================================
;302,スキンシップ
;==================================================
@M_KOJO_MESSAGE_COM_K63_302
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
IF TCVAR:発情
	IF TALENT:恋慕
		TFLAG:192 = 1
	ELSE
		TFLAG:192 = -1
	ENDIF
ELSE
	CALL TRAIN_MESSAGE
ENDIF
CALL M_KOJO_MESSAGE_COM_K63_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(302)
		PRINTFORMW 「ちょ、ちょっと！気安く触らないでよ！」
		PRINTFORMW 「あ、ごめん…いきなりで驚いただけよ……」
		SIF TALENT:思慕
			PRINTFORMW 「まぁ…%CALLNAME:MASTER%にだったら少しくらい……」
		RETURN 1
	ENDIF
	
	;恋慕（お風呂）
	IF TALENT:恋慕 && BATHROOM()
		PRINTFORMW 「背中流してくれるの？」
		PRINTFORMW 「ありがとう……」
		PRINTFORML 
		PRINTFORMW 「%TEXTR("そんな…まじまじと見ないでよ……/あはっ♪くっ、くすぐったいわっ！ふふっ/はい、じゃあ次は私が……")%」
		RETURN 1
	ENDIF
	IF TCVAR:発情
		FONTBOLD
		CALL HPH_PRINT,@"「ひゃん゛っっHPH」","W"
		FONTREGULAR
		IF TALENT:恋慕
			CFLAG:溜まってる度 += 100
			PALAM:TARGET:潤滑 += 500
			BASE:TARGET:ムード += 500
			PRINTFORMW 「ご、ごめんなさい……今触られるのは困るっていうか……その、嫌ってわけじゃ……」
			PRINTFORMW （うう、%CALLNAME:MASTER%に触られるとカラダが交尾の準備しちゃう……）
			IF TALENT:自己愛 >= 0
				CFLAG:TARGET:同行 = 60
				CFLAG:MASTER:同行 = 60
				PRINTFORMW 「ほら……わかるでしょ？　ね、向こう行きましょ？」
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の袖を握りしめた
			ENDIF
		ELSEIF TALENT:思慕
			PRINTFORMW 「ご、ごめんなさい……ちょっと今はそっとしておいて欲しいかなって……」
		ELSE
			PRINTFORMW 「ちょっと触らないでよ！離れて！」
			CFLAG:ブチギレ = 1
		ENDIF
		CALL COLORMESSAGE(@"欲求不満が上がった",C_YELLOW,1)
		CFLAG:溜まってる度 += 100
		IF PALAM:TARGET:潤滑 < 3000
			CALL COLORMESSAGE(@"潤滑が上がった",C_YELLOW,1)
			PALAM:TARGET:潤滑 += 500
		ENDIF
		RETURN 1
	ENDIF
	;恋慕
	IF TALENT:恋慕
		CALL KAGEROU_SKINSHIP
		SELECTCASE RESULT
		;手を握る
		CASE 0
			IF RAND:2
				PRINTFORMW 「あ…ちょっ……」
				PRINTFORMW 「私の手、爪が立ってると危ないから、気をつけてね…」
			ELSE
				PRINTFORMW 「ふふ、仄かだけど%CALLNAME:MASTER%の温もりが伝わってくるわ……」
				PRINTFORMW 「これって恋人つなぎっていうんじゃなかったかしら……」
				PRINTFORMW 「ちょっと、恥ずかしいからそのくらいにしてよ……」
			ENDIF
		;頬を撫でる
		CASE 1
			IF RAND:2
				PRINTFORMW 「あっ…どしたの%CALLNAME:MASTER%……」
				PRINTFORMW 「ふふ、私もしてあげる……」
			ELSE
				PRINTFORMW 「わっ！急にやられるとびっくりするわ」
				PRINTFORMW 「つ、つねったりしないでね」
			ENDIF
		;耳に触る
		CASE 2
			IF RAND:2
				PRINTFORMW 「こっちも本物の耳よ」
				PRINTFORMW 「妖獣の持つアヤカシの原理よ♪」
			ELSE
				PRINTFORMW 「こっちの耳は可動するのよ、ほらっ」
				PRINTFORMW 「隠すときはこう、寝かせて……」
			ENDIF
		;顎を擽る
		CASE 3
			IF RAND:2
				PRINTFORMW 「あっ、それ気持ちいい……」
				PRINTFORMW 「で、でもくすぐったいわ」
			ELSE
				PRINTFORMW 「ふぁあ…くすぐったいわ……」
				PRINTFORMW 「ああ…あと、犬みたいで恥ずかしい……」
			ENDIF
		;髪に触る
		CASE 4
			IF RAND:2
				PRINTFORMW 「ちゃんとお手入れはしてるつもりだけど…」
				PRINTFORMW 「そうやってまじまじと観察されるとなんだか緊張するわね……」
			ELSE
				PRINTFORMW 「そんな嗅いだりしないでよ～…」
				PRINTFORMW 「恥ずかしいわ……」
			ENDIF
		;肩を揉む
		CASE 5
			PRINTFORMW 「あ～気持ちいいわ～♪」
			IF RAND:2
				PRINTFORMW 「なかなか上手じゃない」
				PRINTFORMW 「さては霊夢に仕込まれたわね？」
			ELSE
				PRINTFORMW 「今度は私がやってあげるわ」
				PRINTL 
				PRINTFORMW 「お客さんずいぶん凝ってますね～」
				PRINTFORMW 「どうかしら？上手くできてる？」
			ENDIF
		;背を撫でる
		CASE 6
			IF RAND:2
				PRINTFORMW 「ふぁ～……！」
				PRINTFORMW 「ぞくってするけど、なぜか落ち着くわね」
			ELSE
				PRINTFORMW 「ああ…なんだか心地いいわ……」
				PRINTFORMW 「とっても安らいで…眠っちゃいそう……」
			ENDIF
		;脇腹をつつく
		CASE 7
			IF RAND:2
				PRINTFORMW 「あはは！くすぐったいじゃない！」
				PRINTFORMW 「%CALLNAME:MASTER%にもやったげるわ」
				PRINTFORMW 「ほれっ！ほれほれっ！」
			ELSE
				PRINTFORMW 「ちょっとくすぐったい！あはは！」
				PRINTFORMW 「きゃっ！もうやめてって！」
				PRINTFORMW 「くすぐったいってば～！」
			ENDIF
		;鼻を擽る
		CASE 8
			IF RAND:2
				PRINTFORMW 「ふぁ…むずむずするわ～……」
				PRINTFORMW 「へくしっ！」
			ELSE
				PRINTFORMW 「ふふ、何がしたいのかしらね」
				PRINTFORMW 「%CALLNAME:MASTER%にもやったげるわよ」
				PRINTFORMW 「どう？変な気分になるでしょ？」
			ENDIF
		;ぺたぺた
		CASE 9
			PRINTFORMW 「さっきから何ぺたぺた触ってんのよ」
			IF RAND:2
				PRINTFORMW 「別に嫌じゃないから、いくらでも触っていいけど…」
				PRINTFORMW 「なんか怪しいわよ…%CALLNAME:MASTER%」
			ELSE
				PRINTFORMW 「そんなに触りたいなら…その……」
				PRINTFORMW 「物陰にでも行ってくれれば……」
			ENDIF
		;人狼
		CASE 100
			CALL ERATOHO_JINROU_GAME
		ENDSELECT

	;恋慕じゃない
	ELSE
		CALL KAGEROU_SKINSHIP
		SELECTCASE RESULT
		;手を握る
		CASE 0
			PRINTFORMW 「あら、綺麗な手をしてるのね」
			PRINTFORMW 「毎日お手入れしてたり？」
		;頬を撫でる
		CASE 1
			PRINTFORMW 「なになになにー！？」
			PRINTFORMW 「もう、いつもいきなりでびっくりするのよ！」
		;耳に触る
		CASE 2
			PRINTFORMW 「こっちも本物の耳よ」
			PRINTFORMW 「どういう原理かって？ふふふ、秘密♪」
		;顎を擽る
		CASE 3
			PRINTFORMW 「……それ、犬のつもり？」
			PRINTFORMW 「怒るわよ？」
		;髪に触る
		CASE 4
			PRINTFORMW 「私の髪がどうだってのよ……」
			PRINTFORMW 「まぁ髪は女の命だからね」
			PRINTFORMW 「なによ、妖怪が言ったら変なわけ？」
		;肩を揉む
		CASE 5
			PRINTFORMW 「あーいいわねぇ。気持ちいい」
			PRINTFORMW 「もっとやってくれていいわよ～」
		;背を撫でる
		CASE 6
			PRINTFORMW 「きゃっ！なによもう」
			PRINTFORMW 「ぞくぞく～～ってきたわ」
		;脇腹をつつく
		CASE 7
			PRINTFORMW 「わわっ！なにするのよ！」
			PRINTFORMW 「このっ！このっ！」
		;鼻を擽る
		CASE 8
			PRINTFORMW 「ふぁ……なに……へっくしっ！」
			PRINTFORMW 「もー！なんなのよー！」
		;ぺたぺた
		CASE 9
			PRINTFORMW 「さっきから何ぺたぺた触ってんのよ」
			PRINTFORMW 「神社に住んでるなら、妖怪だって珍しくないでしょうに」
			PRINTFORMW 「そんなことして食われたって文句言えないわよ？」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
