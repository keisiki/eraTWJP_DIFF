	ELSE
		PRINTFORMW 「%CALLNAME:MASTER%、引いてる引いてる！」
		RESETCOLOR
		PRINTFORMDL %LOCALS%が釣れた！
		;ローター以降を弾いてサイズと漁獲数の処理
		IF INRANGE(TFLAG:193, 1, 60) && TFLAG:193 != 21
			;サイズの出力
			LOCAL:1 = FISHING_SIZE(TFLAG:193)
			PRINTFORML 　サイズ：{LOCAL:1}cm
			;ここまでの最高サイズと比較して大きければ記録
			IF LOCAL:1 > FISHSIZE_RECORD:(TFLAG:193)
				SETCOLOR C_YELLOW
				SIF FISH_EXP:(TFLAG:193)
					PRINTFORML 　今まで釣った%LOCALS%の中でも最大サイズだ！
				RESETCOLOR
				FISHSIZE_RECORD:(TFLAG:193) = LOCAL:1
			ENDIF
			;漁獲数の加算
			FISH_EXP:(TFLAG:193) ++
		ENDIF
		;説明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
		CALL M_KOJO_COLOR_K63
	ENDIF
	SELECTCASE TFLAG:193
		CASE 11 TO 20, 24 TO 60
			IF TIME_PROGRESS(TCVAR:MASTER:空腹時刻) && TIME_PROGRESS(TCVAR:TARGET:空腹時刻)
				PRINTFORMW 「ね、%CALLNAME:MASTER%、お腹空かない？」
				CALL ASK_YN("この場で食べる","食べない")
				IF !RESULT
					CALL PRINT_FACE, [[影狼]], "笑顔", "服", "", ""
					PRINTFORMW 「やったぁ！」
					PRINTFORMW 釣った%LOCALS%を焼いて食べた
					CALL LOST_ITEM("うまい魚", 1, 1)
					TCVAR:[[影狼]]:空腹時刻 = 1440 * DAY + TIME + 200
					CALL RECOVER_PERMIL([[影狼]], 400, "体力", 0, "食事による回復")
					CALL RECOVER_PERMIL([[影狼]], 400, "気力", 0, "食事による回復")
					TCVAR:MASTER:空腹時刻 = 1440 * DAY + TIME + 200
					CALL RECOVER_PERMIL(MASTER, 400, "体力", 0, "食事による回復")
					CALL RECOVER_PERMIL(MASTER, 400, "気力", 0, "食事による回復")
					CALL RECOVER_PERMIL(MASTER, 200, "精力", 0, "食事による回復")
					TIME += 20
					CFLAG:1004 += 3
				ENDIF
			ENDIF
	ENDSELECT
ENDIF
RETURN 1

