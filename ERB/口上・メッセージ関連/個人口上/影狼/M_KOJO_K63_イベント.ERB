		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会っている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:思慕 && !RAND:5
			PRINTFORML 「あーあ」
			PRINTFORMW 「%CALLNAME:MASTER%が狼男だったらよかったのに」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

