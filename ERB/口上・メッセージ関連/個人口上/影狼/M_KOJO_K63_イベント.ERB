			CALL COLORMESSAGE(@"影狼は【解放】を得た",C_YELLOW,1)
			TALENT:自己愛 = 1
			RETURN 1
		ENDIF
		IF TALENT:自己愛 < 0 && TCVAR:360 && !RAND:4 && CFLAG:MASTER:初期位置 == CFLAG:MASTER:現在位置
			PRINTFORMW 「あっ！？　ご、ごめんなさい！」
			PRINTFORMW 「その、声我慢してたから、枕……」
			PRINTFORMW 牙で大穴が空いている……
		ELSEIF TCVAR:360 >= 10
			PRINTFORMW 「その、嫌だったわけじゃないけど、あんまりイかせすぎないで……」
			PRINTFORMW 「わけわかんなくなっちゃうから……%K17_BLUSH(2)%」
			CALL JUEL_UP_SPECIAL([[影狼]],"恥情","露出癖",2,1)
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
	;-------------------------------------------------
	;事後描写
	;記入チェック（=0, 非表示、1, 表示）
