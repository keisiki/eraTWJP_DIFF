;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;今日会っていない
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG && !RAND:4
		IF ARG >= 5
			IF TALENT:恋慕
				CALL PRINT_FACE, [[影狼]], "通常", "服", "", ""
				PRINTFORML 「もうずいぶん%CALLNAME:MASTER%と会ってない気がするわ」
				PRINTFORMW 「会いたいなー…」
				CFLAG:[[影狼]]:1004 ++
			ELSEIF TALENT:思慕 && !RAND:2
				CALL PRINT_FACE, [[影狼]], "通常", "服", "", ""
				PRINTFORMW 「ここのところ見ないけど、%CALLNAME:MASTER%はどうしてるのかしら」
				CFLAG:[[影狼]]:1004 ++
			ELSEIF ABL:親密 >= 3 && !RAND:3
				CALL PRINT_FACE, [[影狼]], "通常", "服", "", ""
				PRINTFORMW 「そういえば最近%CALLNAME:MASTER%を見ないわねえ」
			ENDIF
		ELSE
