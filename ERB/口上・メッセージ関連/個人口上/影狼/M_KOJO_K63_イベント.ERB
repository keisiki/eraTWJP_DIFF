		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;愛液が垂れてきた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;事後のムード
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 3
			IF !TALENT:妊娠 && 充填率(TARGET,4) >= 10 && EXP:Ｖ性交経験 > 15
				IF !EXP:出産経験
					PRINTFORMW 「ふふ、赤ちゃん、できるかしら」
				ELSEIF !RAND:4
					PRINTFORMW 「この分だとすぐもう一人できちゃいそうね」
					PRINTFORMW 「%TEXTR("今からでも名前、考えておきましょうか/お金の準備は任せたわよ？")%」
				ELSE
					PRINTFORMW 「私、子供はたっくさん欲しいわ」
					PRINTFORMW 「@%TEXTR("群れを作ってシカとか大きいのを狩るの！/花札柄の服を色々着せて役を作るの！/家族で並んで月に向かって吠えるの！もちろん%CALLNAME:MASTER%もね")%」
				ENDIF
			ENDIF
			IF ABL:奉仕精神 >= 3 && ABL:舌 >= 2 && ABL:精液中毒 >= 3 && STAIN:PLAYER:2 & 汚れ_精液 && !(STAIN:PLAYER:2 & 汚れ_アナル)
				STAIN:0 |= STAIN:PLAYER:2
				PRINTFORML 「ちょっと待って」
				PRINTFORMW 「服着る前におちんちん綺麗にしてあげるわね」
				CALL HPH_PRINT,@"「はむっ、んっ、んんっ、ちゅっ、ぢゅうぅぅ～～～っHPH」","W"
				PRINTFORMW 「ぷはっ……はい、おしまい」
				PRINTFORMW 「ふふ、またよろしくね♪」
				PRINTFORM 影狼が先端に軽く口づけると、%CALLNAME:MASTER%のペニスは
				IF BASE:MASTER:精力 > 300
					PRINTFORML またもや堅さを取り戻した
					PRINTFORMW 「懲りないわねえ、おしまいって言ったでしょ」
				ELSE
					PRINTFORMW ビクンと小さく跳ねた
				ENDIF			
				CALL JUEL_UP_SPECIAL([[影狼]],"恭順","奉仕精神",2,1)
				EXP:精液経験 ++
				EXP:口淫経験 ++
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;服を着る
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 4
			IF INRANGE(DAY:3,13,17)
				IF FIRSTTIME("毛深着衣")
					PRINTFORMW 「見ないでよね、ゼッタイ」
					PRINTFORMW 「こればっかりは本気で怒るわよ」
				ELSE
					PRINTFORMW 「……見てないわよね？」
				ENDIF
			ELSE
				PRINTFORMW 「あんな事した後だけど、あんまり見ないで……」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
