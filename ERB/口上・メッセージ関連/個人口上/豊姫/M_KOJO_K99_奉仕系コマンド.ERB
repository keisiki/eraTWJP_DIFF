@M_KOJO_MESSAGE_COM_K99_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ふふっHPH もうイッちゃいそう……？HPH」","L"
						CALL HPH_PRINT,@"「いいのよ……我慢しなくても……HPH」","L"
						CALL HPH_PRINT,@"「私が全部受け止めてあげるから……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 豊姫の竿を扱く速度が徐々に激しさを増していく
							PRINTFORMDL 当然%CALLNAME:MASTER%はそれに耐えられるはずもなく
							PRINTFORMDW 口を開け待機する豊姫の顔面に精液をぶちまけることになった……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「んー……ちょっと勿体ない気もするけど……HPH」","L"
						CALL HPH_PRINT,@"「でも、貴方がそうしたいのなら……HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 最後は膣内で。そう思っていた豊姫は少しだけ物足りなさそうにしていた……
					CASE 2
						SIF !CFLAG:99:1998
							PRINTFORMDL 豊姫の巧みな手淫で%CALLNAME:MASTER%はあっという間に射精へと至ってしまった
						CALL HPH_PRINT,@"「ぁ……HPH 凄い量……HPH」","L"
						CALL HPH_PRINT,@"「それに、このニオイ……はぁぁ……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 豊姫は手の平にべっとりと付着する精液に顔を近付けると
							PRINTFORMDW 限界まで深呼吸し濃厚な穢れの臭いを肺の中に収めた……
						ENDIF
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%のおちんちん触ってると……HPH」","L"
						CALL HPH_PRINT,@"「私も……興奮してきちゃって……HPH」","L"
						CALL HPH_PRINT,@"「ぁぁ、駄目……我慢できない……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 片手で竿を擦り上げながら空いた手で自らを慰める豊姫
							PRINTFORMDW その光景は、見ているだけで射精してしまいそうな程に淫らだった……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「穢れが溜まっているのはここかしら……？HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 豊姫はやわやわと陰嚢を揉み解しながら規則的に竿を擦り上げている
						CALL HPH_PRINT,@"「駄目じゃない……こんなに溜めちゃ……HPH」","L"
						CALL HPH_PRINT,@"「私がスッキリさせてあげるからね……HPH」","W"
					CASE 2
						CALL HPH_PRINT,@"「ねぇ、聞こえるでしょう……？HPH」","L"
						CALL HPH_PRINT,@"「ニチャニチャッてエッチな音してるの……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 先走りにまみれた指を竿に絡ませ、優しく上下に扱き上げられる
							PRINTFORMDW 直接的な快楽に視覚と音が合わさり、%CALLNAME:MASTER%はイかないように堪えるのがやっとだった……
						ENDIF
				ENDSELECT
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「もうイッちゃいそう……？」
						PRINTFORML 「いいわよ……いっぱい射精して……♪」
						SIF !CFLAG:99:1998
							PRINTFORMDW 豊姫は%CALLNAME:MASTER%の敏感な部分を重点的に擦り上げ射精へと導いた……
					CASE 1
						PRINTFORML 「こうすると気持ち良いでしょう……？」
						PRINTFORML 「射精そうになったらいつでも……きゃっ」
						SIF !CFLAG:99:1998
							PRINTFORMDL 豊姫が言い終わるより前に%CALLNAME:MASTER%は暴発気味に精を迸らせる
						PRINTFORML 「……ふふっ、お疲れ様。いっぱいでたわね」
						SIF !CFLAG:99:1998
							PRINTFORMDW 顔に付いた精液を拭いながら、豊姫は%CALLNAME:MASTER%に微笑みかけた……
					CASE 2
						PRINTFORML 「今はただ気持ち良くなることだけ考えればいいの……」
						PRINTFORML 「だから我慢しないで……私に身を任せて……」
						IF !CFLAG:99:1998
							PRINTFORMDL 豊姫のその言葉で%CALLNAME:MASTER%は無意識に射精を我慢していたことに気が付いた
							PRINTFORMDL 彼女の言う通りに全身の力を抜いて快楽に身を委ねると、あっという間に射精へと至ってしまった……
						ENDIF
						PRINTFORMW 「……んっ♪ 頑張ったわね。偉いわ」
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「さ、肩の力を抜いて……」
						PRINTFORML 「私が気持ち良くしてあげるから……」
						SIF !CFLAG:99:1998
							PRINTFORMDW 豊姫は潤滑剤代わりに唾液を垂らすと、ゆっくりと竿を扱き始めた……
					CASE 1
						PRINTFORML 「貴方のおちんちん、凄く熱くなってる……」
						PRINTFORML 「こんなのが私のナカに挿入ったら……ぁっ」
						PRINTFORMW 「今、ビクビクッてした……私とエッチするの想像しちゃったの……？」
					CASE 2
						PRINTFORML 「ねぇ……もっと見せて……？」
						PRINTFORML 「貴方の気持ち良いって表情……♪」
						SIF !CFLAG:99:1998
							PRINTFORMDW 豊姫は快楽に歪む%CALLNAME:MASTER%の顔を見つめながら竿を扱いている……
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

