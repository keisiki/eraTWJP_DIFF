;==================================================
;104,アイマスク
;==================================================
@M_KOJO_MESSAGE_COM_K99_104
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K99_104_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_104_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMDL 
		RETURN 1
	;通常
	ELSE
		;装着
		IF TEQUIP:アイマスク
			;恋慕
			IF TALENT:恋慕
				;マゾっ気で分岐
				SELECTCASE ABL:マゾっ気
					;マゾっ気２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「あ……待って、言わないで……HPH」","L"
								CALL HPH_PRINT,@"「何をされるか分からない方がワクワクするから……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW 自発的にアイマスクを装着した豊姫は興奮した様子で次に行われる行為を待っている……
							CASE 1
								CALL HPH_PRINT,@"「これ……何も見えなくて……HPH」","L"
								CALL HPH_PRINT,@"「凄く……興奮しちゃう……HPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL 豊姫は何をされるか分からない恐怖に息を荒げている
									PRINTFORMDL %CALLNAME:MASTER%が耳元でわざと大きな音を立てて脅かしてやると
									PRINTFORMDW 豊姫はビクビクッと躰を跳ねさせ、股から興奮の蜜を滴らせた……
								ENDIF
							CASE 2
								SIF !CFLAG:99:1998
									PRINTFORMDL %CALLNAME:MASTER%はアイマスクを装着した豊姫を抱き締めると、彼女の首筋をねっとりと舐め上げた
								CALL HPH_PRINT,@"「ひゃんっHPH ん……ふぅ……っHPH」","L"
								CALL HPH_PRINT,@"「見えないと……感覚が鋭くなって……HPH」","L"
								CALL HPH_PRINT,@"「はぁぁ……HPH おかしくなっちゃいそう……HPH」","W"
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「私は…貴方の顔を見ながらするほうが好きかな……」
								SIF !CFLAG:99:1998
									PRINTFORMDW アイマスクを装着した豊姫は少しだけ不満そうにしている
							CASE 1
								PRINTFORML 「……ねぇ、やっぱり外しても良い？」
								PRINTFORML 「貴方の顔が見えないのは……少し怖いわ」
								SIF !CFLAG:99:1998
									PRINTFORMDW 豊姫は怯えてしまっている。その姿が%CALLNAME:MASTER%の嗜虐心を煽った
							CASE 2
								PRINTFORML 「これで良いのよね。%CALLNAME:MASTER%」
								PRINTFORML 「……%CALLNAME:MASTER%？」
								PRINTFORML 「ねぇ、どうして黙っているの？」
								SIF !CFLAG:99:1998
									PRINTFORMDW 暫く黙って様子を見ていたが、豊姫は実にそそる反応をしてくれた
						ENDSELECT
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				;マゾっ気で分岐
				SELECTCASE ABL:マゾっ気
					;マゾっ気２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「アイマスクを外して、もし貴方が居なくなってたら……HPH」","L"
								CALL HPH_PRINT,@"「……ああっHPH それって凄く興奮しちゃう……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW 豊姫は妄想を膨らませて股を濡らしている。本当に放置してやろうか……
							CASE 1
								CALL HPH_PRINT,@"「さて、次は何をされるのかしら……HPH」","L"
								CALL HPH_PRINT,@"「うふふっHPH ワクワクするわ……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW 豊姫は次に行われる行為に期待を膨らませている……
							CASE 2
								CALL HPH_PRINT,@"「ん……ちょっと待っててね……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:MASTER%がアイマスクを付けるように指示すると、豊姫は素直に従った
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「これを付ければ良いのね？」
								PRINTFORML 「……ん、真っ暗で何も見えないわ」
								PRINTFORMW 「ねぇ、まだそこに居る？」
							CASE 1
								PRINTFORML 「ふふっ、真っ暗ね。幼い頃に依姫と探検した日のことを思い出すわ」
								PRINTFORML 「真っ暗な部屋に閉じ込められて……あの子、わんわん泣いてたっけ」
								SIF !CFLAG:99:1998
									PRINTFORMDW 思い出話も程々に、%CALLNAME:MASTER%は行為を再開した
							CASE 2
								PRINTFORML 「これを付けると、眠くなっちゃいそう……」
								PRINTFORML 「ふぁ……もし寝てしまったら起こしてね……」
								SIF !CFLAG:99:1998
									PRINTFORMDW 豊姫は既に眠そうにしている
						ENDSELECT
				ENDSELECT
				RETURN 1
			ENDIF
		;解除
		ELSE
			;マゾっ気で分岐
			SELECTCASE ABL:マゾっ気
				;マゾっ気２以上
				CASE IS > 1
					CALL HPH_PRINT,@"「顔を見ながらするのも良いけど……HPH」","L"
					CALL HPH_PRINT,@"「偶には、こういうのも悪くないわね……HPH」","W"
				;マゾっ気２未満
				CASEELSE
					PRINTFORML 「んっ、眩しい……」
					SIF !CFLAG:99:1998
						PRINTFORMDW 豊姫は目をシパシパと瞬かせている
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

;==================================================
;105,縄
;==================================================
@M_KOJO_MESSAGE_COM_K99_105
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K99_105_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_105_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMDL 
		RETURN 1
	;通常
	ELSE
		;縛った時
		IF TEQUIP:縄
			;恋慕
			IF TALENT:恋慕
				;マゾっ気で分岐
				SELECTCASE ABL:マゾっ気
					;マゾっ気２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「もっとキツく……んくっHPH もっとぉっHPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL これ以上きつくしたら痣になってしまう
									PRINTFORMDW %CALLNAME:MASTER%がそう言っても、豊姫は更なる苦痛を求めてきた……
								ENDIF
							CASE 1
								CALL HPH_PRINT,@"「これから何をされてしまうのかしら……HPH」","L"
								CALL HPH_PRINT,@"「無理矢理、貴方に穢されて……はんっHPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL 『縛れたぞ』と%CALLNAME:MASTER%が尻を引っ叩いてやると
									PRINTFORMDW 豊姫はビクビクと躰を震わせ、股から淫汁を溢れさせた……
								ENDIF
							CASE 2
								IF !CFLAG:99:1998
									PRINTFORMDL %CALLNAME:MASTER%はお手本のような亀甲縛りで豊姫を縛り上げると
									PRINTFORMDL 股に食い込んだ縄を引っ張り、縛り具合を確認した……
								ENDIF
								CALL HPH_PRINT,@"「っ、んHPH 大丈夫……痛くないわ……HPH」","L"
								CALL HPH_PRINT,@"「ねぇ……次はどうするの……？HPH」","W"
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「痛ッ……こんなの跡になっちゃうわ……」
								IF !CFLAG:99:1998
									PRINTFORMDL 神でさえ抜け出せないフェムトファイバーの縄で
									PRINTFORMDW %CALLNAME:MASTER%は豊姫をきつく縛り上げた
								ENDIF
							CASE 1
								PRINTFORML 「ね、ねぇ……やっぱり止めましょう……？」
								PRINTFORML 「なんでもしてあげるから……ね？」
								IF !CFLAG:99:1998
									PRINTFORMDL こういった事に慣れていない豊姫の躰は緊張で強張っている
									PRINTFORMDW 『優しくする』そう言いながら%CALLNAME:MASTER%は豊姫を縄で縛り上げた
								ENDIF
							CASE 2
								PRINTFORML 「これも貴重な経験……」
								PRINTFORML 「ん、くっ……なの、かしら……」
								SIF !CFLAG:99:1998
									PRINTFORMDW 豊姫は縄で縛られる側の立場を身をもって味わっている
						ENDSELECT
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				;マゾっ気で分岐
				SELECTCASE ABL:マゾっ気
					;マゾっ気２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「んんっHPH はぁ……大丈夫……HPH」","L"
								CALL HPH_PRINT,@"「ちょっと、キツかっただけだから……HPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL 皮膚に食い込む縄の痛みに豊姫は躰を震わせ、熱い吐息を漏らす
									PRINTFORMDW 不浄な者の出入りを禁じる為の縄は、快楽を感じる為の道具へと変わっていた
								ENDIF
							CASE 1
								CALL HPH_PRINT,@"「これ……きっと痣になっちゃうわ……HPH」","L"
								CALL HPH_PRINT,@"「ああ……依姫になんて説明したら……HPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL 欲望の為に自ら望んで縄に縛られたと説明した時、愛する妹から受ける冷めた視線……
									PRINTFORMDL 軽蔑されるだろうか。罵倒されるだろうか。口も聞いてくれなくなるだろうか
									PRINTFORMDW どちらに転んでも、今の豊姫にとってはご褒美でしかなかった
								ENDIF
							CASE 2
								CALL HPH_PRINT,@"「これも、一つの愛のカタチなのね……HPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL %CALLNAME:MASTER%に縛られ、転がされた豊姫は
									PRINTFORMDW 何かを期待するようにこちらを見つめている……
								ENDIF
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「誰かが縛られるのを見るのは好きだけど……」
								PRINTFORML 「自分が縛られるのは……ちょっと……」
								IF !CFLAG:99:1998
									PRINTFORMDL 縛られる側の気持ちを知ってもらおう
									PRINTFORMDW %CALLNAME:MASTER%は気持ちキツめに豊姫を縄で縛り上げた
								ENDIF
							CASE 1
								PRINTFORML 「ぁ、くっ……縄が、食い込んで……っ」
								IF !CFLAG:99:1998
									PRINTFORMDL 豊姫が動けないようにしっかりと拘束し、座らせる
									PRINTFORMDW 縛られ慣れていないのか、躰は強張っていた
								ENDIF
							CASE 2
								PRINTFORML 「も、もうちょっと緩くできない……？」
								PRINTFORML 「その、跡になっちゃうから……」
								SIF !CFLAG:99:1998
									PRINTFORMDW 豊姫はあまり気乗りしていないようだ……
						ENDSELECT
				ENDSELECT
				RETURN 1
			ENDIF
		;解いた時
		ELSE
