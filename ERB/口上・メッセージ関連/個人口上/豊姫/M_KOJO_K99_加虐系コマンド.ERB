				;マゾっ気２未満
				CASEELSE
					PRINTFORML 「はぁ……この跡、消えるかしら……」
					SIF !CFLAG:99:1998
						PRINTFORMDW 豊姫は肌についた縄の跡を手で摩っている
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
