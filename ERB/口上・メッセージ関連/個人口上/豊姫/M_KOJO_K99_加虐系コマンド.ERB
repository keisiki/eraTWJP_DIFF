;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K99_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K99_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMDL 
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;マゾっ気で分岐
			SELECTCASE ABL:マゾっ気
				;マゾっ気２以上
				CASE IS > 1
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「くぅんっHPH っはぁHPH んあぁっHPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL パンッと尻を叩く度に豊姫が甘い嬌声を上げる
								PRINTFORMDW 被虐の悦びに目覚めた豊姫の躰は、痛みを気持ち良いものと認識していた……
							ENDIF
						CASE 1
							CALL HPH_PRINT,@"「ん、ぐっHPH もっと、叩いて……っHPH」","L"
							CALL HPH_PRINT,@"「気の済むまで、もっと……っHPH ぅあっHPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL 何度も叩かれ、腫れた部分から発せられる灼けるような痛み
								PRINTFORMDW この痛みこそ、豊姫が求めていたものだった……
							ENDIF
						CASE 2
							CALL HPH_PRINT,@"「だ、だめよ……HPH そんなに振りかぶったら……HPH」","L"
							CALL HPH_PRINT,@"「私のお尻……壊れちゃ、うぅんんっHPH HPH HPH」","L"
							SIF !CFLAG:99:1998
								PRINTFORMDL 腕を鞭のようにしならせて、渾身の力で豊姫の臀部を打ち据える
							CALL HPH_PRINT,@"「は……はぁ……っHPH ぁ……あぁ……っHPH」","L"
							SIF !CFLAG:99:1998
								PRINTFORMDW 白い肌と赤い腫れのコントラストは、まるで桃のように美しい……
					ENDSELECT
				;マゾっ気２未満
				CASEELSE
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「くぅっ！ 痛い、けど……っ」
							PRINTFORML 「大丈夫……続けて……んんっ！」
							SIF !CFLAG:99:1998
								PRINTFORMDW 豊姫は瞳に涙を浮かべながらも、%CALLNAME:MASTER%の行為を受け入れている……
						CASE 1
							PRINTFORML 「大丈夫よ……遠慮しなくていいから……」
							PRINTFORML 「んくっ！ そう、もっと……ぅんっ！」
							IF !CFLAG:99:1998
								PRINTFORMDL 気丈に振舞う豊姫のお尻を%CALLNAME:MASTER%は容赦なく叩き続けた
								PRINTFORMDW 見る者を魅了する形の良い桃尻は、痛々しく腫れ上がっている……
							ENDIF
						CASE 2
							PRINTFORML 「はぁ……はぁ……っ」
							PRINTFORML 「もう……いいの……？」
							SIF !CFLAG:99:1998
								PRINTFORMDW 痛々しく腫れ上がった豊姫のお尻を、%CALLNAME:MASTER%は優しく撫でてあげた……
					ENDSELECT
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			;マゾっ気で分岐
			SELECTCASE ABL:マゾっ気
				;マゾっ気２以上
				CASE IS > 1
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ん゛ん゛っHPH もっと、強く……っHPH」","L"
							CALL HPH_PRINT,@"「私のこと……痛め付けて……っHPH ぁ、くっHPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL すっかりこういうことが大好きになってしまった豊姫の求めに応じて
								PRINTFORMDW %CALLNAME:MASTER%は彼女の尻を、彼女が満足するまで叩き続けてやった……
							ENDIF
						CASE 1
							CALL HPH_PRINT,@"「くぅっHPH ふっHPH んぅっHPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL %CALLNAME:MASTER%に尻を叩かれる度、豊姫は気持ち良さそうに声を漏らしている
								PRINTFORMDW 元々素質があったのだろう。豊姫の躰はすんなりと痛みを受け入れるようになっていた
							ENDIF
						CASE 2
							CALL HPH_PRINT,@"「ダメ……優しくしないで……っHPH」","L"
							CALL HPH_PRINT,@"「痛くされないと、気持ち良くなれないの……っHPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL 尻が赤くなるまで叩いてやったのに、まだ足りないと豊姫が言う
								PRINTFORMDW それなら手加減は無用だと、%CALLNAME:MASTER%は渾身の力で彼女の尻を打ち据えてやった
							ENDIF
					ENDSELECT
				;マゾっ気２未満
				CASEELSE
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「はくぅっ……っ、うぅ……」
							PRINTFORML 「酷いわ……いきなり叩くなんて……」
							SIF !CFLAG:99:1998
								PRINTFORMDW 豊姫のお尻には、%CALLNAME:MASTER%の手形がクッキリと残っている……
						CASE 1
							PRINTFORML 「八意様にもぶたれたことないのに……ぅんんっ！」
							IF !CFLAG:99:1998
								PRINTFORMDL %CALLNAME:MASTER%が尻を叩く度に乾いた音が辺りに響く
								PRINTFORMDW 豊姫はきゅっと唇を噛み、痛みを堪えている……
							ENDIF
						CASE 2
							PRINTFORML 「痛いのは……ぁくっ！ んっ！ あぁっ！」
							IF !CFLAG:99:1998
								PRINTFORMDL %CALLNAME:MASTER%は豊姫が逃げないように腰を掴まえると
								PRINTFORMDW 彼女のお尻を容赦なく何度も叩いた……
							ENDIF
					ENDSELECT
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

