						CALL HPH_PRINT,@"「んれ……るりゅ……っHPH くちゅ……ふちゅ……っHPH」","L"
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「ちゅ……ん、そろそろね……HPH レイセン、頑張ってHPH」","L"
						CALL M_KOJO_COLOR_K100
