;二人目はいらない場合はここで処理
;二人目
LOCAL = MASTER_POSE(4,1,1)
LOCAL:1 = RESULT
LOCAL:2 = TARGET
TARGET = LOCAL
TRYCALLFORM M_KOJO_MESSAGE_SCOM_K{TARGET}_9_2
TARGET = LOCAL:2
RETURN LOCAL:1

;一人目口上
@M_KOJO_MESSAGE_SCOM_K99_9_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	LOCAL = MASTER_POSE(4,1,1)
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;相方がレイセン
	ELSEIF LOCAL == 100
		;豊姫とレイセンが恋慕
		IF TALENT:99:恋慕 && TALENT:100:恋慕
			;あなたが射精した
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMDL %CALLNAME:MASTER%が射精すると、豊姫がペニスの先端をパクッと咥えた
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「ん、ふ……HPH ちゅる……こくん……っHPH」","L"
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「ああっ……豊姫様ズルいです、私だって……んむぅっHPH」","L"
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「ん……くぷ……HPH はぁ……これで満足かしら……？HPH」","L"
						PRINTFORMDW レイセンと豊姫は濃厚なキスを交わしながら、口内に溜まった精液をシェアしている……
						RESETCOLOR
						EXP:99:キス経験 ++
						EXP:100:キス経験 ++
					CASE 1
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「れりゅ……ちゅ……HPH れる……れろ……っHPH」","L"
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「射精していいのよ……HPH ぜーんぶ、私とレイセンで飲んであげるから……HPH」","L"
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「らひて……HPH ちゅる、ん、れろれろ……っHPH」","L"
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「ほら、この子もこう言っていることだし……ねHPH」","L"
						PRINTFORMDL 一心不乱に肉棒をしゃぶるレイセンに顔を寄せて奉仕に加わる豊姫
						PRINTFORMDW もう限界だ。%CALLNAME:MASTER%が腰を浮かせると、二人は精液を飲もうと雛鳥のように口を開けて射精を待機した……
						RESETCOLOR
					CASE 2
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「ちゅぱ……ちゅう……っHPH いっぱい……HPH」","L"
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「れろ……ぢゅるる……っHPH 穢れを射精しなさい……HPH」","L"
						PRINTFORMDL 顔を寄せ合い、射精寸前のペニスにねっとりと舌を這わせるレイセンと豊姫
						PRINTFORMDW %CALLNAME:MASTER%が達すると、二人は息を荒くして迸る精液を奪い合った……
						RESETCOLOR
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「ちゅ……はぁ……HPH んれる……れろぉ……っHPH」","L"
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「凄い……豊姫様、あんなに……HPH わ、私も……っHPH」","L"
						PRINTFORMDL レイセンと豊姫は競い合うように雄々しく勃起したペニスを舐めしゃぶっている
						PRINTFORMDW 二人の愛情のこもったフェラチオは強い快楽と幸福感を%CALLNAME:MASTER%に与えてくれた
						RESETCOLOR
					CASE 1
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「せっかく二つあるんだから、二人で気持ち良くさせてあげないとHPH」","L"
						CALL HPH_PRINT,@"「さ、貴女はそっちHPH あー…むっ……HPH ちゅう……れろ……っHPH」","L"
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「はむ……ん……っHPH ちゅぱ……ちゅ……っHPH」","L"
						PRINTFORMDW レイセンと豊姫は%CALLNAME:MASTER%の陰嚢を口に含み、舌で転がし愛撫している……
						RESETCOLOR
					CASE 2
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「んぅ……んれ……HPH れる……ちゅう……っHPH」","L"
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「ちゅう……ぢゅる……HPH ちゅ……ちゅぷ……っHPH」","L"
						PRINTFORMDL 豊姫が敏感なペニスの先端、レイセンが竿の部分に舌を這わせている……
						CALL HPH_PRINT,@"「ちゅぽんっHPH ふぅ……さあレイセン、交代よHPH」","L"
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「はい……HPH あむ、んむ……っHPH れる……れろ……っHPH」","W"
						RESETCOLOR
				ENDSELECT
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			;あなたが射精した
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「んりゅ……ちゅ……っHPH はむ……ん……っHPH」","L"
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「レイセン、舌はこういう風に遣うのよ。れろ……ちゅむ……HPH」","L"
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「こう、れふか……？ ちゅぱ……んっHPH んれる……HPH」","L"
						PRINTFORMDL 拙い舌遣いで竿を舐めるレイセンと、的確に弱い所を責めてくる豊姫
						PRINTFORMDW 射精寸前のギリギリで焦らされた%CALLNAME:MASTER%のペニスは、もう限界を訴えていた……
						RESETCOLOR
					CASE 1
						CALL M_KOJO_COLOR_K99
						CALL HPH_PRINT,@"「ふふっHPH もう射精そうでしょう？」","L"
						CALL HPH_PRINT,@"「分かるわよ……HPH ねぇ、レイセン？」","L"
						CALL M_KOJO_COLOR_K100
						CALL HPH_PRINT,@"「えっ！？ あ、あの、それは、その……HPH」","L"
						PRINTFORMDL あと一押しを求めて痙攣する%CALLNAME:MASTER%のペニスからレイセンは目が離せない
						PRINTFORMDW 豊姫はクスリと笑うと、レイセンに見せつけるようにして舌を遣った奉仕を再開した……
						RESETCOLOR
					CASE 2
