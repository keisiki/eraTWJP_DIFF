;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K99
	;時間停止中
	IF FLAG:時間停止
		PRINTFORMDL %CALLNAME:MASTER%は時間を止めたまま豊姫のスカートに手を伸ばして無造作にめくり上げ、
		PRINTFORMDW その下に広がる光景を思う存分視姦した……
	;押し倒され
	ELSEIF CFLAG:うふふ == 2
		CALL TRAIN_MESSAGE
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMDL %CALLNAME:MASTER%がスカートをめくり上げようとすると
			PRINTFORMDL 豊姫は自らスカートをたくし上げ、その下に広がる光景を見せつけてきた
			PRINTFORMDL 豊姫の瞳は潤み、%CALLNAME:MASTER%に何かを期待するような視線を送っている……
			PRINTFORMDW 
			;穿いてる
			IF TEQUIP:0 > 1
				PRINTFORMDL %CALLNAME:MASTER%はしゃがみ込み、至近距離で豊姫の下着を鑑賞している
				PRINTFORML （ぁぁ……%CALLNAME:MASTER%の息が掛かって……）
				CALL HPH_PRINT,@"「………んっHPH」","L"
				PRINTFORMDW 少しずつ広がっていく愛液の染みを指でつつくと、豊姫はゾクリと躰を震わせた……
				PALAM:潤滑 += 500
			;穿いてない
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%がスカートの中を覗くと、豊姫は下着を穿いていなかった
				CALL HPH_PRINT,@"「私のこと……はしたない女だって思うかしら……？HPH」","L"
				CALL HPH_PRINT,@"「でも、こうしたのは貴方なのよ……？HPH」","L"
				PRINTFORMDW %CALLNAME:MASTER%はしゃがみ込み、鼻息の掛かる距離で丸出しの秘所をじっくりと鑑賞した……
				PALAM:潤滑 += 1000
			ENDIF
		;思慕
		ELSEIF TALENT:思慕
			PRINTFORMDL %CALLNAME:MASTER%は豊姫に歩み寄ると躊躇なくスカートをめくり上げた
			PRINTFORMDW 
			;穿いてる
			IF TEQUIP:0 > 1
				PRINTFORMDL にも関わらず、豊姫は平然としている
				PRINTFORML 「ふふっ♪ もっと近くで見てもいいのよ？」
				PRINTFORML 「貴方の為に選んだ下着なんだから♪」
				PRINTFORMDW 豊姫は自らスカートを持つと、%CALLNAME:MASTER%に今日の下着を見せつけてきた
			;穿いてない
			ELSE
				PRINTFORMDL すると、豊姫の丸出しの秘所が露わになった
				PRINTFORML 「……その、満足したら手を離してね？」
				PRINTFORMDW 豊姫は顔を赤くして恥辱に耐えている……
			ENDIF
		;それ以外
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%は豊姫に歩み寄ると躊躇なくスカートをめくり上げた
			PRINTFORMDL 豊姫は%CALLNAME:MASTER%の所業に目を丸くしている
			PRINTFORMDW 
			;穿いてる
			IF TEQUIP:0 > 1
				PRINTFORML 「………えっと」
				PRINTFORML 「ダメよ？ こんなことしちゃ」
				PRINTFORMDW %CALLNAME:MASTER%は豊姫に窘められてしまった
			;穿いてない
			ELSE
				PRINTFORML 「きゃっ……」
				PRINTFORMDL %CALLNAME:MASTER%がスカートをめくり上げると丸出しの秘所が露わになった
				PRINTFORML 「こ、これはその……たまたまで」
				PRINTFORMW 「………皆にはナイショよ？」
			ENDIF
		ENDIF
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
