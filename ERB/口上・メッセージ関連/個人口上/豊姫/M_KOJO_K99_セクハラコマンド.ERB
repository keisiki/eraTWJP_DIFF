@M_KOJO_MESSAGE_COM_K99_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K99
	;時間停止中
	IF FLAG:時間停止
		PRINTFORMDL %CALLNAME:MASTER%は豊姫にギュッと抱き付いた
		PRINTFORMDW 時間を止められている豊姫は無抵抗のまま抱き締められている…
	;睡眠中
	ELSEIF CFLAG:睡眠
		PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:添い寝中 ? 添い寝する # \@豊姫にギュッと抱き付いた
		PRINTFORMDW 豊姫は%CALLNAME:MASTER%の胸元に頬を擦り寄せて幸せそうに寝ている……
	;押し倒され
	ELSEIF CFLAG:うふふ == 2
		CALL TRAIN_MESSAGE
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:添い寝中 ? 添い寝する # \@豊姫をギュッと抱き締めた
			PRINTFORMDL 豊姫は%CALLNAME:MASTER%の胸元に頬を擦り寄せ幸せそうにしている……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「さ、おいで？」
					PRINTFORMDL %CALLNAME:MASTER%が抱き付こうとすると
					PRINTFORMDL 豊姫は手を広げてこちらを迎えてきた
					PRINTFORMW 「はぁ……やっぱり貴方の傍が一番落ち着くわ……」
				CASE 1
					PRINTFORML 「待って……もう少しこのまま……」
					PRINTFORML 「貴方の温もりを感じていたいの……」
					PRINTFORMDW 豊姫は目を閉じ%CALLNAME:MASTER%にぴったりと密着している
				CASE 2
					PRINTFORML 「ねぇ……%CALLNAME:MASTER%……」
					PRINTFORML 「私がして欲しいこと……分かる……？」
					PRINTFORMDW 豊姫は唇を指でなぞりながら、期待に満ちた目で%CALLNAME:MASTER%を見ている……
			ENDSELECT
		;思慕
		ELSEIF TALENT:思慕
			PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:添い寝中 ? 添い寝する # \@豊姫をギュッと抱き締めた
			PRINTFORMDL 豊姫は嬉しそうだ……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「もっとギュッてしても良いのよ？」
					PRINTFORML 「ほら、こういう風に……♪」
					PRINTFORMDW 豊姫は胸を押し付けるようにして%CALLNAME:MASTER%に密着してきた
				CASE 1
					PRINTFORML 「こうしていると、なんだか落ち着くわ」
					PRINTFORML 「あら、貴方もそうなの？」
					PRINTFORML 「ふふっ♪ それじゃあもう少しこのままでいましょうか」
					PRINTFORMDW 二人は満足するまで互いの体温を感じ合った……
				CASE 2
					PRINTFORML 「ちょ、ちょっと%CALLNAME:MASTER%……」
					PRINTFORML 「ギュッてしてくれるのは嬉しいけど……その……」
					PRINTFORML 「く、苦しいわ……もう少し優しくして……」
					PRINTFORMDW 豊姫は遠慮がちに%CALLNAME:MASTER%の肩をトントンと叩いている
			ENDSELECT
		;それ以外
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:添い寝中 ? 添い寝する # \@豊姫をギュッと抱き締めた
			PRINTFORMDL 豊姫は抵抗せず%CALLNAME:MASTER%に身を任せている……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「どうしたの？ 急に抱き付いてきて」
					PRINTFORML 「……なんとなく？ うふふっ」
					PRINTFORMW 「そう。それならしょうがないわね」
				CASE 1
					PRINTFORMDL 背後から豊姫に抱き付き髪に顔を埋めてみると、微かに桃の香りがした
					PRINTFORMW 「ん、ぅ……もう少し鼻息を弱めて。くすぐったいわ」
				CASE 2
					PRINTFORML 「………くすっ」
					PRINTFORML 「ごめんなさい。あまりに突然だったから」
					PRINTFORML 「なんだか可笑しくなってしまって。つい」
					PRINTFORMDL 急に抱き付くというセクハラ紛いの行動をされたにも関わらず
					PRINTFORMDW 豊姫はなんだか嬉しそうにしている……
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
;==================================================
;312,キスする
;TFLAG:193(99=拒絶(キス不可) 1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 50=手淫 60=キス 性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_SUCCESS_COM_K99_312
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K99_312
;CALL TRAIN_MESSAGE
DRAWLINE
CALL M_KOJO_MESSAGE_COM_K99_312_1
RETURN RESULT

;フラグがONの場合コマンド実行による反感を減少
@M_KOJO_EXTRASOURCE_COM_K99_312
IF CFLAG:99:1000 == 1
	SOURCE:反感 = SOURCE:反感 / 5
ENDIF

@M_KOJO_MESSAGE_COM_K99_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K99
	;時間停止中
	IF FLAG:時間停止
		PRINTFORMDL %CALLNAME:MASTER%は豊姫に口付けをした
		PRINTFORMDW 時間を止められている豊姫は無抵抗のまま口付けされている……
	;睡眠中
	ELSEIF CFLAG:睡眠
		PRINTFORMDL %CALLNAME:MASTER%は眠っている豊姫の口を、自分の口で塞いだ……
		PRINTFORMDW 唇を離すと豊姫は僅かに微笑んでいた……
	;押し倒され
	ELSEIF CFLAG:うふふ == 2
		;どのカウンターに対して実行したかで分岐
		SELECTCASE TCVAR:20
			;前から抱きつく
			CASE 32
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「顔をこっちに向けて……HPH」","L"
						CALL HPH_PRINT,@"「んー……ちゅ……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 顔を赤くする%CALLNAME:MASTER%の頬に手を添えると
							PRINTFORMDW 豊姫は微笑みながら、優しく唇を重ねてきた……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「良い子ね……HPH ん……ちゅ……HPH」","L"
						CALL HPH_PRINT,@"「さあ……次は貴方から……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 豊姫は目を瞑って%CALLNAME:MASTER%のキスを待っている
							PRINTFORMDW %CALLNAME:MASTER%は優しく彼女を抱き寄せると、そっと唇を触れ合わせた……
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「待って……貴方はそのまま……HPH」","L"
						CALL HPH_PRINT,@"「ここは、私に任せて……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL キスをしようとした%CALLNAME:MASTER%を優しく制すると
							PRINTFORMDW 豊姫は%CALLNAME:MASTER%を抱き締めたまま、少し濃厚に唇を重ねてきた……
						ENDIF
				ENDSELECT
			;手淫
			CASE 50
				SELECTCASE RAND:3
					CASE 0
						SIF !CFLAG:99:1998
							PRINTFORMDL いきり立つ肉棒に優しく扱かれながら、%CALLNAME:MASTER%は豊姫と口付けを交わす
						CALL HPH_PRINT,@"「ん……っHPH 貴方のおちんちん、とっても硬くなってるわ……HPH」","L"
						CALL HPH_PRINT,@"「ふふっHPH いっぱい気持ち良くして……あ・げ・るHPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 耳元でそう囁かれてしまえば、%CALLNAME:MASTER%はコクコクと頷くしかなかった
					CASE 1
						CALL HPH_PRINT,@"「ちゅく……ちゅ……HPH れる……はぁ……HPH」","L"
						CALL HPH_PRINT,@"「どう……？ 気持ち良いかしら……？HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL クチュクチュと鈴口を穿りながら、豊姫が%CALLNAME:MASTER%にそう尋ねる
							PRINTFORMDW %CALLNAME:MASTER%が頷くと、豊姫はちゃんと言えたご褒美に強く竿を扱いてくれた
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「ふふっHPH 気持ち良いよね……HPH」","L"
						CALL HPH_PRINT,@"「キスをしながらだと、もっと気持ち良くなれると思うわ……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 豊姫は息を荒げる%CALLNAME:MASTER%の顔を自分に向けさせると
							PRINTFORMDW 口付けをしながら、シコシコと竿を扱き上げてきた……
						ENDIF
				ENDSELECT
			;85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ
			CASE 85,87 TO 89
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「こら、動いちゃ駄目よ……HPH」","L"
						CALL HPH_PRINT,@"「貴方にキスができないじゃない……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL キュウッと\@ TCVAR:20 == 85 || TCVAR:20 == 88 ? 膣 # アヌス \@を締め付けて%CALLNAME:MASTER%の動きを止めると
							PRINTFORMDW 豊姫は顔を近付けて、情熱的に唇を重ねてきた……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「はっ……んっ……HPH 繋がりながら、キスすると……っHPH」","L"
						CALL HPH_PRINT,@"「頭……ボーッとしてきちゃう……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 上も下も繋がって、まるで一つになったような感覚を味わいながら
							PRINTFORMDW %CALLNAME:MASTER%は豊姫と、長い時間唇を重ね合った……
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「ほら……ベーッて、舌を出しなさい……HPH」","L"
						CALL HPH_PRINT,@"「あー……む……HPH ちゅう……ぢゅる……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:MASTER%が求めに応じて舌を出すと、豊姫はそれを咥え、じゅるじゅると吸い上げてきた
							PRINTFORMDW そしてそのまま、緩やかに腰を動かし始めた……
						ENDIF
				ENDSELECT
			;だいしゅきホールド
			CASE 86
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ん、む……HPH ちゅ……ふぅ……HPH」","L"
						CALL HPH_PRINT,@"「まだ、足りないわ……HPH はむ……ちゅ……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:MASTER%は豊姫と深く繋がったまま強く抱き合い、熱い口付けを交わした
							PRINTFORMDW 豊姫は結合の時間をより長くしたいのか、ゆったりとしたストロークを開始した……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「ふっ……んっ……HPH はぁ……れる……っHPH」","L"
						CALL HPH_PRINT,@"「もっとよ……もっと、私を求めて……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:MASTER%は豊姫と深く繋がったまま強く抱き合い、熱い口付けを交わした
							PRINTFORMDL 豊姫は我慢できない様子で激しく腰を振り始めた
							PRINTFORMDW 接合部からはとめどなく愛液が迸り、辺りにはグチュグチュと水音が響いている……
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「もう、イきそうなのね……HPH」","L"
						CALL HPH_PRINT,@"「ちゅ……HPH 一緒にイきましょう……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 射精しそうな%CALLNAME:MASTER%は豊姫と深く繋がったまま強く抱き合い、熱い口付けを交わした
							PRINTFORMDL 豊姫もまた、%CALLNAME:MASTER%と同時に達するべく腰の動きを激しくしていく
							PRINTFORMDW %CALLNAME:MASTER%は豊姫と一頻り愛情を確かめ合った後、爆発寸前のペニスを最奥まで突き入れた……
						ENDIF
				ENDSELECT
			;それ以外
			CASEELSE
				;愛撫系コマンドのキスを読み込む
				CALL M_KOJO_MESSAGE_COM_K99_20_1
		ENDSELECT
	;失敗
	ELSEIF TFLAG:193 == 99
		PRINTFORMDL %CALLNAME:MASTER%は豊姫にキスをしようと顔を近付けた
		PRINTFORMDL ……が、扇子で遮られてしまった……
		PRINTFORMDW 
		PRINTFORML 「こら、ダメでしょう？」
		PRINTFORML 「貴方の気持ちは嬉しいけど……」
		PRINTFORMW 「こういうのは、もっと大切な人にしてあげないと」
	;通常
	ELSE
