;==================================================
;310,尻を撫でる
;TFLAG:193(1=不快(TALENT:臆病なら涙目) 2&&3=恥ずかしがる(TALENT:男嫌い兼MASTERが男なら、身をすくめる) 4=されるがまま)
;CFLAG:うふふ==2&&TCVAR:20(パンツ無し顔面騎乗(70=クンニ強制 71=フェラチオ強制 72=アナル奉仕) 35=スカート内見せつけ 52=素股)
;CFLAG:うふふ==2&&TEQUIP:シックスナイン(パンツありTCVAR:20(70～72)はこちら)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_SUCCESS_COM_K99_310
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K99_310
;CALL TRAIN_MESSAGE
DRAWLINE
CALL M_KOJO_MESSAGE_COM_K99_310_1
RETURN RESULT

;フラグがONの場合コマンド実行による反感を減少
@M_KOJO_EXTRASOURCE_COM_K99_310
IF CFLAG:99:1000 == 1
	SOURCE:反感 = SOURCE:反感 / 5
ENDIF

@M_KOJO_MESSAGE_COM_K99_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K99
	;時間停止中
	IF FLAG:時間停止
		PRINTFORMDL %CALLNAME:MASTER%は背後から豊姫のお尻に手を伸ばした
		PRINTFORMDW 時間を止められている豊姫は無抵抗のままお尻を撫でられている…
	;睡眠中
	ELSEIF CFLAG:睡眠
		PRINTFORMDL %CALLNAME:MASTER%は横になって眠る豊姫のお尻に手を伸ばした
		PRINTFORMDW 豊姫は少し頬を染めて%CALLNAME:MASTER%にされるがままになっている…
	;押し倒され
	ELSEIF CFLAG:うふふ == 2
		CALL TRAIN_MESSAGE
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMDL %CALLNAME:MASTER%は\@ PREVCOM == 305 ? 膝に寝転んだまま # 背後から \@豊姫のお尻に手を伸ばした
			;溜まってる度が３０％以上で発情していない場合１０分の１の確率で発情
			IF CFLAG:TARGET:溜まってる度 >= 300 && !TCVAR:発情 && !RAND:10
				PRINTFORMD %CALLNAME:MASTER%が我が物顔で揉みしだく内に、豊姫はすっかり
				CALL COLORMESSAGE(@"[発情]",C_PINK,0,1)
				PRINTFORMDL してしまったようだ……
				TCVAR:発情 = 1
			;通常
			ELSE
				PRINTFORMDL 豊姫は熱っぽい吐息を漏らしながら%CALLNAME:MASTER%にされるがままになっている……
			ENDIF
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「んっ……HPH そんな触り方じゃなくて……」","L"
					CALL HPH_PRINT,@"「もっとモノみたいに扱っても良いのよ……？HPH」","L"
					PRINTFORMDL 豊姫は所有物のように扱われることをお望みのようだ
					PRINTFORMDL それならと%CALLNAME:MASTER%が無遠慮に尻を鷲掴みにしてやっても
					PRINTFORMDW それが望みだとばかりに豊姫はなんの抵抗もせず、甘い吐息を漏らすのみだった
				CASE 1
					CALL HPH_PRINT,@"「貴方の触り方、すごくエッチだから……HPH」","L"
					CALL HPH_PRINT,@"「はぁ……変な気分になっちゃいそう……HPH」","L"
					PRINTFORMDW %CALLNAME:MASTER%は豊姫の桃尻の感触を思う存分楽しんだ
				CASE 2
					CALL HPH_PRINT,@"「ぁ……はぁ……HPH ん、んん……HPH」","L"
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%……もっと……HPH」","L"
					PRINTFORMDW 豊姫は恍惚の表情で%CALLNAME:MASTER%のセクハラを受け入れている
			ENDSELECT
			PALAM:潤滑 += 500
		;思慕
		ELSEIF TALENT:思慕
			PRINTFORMDL %CALLNAME:MASTER%は\@ PREVCOM == 305 ? 膝に寝転んだまま # 背後から \@豊姫のお尻に手を伸ばした
			PRINTFORMDL 豊姫は%CALLNAME:MASTER%から受けるセクハラを楽しんでいるようだ……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「あっ……もう、ビックリするじゃない」
					PRINTFORML 「言ってくれればちゃんと触らせてあげるのに」
					PRINTFORMDW %CALLNAME:MASTER%が事後承諾を求めると、豊姫はクスリと笑って尻を差し出してきた……
				CASE 1
					PRINTFORML 「いやー痴漢よー穢されちゃうわー」
					PRINTFORMDL 豊姫は気の抜けるような棒読みで助けを求めている
					PRINTFORML 「どう？ それっぽいんじゃないかしら」
					PRINTFORMDW なんだかんだで豊姫も楽しんでいるようだ
				CASE 2
					PRINTFORML 「貴方は本当にお尻が好きなのねぇ」
					PRINTFORML 「ダメよ？ 他の子にこんなことしちゃ」
					PRINTFORMDW もう慣れてしまったのか、%CALLNAME:MASTER%のセクハラに豊姫は全く動じていない
			ENDSELECT
		;それ以外
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%は\@ PREVCOM == 305 ? 膝に寝転んだまま # 背後から \@豊姫のお尻に手を伸ばした
			PRINTFORMDL 豊姫は照れくさそうな表情を浮かべながらも%CALLNAME:MASTER%にされるがままになっている……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「きゃっ……」
					PRINTFORMDL %CALLNAME:MASTER%は豊姫の桃尻をいやらしく撫で回している
					PRINTFORMW 「もう……いけない人ね」
				CASE 1
					PRINTFORML 「んっ……ダメよ……こんなこと……」
					PRINTFORMDL 口ではそう言う豊姫だが抵抗する気は無いようだ
					PRINTFORMDW %CALLNAME:MASTER%はそれを良い事に彼女の尻を満足するまで撫で続けた
				CASE 2
					PRINTFORML 「あの……ね？」
					PRINTFORML 「さすがに……その、こういうことは……」
					PRINTFORMDL 豊姫は困ったように視線を彷徨わせている
					PRINTFORMDW 頬を張られるのを覚悟していた%CALLNAME:MASTER%は少し拍子抜けしてしまった
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
