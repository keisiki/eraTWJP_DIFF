;CALL TRAIN_MESSAGE
DRAWLINE
CALL M_KOJO_MESSAGE_COM_K99_309_1
RETURN RESULT

;フラグがONの場合コマンド実行による反感を減少
@M_KOJO_EXTRASOURCE_COM_K99_309
IF CFLAG:99:1000 == 1
	SOURCE:反感 = SOURCE:反感 / 5
ENDIF

@M_KOJO_MESSAGE_COM_K99_309_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K99
	;時間停止中
	IF FLAG:時間停止
		PRINTFORMDL %CALLNAME:MASTER%は豊姫の頭に手を伸ばした
		PRINTFORMDW 時間を止められた豊姫は無抵抗のまま頭を撫でられている……
	;睡眠中
	ELSEIF CFLAG:睡眠
		PRINTFORMDL %CALLNAME:MASTER%は眠っている豊姫の頭を優しく撫でた
		PRINTFORMDW 豊姫は嫌がる素振りもなく%CALLNAME:PLAYER%に撫でられている……
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMDL %CALLNAME:MASTER%は豊姫の頭を優しく撫でた
			PRINTFORMDL 豊姫はリラックスしたような表情を浮かべて目を細めた……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「こうして甘えさせてくれるのは貴方だけ……」
					PRINTFORML 「だから、もっと撫でて……♪」
					PRINTFORMDW 豊姫は頭を撫でる%CALLNAME:MASTER%に寄りかかって甘えている
				CASE 1
					PRINTFORML 「ふふっ、いいこいいこ」
					PRINTFORMDL %CALLNAME:MASTER%はいつの間にか攻守逆転し豊姫に撫でられていた
					PRINTFORML 「あら、どうしたの？ 顔赤くして」
					PRINTFORMW 「もう今更でしょう？ 何を恥ずかしがる必要があるの？」
				CASE 2
					PRINTFORML 「ぁ……もう止めてしまうの？」
					PRINTFORML 「もう少しだけ撫でてくれないかしら……ダメ？」
					PRINTFORMDW 上目遣いでおねだりしてくる豊姫の頭を彼女が満足するまで撫でてあげた
			ENDSELECT
		;思慕
		ELSEIF TALENT:思慕
			PRINTFORMDL %CALLNAME:MASTER%は豊姫の頭を優しく撫でた
			PRINTFORMDL 豊姫はなんだか嬉しそうにしている……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「そうだ、今度依姫の頭を撫でてみてくれない？」
					PRINTFORML 「うふふー、あの子の怒った顔が目に浮かぶわー♪」
					PRINTFORMDW 豊姫は%CALLNAME:MASTER%に頭を撫でられながら楽しそうに笑っている
				CASE 1
					PRINTFORML 「ん、撫でたいの？」
					PRINTFORML 「……優しくお願いね」
					PRINTFORMDW 豊姫は帽子を脱ぐと、%CALLNAME:MASTER%に頭を差し出した
				CASE 2
					PRINTFORML 「どう？ 満足したかしら？」
					PRINTFORML 「それじゃあ今度は私の番ね♪」
					PRINTFORMDW %CALLNAME:MASTER%は豊姫に優しく頭を撫でられた
			ENDSELECT
		;それ以外
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%は豊姫の頭を優しく撫でた
			PRINTFORMDL 豊姫は少しだけ頬を染めつつ、%CALLNAME:MASTER%のやりたいようにやらせている
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「撫でられる側に回るというのは少し恥ずかしいわね」
					PRINTFORMW 「でもこの感じ……なんだか懐かしいわ」
				CASE 1
					PRINTFORML 「レイセンに貴方の面倒を見るよう言付けたのだけど……ちゃんとやっている？」
					PRINTFORML 「……ふふっ、そう。それはよかった」
					PRINTFORMW 「私の頭もいいけど、あの子の頭も撫でてあげてね。意外と触り心地良いのよ？」
				CASE 2
					PRINTFORML 「撫でるのは良いけれど、もう少し優しくお願いね？」
					PRINTFORMDW 豊姫は%CALLNAME:MASTER%にグチャグチャにされた髪を整えている
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
