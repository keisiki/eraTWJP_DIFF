;301,お茶を淹れる
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K99_301
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K99_301
;CALL TRAIN_MESSAGE
DRAWLINE
CALL M_KOJO_MESSAGE_COM_K99_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K99
	;恋慕
	IF TALENT:恋慕
		;判定
		SELECTCASE TFLAG:193
			;大成功
			CASE 1
				PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:行動 == 5 ? 休憩がてら #  \@お茶を淹れて豊姫\@ GET_TARGETNUM() > 1 ? 達 # \@に差し出した…
				PRINTFORMDL 豊姫と%CALLNAME:MASTER%はお茶を飲みながら\@ CFLAG:TARGET:行動 == 5 ? 休憩時間を楽しんだ # 楽しいひとときを過ごした \@…
				PRINTFORMDW 
				PRINTFORML 「せっかくお茶を淹れてくれたのだから、私も何かお出ししなくてはね」
				PRINTFORML 「えーっと、何か持ってたかしら……あっ」
				PRINTFORMW 「飴ちゃんがあったわ。はい、どうぞ♪」
				CALL GET_ITEM("飴", 1, 1)
			;成功
			CASE 0
				PRINTFORMDL %CALLNAME:MASTER%が『喉が渇いた』と言うと、既に豊姫がお茶の用意をしてくれていた
				PRINTFORMDW 
				PRINTFORML 「熱いお茶で良かったよね？」
				PRINTFORML 「はい、どうぞ。火傷しないように気を付けて」
				PRINTFORMDW %CALLNAME:MASTER%と豊姫は隣り合って座りながら、のんびりとした時間を楽しんだ……
			;失敗
			CASE -1
				PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:行動 == 5 ? 休憩がてら #  \@お茶を淹れて豊姫\@ GET_TARGETNUM() > 1 ? 達 # \@に差し出した…
				PRINTFORMDL ……どうやら豊姫の口に合わなかったようだ
				PRINTFORMDW 
				PRINTFORML 「ね、ねえ……%CALLNAME:MASTER%……？」
				PRINTFORML 「それって……その、コーヒーよね？」
				PRINTFORML 「……私、その……コーヒーは……」
				PRINTFORMW 「……………お砂糖、たっぷりお願いね？」
		ENDSELECT
	;思慕
	ELSEIF TALENT:思慕
		;判定
		SELECTCASE TFLAG:193
			;大成功
			CASE 1
				PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:行動 == 5 ? 休憩がてら #  \@お茶を淹れて豊姫\@ GET_TARGETNUM() > 1 ? 達 # \@に差し出した…
				PRINTFORMDL 豊姫と%CALLNAME:MASTER%はお茶を飲みながら\@ CFLAG:TARGET:行動 == 5 ? 休憩時間を楽しんだ # 楽しいひとときを過ごした \@…
				PRINTFORMDW 
				PRINTFORML 「あら、茶柱」
				PRINTFORMW 「飲みづらいから捨てちゃいましょう」
			;成功
			CASE 0
				PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:行動 == 5 ? 休憩がてら #  \@お茶を淹れて豊姫\@ GET_TARGETNUM() > 1 ? 達 # \@に差し出した…
				PRINTFORMDW 
				PRINTFORML 「ありがとう、頂くわ」
				PRINTFORML 「ずずっ……ふぅ……」
				PRINTFORMW 「……落ち着く」
			;失敗
			CASE -1
				PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:行動 == 5 ? 休憩がてら #  \@お茶を淹れて豊姫\@ GET_TARGETNUM() > 1 ? 達 # \@に差し出した…
				PRINTFORMDL ……どうやら豊姫の口に合わなかったようだ
				PRINTFORMDW 
				PRINTFORML 「これ……出涸らしね」
				PRINTFORML 「ああ、淹れ直さなくてもいいのよ」
				PRINTFORMW 「偶にはこういう薄いお茶も悪くないわ」
		ENDSELECT
	;それ以外
	ELSE
		;判定
		SELECTCASE TFLAG:193
			;大成功
			CASE 1
				PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:行動 == 5 ? 休憩がてら #  \@お茶を淹れて豊姫\@ GET_TARGETNUM() > 1 ? 達 # \@に差し出した…
				PRINTFORMDL 豊姫と%CALLNAME:MASTER%はお茶を飲みながら\@ CFLAG:TARGET:行動 == 5 ? 休憩時間を楽しんだ # 楽しいひとときを過ごした \@…
				PRINTFORMDW 
				PRINTFORML 「ずずっ……ふぅ……」
				PRINTFORMW 「……結構なお点前でした」
			;成功
			CASE 0
				PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:行動 == 5 ? 休憩がてら #  \@お茶を淹れて豊姫\@ GET_TARGETNUM() > 1 ? 達 # \@に差し出した…
				PRINTFORMDW 
				PRINTFORML 「あら、ありがとう」
				PRINTFORMW 「んくっ……ふぅ……」
			;失敗
			CASE -1
				PRINTFORMDL %CALLNAME:MASTER%は\@ CFLAG:TARGET:行動 == 5 ? 休憩がてら #  \@お茶を淹れて豊姫\@ GET_TARGETNUM() > 1 ? 達 # \@に差し出した…
				PRINTFORMDL ……どうやら豊姫の口に合わなかったようだ
				PRINTFORMDW 
				PRINTFORML 「頂くわ。……ん、あちゅっ」
				PRINTFORMW 「ふぅっ、ふぅっ……氷とかないかしら」
		ENDSELECT
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
