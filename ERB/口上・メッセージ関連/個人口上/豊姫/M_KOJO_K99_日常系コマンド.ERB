;フラグがONの場合コマンド実行による反感を減少
@M_KOJO_EXTRASOURCE_COM_K99_302
IF CFLAG:99:1000 == 1
	SOURCE:反感 = SOURCE:反感 / 5
ENDIF

@M_KOJO_MESSAGE_COM_K99_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;お風呂
	IF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
