						CALL HPH_PRINT,@"「ちゅ……れろ……っHPH は、ぁ……んん……っHPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 豊姫とゆったりと愛し合いながら少しずつ腰の動きを加速させていく
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%……HPH もう、イきそう……っHPH っあHPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW %CALLNAME:MASTER%が深く腰を突き入れ射精すると、それを待っていたかのように豊姫も同時に絶頂した
					CASE 2
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%……キス、して……？HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 腰の動きを加速させながら、%CALLNAME:MASTER%は豊姫の求めに応じてキスをしてやる
						CALL HPH_PRINT,@"「ん……ちゅ……っHPH れる……ぢゅるる……っHPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW やがて我慢できず射精すると、豊姫は%CALLNAME:MASTER%をギュッと抱き締めて膣内に広がる熱にうっとりと吐息を漏らした
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ん……ぁ……っHPH こうやって……ギュッてすると……HPH」","L"
						CALL HPH_PRINT,@"「貴方の鼓動……ドクドクッて、伝わってくるわ……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:MASTER%の腰に脚を絡ませて豊姫がギュッと密着してくる
							PRINTFORMDW 『本当の恋人みたいだ』そう囁くと豊姫の心臓の鼓動がドクンと高鳴った
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「ふ、む……HPH ん……ちゅぷ……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 豊姫と口付けをし舌を絡ませながら、腰を動かし膣内の感触を味わう
							PRINTFORMDL 行為が激しさを増すなか、絡め取った舌をジュルルッと吸い上げてやると
							PRINTFORMDW それに呼応するように、豊姫も挿入されたペニスをキュウッと締め付けた
						ENDIF
					CASE 2
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:MASTER%はいったん動きを止めると、豊姫の躰を抱き上げて竿の中程までペニスを引き抜いた
							PRINTFORMDL 抽挿によって掻き出された愛液が二人の間で糸を引いている。軽く腰を動かすとヌチャヌチャと厭らしい音が鳴った
						ENDIF
						CALL HPH_PRINT,@"「私達……きっと相性が良いのね……HPH」","L"
						CALL HPH_PRINT,@"「だって……こんなに気持ち良いんだもの……HPH」","W"
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		CALL HPH_PRINT,@"「ああっ……やっぱり、この格好が一番好き……HPH」","L"
		CALL HPH_PRINT,@"「だって……貴方に愛されてるって実感できるから……HPH」","L"
		IF !CFLAG:99:1998
			PRINTFORMDL 豊姫と深く繋がったまま額を合わせて睦言を交わす
			PRINTFORMDL キュウキュウと締め付けてくる膣の感触に我慢できず腰を動かすと
			PRINTFORMDW 豊姫は%CALLNAME:MASTER%の首に手を回し、躰を密着させ艶やかな声を漏らした
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		CALL HPH_PRINT,@"「ねぇ……%CALLNAME:MASTER%……HPH」","L"
		CALL HPH_PRINT,@"「キス、しましょう……？HPH」","L"
		IF !CFLAG:99:1998
			PRINTFORMDL 豊姫は%CALLNAME:MASTER%の首に手を回してキスを求めている
			PRINTFORMDW %CALLNAME:MASTER%は動きを止めると、顔を近付け恋人にするようなキスをしてやった
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

