;==================================================
;9,ダブルフェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K65_9
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K65_9_1
;戻り値とTARGETを保存、TARGETを二人目に一時的に交代して二人目口上を呼び出した後、TARGETをもどして保存した戻り値でRETURNする
;二人目はいらない場合はここで処理
;TCVAR:352 == 1のとき、二人目口上を呼ばない
IF TCVAR:352 == 0
	;二人目
	LOCAL = MASTER_POSE(4,1,1)
	LOCAL:1 = RESULT
	LOCAL:2 = TARGET
	TARGET = LOCAL
	TRYCALLFORM M_KOJO_MESSAGE_SCOM_K{TARGET}_9_2
	TARGET = LOCAL:2
	RETURN LOCAL:1
ELSE
	RETURN 1
ENDIF
;一人目口上
@M_KOJO_MESSAGE_SCOM_K65_9_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;二人目口上キャンセルフラグ設定
	TCVAR:352 = 0
	;はたて、映姫口上と同じくLOCALに二人目を入れてしまう
	LOCAL = MASTER_POSE(4,1,1)
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMDW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;二人目がはたて
	ELSEIF LOCAL == 42
		;二人目口上呼ばないフラグON
		TCVAR:352 = 1
		;はたて・椛ともにレズっ気がある
		;PRINTFORMDL {GETBIT(STAIN:口,汚れ_精液)}{GETBIT(STAIN:42:0,4)}　{GETBIT(STAIN:65:0,4)}
		IF ABL:42:レズっ気> 0 && ABL:65:レズっ気 > 0 && RAND:3 == 0
			;二人とも口に精液が付いてる
			;うまく動かんな
			IF GETBIT(STAIN:42:0,4) && GETBIT(STAIN:65:0,4) && RAND:2
				SETCOLOR 0xA260BF
				CALL HPH_PRINT,@"『れるっ…椛ってば、口の周りが%MASTERNAME:42%のせーしでべっとべと…ん、ちゅっ』","L"
				RESETCOLOR
				CALL HPH_PRINT,@"「はたてさんだって、ほっぺに付いちゃってますよ…ぺろ、ちゅ、ちゅく……」","L"
				PRINTFORMDW
				PRINTFORMDL はたてと椛は交互にペニスを咥えながら、お互いの顔に飛び散った精液を舐めとっていく
				PRINTFORMDL 顔に残っていたモノがなくなると、今度は舌を絡めあって口内の白濁を分け合いはじめた。
				PRINTFORMDW 
				PRINTFORML 「れる、ちゅ、ちゅむ……%CALLNAME:MASTER%さん…せーえき、もっとください……」
				SETCOLOR 0xA260BF
				PRINTFORMW 『こうやって、仲良くはんぶんこするからぁ…』
				RESETCOLOR
				STAIN:42:0 = 0
				STAIN:65:0 = 0
			ELSEIF ABL:65:精液中毒 > 3 && ABL:42:精液中毒 > 3 && RAND:2
				CALL HPH_PRINT,@"「ん……はたてさんのアソコ、\@ EX:42:膣内精液 >= 300 ? %CALLNAME:MASTER%さんのと混ざってとろっとろで # おちんちん舐めてびしょびしょに濡れちゃってま \@すね…」","L"
				PRINTFORMDL 椛の手がはたてのスベスベした太ももを撫で上げ、その付け根にある秘部を指先でくすぐる。
				SETCOLOR 0xA260BF
				PRINTFORML 『んむっ…椛だって、%MASTERNAME:42%にごほーしするだけでビッショビショにしてるくせに…』
				RESETCOLOR
				PRINTFORMDW 二人の少女は%CALLNAME:MASTER%のペニスを競うように舐め上げながら、互いのワレメに指を這わせた…
				PRINTFORMDL
				SETCOLOR 0xA260BF
				PRINTFORML 『はむ、れるぅ、りゅるる…%MASTERNAME:42%聞こえる？椛のアソコからぐちゅぐちゅ音鳴っちゃってるの。やーらしー』
				RESETCOLOR
				PRINTFORML 「はたてさんだって、さっきから腰ピクピクさせちゃって…ぺろ、れるるる、ちゅりゅっ」
				PRINTFORMDW 美しい少女たちが相互に秘裂をなぶり合いながらも、懸命にあなたのペニスへ奉仕を続ける…
