;==================================================
;7,挿入子宮口責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K65_7
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K65_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K65_7_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF EXP:子宮口開発経験 > 50
			CALL HPH_PRINT,@"「あ゛っぁHPHうぅ…？」","L"
			CALL HPH_PRINT,@"「ひっ、がっHPH%CALLNAME:MASTER%さん、待っ…ぇ゛あぁ゛っHPH」","L"
			PRINTFORMDW 既にしっかりと開発されていた最奥を突き上げられて生じる激烈な快感に、椛は目を白黒させている…
		ELSE
			PRINTFORML 「ぐっ、ぅ…」
			PRINTFORML 「%CALLNAME:MASTER%さん、ちょ、ちょっと、そんなに奥まで、ぐりぐりって…！」
			PRINTFORMDW 子宮口をグイグイと押し上げるような抽送に、椛は少し苦しそうな声を上げた。
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	ELSE
		SELECTCASE EXP:子宮口開発経験
			;気持ちいい
			CASE IS > 50
				IF TALENT:恋慕 && RAND:2 == 0
					IF TCVAR:発情
						SELECTCASE RAND:2
							CASE 0
								CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
								PRINTFORMDL \@ TEQUIP:65:口接触部位 == 4 ? 椛はむしゃぶりつくようにあなたの唇を奪いながら # 最奥をこね回されるのがたまらないと言った愉悦の表情で \@、自ら腰を深く落として子宮口と鈴口をキスさせている。
								CALL HPH_PRINT,@"「はぁっぁ゛…HPH%CALLNAME:MASTER%さんのおちんちん、すごすぎっ…HPHもうずっと、繋がったままで、いたいですぅ…HPH」","L"
								PRINTFORMDW しなだれかかる柔らかな肉体の熱が、甘い吐息が、媚びるような声が。すべてが一体となって、椛は%CALLNAME:MASTER%を昂らせていく…
							CASE 1
								CALL HPH_PRINT,@"「すっご、い゛ぃ…HPHこれ、ばかになっひゃうぅ゛ぅ…HPHきもちい゛ひっHPH%CALLNAME:MASTER%さんっHPHう゛っHPH」","L"
								PRINTFORMDL 子宮口をこじ開けるような力強い抽送に、椛は半ば朦朧となって快楽を訴えている…
								IF TALENT:妊娠
									CALL HPH_PRINT,@"「も、もう゛っ、あかちゃん、い゛るっHPHのにぃっHPHもっと、ほしくなっちゃっ゛HPHひうぅぅっHPH」","W"
								ELSE
									CALL HPH_PRINT,@"「このっHPHぉ゛っHPHこのままっHPHうっHPHおくに、だしてっHPHはらませ、てえ゛ぇっHPH」","W"
								ENDIF
						ENDSELECT
					ELSE
						SELECTCASE RAND:2
							CASE 0
								;騎乗位
								IF TEQUIP:体位２ == 3
									CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
									PRINTFORML （\@ TEQUIP:65:口接触部位 == 4 ? もっと愛してっておねだりするみたいにキスしながら # 一番奥に欲しいっておねだりするみたいに \@、自分から腰を振っちゃって…）
									PRINTFORML （わたし、いつからこんなえっちな娘になっちゃったんだろう…）
								ELSE
									CALL HPH_PRINT,@"「%CALLNAME:MASTER%さんっ、%CALLNAME:MASTER%さんんっHPHお゛っHPH…ぅ、うあ゛ぁっHPH」","L"
									PRINTFORML （一番奥をこんなにぐちょぐちょにされて、\@ TEQUIP:乳首吸い ? おっぱいまで吸われて # ケダモノみたいにえっちな声を出しちゃって \@…）
								ENDIF
								CALL HPH_PRINT,@"（でも、きもちいい…HPH本気の子作りセックス、やめられないよぉ…HPH）","W"
							CASE 1
								CALL HPH_PRINT,@"「ふぁ゛っHPHあっHPHあっHPHあ゛っHPHあああ～～～～～っHPH」","L"
								PRINTFORMDL 椛は狂おしい悦鳴を上げながら、最奥で愛される快楽に打ち震えている…
								CALL HPH_PRINT,@"（大好きな%CALLNAME:MASTER%さんにこんなに激しく愛されて…）","L"
								CALL HPH_PRINT,@"（わたし、幸せぇ…HPH）","W"
						ENDSELECT
					ENDIF
				ELSE
					IF TCVAR:発情
						SELECTCASE RAND:2
							CASE 0
								CALL HPH_PRINT,@"「はぅ゛っHPHあ゛っHPHおっ、ふぅぅ゛っ…HPHぐっ、ああ゛ぁ…HPH」","L"
								PRINTFORMDL \@ 充填率(TARGET,1) >= 100 ? お腹が膨れ上がってしまうほどに精液を詰め込まれた # ぴったりと押し付けられた肉棒に \@子宮を揺らされる強烈な快楽に、椛はあられもない声を上げている…
								CALL HPH_PRINT,@"「%K65_AEGI()%…きもちぃ、ぃぃぃ…HPH」","W" 
							CASE 1
								CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
								PRINTFORML 「ふぁあ゛っHPHう～っHPHぅ゛ぅっ～HPH」
								PRINTFORMDW 発情で熱く火照った膣壁が、最奥を小突くたびにうねってあなたのペニスをさらに奥へ奥へといざなう…
						ENDSELECT
					ELSE
						SELECTCASE RAND:2
							CASE 0
								CALL HPH_PRINT,@"「はっHPHあ゛ぁっHPH%K65_AEGI()%」","L"
								PRINTFORMDL %CALLNAME:MASTER%\@ TEQUIP:体位２ == 3 ? にまたがった椛は、腰を深く落として # の抽送にあわせて腰を振る椛は、 \@怒張の先端を子宮口に自ら押し当てて、より強い快楽を求めている…
								CALL HPH_PRINT,@"「お、おぐっ…%K65_AEGI(,2)%…きもち、いい゛っ…HPH」","W"
							CASE 1
								CALL HPH_PRINT,@"「%K65_AEGI(,3)%…あぐううっHPHや、やぁっHPHとまってっ…！とまって、くらっ、さいっ！！！」","L"
								PRINTFORMW 「か、はっ…あうぅ…こ、こんなに激しく、奥ばっかりコツコツされちゃったら…」
								PRINTFORML
								PRINTFORML 「頭がダメになって、%CALLNAME:MASTER%さんのおちんちんのことしか、考えられなくなっちゃいま…」
								CALL HPH_PRINT,@"「あ゛ぁっあHPHあっHPHお゛っHPHひぐぅうHPHやら、やだぁっHPH」","L"
								IF RAND:5 == 1
									CALL HPH_PRINT,@"「さ、さそいうけって、な、なんですかっあ゛HPHあぁああ゛っHPH」","W"
								ELSE
									PRINTFORMDW 無自覚にオスを誘惑する椛の言葉に惹かれるように、%CALLNAME:MASTER%は一層深く肉棒を膣奥へ突き立てた…
								ENDIF
						ENDSELECT
					ENDIF
				ENDIF
			;ほぐれかけ
			CASE IS > 25
				IF TCVAR:発情
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「んぅぅう゛っHPHう゛っ、う～～～～っHPH」","L"
							CALL HPH_PRINT,@"「こ、これ、うぁあっ…HPHクセに、なっちゃいそ…HPHはあ゛ぁHPH」","L"
							PRINTFORMDW ほころびかけた子宮口を抉られる快楽に、、椛はびくびくと体を跳ねさせている…
						CASE 1
							CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
							PRINTFORMDL 発情して感度の上がっている椛は、最奥をこね回される愉悦で腰を浮かせている…
							CALL HPH_PRINT,@"「ひうぅう…奥、すごひぃ…HPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「はう゛っHPH…あ゛っ…HPHんいぃ…HPHなにっ、これぇ…っHPH」","L"
							PRINTFORMDL 性感帯として開発されつつある最奥を小突かれるたび、鋭い快楽が椛を襲う。
							PRINTFORMDW これまでとは全く異なる感覚に、椛は目を白黒させている…
						CASE 1
							CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
							CALL HPH_PRINT,@"「や、やめっHPH%CALLNAME:MASTER%さんっ…これっ、こんなのっHPH」","L"
							CALL HPH_PRINT,@"（こんなに気持ちいいセックス覚えたら、クセになっちゃう…HPHハマッちゃうよぉ…HPH）","W"
					ENDSELECT
				ENDIF
			;きつい 発情中でも思わず素面に
			CASEELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「う、ぐっ…そ、そんなに、おくまでっ……」
							PRINTFORMDW 椛はくぐもった声を上げている…
						CASE 1
							PRINTFORML 「か、はっ…が、がっつきすぎっ…」
							PRINTFORMW 「%CALLNAME:MASTER%さんっ…すこし、くるしい、です……」
					ENDSELECT
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
RETURN 1

