					CASE 1
						CALL HPH_PRINT,@"「はぁぅ、あぁ、あ～っHPH」","L"
						IF TEQUIP:50 == PLAYER
							CALL HPH_PRINT,@"「そこっ…そこほじられるの、ぅ…ぁあ…HPH」","L" 
