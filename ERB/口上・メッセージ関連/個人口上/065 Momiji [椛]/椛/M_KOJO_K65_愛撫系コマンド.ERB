				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI("控えめ",2)%…%CALLNAME:MASTER%さんとするようになってから、自分でする頻度も増えました、ね…」","L"
						CALL HPH_PRINT,@"「自分でするときも、%K65_AEGI("控えめ",3)%…%CALLNAME:MASTER%さんに触れてもらった時のことを思い出しながら…」","L"
