						CALL HPH_PRINT,@"「んううっHPHはふっ、ぁ…HPHえっちな子で、ごめんなさいぃ……HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「あはぁっ…%CALLNAME:MASTER%さんのおちんちんすっごぃ…%K65_AEGI(,3)%」","L"
						;後背位、背面座位でない
						IF !GROUPMATCH(TEQUIP:体位２,2,5)
							CALL HPH_PRINT,@"「言われたとおり、んぅっHPHえっちなオナニー見せてあげますからっHPH」","L"
							CALL HPH_PRINT,@"「もっともっとおちんちん硬くして、気持ちよくしてくださいっ…HPH」","L"
						;後背位、背面座位
						ELSE
							CALL HPH_PRINT,@"「あぁ～～っ…HPH%CALLNAME:MASTER%さんにズボズボしてもらいながらっHPHあっHPH自分でもするの、きもちいいっ…HPH」","L"
							CALL HPH_PRINT,@"「こんなのっHPH%K65_AEGI("控えめ",2)%…こんなの知っちゃったら、もう、一人じゃ、できなくなっちゃい、まっHPHぁっHPH」","L"
						ENDIF
						PRINTFORMDW 椛はあなたに貫かれたまま秘所と乳首をこね回し、しどけない嬌声を上げてよがっている…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%さんとえっちしてる最中なのに、%K65_AEGI(,2)%…自分でも、するなんてっ…」","L"
						PRINTFORMDL 羞恥に耐えかねたように首を振る椛。だが、その手が自らの秘所をまさぐる動きが止まることはない。
						CALL HPH_PRINT,@"「み、見ないで、ください、%CALLNAME:MASTER%さん…%K65_AEGI("控えめ",2)%…あぁあっHPH」","L"
						PRINTFORMDW 言葉と裏腹に熱くうねる膣肉が%CALLNAME:MASTER%を包みこみ、強烈な快楽を与えてくる…
					CASE 1
						;騎乗位
						IF TEQUIP:体位２ == 3
							CALL HPH_PRINT,@"「…うぁっ…%K65_AEGI("控えめ",2)%…」","L"
							CALL HPH_PRINT,@"（%CALLNAME:MASTER%のおちんちんに自分からご奉仕して、そのうえアソコやおっぱいまで自分でいじるなんて…）","L"
							CALL HPH_PRINT,@"（こんなにいやらしくて恥ずかしいことなのに、なんでこんなに気持ちいいのっ…HPH）","W"
						;それ以外
						ELSE
							CALL HPH_PRINT,@"「%K65_AEGI()%…はーっ、はーっ…あれ…？」","L"
							PRINTFORMDL %CALLNAME:MASTER%はいったん腰を止め、彼女に問いかけてみた。
							PRINTFORMW 「……えぅ…そんなこと、恥ずかしいから言いたくないですよ……」
							PRINTFORML
							CALL HPH_PRINT,@"「んうぅう゛ぅっHPHひっう゛HPHあぁっHPHいいますっ！いいますからっぁHPH」","L"
							CALL HPH_PRINT,@"「ひとりでっHPHひとりでするときはっHPH%CALLNAME:MASTER%さんのことっHPHう゛っHPHおもい、だして、してますっHPH」","L"
							CALL HPH_PRINT,@"「きもちっ…きもちいひっHPHほんものの、%CALLNAME:MASTER%さんおちんちんっHPHすごぃっHPHきもちいぃっ…HPH」","W"
						ENDIF
				ENDSELECT
			ENDIF
		;おなかたぷたぷ
		ELSEIF 充填率(65,1) >= 30 && ABL:膣射中毒 > 4 && RAND:2
			IF TCVAR:発情
				SELECTCASE RAND:2
					CASE 0
