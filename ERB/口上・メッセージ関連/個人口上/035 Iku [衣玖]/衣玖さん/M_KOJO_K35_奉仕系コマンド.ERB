	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「む、胸で……挟むんですか……？」
		PRINTFORML  おずおずとあなたの一物を胸で挟み込む
		PRINTFORMW 「こう……ですか？気持ちいいですか……？」
		PRINTFORML  胸を動かすように指示しそこに涎を垂らすよう教え込む
		PRINTFORMW 「涎をって……わかりました……んっくちゅぅ……えろぉ……こへれいいれるか？」
		PRINTFORMW  動きは拙いものの潤滑液を得た胸を動かす度快感が走る
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:140
		PRINTFORML 「ふふっ%UNICODE(0x2764)%おっぱいの中でビクビクゥっておちんちん悦んでますよぉ%UNICODE(0x2764)%%UNICODE(0x2764)%」
		PRINTFORMW 「もぉっと腰ふってびゅー%UNICODE(0x2764)%びゅー%UNICODE(0x2764)%って胸の中に射精してくださいね%UNICODE(0x2764)%」
	ELSEIF ABL:胸 >= 4
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「んふふ%UNICODE(0x2764)%いやらしい音立てて……そんなにおっぱいに射精したいんですか？」
			PRINTFORMW 「我慢なさらないでください……ちゅ%UNICODE(0x2764)%%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「谷間から寂しい寂しいって亀頭が泣いてますねぇ%UNICODE(0x2764)%」
			PRINTFORML 「ふふっ可愛いですね……%UNICODE(0x2764)%」
			PRINTFORMW 「ちゅっ%UNICODE(0x2764)%あむぅ%UNICODE(0x2764)%んっじゅむぅ%UNICODE(0x2764)%」
		ENDSELECT
	ELSEIF ABL:胸 >= 2
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「段々コツわかってきました……こうされるとイイんですよね？」
			PRINTFORMW 「ふふっ……もう先走りだけでヌルヌルになっちゃってますよ%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「挟まれながら口でされるのたまらないみたいですね%UNICODE(0x2764)%」
			PRINTFORMW 「ああぁ…おっきくて立派で素敵ですね%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「んっ……中々難しいですね……」
			PRINTFORMW 「どうですか？気持ちいいですか？」
		CASE 1
			PRINTFORMW 「挟みながら咥えるの結構大変ですね……あむっ……じゅむぅ%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

