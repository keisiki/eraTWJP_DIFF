;302,スキンシップ
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K64_302
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K64_302
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K64_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K64_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;お風呂
	IF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「よし、私が背中を流してやろうか」
				PRINTFORML 「自分でやるって？ おいおい、遠慮すんな！」
				PRINTFORMDL 背中ごと削ぎ落されやしないだろうか……
				PRINTFORMDW そう考えていた%CALLNAME:MASTER%は無意識に勇儀から距離を取っていた
			CASE 1
				PRINTFORML 「前は自分で洗え？ 面倒だねぇ」
				PRINTFORML 「乗り掛かった舟なんだ、最後までやり切っとくれよ」
				PRINTFORMDL どうやら拒否権など初めから無いらしい
				PRINTFORMDL 結局、%CALLNAME:MASTER%は勇儀の体を隅々まで洗う羽目になった
				PRINTFORMW 「～♪」
			CASE 2
				PRINTFORML 「熱い湯に浸かり、酒を呑み、朋友と背中を流し合う」
				PRINTFORML 「これぞ風呂の醍醐味。%MASTERNAME:64%もそう思うだろ？」
				PRINTFORMDW 勇儀と洗いっこをしながら色々なことを語り合った
		ENDSELECT
	;通常
	ELSE
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「おい、ちょ、くッ、はははははッ！」
					PRINTFORMDL %CALLNAME:MASTER%は勇儀の脇をこちょこちょとくすぐっている
					PRINTFORML 「分かった！私の負けだ！だから…ッ」
					PRINTFORMW 「いひっ、そ、そこはッ！ あっはははっはッ！」
				CASE 1
					PRINTFORML 「ふふ、そんなトコばかり触って……」
					PRINTFORML 「%MASTERNAME:64%が触りたいのは、ココだろう？」
					PRINTFORMDW 導かれるまま、%CALLNAME:MASTER%は勇儀の色々な所を撫で回した……
				CASE 2
					PRINTFORML 「なあ、手を握っとくれよ」
					PRINTFORMDL %CALLNAME:MASTER%が求めに応じて手を握ると、勇儀は優しく指を絡ませてきた
					PRINTFORMW 「ふふ、%MASTERNAME:64%の手は温かいな」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「こら、じゃれつくなって」
					PRINTFORML 「これじゃあ酒が呑めないだろ」
					PRINTFORMDW そう言いつつも勇儀は楽しそうだ
				CASE 1
					PRINTFORMDL %CALLNAME:MASTER%は勇儀の手をさりげなく握ってみた
					PRINTFORML 「お、腕相撲か？ おらッ！」
					PRINTFORMDL そして、気が付けば空を見上げていた……
					PRINTFORML 「なんだなんだ、その程度か？」
					PRINTFORMW 「張り合いが無いったらありゃしない」
				CASE 2
					PRINTFORML 「肩を揉みたいだ？」
					PRINTFORML 「そいつは有難い、最近肩こりが酷くてなあ」
					PRINTFORMDL %CALLNAME:MASTER%は勇儀の背後に回ると、真心を込めて肩を揉んであげた
					PRINTFORMW 「おっ、いい塩梅だ。その調子で頼むよ」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL 勇儀の名前を呼びながらさりげなく肩に手を置いてみた
					PRINTFORMW 「んー？」
				CASE 1
					PRINTFORMDL %CALLNAME:MASTER%は勇儀の力こぶを触らせてもらった
					PRINTFORML 「腕っぷしには自信がある」
					PRINTFORMW 「なんなら、今ここで試してみるかい？」
				CASE 2
					PRINTFORML 「ここまで気安く触れてくるのはあんたくらいだよ」
					PRINTFORMDW 無遠慮に触ってくる%CALLNAME:MASTER%に勇儀は呆れた表情を浮かべている
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
