RETURN RESULT

@M_KOJO_MESSAGE_COM_K64_411_1

#DIM 正解
#DIM 経験値
#DIM 制限時間
#DIM 倍率
#DIM FEINT
#DIM イカサマ
VARSET LOCAL
イカサマ = 0
制限時間 = 2000 - (ABL:64:戦闘能力 - ABL:MASTER:戦闘能力) * 100

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;本気の戦闘訓練は一日一回だけ
	IF TCVAR:64:353 == 0
		TCVAR:64:353 = 1
		;特訓デイリーからお借りしました
		CALL M_KOJO_COLOR_K64
		PRINTFORML 「強くなる為の努力を惜しまない、その心意気や良し！」
		PRINTFORML 「山の四天王が一人、力の勇儀が全力を持って手助けしようじゃないか！」
		PRINTFORMDW 勇儀が拳を打ち鳴らし、臨戦態勢をとる
		PRINTFORMW 「さあ、準備ができたらかかってきな！」
		RESETCOLOR
		PRINTFORML 
		CALL ASK_YN("訓練開始","やっぱり止める")
		IF RESULT
			CALL M_KOJO_COLOR_K64
			PRINTFORML 「どうした、強くなりたいってのは嘘だったのかい？」
			PRINTFORMW 「……はぁ、まあいいけどね」
			RESETCOLOR
		ELSE
			正解 = RAND:4
			経験値 = MAX(ABL:64:戦闘能力 * 5, 5)
			SELECTCASE RAND:20
				CASE IS <= 11
				;普通に
					LOCALS:0 = 上
					LOCALS:1 = 右
					LOCALS:2 = 左
					LOCALS:3 = 下
					倍率 = 100
				CASE 12 TO 15
				;ローマ字
					LOCALS:0 = UE
					LOCALS:1 = MIGI
					LOCALS:2 = HIDARI
					LOCALS:3 = SHITA
					倍率 = 160
				;英語
				CASE 16,17,18
					LOCALS:0 = UP
					LOCALS:1 = RIGHT
					LOCALS:2 = LEFT
					LOCALS:3 = DOWN
					倍率 = 120
				;スペイン語
				CASE 19
					LOCALS:0 = encima de
					LOCALS:1 = a la derecha
					LOCALS:2 = a la izquierda
					LOCALS:3 = abajo de
					倍率 = 300
			ENDSELECT
			;あなたの戦闘能力が高いとフェイントかけてくる
			IF ABL:MASTER:戦闘能力 >= 5 && !RAND:5
				FEINT = 1
				制限時間 += 400
				倍率 += 80
			ELSE
				FEINT = 0
			ENDIF
			CALL FISHER_YATES_SHAFFLE(4)
			PRINTFORMW %CALLNAME:TARGET%の攻撃を避けよう（クリックで開始）
			PRINTFORM %CALLNAME:TARGET%の
			SETCOLOR C_YELLOW
			IF FEINT
				PRINTFORM %LOCALS:(RAND:4)%
				RESETCOLOR
				PRINTFORM と見せかけて%LOCALS:(3 - 正解)%
			ELSE
				PRINTFORM %LOCALS:(3 - 正解)%
				RESETCOLOR
			ENDIF
			PRINTFORML からの攻撃！
			$LOOP_IKASAMA
			FOR LOCAL:10,0,4
				LOCAL = SHAFFLE_ARRAY:(LOCAL:10)
				PRINTFORML [{LOCAL}] - %LOCALS:LOCAL%に避ける
			NEXT
			SIF !イカサマ && BASE:MASTER:TSP > 500
				PRINTFORML [{4}] - 時間停止
			TINPUT 制限時間, -1, 1
			IF RESULT == 正解
				PRINTFORML %CALLNAME:MASTER%は攻撃を回避した！
				IF !イカサマ
					CALL M_KOJO_COLOR_K64
					PRINTFORMDL 攻撃を躱された勇儀の瞳が驚愕に見開かれる
					PRINTFORMDW %CALLNAME:MASTER%はその一瞬の隙を見逃さす、渾身の力でもって勇儀の顎を打ち据えた
					PRINTFORML 「ぐ……ッ」
					PRINTFORML 「…ッは、ははッ……こりゃ、一本取られたねえ」
					PRINTFORMW 「私の本気の一撃を躱すとは、大したもんだよ」
					PRINTFORMDL 勇儀が首をゴキゴキと鳴らしながら%CALLNAME:MASTER%を賞賛する
					PRINTFORMDW %CALLNAME:MASTER%も勇儀に訓練に付き合ってくれた礼と、労りの言葉をかけた
					PRINTFORML 「何か掴めたようだね、その目を見りゃ分かる」
					PRINTFORMW 「次は訓練なんて言わず、本気の勝負を期待してるよ」
					RESETCOLOR
				ELSE
					IF CFLAG:64:1100== 1
						CALL M_KOJO_COLOR_K64
						PRINTFORMDW %CALLNAME:MASTER%が時間停止を解除した瞬間、それを読んでいたかのように勇儀の裏拳が顔面にクリーンヒットした
						PRINTFORML 「雰囲気が随分と余裕だったから、もしやと思ったが」
						PRINTFORMW 「どうやら大当たりだったみたいだね」
						PRINTFORMDW どうやら完全に見切られていたらしい……%CALLNAME:MASTER%は痛む顔を擦りながらなんとか立ち上がった
						RESETCOLOR
						BASE:MASTER:体力 -= 1000
						SETCOLOR C_RED
						FONTBOLD
						PRINTFORMW 手痛い反撃を受け%CALLNAME:MASTER%の体力が大きく減った
						RESETCOLOR
					ELSE
						CALL M_KOJO_COLOR_K64
						PRINTFORMDL %CALLNAME:MASTER%は時間を止めて勇儀の背後に回ると、彼女の背中をポンポンと叩いた
						PRINTFORML 「……なるほどねえ、それがお前の能力ってことか」
						PRINTFORMW 「なら、次からはそう弁えた上で立ち回らせてもらうとするよ」
						RESETCOLOR
						CFLAG:64:1100 = 1
					ENDIF
				ENDIF
				経験値 = 経験値 * 倍率 / 100
				EXP:MASTER:戦闘経験 += 経験値
				EXP:64:戦闘経験 += 経験値 * 2
				CFLAG:64:好感度 += 経験値 * 2
				CFLAG:64:信頼度 += 経験値 * 2
				PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:MASTER%）
				PRINTFORML 戦闘経験 ＋{経験値 * 2}（%CALLNAME:TARGET%）
				PRINTFORML 好感度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
				PRINTFORML 信頼度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			ELSEIF RESULT == 4 && !イカサマ && BASE:MASTER:TSP > 500
				イカサマ = 1
				倍率 /= 5
				PRINTFORMW %CALLNAME:MASTER%は時点停止能力で攻撃を見極めた！
				GOTO LOOP_IKASAMA
			ELSE
				CALL M_KOJO_COLOR_K64
				PRINTFORMDL %CALLNAME:MASTER%が躱すよりも早く、勇儀の拳が鳩尾を打ち据える
				PRINTFORMDW その一撃はあまりにも重く、%CALLNAME:MASTER%はもんどり打って地面に倒れ込んだ
				PRINTFORML 「大丈夫かい？ ちょっとやりすぎたかな」
				PRINTFORMW 「まあでも、良い経験になったんじゃないか？」
				RESETCOLOR
				BASE:MASTER:体力 -= 400 * (1 + ABL:64:戦闘能力)
				PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:TARGET%）
				EXP:64:戦闘経験 += 経験値
				CFLAG:64:好感度 += 経験値 / 2
				CFLAG:64:信頼度 += 経験値 / 2
				PRINTFORML 好感度 ＋{経験値}（%CALLNAME:TARGET%）
				PRINTFORML 信頼度 ＋{経験値}（%CALLNAME:TARGET%）
			ENDIF
		ENDIF
	ELSE
		PRINTFORMDL あんな訓練を何度もしていては身が持たない
		PRINTFORMDW %CALLNAME:MASTER%は勇儀を誘って軽く体を動かすにとどめた
	ENDIF
ENDIF
RETURN 1
