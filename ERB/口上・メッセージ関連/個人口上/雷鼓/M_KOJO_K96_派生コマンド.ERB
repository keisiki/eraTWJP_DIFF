;==================================================
;5,手淫フェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K96_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K96_5_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K96_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		;時姦中
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で雷鼓の口腔を犯し始めた。
			ELSE
				PRINTFORMW 「…んちゅ…ん…」
				PRINTFORMW 「あっ…いっぱい出して…%UNICODE(0x2665) *1%」
			ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 凍てついた時の中で雷鼓の手に陰茎を握らせながら口腔を犯し始めた。
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…ちゅ…」
			PRINTFORMW 「…れろ…出そう…？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「っ…ん…」
			PRINTFORMW 「…じゅる…ん…」
		ELSE
			PRINTFORMW 「っく…じゅぶ…」
			PRINTFORMW 「…ん…絞りとってあげる…%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「あ…れろ…」
			PRINTFORMW 「…ん…ちゅ…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「じゅぶ…ん…」
			PRINTFORMW 「ん…苦…」
		ELSE
			PRINTFORMW 「…ちゃんと洗ってる？」
			PRINTFORMW 「綺麗にしてあげるわ…」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「っ…じゅる…」
			PRINTFORMW 「ん…これ…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…っ…」
			PRINTFORMW 「…ん…じゅる…」
		ELSE
			PRINTFORMW 「…んむ…」
			PRINTFORMW 「れろ…ん…」
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K96_{SELECTCOM}_1
RETURN 1

