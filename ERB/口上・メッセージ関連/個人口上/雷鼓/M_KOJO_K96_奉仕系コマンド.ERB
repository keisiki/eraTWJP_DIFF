;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K96_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K96_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K96_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で雷鼓の手に陰茎を握らせた。
			ELSE
				PRINTFORMW 「ん？」
				PRINTFORMW 「分かったわ、イかせてあげるわ」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 凍てついた時の中で雷鼓の手に陰茎を握らせた。
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「ん？もう出ちゃうの？」
			PRINTFORMW そう言うと雷鼓は一層激しく扱き始めた。
			PRINTFORMW 「可愛いわね、ビクビクしてて%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ん…大きくなってる%UNICODE(0x2665) *1%」
			PRINTFORMW そう言うと雷鼓は一層激しく扱き始めた。
			PRINTFORMW 「ザーメン出しちゃいなさい%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「そろそろ出ちゃいそうね」
			PRINTFORMW そう言うと雷鼓は一層激しく好き始めた。
			PRINTFORMW 「いっぱい出して…%UNICODE(0x2665) *1%」
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…ほら、」
			PRINTFORMW 「気持よくしてあげる…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ん…太くて固い…」
			PRINTFORMW 「…でもすぐに出ちゃいそうね」
		ELSE
			PRINTFORMW 「ん…暴れん坊さんね…」
			PRINTFORMW 「気持よくしてあげる…%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「ん…気持ちいい？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「こういうの、あんまりしたこと無いけど」
			PRINTFORMW 「手先は器用なの、私」
		ELSE
			PRINTFORMW 「いっぱい…出しちゃいなさい」
			PRINTFORMW 「…私の手で気持よくしてあげる」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん…気持ち悪いわね」
			PRINTFORMW 「…早く出しちゃいなさい」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「こういうの、あまり好きじゃないけど」
			PRINTFORMW 「好きなら気持よくしてあげる」
		ELSE
			PRINTFORMW 「…ん…ここが弱点？」
			PRINTFORMW 「…大きくなってきたわね…」
		ENDIF
	ENDIF
ENDIF
RETURN 1

