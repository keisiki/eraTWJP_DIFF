#DIM 経験値
#DIM 制限時間
#DIM 倍率
#DIM FEINT
#DIM イカサマ
VARSET LOCAL
イカサマ = 0
制限時間 = 2000 - (ABL:64:戦闘能力 - ABL:MASTER:戦闘能力) * 100

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;本気の戦闘訓練は一日一回だけ
	IF !TCVAR:64:353
		;特訓デイリーからお借りしました
		CALL M_KOJO_COLOR_K64
		PRINTFORML 「強くなる為の努力を惜しまない、その心意気や良し！」
		PRINTFORML 「山の四天王が一人、力の勇儀が全力を以って手助けしようじゃないか！」
		PRINTFORMDW %CALLNAME:TARGET%が拳を打ち鳴らし、臨戦態勢をとる
		PRINTFORMW 「さあ、準備ができたらかかってきな！」
		RESETCOLOR
		PRINTFORML 
		CALL ASK_YN("訓練開始","やっぱり止める")
		IF RESULT
			CALL M_KOJO_COLOR_K64
			PRINTFORML 「どうした、強くなりたいってのは嘘だったのかい？」
			PRINTFORMW 「……はぁ、気が削がれた。酒でも呑むとするか」
			RESETCOLOR
		ELSE
			TCVAR:64:353 = 1
			正解 = RAND:4
			経験値 = MAX(ABL:64:戦闘能力 * 5, 5)
			SELECTCASE RAND:20
				;漢字
				CASE IS <= 11
					LOCALS:0 = 上
					LOCALS:1 = 右
					LOCALS:2 = 左
					LOCALS:3 = 下
					倍率 = 100
				;ひらがな
				CASE 12 TO 15
					LOCALS:0 = うえ
					LOCALS:1 = みぎ
					LOCALS:2 = ひだり
					LOCALS:3 = した
					倍率 = 100
				;カタカナ
				CASE 16,17,18
					LOCALS:0 = ウエ
					LOCALS:1 = ミギ
					LOCALS:2 = ヒダリ
					LOCALS:3 = シタ
					倍率 = 100
				;スワヒリ語
				CASE 19
					LOCALS:0 = juu
					LOCALS:1 = kulia
					LOCALS:2 = kushoto
					LOCALS:3 = chini
					倍率 = 320
			ENDSELECT
			;あなたの戦闘能力が高いとフェイントかけてくる
			IF ABL:MASTER:戦闘能力 >= 5 && !RAND:5
				FEINT = 1
				制限時間 += 400
				倍率 += 80
			ELSE
				FEINT = 0
			ENDIF
			CALL FISHER_YATES_SHAFFLE(4)
			PRINTFORMW %CALLNAME:TARGET%の攻撃を避けよう（クリックで開始）
			PRINTFORM %CALLNAME:TARGET%の
			SETCOLOR C_YELLOW
			IF FEINT
				PRINTFORM %LOCALS:(RAND:4)%
				RESETCOLOR
				PRINTFORM と見せかけて%LOCALS:(3 - 正解)%
			ELSE
				PRINTFORM %LOCALS:(3 - 正解)%
				RESETCOLOR
			ENDIF
			PRINTFORML からの攻撃！
			$LOOP_IKASAMA
			FOR LOCAL:10,0,4
				LOCAL = SHAFFLE_ARRAY:(LOCAL:10)
				PRINTFORML [{LOCAL}] - %LOCALS:LOCAL%に避ける
			NEXT
			SIF !イカサマ && BASE:MASTER:TSP > 500
				PRINTFORML [{4}] - 時間停止
			TINPUT 制限時間, -1, 1
			IF RESULT == 正解
				PRINTFORML %CALLNAME:MASTER%は攻撃を回避した！
				IF !イカサマ
					CALL M_KOJO_COLOR_K64
					PRINTFORMDL 攻撃を躱された%CALLNAME:TARGET%の瞳が驚愕に見開かれる
					PRINTFORMDW %CALLNAME:MASTER%はその一瞬の隙を見逃さす、渾身の力でもって%CALLNAME:TARGET%の顎を打ち据えた
					PRINTFORML 「ぐッ――！」
					PRINTFORML 「ッは、ははッ！ こりゃ、一本取られたねえ」
					PRINTFORMW 「私の本気の一撃を躱すとは、大したもんだよ」
					PRINTFORMDL %CALLNAME:TARGET%が首をゴキゴキと鳴らしながら%CALLNAME:MASTER%を賞賛する
					PRINTFORMDW %CALLNAME:MASTER%も%CALLNAME:TARGET%に訓練に付き合ってくれた礼と、労りの言葉をかけた
					PRINTFORML 「何か掴めたようだね、その目を見りゃ分かる」
					PRINTFORMW 「次は訓練なんて言わず、本気の勝負を期待してるよ」
					RESETCOLOR
				ELSE
					;二回目以降は２分の１の確率で反撃される
					IF CFLAG:64:1100 && !RAND:2
						CALL M_KOJO_COLOR_K64
						PRINTFORML 「そこだッッ！！！」
						PRINTFORMDW %CALLNAME:MASTER%が時間停止を解除した瞬間、それを読んでいたかのように%CALLNAME:TARGET%の裏拳が顔面にクリーンヒットした
						PRINTFORML 「雰囲気が随分と余裕そうだったから、もしかしてと思ったけど……やっぱりね」
						PRINTFORMW 「言っただろ？『次からはそう弁えた上で立ち回らせてもらう』ってさ」
						PRINTFORMDW ――完全に見切られていたようだ。%CALLNAME:MASTER%は痛む顔を擦りながらなんとか立ち上がった
						RESETCOLOR
						BASE:MASTER:体力 -= 1000
						SETCOLOR C_RED
						FONTBOLD
						PRINTFORMW 手痛い反撃を受け%CALLNAME:MASTER%の体力が大きく減った
						FONTREGULAR
						RESETCOLOR
					;通常
					ELSE
						CALL M_KOJO_COLOR_K64
						PRINTFORMDL %CALLNAME:MASTER%は時間を止めて%CALLNAME:TARGET%の背後に回ると、彼女の肩をポンと叩いた
						PRINTFORML 「――なるほどね。それがお前の能力ってことか」
						PRINTFORMW 「なら、次からはそう弁えた上で立ち回らせてもらうとするよ」
						RESETCOLOR
						SIF !CFLAG:64:1100
							CFLAG:64:1100 = 1
					ENDIF
				ENDIF
				BASE:MASTER:体力 -= 100 * (1 + ABL:64:戦闘能力)
				BASE:MASTER:気力 -= 100 * (1 + ABL:64:戦闘能力)
				経験値 = 経験値 * 倍率 / 100
				EXP:MASTER:戦闘経験 += 経験値
				EXP:64:戦闘経験 += 経験値 * 2
				CFLAG:64:好感度 += 経験値 * 2
				CFLAG:64:信頼度 += 経験値 * 2
				PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:MASTER%）
				PRINTFORML 戦闘経験 ＋{経験値 * 2}（%CALLNAME:TARGET%）
				PRINTFORML 好感度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
				PRINTFORML 信頼度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			ELSEIF RESULT == 4 && !イカサマ && BASE:MASTER:TSP > 500
				イカサマ = 1
				倍率 /= 5
				PRINTFORMW %CALLNAME:MASTER%は時点停止能力で攻撃を見極めた！
				GOTO LOOP_IKASAMA
			ELSE
				CALL M_KOJO_COLOR_K64
				PRINTFORMDL %CALLNAME:MASTER%が躱すよりも早く、%CALLNAME:TARGET%の拳が鳩尾を打ち据える
				PRINTFORMDW その一撃はあまりにも重く、%CALLNAME:MASTER%はもんどり打って地面に倒れ込んだ
				PRINTFORML 「おーい、大丈夫かー？ …ちょっとやりすぎたかな」
				PRINTFORMW 「ま、負けを知るってのも大事だ。これも良い経験になったと思いな」
				RESETCOLOR
				BASE:MASTER:体力 -= 400 * (1 + ABL:64:戦闘能力)
				BASE:MASTER:気力 -= 400 * (1 + ABL:64:戦闘能力)
				PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:TARGET%）
				EXP:64:戦闘経験 += 経験値
				CFLAG:64:好感度 += 経験値 / 2
				CFLAG:64:信頼度 += 経験値 / 2
				PRINTFORML 好感度 ＋{経験値}（%CALLNAME:TARGET%）
				PRINTFORML 信頼度 ＋{経験値}（%CALLNAME:TARGET%）
			ENDIF
		ENDIF
	ELSE
		PRINTFORMDL あんな訓練を何度もしていては身が持たない
		PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%を誘って軽く体を動かすにとどめた
	ENDIF
ENDIF
RETURN 1
