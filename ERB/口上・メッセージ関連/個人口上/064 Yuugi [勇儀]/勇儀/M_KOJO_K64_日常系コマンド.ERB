		TCVAR:64:352 = 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%のお腹をなでた
				PRINTFORMDL %CALLNAME:TARGET%はなんだか気持ち良さそうにしている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「撫でるんなら、もっとこう……」
						PRINTFORMDL %CALLNAME:TARGET%が%CALLNAME:MASTER%の手を取り、子宮の辺りへ……
						PRINTFORMW 「――気持ち良くさせる方法があるだろう？」
					CASE 1
						PRINTFORML 「ほら、もっと撫でなって」
						PRINTFORML 「――そう、その辺り……ふぅっ」
						PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%の子宮の辺りを撫で回した……
					CASE 2
						PRINTFORML 「うん？ 腹を撫でてもいいかって？」
						PRINTFORML 「どうぞお好きに。気の済むまで撫でりゃいいさ」
						PRINTFORMDW %CALLNAME:TARGET%はお腹を撫でる%CALLNAME:MASTER%の手に自らの手を重ねた
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%のお腹をなでた
				PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%との触れ合いを楽しんでいるようだ……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「あんたが撫でまわすから、最近やけに腹の調子が良くてね」
						PRINTFORML 「――関係あるのかって？ まぁいいじゃないか、そういうことにしておけば」
						PRINTFORMW 「その方が、あんたも心置きなく撫でることができるだろ？」
					CASE 1
						PRINTFORML 「おい、ちょ、くッ、はははははッ！」
						PRINTFORML 「いひっ、そ、そこはッ！ あっはははっはッ！」
						PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%の脇腹をくすぐって遊んだ……
					CASE 2
						PRINTFORML 「未だに何がいいのかよく分からんが」
						PRINTFORML 「まあ、あんたが楽しそうだから良しとするよ」
						PRINTFORMDW %CALLNAME:TARGET%はお腹を撫でられることにすっかり慣れてしまったようだ
				ENDSELECT
			ENDIF
		;思慕
		ELSEIF TALENT:思慕
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%のお腹をなでた
				PRINTFORMDL %CALLNAME:TARGET%はくすぐったそうにしながらもされるがままになっている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「――気は済んだかい？」
						PRINTFORML 「それじゃあ、今度は私の番な」
						PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%にお腹を撫でられてしまった……
					CASE 1
						PRINTFORML 「ったく、くすぐったいって言ってんのに聞きゃしない」
						PRINTFORML 「――分かった、私の負けだよ。好きなだけ触りなって」
						PRINTFORMDW お言葉に甘えて、%CALLNAME:MASTER%は好きなだけ%CALLNAME:TARGET%のお腹を撫でまわした……
					CASE 2
						PRINTFORML 「ん、んん……っ？」
						PRINTFORMDL ゆっくりとお腹を撫でながら、少しずつ手を下にずらしていく
						PRINTFORMDL ちょうど子宮の上辺りに到達したところで、そこをグイッと押し込んだ
						PRINTFORML 「っあ……い、今なにをしたんだ？」
						PRINTFORMDW くすぐったさとは違う謎の快感に%CALLNAME:TARGET%は困惑している様子だ……
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%のお腹をなでた
				PRINTFORMDL %CALLNAME:TARGET%はくすぐったそうにしている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「――ん？ 人を喰ったことはあるのかって？」
						PRINTFORML 「そりゃあ勿論あるさ、私は鬼だぞ？」
						PRINTFORML 「昔は私の首を狙ってたヤツも多くてねぇ」
						PRINTFORMW 「私が勝ったらお前を喰らうって条件で勝負したもんさ」
					CASE 1
						PRINTFORML 「ぅおっと、相変わらずいきなりだね」
						PRINTFORML 「言っただろ？ 不意打ちされるのは好きじゃない」
						PRINTFORMDW そう言いつつも%CALLNAME:TARGET%はお腹を撫でさせてくれた
					CASE 2
						PRINTFORMDL %CALLNAME:TARGET%のお腹を撫でていると、彼女のお腹がグゥと鳴った
						PRINTFORML 「腹が空いてきたねぇ……何か適当につまめるもん持ってないかい？」
						PRINTFORMW 「ああ、煎った豆以外で頼むよ」
				ENDSELECT
			ENDIF
		;それ以外
		ELSE
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%のお腹をなでた
				PRINTFORMDL %CALLNAME:TARGET%は大人しくしている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「んっ……おい、これに何の意味があるってんだい？」
						PRINTFORMDL %CALLNAME:MASTER%は何故お腹を撫でるのかを%CALLNAME:TARGET%に熱弁した
						PRINTFORMW 「お、おう……まあ減るもんじゃなし、好きにしなよ」
					CASE 1
						PRINTFORML 「そんなに腹を撫でたいなら、自分の腹でも撫でてりゃいいじゃないか」
						PRINTFORML 「わざわざ私の腹なんて撫でなくたって……んんっ」
						PRINTFORMDW %CALLNAME:TARGET%はむず痒そうに身を捩っている……
					CASE 2
						PRINTFORML 「私の腹なんて撫でて楽しいのかい？」
						PRINTFORML 「――楽しい？ けったいなヤツだねぇ」
						PRINTFORMDW %CALLNAME:TARGET%は首を傾げている……
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%のお腹をなでた
				PRINTFORMDL %CALLNAME:TARGET%は怪訝そうにしている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「腹を触るのは構わないけど、一声くらい掛けておくれよ」
						PRINTFORML 「不意打ちされるのは好きじゃないんだ」
