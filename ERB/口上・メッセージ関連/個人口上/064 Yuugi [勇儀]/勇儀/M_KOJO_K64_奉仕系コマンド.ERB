		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		;このコマンドであなたが射精
		IF NOWEX:MASTER:射精
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ははっHPH グツグツ上ってきたHPH」","L"
					CALL HPH_PRINT,@"「だけど、射精ないねぇ……どうしてだい？HPH」","L"
