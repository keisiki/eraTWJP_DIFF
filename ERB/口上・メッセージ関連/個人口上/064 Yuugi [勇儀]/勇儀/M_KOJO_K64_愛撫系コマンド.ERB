@M_KOJO_MESSAGE_COM_K64_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;膣内射精直後
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			;恋慕
			IF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMDL %CALLNAME:MASTER%は精液を零す膣に口を付け、ぢゅずずっと吸い上げた
						CALL HPH_PRINT,@"「私の分も残しておいておくれよ……？HPH」","L"
						CALL HPH_PRINT,@"「せっかくあんたが注いでくれたんだ……んっHPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「ふふ……こんなに注がれちゃ、さすがの私でも呑み切れないな……HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%が陰唇を広げると、コポッと音を立てて中に出した精液が溢れ出てきた
						PRINTFORMDW %CALLNAME:MASTER%がそれを舐め取ると、%CALLNAME:TARGET%はぴくッと腰を跳ねさせた
					CASE 2
						CALL HPH_PRINT,@"「はー……HPH こりゃまた、たんまりと射精したねぇ……HPH」","L"
						PRINTFORMDL %CALLNAME:MASTER%が舐めても舐めても、%CALLNAME:TARGET%の膣からはとめどなく精液が溢れ出てくる
						CALL HPH_PRINT,@"「まだ満足してないだろう？ ほら、ちゃっちゃと突っ込みなHPH」","W"
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「まさか本当に舐めるとはねぇ……で、味は？」
						PRINTFORML 「――マズい？ ははっ、そりゃそうだ！」
						PRINTFORMDW %CALLNAME:MASTER%は中出ししたばかりのおまんこを舐めたことをちょっぴり後悔した
					CASE 1
						PRINTFORML 「あんたが射精したものだ。シッカリと綺麗にしなよ？」
						PRINTFORMDL %CALLNAME:TARGET%が足を広げると、中に注いだ精液がコポリと溢れ出た
						PRINTFORMDW 愛液と精液が混ざった汁は、あまり美味しいとは思えなかった……
					CASE 2
						PRINTFORMDL %CALLNAME:TARGET%のおまんこを舐めると、当然ながら精液の味がした
						PRINTFORML 「ちょ、おい、自分で舐めといて気持ち悪くなってんのかい？」
						PRINTFORML 「ああもういいから。ほら、これで口ゆすぎな」
						PRINTFORMDW %CALLNAME:TARGET%が酒を渡してくる。今はこんなものでも有難い……
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「はぁ……ぁ、んっHPH あぁっHPH」","L"
				PRINTFORMDL %CALLNAME:TARGET%の膣穴に舌を差し入れ、湧き出す蜜を掻き出すように舌を前後させる
				PRINTFORMDW %CALLNAME:TARGET%の膣は、もっともっとと求めるように%CALLNAME:MASTER%の舌を締め付けている……
			CASE 1
				CALL HPH_PRINT,@"「外ばっかじゃなくて、ナカもさ……HPH」","L"
				CALL HPH_PRINT,@"「っぉ、ああ……っHPH きた……っHPH」","L"
				PRINTFORMDW ゾリゾリと膣壁を舐められる感触に、%CALLNAME:TARGET%は頬を緩ませている……
			CASE 2
				PRINTFORMDL %CALLNAME:MASTER%は舌先でクリを転がした後、口を付けちゅうっと吸い上げた
				CALL HPH_PRINT,@"「ふんっHPH んんっHPH ――はぁっHPH」","L"
				CALL HPH_PRINT,@"「はぁ……HPH 今のは効いたよ……HPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%は強い快楽の余韻に、断続的に腰を震わせている……
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ぁ、はっ♪ 美味いだろ？ 私のまんこは」
				PRINTFORML 「そらっ、もっと呑みたかったらちゃんと舐めな♪」
				PRINTFORMDW %CALLNAME:TARGET%はそう言うと、腰を浮かせ%CALLNAME:MASTER%の顔に秘所を押し付けた……
			CASE 1
				PRINTFORML 「ふふ、舐めたいのかい？」
				PRINTFORMDL %CALLNAME:TARGET%がくぱっと秘所を押し広げる……
				PRINTFORMW 「いいよ、来な……んっ♪」
			CASE 2
				PRINTFORML 「ん、く、ははっ！ くすぐったい！」
				PRINTFORML 「く、ふふっ！ そんな焦らなくても逃げやしないからさ」
				PRINTFORMDW %CALLNAME:TARGET%はそう言って、股に顔を埋める%CALLNAME:MASTER%の頭をポンポンと撫でた
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
