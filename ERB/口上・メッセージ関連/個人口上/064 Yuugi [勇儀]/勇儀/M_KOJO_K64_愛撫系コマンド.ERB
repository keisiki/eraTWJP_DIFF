@M_KOJO_MESSAGE_COM_K64_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「あ、んっHPH ――んんっHPH」","L"
				CALL HPH_PRINT,@"「――っあ、そこっHPH っん、はぁ……っHPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%は腰を浮かせて、秘所を這う指の感触を楽しんでいる……
			CASE 1
				CALL HPH_PRINT,@"「――可愛い声だって？ ふふ……HPH」","L"
				CALL HPH_PRINT,@"「あんたに触られてるんだ……声くらい出るさ……HPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%はそう言うと、%CALLNAME:MASTER%の背中に手を回してきた……
			CASE 2
				CALL HPH_PRINT,@"「こっちも忘れないでおくれよ……HPH」","L"
				CALL HPH_PRINT,@"「ん……ちゅ……っHPH はぁ……ちゅ……っHPH」","L"
				PRINTFORMDW %CALLNAME:MASTER%は愛撫の手を止めて、%CALLNAME:TARGET%と舌を絡ませ合った……
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ん、ん……もっとこう、ガツッといきなよ」
				PRINTFORML 「それじゃあ、くすぐったいだけ……ぅんっ！？」
				PRINTFORMDW 不意に陰核を摘み上げると、%CALLNAME:TARGET%の腰がビクッと跳ねた……
			CASE 1
				PRINTFORML 「んむっ……ちゅ……ちゅぷ……」
				PRINTFORML 「はぁ……これが、あんたの『鬼退治』かい？」
				PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%の身体を弄りながら、頷く代わりにもう一度唇を重ねた
			CASE 2
				PRINTFORML 「どうしてかねぇ……あんたと抱き合ってると……」
				PRINTFORML 「とっておきの酒を呑んだ時みたいに、身体が熱くなってくるよ……」
				PRINTFORMDW %CALLNAME:TARGET%は肩の力を抜いて、%CALLNAME:MASTER%の愛撫を受け入れている……
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
