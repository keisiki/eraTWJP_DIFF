@M_KOJO_MESSAGE_COM_K64_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K64
	;時間停止中
	IF FLAG:時間停止
		PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%にギュッと抱きついた
		PRINTFORMDW 時間を止められている%CALLNAME:TARGET%は無抵抗のまま抱きしめられている…
	;押し倒され
	ELSEIF CFLAG:うふふ == 2
		CALL AUTO_AEGI
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:PLAYER%は\@ CFLAG:TARGET:添い寝中 ? 添い寝する # \@%CALLNAME:TARGET%にギュッと抱きついた
				PRINTFORMDL %CALLNAME:MASTER%の胸元に頬をすり寄せて幸せそうにしている…
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMDL 満足した%CALLNAME:MASTER%が離れようとすると、%CALLNAME:TARGET%が背中に手を回してきた
						PRINTFORML 「もうちょいこのまま……いいだろ？」
						PRINTFORMW 「あんたとくっついてると落ち着くんだよ……」
					CASE 1
						PRINTFORML 「あんただけだよ。こんな私を笑わないでいてくれるのは」
						PRINTFORMDL 恐れられた山の四天王ではなく一人の女性として、
						PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%に甘えている……
					CASE 2
						PRINTFORML 「……%MASTERNAME:64%」
						PRINTFORML 「――んっ」
						PRINTFORMDW 抱き合ったまま見つめ合い……二人は唇を重ねた
						EXP:キス経験 ++
						EXP:MASTER:キス経験 ++
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:PLAYER%は\@ CFLAG:TARGET:添い寝中 ? 添い寝する # \@%CALLNAME:TARGET%にギュッと抱きついた
				PRINTFORMDL %CALLNAME:TARGET%は嬉しそうに抱き返してきた……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「あんたとももう長い付き合いだ。力の加減くらい心得てる」
						PRINTFORML 「私としては、思い切り抱きつきたいところだけどね」
						PRINTFORMDW %CALLNAME:TARGET%が%CALLNAME:MASTER%を優しく抱擁する。彼女を受け止めきれない自分の身体を恨めしく感じた
					CASE 1
						PRINTFORMDL %CALLNAME:TARGET%の大きな胸がむにゅんと%CALLNAME:MASTER%に押し当てられる
						PRINTFORML 「――当たってるって？ ふふん」
						PRINTFORMW 「当 て て ん の さ ♪」
					CASE 2
						PRINTFORML 「――痛いって？ ははっ、悪いね」
						PRINTFORML 「うっかりすると、つい力が入っちまうんだよ」
						PRINTFORMDW 全身の骨が悲鳴を上げる。だが%CALLNAME:TARGET%が楽しそうなので%CALLNAME:MASTER%は我慢した
				ENDSELECT
			ENDIF
		;思慕
		ELSEIF TALENT:思慕
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%を抱き締めた
				PRINTFORMDL %CALLNAME:TARGET%はお返しとばかりに%CALLNAME:MASTER%を抱き返した……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMDL %CALLNAME:TARGET%に抱き締められた瞬間、ミシリと骨が軋みを上げた
						PRINTFORML 「っとと、悪い！力込めすぎた！」
						PRINTFORMW 「加減ってやつはどうにも不得手でねぇ……」
					CASE 1
						PRINTFORML 「そんなに心地良いってなら私もやってみようかね」
						PRINTFORM 「――ふんっ！
						CALL COLORMESSAGE(@"（バキ ベキ メキィ ッッ！！！）",C_RED,0,1)
						PRINTFORML 」
						PRINTFORMDW 抱き締めすぎだぜ%CALLNAME:TARGET%……
					CASE 2
						PRINTFORML 「%MASTERNAME:64%、もっと体を鍛えた方がいいんじゃないか？」
						PRINTFORML 「私が思い切り抱き付いても潰れちまわないくらいにさ」
						PRINTFORMDW %CALLNAME:TARGET%にはぜひ力の抜き方を学んで欲しい……
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%を抱き締めた
				PRINTFORMDL %CALLNAME:TARGET%は満更でもなさそうにしている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「ああ分かってるよ、相撲じゃないってことくらい」
						PRINTFORML 「…………」
						PRINTFORML 「落ち着かないね。何か喋っておくれよ」
						PRINTFORMDW 甘い言葉を囁いてみると、%CALLNAME:TARGET%は押し黙ってしまった
					CASE 1
						PRINTFORML 「んな引っ付かれたら暑っ苦しいじゃないか」
						PRINTFORML 「……はぁ、言っても聞かないのが%MASTERNAME:64%だったね」
						PRINTFORMDW %CALLNAME:MASTER%は満足するまで%CALLNAME:TARGET%を抱き締めた
					CASE 2
						PRINTFORML 「私みたいなのにベタベタ甘えて楽しいのかい？」
						PRINTFORML 「――楽しい？ うーん、よく分からんが」
						PRINTFORMW 「あんたが楽しいって言うのなら、まあ悪い気はしないね」
				ENDSELECT
			ENDIF
		;それ以外
		ELSE
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%に抱きついた
				PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%を拒絶しなかった
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「そろそろ酒を呑みたいんだけどねぇ……」
						PRINTFORMDL %CALLNAME:MASTER%は腕に力を込め、まだ離したくないという意思を伝えた
						PRINTFORMW 「あー分かった分かった。あんたの勝ちだよ」
					CASE 1
						PRINTFORML 「人肌恋しいってなら抱きつく相手を間違えてるよ」
						PRINTFORML 「――私がいいって？ まぁ構わないけどさ」
						PRINTFORMDL ギュッと%CALLNAME:TARGET%を抱き締めて、%CALLNAME:MASTER%は目を閉じ耳を澄ませる
						PRINTFORMDW 一定のリズムを刻んでいた心臓の鼓動が、少しずつ早くなっていくのが分かった
					CASE 2
						PRINTFORML 「どうした、酔っ払ってんのかい？」
						PRINTFORMDL %CALLNAME:TARGET%の背中に手を回し、身体を引き寄せ抱き締める
						PRINTFORMDW %CALLNAME:TARGET%は何か言いたそうにしているが、やがて諦めたように小さく息を吐いた
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%に抱きついた
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「あんたが抱きついてると酒が呑めないんだ」
						PRINTFORML 「満足したならさっさと離しておくれよ」
						PRINTFORMDW 可愛い反応を期待したが、相手を間違えたらしい
					CASE 1
						PRINTFORML 「それで、こりゃ一体何の真似だ？」
						PRINTFORML 「――抱きついただけ？ 見りゃ分かる」
						PRINTFORML 「私が聞いてるのは理由だ。怒らないから言ってごらん」
						PRINTFORMDW %TEXTR("ぶつかり稽古です/石につまづいた/身体が勝手に/")%。%CALLNAME:MASTER%は適当な理由をつけて誤魔化した
					CASE 2
						PRINTFORML 「おっと、相撲がしたいのかい？」
						PRINTFORML 「力比べは大好きだ。特に人間相手はな！」
						PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%に綺麗な上手投げを決められてしまった
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
