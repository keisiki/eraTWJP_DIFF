;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_SUCCESS_COM_K64_310
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =1

@M_KOJO_MESSAGE_COM_K64_310
;CALL TRAIN_MESSAGE
DRAWLINE
CALL M_KOJO_MESSAGE_COM_K64_310_1
RETURN RESULT

@M_KOJO_EXTRASOURCE_COM_K64_310
;ムードが高まってる時は反感ゼロ
IF BASE:ムード >= 1000
	SOURCE:反感 = 0
;条件を満たさない場合でも大幅減
ELSE
	SOURCE:反感 = SOURCE:反感 / 10
ENDIF

@M_KOJO_MESSAGE_COM_K64_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K64
	;時間停止中
	IF FLAG:時間停止
		PRINTFORMDL %CALLNAME:MASTER%は背後から%CALLNAME:TARGET%の%HIP(TARGET)%に手を伸ばした
		PRINTFORMDW 時間を止められている%CALLNAME:TARGET%は無抵抗のままお尻を撫でられている…
	;睡眠中
	ELSEIF CFLAG:睡眠
		PRINTFORMDL %CALLNAME:MASTER%は横になって眠る%CALLNAME:TARGET%の%HIP(TARGET)%に手を伸ばした
		IF TALENT:恋慕
			PRINTFORMDW %CALLNAME:TARGET%は少し頬を染めて%CALLNAME:MASTER%にされるがままになっている…
		ELSE
			PRINTFORMDW %CALLNAME:TARGET%は身をよじると無意識に%CALLNAME:PLAYER%の手を払いのけた…
		ENDIF
	;押し倒され
	ELSEIF CFLAG:うふふ == 2
		CALL AUTO_AEGI
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の尻を我が物顔で揉みしだいた
				PRINTFORMDL %CALLNAME:TARGET%は興奮しているようだ……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「溜まってんなら相手になるよ」
						PRINTFORML 「――なんなら、私の方がもう我慢できそうにない」
						PRINTFORMW 「なあ、どうする？ このままヤるかい……？」
					CASE 1
						CALL HPH_PRINT,@"「――んっHPH そうそう、それでいいんだよ……HPH」","L"
						PRINTFORMDL お前の尻は俺のものだ。そう主張するかのように尻を揉みしだく
						PRINTFORMDW 鬼という種族は力で屈服させられることに強い興奮を感じるらしい……
					CASE 2
						PRINTFORML 「私をその気にさせようってなら……」
						PRINTFORMDL %CALLNAME:TARGET%が%CALLNAME:MASTER%の手を取り、自らの股座を触らせる
						PRINTFORMDL ――そこはもう、準備の必要がないくらいぐっしょりと湿っていた
						CALL HPH_PRINT,@"「こっちを弄っておくれよ。なぁ……HPH」","W"
						PALAM:潤滑 += 3000
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の尻を揉みしだいた
				PRINTFORMDL %CALLNAME:TARGET%はされるがままになっている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「――お。なんだ、もう飽きたのかい？」
						PRINTFORML 「撫でたきゃもっと撫でていいんだよ？」
						PRINTFORMDW %CALLNAME:TARGET%はどこか物足りなさそうにしている……
					CASE 1
						PRINTFORML 「偶にはキャーキャー騒いで欲しいって？」
						PRINTFORML 「構わないけど、鼓膜が破けてもしらないよ？」
						PRINTFORMDW スゥッと息を吸い込む%CALLNAME:TARGET%を%CALLNAME:MASTER%は慌てて制止した
					CASE 2
						PRINTFORML 「いちいち許可取る必要なんてないさ」
						PRINTFORML 「あんたが撫でたきゃ、撫でりゃいい」
						PRINTFORMDW %CALLNAME:TARGET%は涼しい顔で酒を呷っている……
						BASE:酒気 += 20
				ENDSELECT
			ENDIF
		;思慕
		ELSEIF TALENT:思慕
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の尻を撫でまわした
				PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%からの痴漢を楽しんでいる……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「ほぉー、結構いいケツしてるじゃないか」
						PRINTFORML 「こりゃあ、気を付けたほうがいいかもねぇ……」
						PRINTFORMDL 痴漢行為の代償として%CALLNAME:MASTER%は%CALLNAME:TARGET%に尻を揉みしだかれている
						PRINTFORMDW ニヤニヤと笑う%CALLNAME:TARGET%はまるでセクハラオヤジのようだ……
					CASE 1
						PRINTFORML 「%MASTERNAME:64%の手癖の悪さにも困ったもんだよ」
						PRINTFORMDL そう言う%CALLNAME:TARGET%だが、あまり嫌がっているようには見えない
						PRINTFORMDW むしろ、悦んでいるようにさえ見える……
					CASE 2
						PRINTFORML 「そんなみみっちい撫で方してないでさぁ」
						PRINTFORMDL %CALLNAME:TARGET%が%CALLNAME:MASTER%の手を掴み、尻を鷲掴みにさせる
						PRINTFORMW 「もっと堂々と撫でな。それでこそ男ってもんだろ」
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の尻をサワサワと撫でた
				PRINTFORMDL %CALLNAME:TARGET%は時折くすぐったそうに身を捩っている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「尻で思い出した。温泉では石鹸を落とさないように気を付けな？」
						PRINTFORML 「特に、あんたみたいな色男はな！ あっはっは！」
						PRINTFORMDW 理由は分からないが、地底の温泉街に行くときは気を付けよう……
					CASE 1
						PRINTFORML 「溜まってるなら地底で綺麗所でも探してきなよ」
						PRINTFORML 「地底の野郎共は酒さえありゃそれでいいような連中ばかりだから……」
						PRINTFORMW 「『意外と』男に飢えてるヤツは多いんだよ」
					CASE 2
						PRINTFORML 「まだ触ってんのか？ 飽きないねぇ」
						PRINTFORML 「あんまりしつこいと、あんたの尻も触っちまうよ？」
						PRINTFORMDW %CALLNAME:TARGET%は手をワキワキと動かしている……
				ENDSELECT
			ENDIF
		;それ以外
		ELSE
			;ムードが高まってる時
			IF BASE:ムード >= 1000
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の尻をそっと撫でた
				PRINTFORMDL 痴漢されているというのに%CALLNAME:TARGET%は平然としている……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「ほら、もっと触りなよ。怒りゃしないからさ」
						PRINTFORML 「鬼に欲情するヤツなんて今時そう居ないからね」
						PRINTFORMW 「あんたみたいな命知らずを見てると、気分が良くなるんだよ」
					CASE 1
						PRINTFORML 「――恥ずかしくないのかって？」
						PRINTFORML 「どうして？ ただ尻を揉まれてるだけだろ？」
						PRINTFORMDW 鬼の羞恥心というのを%CALLNAME:MASTER%はいまいち掴み切れていなかった……
					CASE 2
						PRINTFORML 「酔っ払った鬼だってこんな真似しやしないよ」
						PRINTFORML 「――ふふん、褒めんてんのさ。あんたの度胸をね」
						PRINTFORMDW 痴漢されているというのに、%CALLNAME:TARGET%はなんだか嬉しそうだ……
				ENDSELECT
			;それ以外
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の尻に手を伸ばした……
				PRINTFORMDW 
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「っと、いきなり撫でんじゃない」
						PRINTFORML 「酒が零れちまった……あー勿体ない」
						PRINTFORMDW 不意打ち気味に尻を撫でたので%CALLNAME:TARGET%に怒られてしまった
					CASE 1
						PRINTFORML 「――あん？ どうしたんだ手なんか伸ばして」
						PRINTFORMDL %CALLNAME:TARGET%の尻を撫でようとしたが……既の所でバレてしまった
						PRINTFORML 「握手がしたい？ なんだ、ならハッキリそう言ってくれりゃよかったのに！」
						PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%と固い握手を交わした……
						CALL CHANGE_CFLAG(2, 64, 10)
					CASE 2
						PRINTFORML 「こないだ呑み屋で呑んでる時に酔った野郎に浣腸されてさぁ」
						PRINTFORML 「どうしたかって？ 胃の中空っぽになるまで腹ぶん殴ってやったよ！」
						PRINTFORMDW ……尻を触るのはやめておこう……
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
