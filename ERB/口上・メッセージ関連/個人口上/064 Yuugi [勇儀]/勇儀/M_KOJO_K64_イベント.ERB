;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K64_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF !TCVAR:64:今日会ったか
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 「%MASTERNAME:64%！ ほら、こっち来なよ！」
				PRINTFORMDL %CALLNAME:MASTER%が挨拶すると、%CALLNAME:TARGET%が嬉しそうに手招きしてきた
				PRINTFORMW 「今日はどうする？ %MASTERNAME:64%がしたいこと言ってごらん」
			;思慕
			ELSEIF TALENT:思慕
				PRINTFORML 「お、%MASTERNAME:64%。そっちから会いに来てくれるとはね」
				PRINTFORMW 「そろそろ顔を出しに行こうと思ってたんだが、手間が省けて助かったよ」
			;それ以外
			ELSE
				PRINTFORML 「おや、%MASTERNAME:64%じゃないか」
				PRINTFORMW 「私に何か用かい？」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF !TCVAR:64:今日会ったか
			;お酒くれるイベント
			IF ABL:親密 >= 3 && !TALENT:妊娠 && !TALENT:育児中 && !RAND:5
				PRINTFORML 「お、いたいた」
				PRINTFORMDL %CALLNAME:MASTER%を見つけた%CALLNAME:TARGET%が酒瓶を手に近付いてきた
				PRINTFORML 「実は、良い酒が手に入ったんだが……一人で呑むのも寂しくってね」
				PRINTFORMW 「良かったら一緒に呑まないか？」
				CALL GET_ITEM("鬼殺し", 1, 1)
			;恋慕
			ELSEIF TALENT:恋慕
				PRINTFORML 「おお！%MASTERNAME:64%！ そこに居たか！」
				PRINTFORMW 「姿が見えないから心配したよ！」
			;思慕
			ELSEIF TALENT:思慕
				PRINTFORML 「よっ！ %MASTERNAME:64%」
				PRINTFORMW 「暇だからね、遊びに来たよ」
			;それ以外
			ELSE
				PRINTFORMW 「おう、%MASTERNAME:64%。邪魔するよ」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「おう、%MASTERNAME:64%。湯加減はどうだい？」
		PRINTFORMDL %CALLNAME:TARGET%は前を隠そうともせずにズカズカと風呂に入ってきた
		PRINTFORML 「なんだいその顔は？」
		PRINTFORMW 「お互い、もう恥ずかしがるような仲でもないだろ？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「っと、悪いね。入ってるとは思わなかった」
		PRINTFORML 「――あん？ 良かったら一緒に？」
		PRINTFORML 「悪かないけど……今日はやめとくよ」
		PRINTFORMDW %CALLNAME:TARGET%は「どうぞごゆっくり」と言うと風呂の戸を閉めた
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「なんだ、まだ入ってたのか？」
		PRINTFORMW 「後がつかえてるんだ、さっさと出な」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF !TCVAR:64:今日会ったか
			;親密３以上で妊娠も育児もしてない場合は一定確率で酔ってる
			IF ABL:親密 >= 3 && !RAND:5 && (!TALENT:妊娠 || !TALENT:育児中)
				PRINTFORML 「ん～？ ああ！%MASTERNAME:64%じゃないか！」
				PRINTFORML 「悪いなあ、さっきまで呑んでてさぁ。一瞬誰か分からなかったわ」
				PRINTFORML 「そうだ、駆けつけ三杯ってわけじゃあないが、これから一緒に呑まないか？」
				PRINTFORMW 「なに、遠慮するなって！酒なら文字通り吐いて捨てるほどあるからさ！」
				BASE:酒気 += 2000
				TALENT:64:機嫌 = 1
			;恋慕
			ELSEIF TALENT:恋慕
				PRINTFORML 「おお！%MASTERNAME:64%じゃないか！」
				PRINTFORMW 「どうしたんだいこんな所で」
			;思慕
			ELSEIF TALENT:思慕
				PRINTFORML 「%MASTERNAME:64%、よく来たね」
				PRINTFORMW 「茶は出せないけど、酒ならいくらでも出せるよ」
			;それ以外
			ELSE
				PRINTFORML 「ああ、あんたか」
				PRINTFORMW 「こんなところで会うとは奇遇だね」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF !TCVAR:64:今日会ったか
			;一回だけ地底案内
			IF CFLAG:MASTER:現在位置 / 100 == 9 && !CFLAG:64:1003
				PRINTFORML 「おっと、お二人さん、今日はどこへ行くか決まってるのかい？」
				PRINTFORML 「――って、男と女が寄り添って歩いてるんだ。行くとこなんか決まってるか」
				PRINTFORML 「温泉はあっち。連れ込み宿はそっちだよ。土産を買うなら表の店はぼったくりが多いから気を付けな」
				PRINTFORMW 「それじゃあ邪魔者は退散するよ。怨霊集まる最凶の温泉街でどうぞごゆっくり」
				CFLAG:64:1003 = 1
			;通常
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「おっ、%MASTERNAME:64%じゃないか。逢引でもしてるのかい？」
						PRINTFORML 「はっはっは！ 何もそんな顔しなくても！」
						PRINTFORML 「お二人さんの仲を邪魔するほど私も野暮じゃないさ」
					CASE 1
						PRINTFORML 「仲が良いのはいいことだ」
						PRINTFORML 「楽しんできな。しっぽりとね」
					CASE 2
						PRINTFORML 「おや、%MASTERNAME:64%と……へぇ」
						PRINTFORML 「邪魔しちゃ悪いか。また今度話を聞かせておくれよ」
				ENDSELECT
				PRINTFORMDW %CALLNAME:TARGET%はそう言うと、ヒラヒラと手を振り離れていった
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
	;ARG:1にデート相手、ARG:1が0の場合は非デート
	;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1とデート中
		IF ARG:1
		;一人で外出
		ELSE
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K64_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			GOTO YUGI_OHAYO
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF !TALENT:妊娠 && !TALENT:育児中 && !RAND:5
				PRINTFORML 「おはようさん、よく眠れたかい？」
				PRINTFORML 「%MASTERNAME:64%に用があったんだけど、寝てたからね」
				PRINTFORML 「こうして呑んで待ってたってわけさ」
				PRINTFORMDL %CALLNAME:TARGET%の周りには酒瓶がいくつも転がっている
				PRINTFORMDW ……後片付けが大変そうだ
				BASE:酒気 += 500
			ELSE
				PRINTFORML 「おはよう！ ようやくお目覚めかい？」
				PRINTFORMW 「……って、まだ寝惚けてるようだね？ ほら、水だよ」
				PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%に礼を言い、水を呷る
				PRINTFORMDW ブーーーッッ！！！ 水だと思っていたそれは、なんと酒だった！
				PRINTFORML 「あっはっはっ！ バッチリ目が覚めたようだね！」
				PRINTFORMW 「人間にはちょっとばかし強い酒だ。良い気付けになったろう？」
				BASE:MASTER:酒気 += 100
			ENDIF
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		$YUGI_OHAYO
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ああ、起きたか」
				PRINTFORMW 「着替えは用意してあるよ。さっさと顔洗ってきな」
			CASE 1
				PRINTFORML 「ん、おはようさん」
				PRINTFORML 「そろそろ起きると思ってた。飯の用意できてるよ」
				PRINTFORMDL %CALLNAME:MASTER%がまるで奥さんみたいだと言うと、%CALLNAME:TARGET%は恥ずかしそうに頬を染めた
				PRINTFORMW 「そう言われると照れるね……けど、悪い気はしないよ」
			CASE 2
				PRINTFORML 「ふふ、ようやくお目覚めかい？」
				PRINTFORMDL %CALLNAME:MASTER%が目を覚ますと、%CALLNAME:TARGET%と目が合った
				PRINTFORML 「それより、さっさと離してくれないか？」
				PRINTFORML 「私はまあ良いんだが……誰かに見られたらマズいだろう？」
				PRINTFORMDL どうやら、いつの間にか%CALLNAME:TARGET%を抱き枕にして寝ていたらしい
				PRINTFORMW 「まったく、鬼を抱き枕にして寝るなんて…古今東西、%MASTERNAME:64%くらいなものだよ」
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「ぐぅ……すぴー……」
		PRINTFORMDL %CALLNAME:TARGET%はまだ寝ているようだ
		PRINTFORMDW %CALLNAME:MASTER%は彼女を起こさないように、そっと布団から這い出た
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;共通
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ふあぁ～……良く寝た」
				PRINTFORMDL %CALLNAME:TARGET%は『うーん』と背伸びをすると、顔をパチンと叩いた
				PRINTFORMW 「よし、起きるか」
			CASE 1
				PRINTFORML 「ぁぁ……%MASTERNAME:64%、起きてたのか……」
				PRINTFORML 「……私はもう少し寝かせてもらうとするよ……」
				PRINTFORMDL %CALLNAME:MASTER%は再び寝息を立て始めた%CALLNAME:TARGET%の角をガシッと掴み、グワングワンと顔を揺すってみた
				PRINTFORMW 「うわ、ちょっ！ 起きる！ 起きるからやめろ！」
			CASE 2
				PRINTFORML 「ふご……ん、ぁぁ……もう朝か……？」
				PRINTFORMDL 大いびきをかいていた%CALLNAME:TARGET%が目を覚ました
				PRINTFORMDL 服は大いに乱れ、%CALLNAME:MASTER%はとても目のやり場に困ってしまう
				PRINTFORML 「ふあぁ～～……んー、ふぅ……」
				PRINTFORMW 「うっし、そろそろ起きるか」
		ENDSELECT
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K64_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰って寝る（～は【現在地】から【キャラの自室】に戻りました）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF ARG:1
			;デート中に眠くて帰って寝る（拠点キャラのみ）
			PRINTFORMW 
		ELSEIF CFLAG:うふふ
			;うふふ中に眠くて自室に帰って寝る（拠点キャラのみ）
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている（～は眠そうにしている）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……んー……起きてるよー……」
				PRINTFORMDW %CALLNAME:TARGET%はとても眠そうだ
			CASE 1
				PRINTFORML 「…ッ、クソ…まぶたが重い……」
				PRINTFORMDW 力自慢の%CALLNAME:TARGET%でもまぶたは鍛えていなかったようだ
			CASE 2
				PRINTFORML 「……ん、あぁ……なんて？」
				PRINTFORMW 「悪いね……ちょっと、眠くてさ……」
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（～は眠っている）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「ぐぅ……ぐぅう……」
				PRINTFORMDW %CALLNAME:TARGET%はオッサンみたいないびきをかいている
			CASE 1
				PRINTFORML 「すぅ……すぅ……」
				PRINTFORMDW %CALLNAME:TARGET%は大人しく眠っている
			CASE 2
				PRINTFORML 「……%MASTERNAME:64%……zzz」
				PRINTFORMDW 寝言で名前を呼ばれ、%CALLNAME:MASTER%はビクッとしてしまった
			CASE 3
				PRINTFORML 「もっと……さけぇ……」
				PRINTFORML 「……………ぐぅ」
				PRINTFORMDW 夢の中で宴会でもしているのだろうか……
			CASE 4
				PRINTFORML 「ふがっ………」
				PRINTFORMDL 不意に%CALLNAME:TARGET%のいびきが止まった
				PRINTFORMDW ……暫くするとまたいびきをかきはじめた
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;イタズラ中
	;記入チェック（=0, 非表示、1, 表示）
	;LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG == 4
		;PRINTFORMW 
		;RETURN 1
	;ENDIF
	;-------------------------------------------------
	;衰弱して寝る（体力500以下,うふふ時はうふふ強制終了）
	;（キャラの自室以外：～を【現在地】から【キャラの自室】に運びました）
	;（キャラの自室　　：～は疲れたと言って、横になりました）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:うふふ
			;うふふ中にダウン
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
