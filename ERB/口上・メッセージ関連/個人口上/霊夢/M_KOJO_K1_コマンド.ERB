;=================================================
;特殊コマンド
;=================================================
;-------------------------------------------------
;180,ローション
;-------------------------------------------------
@M_KOJO_MESSAGE_COM_K1_180
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K1_180_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K1_180_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL

	CFLAG:1500 = 0
	;0.イベントがオフだよ　選択肢が出るよ　　初期値の事情だよ
	;1.イベントがオンだよ　選択肢が出るよ
	;2.イベントがオフだよ　選択肢を出さないよ
	;3.イベントがオンだよ　選択肢を出さないよ
	;4.イベントがオフだよ　選択肢が出るよ　　bit2が真だよ
	
	;1=オン（bit0）　2=選択肢を出さないよ（bit1）
	;4=簡単に条件を付け足せるよ（bit2）
	
	;GETBIT (CFLAG:1500,1)　ここの1は二進数の二桁目の事　間違いやすいよ
	;(CFLAG:1500,1)の数値真(0以外主に1)偽(0)を見るよ
	
	;SETBIT STAIN:1,7　二進数の八桁目を真(主に1)にするよ
	;十進数で言えば　+128されていない場合+128するよ
	
	;CLEARBIT STAIN:1,7　二進数の八桁目を偽(0)にするよ
	;十進数で言えば　既に+128されている場合に限り-128するよ
	
	;INVATEBIT STAIN:1,7　二進数の八桁目を反転させるよ
	;十進数で言えば　+128されていない場合+128　既に+128されている場合-128するよ


	;時間停止フラグがかつ参照する事のbit1(選択肢を出さないよ)が偽なら選択肢が出る
	SIF !FLAG:70 && !GETBIT(CFLAG:1500,3)
		CALL M_KOJO_MESSAGE_COM_K1_180_選択肢

	;テンプレ作者様に心からの感謝を。

	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:時間停止
			;時間稼働中に始動している場合の矛盾解消
			;チョコ分岐
			IF GETBIT(STAIN:1,7)
				PRINTFORML 
				PRINTFORMW %CALLNAME:MASTER%は霊夢にチョコを塗った。
				PRINTFORMW 辺りに甘い香りが漂う。
				PRINTFORMW %CALLNAME:MASTER%は深呼吸して、その甘い匂いを堪能した……
			;それ以外　時間停止ローション
			;時間停止塗り塗り分岐は無い　きっとそれが正義
			ELSE
				PRINTFORML 
				PRINTFORMW %CALLNAME:MASTER%は手にローションを一掬いした。
				PRINTFORMW 両手を擦り合わせてから、霊夢の肌にローションを垂らした。
				PRINTFORMW とろり、とローションは肌を伝った。
				PRINTFORMW %CALLNAME:MASTER%はそれから、陰部や胸に重点的に垂らし、薄く延ばした。
				PRINTFORMW 霊夢の肌はローションでてらてらと光っている……
			ENDIF
			RETURN 1

		;以降!FLAG:時間停止　「発言」は概ねここから

