;==================================================
;ムシバトル
;ムシバトルの口上は少し特殊で、PRINTを使うのではなく、
;台詞一行のみをRESULTSに入れてください
;ムシの名前は %MUSHI_NAME% で表示できます
;ムシの名前の最長はアレキサンドラトリバネアゲハの全角14文字です
;文字数制限があります。はみ出ないように気をつけてください
;ARGは残りムシ数か順位です
;==================================================
;生○科学研究所ですって？私のログには何もないな(ADD 2022/01/23)
@M_KOJO_MESSAGE_COM_K9_MUSHI_BATTLE(ARGS, ARG)
#DIM 残りムシ数
#DIM 順位
SELECTCASE ARGS
	CASE "試合前"
		;-------------------------------------------------
		;キャラセレクト時,ARGは使えません
		;全角50文字くらいまで
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:4
				CASE 0
					RESULTS = 「あら、こんにちは。私にムシバトルを挑むなんて、勤勉な学生さんね」
				CASE 1
					RESULTS = 「新しい挑戦者(サンプル)ね？」
				CASE 2
					RESULTS = 「好きで(決闘者に)なった訳じゃ　ないわ！」
				CASE 3
					RESULTS = 「遅かったじゃないの！　さ、決闘を始めましょう」
			ENDSELECT
	;		RESULTS = 「ぶっとばしてさしあげますわ！」
		ENDIF
	CASE "戦場に出した"
		;-------------------------------------------------
		;"【（キャラ）】は《（ムシ）》を戦場に出した！」"の直後
		;全角40文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		残りムシ数 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			RESULTS = 「私の%MUSHI_NAME%をお見せしてよ！」
	;		RESULTS = 「%MUSHI_NAME%！キミにきめた！」
		ENDIF
	CASE "攻撃時"
		;-------------------------------------------------
		;"【（キャラ）】《（ムシ）》の『（攻撃名）』！」"の直前
		;全角40文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		残りムシ数 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			RESULTS = 「これが研究の成果よ！」
	;		RESULTS = 「ぶっとばせ！%MUSHI_NAME%！」
		ENDIF
	CASE "倒れた時"
		;-------------------------------------------------
		;"【（キャラ）】《（ムシ）》は倒れた！！"の直後
		;全角40文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		残りムシ数 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			RESULTS = 「何故こんな事に……私は間違っていたというの？」
	;		RESULTS = 「ああっ！私の%MUSHI_NAME%がっ！」
		ENDIF
	CASE "敗退時"
		;-------------------------------------------------
		;"【（キャラ）】敗退！！"の直後
		;全角40文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		残りムシ数 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:3
				CASE 0
					RESULTS = 「私はただ、勝利が知りたかっただけなのに……」
				CASE 1
					RESULTS = 「そんな目で見ないでちょうだい！」
				CASE 2
					RESULTS = 「ぐ、ぐやじぃぃ～～～……！」
			ENDSELECT
	;		RESULTS = 「私のムシが全滅めつめつ……」
		ENDIF
	CASE "タイマン：勝ち"
		;-------------------------------------------------
		;順位は1位
		;全角20文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		順位 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:3
				CASE 0
					RESULTS = 「まぁ私にかかれば、こんなものよ」
				CASE 1
					RESULTS = 「機会があったら、また手合わせしてね」
				CASE 2
					RESULTS = 「これが研究の成果よ！」
			ENDSELECT
	;		RESULTS = 「%CALLNAME:MASTER%よわーい！」
		ENDIF
	CASE "タイマン：負け"
		;-------------------------------------------------
		;順位は2位
		;全角20文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
