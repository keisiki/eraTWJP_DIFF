;デート帰り
;==================================================
@M_KOJO_SPEVENT_K9_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORML 「楽しかったわ、%CALLNAME:MASTER%。また誘ってね」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「今日は誘ってくれてありがとう、%CALLNAME:MASTER%」
		PRINTFORML 「とっても楽しかったから……その、また誘ってくれると嬉しいわ」
		PRINTFORML 「色々あったけど……その辺はもう怒ってないから、ね？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「……その、今日は……ありがと」
		PRINTFORML 「今日ぐらい楽しかったら、また……誘われてやらないこともないわ」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「……ふぅん」
		PRINTFORML （ちょっとは見直してやってもいいけど……まだ許してやれない、わよね）
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

