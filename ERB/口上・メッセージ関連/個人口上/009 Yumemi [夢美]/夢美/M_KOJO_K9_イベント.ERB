			SELECTCASE RAND:2
				CASE 0
					RESULTS = 「何故こんな事に……私は間違っていたというの？」
				CASE 1
					RESULTS = 「せめて、私に理性があるうちに……次は負けないわ！」
			ENDSELECT
	;		RESULTS = 「くやし～！」
		ENDIF
	CASE "チーム戦：勝ち"
		;-------------------------------------------------
		;順位は1位（やられていても）
		;全角32文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		順位 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:2
				CASE 0
					RESULTS = 「この私がついてるんだから、当然よね」
				CASE 1
