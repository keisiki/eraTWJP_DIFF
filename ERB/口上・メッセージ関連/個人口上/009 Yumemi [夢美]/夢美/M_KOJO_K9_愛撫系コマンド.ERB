;========================================================
;9,自慰
; 我が実装に、一片の悔い無しィ……ッ!!ぬわーーっっ！！
;========================================================
@M_KOJO_MESSAGE_COM_K9_9
CALL M_KOJO_MESSAGE_COM_K9_9_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K9_9_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		; オナニーを覗いたことがあったらちょっとMSG変わるよ
		IF CFLAG:11 == 1
			PRINTFORML 「ちょ……え、み、見たいって……あんた、前にその、私の、見たわよね……？あれじゃ不満……？」
			PRINTFORML 「それに、その、見せるものでもないワケだし……ね？考え直さない？\/\/\/」
			PRINTFORML 「え、こういう状況だからこそ、改めてじっくり見てみたい？」
			PRINTFORML 「%CALLNAME:MASTER%って時々、本気で頭おかしいんじゃないのって思うのよね……」
			PRINTFORML 「はぁ……しょうがないわね……」
			PRINTFORMDW 深々とため息をついた夢美の指が胸と秘所へと向かい、ゆっくりと動き始める。
			PRINTFORML 「うっ……んんっ……は……\/\/\/」
			PRINTFORMDW 徐々に興が乗ってきたのか、手つきがより快感を得られるような動きへと変わる。
			PRINTFORML （%CALLNAME:MASTER%が見てる……そんなにじっくり見なくても……）
			PRINTFORMDW しかし%CALLNAME:MASTER%が見ていることが少なからず夢美の興奮も高める結果になっているようだ。
			PRINTFORML 「あっ、んぅっ、あふ……いやっ、なんでいつもより……そんな、まじまじ見るなぁ……ぁ\/\/\/」
		ELSE
			PRINTFORML 「いっ！？ちょ、ちょっと%CALLNAME:MASTER%、あなた正気で言ってるの？！」
			PRINTFORMDW おれは　いつでも　しょうきだ　！
			PRINTFORML 「だだだだだだだって、お、おなにー……よ？ひ、人に見せるものじゃ、ない、でしょ……？\/\/\/」
			PRINTFORMDW 実はシたことがないのか、と問う%CALLNAME:MASTER%
			PRINTFORML 「ばっ……！そんなわけないでしょ？！自慰の一つや二つぐらいするわよ！……ぁ……\/\/\/」
			PRINTFORML 「あっ、ぁーー……あるけど、あるけど！よ！見せるものじゃないでしょ、って！\/\/\/」
			PRINTFORMDW 夢美のそれだから見たいんだと%CALLNAME:MASTER%が拝み倒すと、
			PRINTFORML 「〜〜〜っ……ほんと、意味分からないんだから\/\/\/」
			PRINTFORML 「わ、わかったわよ……す、すればいいんでしょ……！\/\/\/」
			PRINTFORMDW そう言って顔まで真っ赤にした夢美は、おぼつかない手つきで自分を慰め始めるのだった。
		ENDIF
		RETURN 1
	ENDIF
	;通常の処理
	;ここには各ステータスの状態に応じた口上を表示
	IF TALENT:73 == 1
		;[自慰狂い]を持っている場合(TALENT:73 == 1)
		CALL HPH_PRINT,@"「え？お、おなにー……していいの？HPHHPH」","W"
		CALL HPH_PRINT,@"目にHPHを浮かべた夢美はすぐさま蕩けた表情で自らの身体をまさぐり始め、","W"
		PRINTFORMDW 秘所からぐちゃぐちゃと水音を響かせはじめる。
		CALL HPH_PRINT,@"「ああっ、すごいっHPHきもちいいっHPHゆめみ、おなにーだいすきなのぉHPH」","W"
		CALL HPH_PRINT,@"「まいにちっ、こうしてぇ、くちゅくちゅしちゃうくらい……HPHおなにー、がまんできないのぉっHPH」","W"
		CALL HPH_PRINT,@"「だからぁ、わたしがおなにーでイくとこ……いっぱい見ててね、%CALLNAME:MASTER%HPHHPHHPH」","W"
		CALL M_KOJO_MESSAGE_MASTERBATION_K9
	ELSEIF ABL:14 >= 6 && ABL:30 >= 4
		;露出癖と自慰中毒が高い(Lv>=6 & Lv>=4)場合
		PRINTFORMDW %CALLNAME:MASTER%に弄っている箇所がよく見えるような姿勢を取った夢美が、激しく自慰に耽っている。
		PRINTFORMDW その表情は%CALLNAME:MASTER%に見せているという興奮と、オナニーの快感で蕩けきっている。
		CALL HPH_PRINT,@"「はぁぁっHPHいいっ、良いっ、キモチイイっ、%CALLNAME:MASTER%っ！HPH」","W"
		CALL HPH_PRINT,@"「ねぇっ、あぁっ、ねぇ、んんっ、見てくれてる？見えてる？わたしのオナニー、どうかしらっHPH」","W"
		CALL HPH_PRINT,@"「あぁっ、ダメぇ、気持ちよすぎてぇ、ゆびが止まんないよぉっHPH%CALLNAME:MASTER%っHPH」","W"
		CALL M_KOJO_MESSAGE_MASTERBATION_K9
	ELSEIF ABL:30 >= 4
		;自慰中毒のみ高めの(Lv>=4)場合
		CALL HPH_PRINT,@"「うはぁっ、良いっ、気持ちいいのぉっ！こんな、見られてるっ、のにぃHPH」","W"
		PRINTFORMDW 自慰の快楽に浸ってしまった夢美は%CALLNAME:MASTER%に見られているにもかかわらず、
		PRINTFORMDW 両手を激しく動かしてオナニーを続けている。
		PRINTFORMDW その秘所からはぐちゅぐちゅという水音が響き、潮や愛液が飛び散っている。
		CALL HPH_PRINT,@"「あはぁぁっ、ああっ、あっ、だめっダメぇ、気持ちぃの我慢できないっHPH」","W"
		CALL M_KOJO_MESSAGE_MASTERBATION_K9
	ELSE
		;その他
		PRINTFORML 「ま、またするの……？まったくしょうがないわね……んんぁっ……\/\/\/」
		PRINTFORMDW 夢美は恥ずかしそうにしながらも、自らを慰め始めた。程なくして声に甘いものが混じり始める。
