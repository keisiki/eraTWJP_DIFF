;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K110_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K110_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K110_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「ん……！んん～！」
		PRINTFORMW 「んふっ……お口……犯されちゃった……%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;出す直前
	IF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「んんっ！！！」
			PRINTFORMW サグメは%CALLNAME:MASTER%の射精を感じ取ると喉を使って巧みにそれを促している。
			PRINTFORMW 「じゅる……%UNICODE(0x2665) *1%……んんっ……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ん……？」
			PRINTFORMW サグメは%CALLNAME:MASTER%の射精を感じ取ると喉を使って巧みにそれを促している。
			PRINTFORMW 「じゅる……%UNICODE(0x2665) *1%……じゅぶ……%UNICODE(0x2665) *1%……んちゅ……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「……んぷっ……%UNICODE(0x2665) *1%」
			PRINTFORMW サグメは%CALLNAME:MASTER%の射精を感じ取ると喉を使って巧みにそれを促している。
			PRINTFORMW 「……んん……%UNICODE(0x2665) *1%」
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「んん……%UNICODE(0x2665) *1%」
			PRINTFORMW 「……激しいの……好き……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「っ……じゅぶ……%UNICODE(0x2665) *1%……んんっ……%UNICODE(0x2665) *1%」
			PRINTFORMW 「……喉……%UNICODE(0x2665) *1%……犯して……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「んんっ%UNICODE(0x2665) *1%……んん……%UNICODE(0x2665) *1%」
			PRINTFORMW 「もっと……%UNICODE(0x2665) *1%……喉奥まで犯して……%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「んん……！！」
			PRINTFORMW 「……もっと……ちんちんの味教えて……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「っ……じゅぶ……%UNICODE(0x2665) *1%……んんっ……%UNICODE(0x2665) *1%」
			PRINTFORMW 「……喉……%UNICODE(0x2665) *1%……犯して……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「ん～%UNICODE(0x2665) *1%……んん……%UNICODE(0x2665) *1%」
			PRINTFORMW 「もっと……%UNICODE(0x2665) *1%……穢れ喉奥まで出して……%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「んん……！」
			PRINTFORMW サグメは喉奥までペニスを突きこまれて動揺している。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んん～！」
			PRINTFORMW 「……穢れの臭い……喉に染みついちゃう……」
		ELSE
			PRINTFORMW 「ん～んん……！！！」
			PRINTFORMW サグメは喉奥にペニスを突きこまれて苦しそうに呻いている。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

