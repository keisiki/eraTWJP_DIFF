LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「えっ、あんたがあたしのちんぽを手で…？」
		PRINTFORML 「い、嫌よっ！触んないで！そんなに気安く触ってほしくないの！」
		IF TALENT:MASTER:2 >= 2
			PRINTFORML 「そんなにちんぽ触りたいなら自分のでもいじってればいいじゃないっ！」
		ELSE
			PRINTFORML 「自分に無いからってあたしのちんぽいじるのは意味わかんないわよっ！ほかの奴のじゃだめなの！？」
		ENDIF
		PRINTFORML 「ちょっ、ほんとやめてよ！…んっ…」
		PRINTFORML %CALLNAME:MASTER%は抵抗する%CALLNAME:TARGET%をあしらって、%CALLNAME:TARGET%の%TINKO(TARGET)%を手で握った…
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		;連続
		IF PREVCOM == SELECTCOM
			PRINTFORML 「んっ%UNICODE(0x2764)%んっ%UNICODE(0x2764)%ふっ%UNICODE(0x2764)%きもちーよぉ%UNICODE(0x2764)%%CALLNAME:MASTER%…%UNICODE(0x2764)%」
			PRINTFORML 「あたし、大好きな%CALLNAME:MASTER%にちんぽシゴいてもらってるぅっ%UNICODE(0x2764)%シコシコしてもらっちゃってるぅ%UNICODE(0x2764)%」
			PRINTFORML 「ちんちん付いててよかったっ%UNICODE(0x2764)%かわいいって言われながら、ちんちんよしよししてもらえる身体でよかったっ%UNICODE(0x2764)%%UNICODE(0x2764)%」
			PRINTFORML %CALLNAME:TARGET%はペニスをシゴかれ、恍惚とした表情で快楽を貪っている…
		ELSE
			PRINTFORML 「あっ%UNICODE(0x2764)%またあたしのちんぽ、シコシコしてくれるの？%UNICODE(0x2764)%」
			PRINTFORML 「あんたの手コキ、やさしくて気持ちよくて好きよ…%UNICODE(0x2764)%ちんぽ触ってもらうと安心するの…%UNICODE(0x2764)%」
			PRINTFORML 「さ、早く触って%UNICODE(0x2764)%よしよし撫で撫でしてっ%UNICODE(0x2764)%」
			PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の手によってもたらされる快楽に期待し、%TINKO(TARGET)%を差し出している
			PRINTFORML すっかり手淫の虜になった%CALLNAME:TARGET%に苦笑し、%CALLNAME:MASTER%は差し出されたペニスをやさしく愛撫し始めた
			RETURN 1
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORML 「あの、さ…もし嫌じゃなかったら…またあの、手でしゅっしゅ…ってやるの、して欲しいんだけど…」
		PRINTFORML 「気持ちよかったのか…って…？」
		PRINTFORML 「……うん…、しょーじきすごく気持ちよかったわ…ょ…」
		PRINTFORML %CALLNAME:TARGET%はもじもじと小さく腰を振りながら、顔を赤らめて手淫してほしいと懇願してきた
		PRINTFORML 「そ、それで…してくれる…？%CALLNAME:MASTER%の手で、気持ちよくしてほしいの…%UNICODE(0x2764)%」
		PRINTFORML %CALLNAME:MASTER%は涙目になって手淫を懇願する%CALLNAME:TARGET%に苦笑し、差し出されたペニスを愛撫し始めた
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「またあたしのちんぽが触りたい…って…、えぇー……」
		PRINTFORML 「あんたあたしのこと、男の子だと思ってない？…ちゃんと女の子なのよ！…ちんぽ付いてるけど…」
		RETURN 1
	ENDIF
ENDIF
RETURN 1
