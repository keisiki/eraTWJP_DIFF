;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K107_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K107_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K107_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			CALL HPH_PRINT,@"「んん…♪ちゅぶっ…HPH ん…ろう…？ひもちいい…？」","L"
			PRINTFORMDL 刺激が足りない―%CALLNAME:MASTER%は清蘭のウサ耳を掴んで強引に頭を引き寄せ、喉奥に肉棒を押し込んだ。
			PRINTFORML 「ん゛ぶぅっ！？んぐっ……！ん、むぅぅっ！！」
			PRINTFORMDW 喉奥でペニスを締め付けられる快感を覚えた%CALLNAME:MASTER%は、更なる快感を得ようと腰を動かした…！
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ん゛！？んぅー！！ん゛ーんんー！」
					PRINTFORMDL "イキそうだ"と告げると、清蘭は嫌がるように首を振りながら何かを%CALLNAME:MASTER%に訴えた。
					PRINTFORMDL 射精さないで、とでも言っているのだろう…%CALLNAME:MASTER%は無視して頭を押さえつけ、喉奥に精液を流し込んだ。
					PRINTFORML 「ん゛ぎゅ……！ご…ぶっ…！ん゛……ぐ……」
					PRINTFORMDW 口からペニスを引き抜くと、清蘭は前のめりに倒れて失神してしまった……
				CASE 1
					CALL HPH_PRINT,@"「ん゛ぶう゛うぅぅっ……HPH…んぐ…ご…きゅっ…HPH…ぅ…ぶはっ！！」","L"
					PRINTFORMDL 清蘭の喉奥に射精していたが、流石に苦しくなったのか突き飛ばす様に口からペニスを抜いた。
					PRINTFORML 「う゛え゛ぇっ…！げほっ！げほっ…！ごほっ…！」
					PRINTFORML 「はぁ…はぁ…何でこんな…乱暴するの……？」
					PRINTFORMDW 清蘭は怯えたような眼で%CALLNAME:MASTER%を見ている……
				CASE 2
					;長くてすみません
					CALL HPH_PRINT,@" 「う゛ぐっ！んっ！んんっHPH んぶぅっHPH」","L"
					PRINTFORMDL 喉奥に突き込みながら、"そろそろ射精すよ"と清蘭に告げる。
					PRINTFORML 「！？―ん゛っ…ぶはっ！%CALLNAME:MASTER%待ってよ！急にどうしちゃったの！？」
					PRINTFORMW 「いつもみたいに……優しくしてよぉ……っ！」
					PRINTFORMDL 流石にやりすぎたのか、清蘭が座り込んで泣き出してしまった…"ごめん"と謝り、清蘭の頭を優しく撫でる…
					CALL HPH_PRINT,@"「……%CALLNAME:MASTER%…いいよ♪…許したげるHPH」","W"
					PRINTFORML 「―ふぇっ！？や、止めっ…！んぐぅぅぅっ！」
					PRINTFORMDL 清蘭の気が緩んだ一瞬の隙を突いてウサ耳を掴み、射精寸前だったペニスを再び喉奥に押し込んで精液を叩き込んだ。
					CALL HPH_PRINT,@"「ん゛っ…HPH ん゛う…！ごきゅ……ごくっ…HPH」","L"
					PRINTFORMDW 精液を飲み干すと、清蘭は糸の切れた人形の様にだらりと倒れ込んで静かに涙を流した……
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ん゛ん……！う゛ぅ……じゅ…っ…ぅ」
					PRINTFORML （わたし、なにしてたんだっけ…？あたま…ボーっとしてきた……）
					PRINTFORMDW 清蘭は%TEXTR("頭/ウサ耳")%を掴まれて激しく揺さぶられた為か、意識が朦朧としているようだ。
				CASE 1
					PRINTFORML 「ん゛ぅっ！ん゛ぐっ！う゛っ！ん゛っ！ん゛んんー！」
					PRINTFORMDW 清蘭の%TEXTR("頭/ウサ耳")%を掴んで、何度も喉奥を突き込んだ。
				CASE 2
					PRINTFORML 「う゛っ……お゛ぉっ…！んん…んぶっ…！え゛ぅぅっ…！」
					PRINTFORMW 清蘭は目尻に涙を浮かべながら、%CALLNAME:MASTER%のピストンに耐えている……
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

