@M_KOJO_MESSAGE_COM_K107_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;挿入中分岐
	IF LOCAL:1 && (TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER)
		;Ｃ＆Ｂ感覚が一定以上かつＶ挿入中ならＶ感覚５以上Ａ挿入中ならＡ感覚４以上
		IF ABL:Ｃ感覚 >= 5 && ABL:Ｂ感覚 >= 5 && ((TEQUIP:50 == PLAYER && ABL:Ｖ感覚 >= 5) || (TEQUIP:51 == PLAYER && ABL:Ａ感覚 >= 4))
			;射精した
			IF NOWEX:PLAYER:11
				CALL SEIRAN_SEX
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMDL %CALLNAME:MASTER%は清蘭の腰を掴んで抽挿をしていたが、不意に手を離して彼女の肢体を愛撫した。
						CALL HPH_PRINT,@"「あっHPH んっHPH んんぅっHPH %CALLNAME:MASTER%っHPH 気持ちいいっHPH」","L"
						PRINTFORMDW 内と外から同時に攻め立てられ、清蘭は身体をガクガクと震わせる…！
					CASE 1
						CALL HPH_PRINT,@"「あうぅっHPH おちんちんっHPH おくまで、きてるぅ…HPH」","L"
						CALL HPH_PRINT,@"「もっとHPH きもちいいのっHPH 欲しいHPH いっぱい欲しいぃHPH」","L"
						PRINTFORMDL %CALLNAME:MASTER%の肉棒だけでは飽き足らず、清蘭は自分で陰核をいじり始めた。
						PRINTFORMDL 貪欲に快楽を求める清蘭に興奮した%CALLNAME:MASTER%は、激しく抽挿しながら彼女の胸を丹念に愛撫した。
						CALL HPH_PRINT,@"「うあ゛あぁぁHPH HPH ど、どうじにされたらぁっHPH お゛がしぐなるぅぅぅHPH HPH HPH」","W"
					CASE 2
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%…HPH もっと、触って…？HPH」","L"
						PRINTFORMDL %CALLNAME:MASTER%は抽挿を続けながら、清蘭を愛撫した。
						PRINTFORMDL 耳元に顔を近づけ、"好きだよ"と囁く。
						CALL HPH_PRINT,@"「～～～ッHPH HPH ………もっと言って…HPH」","W"
				ENDSELECT
			ENDIF
			RETURN 1
		;それ以外
		ELSE
			;射精した
			IF NOWEX:PLAYER:11
				CALL SEIRAN_SEX
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ふぇっ！？ああっHPH それ、ダメぇっHPH 感じすぎちゃう、からぁHPH」","L"
						PRINTFORMDW 抽挿しつつ、清蘭の身体を愛撫した。
					CASE 1
						CALL HPH_PRINT,@"「んぅっHPH あHPH あぁっHPH あ…HPH……%CALLNAME:MASTER%？」","L"
						PRINTFORMDL 突然腰の動きを止めた%CALLNAME:MASTER%を清蘭は不安そうに見つめる。
						PRINTFORMDL 乳首や陰核を丹念に愛撫し、清蘭に新たな快感を与えていく…
						CALL HPH_PRINT,@"「んんぅHPH ダメぇ…HPH 今は、びんかんになってる、からぁ…HPH」","L"
						CALL HPH_PRINT,@"「…えっ！？まさか、このまま動―あぁっHPH あんっHPH ふぁああっっHPH HPH」","W"
					CASE 2
						CALL HPH_PRINT,@"「んぅ～～～ッHPH HPH HPH」","L"
						PRINTFORMDL ペニスを清蘭に突き入れながら、ウサ耳へと息を吹きかける。清蘭はゾクゾクと身悶えした。
						PRINTFORMDW 気持ちいいかと聞くと、清蘭は顔を赤らめながらコクリと頷いた。
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;Ｃ感覚・Ｂ感覚が５以上
	ELSEIF ABL:Ｃ感覚 >= 5 && ABL:Ｂ感覚 >= 5
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「ん…っHPH あぁっ……もっと……っHPH」","L"
				PRINTFORMDL 清蘭の脇腹や太股をつつ…と指でなぞって愛撫していく。
				PRINTFORMDL 清蘭が触って欲しそうな箇所にはあえて触れず、焦らすような愛撫を続けていく…
				CALL HPH_PRINT,@"「うぅ…！…んっ…HPH %CALLNAME:MASTER%…！いつもみたいにもっと触ってよぉ……っ！」","L"
				PRINTFORMDW 切ない声を上げて身体をくねらせる清蘭に"どこを触って欲しい？"と聞いた。
				PRINTFORMW 「……%TEXTR("クリ…/おっぱい…/お…おまんこ…/")%」
			CASE 1
				CALL HPH_PRINT,@"「ふぁぁぁぁ…HPH HPH 耳ぃ…HPH ぞくぞくするぅ……HPH」","L"
				PRINTFORMDL 清蘭のウサ耳にふーっと息を吹きかけた。清蘭は気持ちよさそうに身体を震わせている…
				PRINTFORMDL 度重なる%CALLNAME:MASTER%との情事のお陰か、すっかり敏感になっているようだ。
				PRINTFORMDL ……もっと強い刺激を与えてみよう。%CALLNAME:MASTER%はウサ耳を甘噛みした。
				CALL HPH_PRINT,@"「ぃ――ッHPH HPH HPH あっHPH あぅぅ……HPH」","W"
			CASE 2
				CALL HPH_PRINT,@"「んんぅ…HPH んっ…ふ…HPH ひゃうっHPH」","L"
				PRINTFORMDL お互いに身体を愛撫し合っている内に興奮で汗ばんできた。清蘭の首筋を舐め、汗の味を堪能した…
				CALL HPH_PRINT,@"「―わ、私だって…！ ん…ちぅ…HPH れる…っHPH 」","L"
				PRINTFORMDW 互いの汗を舐め合う…何とも倒錯的なプレイに、二人は興奮が抑えられなくなってきた…！
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「んんっ…んぅっ…！…気持ちいい？って言うか、くすぐったい……」
				PRINTFORMDW まだ他人に愛撫される事に慣れていないのか、むず痒そうに身体を捩っている。
			CASE 1
				CALL HPH_PRINT,@"「んんっ…HPH 耳、舐めちゃ…ダメぇ…っHPH」","L"
				PRINTFORMDW %CALLNAME:MASTER%が清蘭のウサ耳を舐めると、くすぐったそうに身を震わせた。
			CASE 2
				CALL HPH_PRINT,@"「ひゃんっHPH」","L"
				PRINTFORMDL 清蘭の身体をまさぐりながら首筋に舌を這わせた。汗と混ざって少し甘い味がする…
				PRINTFORMW 「は、恥ずかしいから味とか言わなくていいっ！」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1

