				CALL HPH_PRINT,@"（口の中…%CALLNAME:MASTER%の匂いと味で、いっぱい…HPH）","L"
				CALL HPH_PRINT,@"（頭クラクラして…何も考えられなくなっちゃう…HPH）","W"
		ENDSELECT
	ENDIF
;それ以外
ELSE
