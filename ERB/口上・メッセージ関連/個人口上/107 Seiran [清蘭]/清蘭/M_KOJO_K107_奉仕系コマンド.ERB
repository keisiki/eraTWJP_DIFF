@M_KOJO_MESSAGE_COM_K107_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「手で…こするの…？ わかった…えいっ！」
		PRINTFORMDL 瞬間、%CALLNAME:MASTER%のペニスに凄まじい激痛が走った…！
		PRINTFORML 「ご、ごめん！……痛かった？大丈夫？」
		PRINTFORMW 「優しく…そーっと…そーっと…！」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;技巧３以上
	ELSEIF ABL:技巧 >= 3
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「おちんちん膨らんでるHPHもう、射精そうなのねHPH」","L"
					PRINTFORMDL 射精が近い事を察した清蘭は絶頂へ向け、しごくスピードを上げていく。
					PRINTFORMDL 自分の絶頂を他人にゆだねるというのは何とも言えない気分だ…！
					CALL HPH_PRINT,@"「ほらほらっHPH 早くイッちゃえHPHイッちゃえーっHPH」","W"
					
				CASE 1
					CALL HPH_PRINT,@"「あぁ…HPHんっHPHんん…HPH精液…いっぱい…HPH」","L"
					PRINTFORMDL %CALLNAME:MASTER%のペニスから発射された大量の精液が、清蘭の手を汚していく…！
					CALL HPH_PRINT,@"「ふふ…HPHもったいない…HPH ん…じゅるるるHPH」","L"
					PRINTFORMDL 清蘭は手に溜まった精液を嫌な顔一つせず啜っている…
					CALL HPH_PRINT,@"「平気よ…♪ %CALLNAME:MASTER%のだもんHPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「あ…HPH おちんちん…もうイキそうなのね？…よーし…♪」","L"
					PRINTFORMDL 清蘭は少しだけペニスを強く握ると、射精させようと動きを加速させた。
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%っHPH早くイってHPH 穢れた精子いっぱい射精してHPH」","W"
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%…HPH どう？気持ちいい？」","L"
					CALL HPH_PRINT,@"「自分でするのと、私の手でするの…どっちが好き…？HPH」","L"
					PRINTFORMDL もはや自分の手で自慰をしても、ここまでの快楽は得られないだろう…
					PRINTFORMDW "清蘭の手の方がいい"と言うと、清蘭は嬉しそうにゾクゾクと身体を震わせた…！
					
				CASE 1
					CALL HPH_PRINT,@"「おちんちん、こんなにパンパンにしてぇっ…HPHどんだけ穢れ溜め込んでるのよぉっHPH」","L"
					CALL HPH_PRINT,@"「悪いおちんちんは…私が浄化してあげるんだから…HPH」","L"
					PRINTFORMDW 清蘭は手でペニスをしごいて"浄化活動"を行っている……
				CASE 2
					PRINTFORML 「ねえ%CALLNAME:MASTER%…気持ちいい？」
					CALL HPH_PRINT,@"「射精したくなったら、いつでも言ってねHPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
	;それ以外
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「え…イキそう！？ま、待って！ティッシュ―きゃっ！！」
					PRINTFORMDL 我慢しようとしたが間に合わず、清蘭の顔や髪に精液がかかってしまった……
					PRINTFORMW 「うえぇ～…！精液、かかったぁ…！……くさいよぉ…！」
				CASE 1
					PRINTFORML 「おちんちん、膨らんでる…！もう、射精そうなの？」
					PRINTFORML 「もっと早く…？ う、うん！わかった…！」
					PRINTFORMDL 絶頂に向け、清蘭はペニスをしごくスピードを上げていく…やがて%CALLNAME:MASTER%は限界を迎え―
					PRINTFORMW 「わっ、わっ！す、すごい…！精液ってこんなふうに出るんだ……」
				CASE 2
					PRINTFORML 「あ…！射精て……んっ！ぅぅ…！…精液、手に付いちゃった……」
					PRINTFORMW 「…………。」
					CALL HPH_PRINT,@"「……れろっHPH …すごい味…HPH」","W"
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「優しく…優しく…そーっと…そーっと…！」
					PRINTFORMDW 清蘭はもの凄く慎重に%CALLNAME:MASTER%のペニスに触っている……こそばゆい…！
				CASE 1
					PRINTFORML 「つばを垂らして……んぁ……手でしこしこ…しこしこ…！」
					PRINTFORMDL 清蘭はたどたどしい手つきで%CALLNAME:MASTER%の肉棒をしごいている…
					PRINTFORMDW 擬音を口にしながら手淫する姿はどこか可愛らしく、何とも言えない気分にさせる…！
				CASE 2
					PRINTFORML 「%CALLNAME:MASTER%、どう…？気持ちいい？」
					CALL HPH_PRINT,@"「…よ、よかった♪…じゃあこのまま続けるね…HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
