			CASE 72
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「ゆびぃ…HPH お尻に…入ってるぅHPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「ひゃああんっHPH す、すごい…HPH 舌が、ぬるんってぇHPH」","W"
