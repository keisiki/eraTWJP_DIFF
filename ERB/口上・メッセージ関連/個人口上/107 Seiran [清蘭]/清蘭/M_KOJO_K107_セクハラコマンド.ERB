				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あぁっHPH な、なにも今触らなくてもぉっHPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「ちょっ…！今お尻触っちゃ…んんっHPH」","W"
				ENDSELECT
		ENDSELECT
