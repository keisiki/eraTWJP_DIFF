							PRINTFORMDW 清蘭の尻を心ゆくまで撫でさせてもらった。
						CASE 1
							CALL HPH_PRINT,@"「んっ…HPHはぁ…HPHあHPH あぁ…HPHあんっHPH」","L"
							CALL HPH_PRINT,@"「なんか…お尻撫でられたら…HPH んっHPH へ、変な気分にぃっHPH」","L"
							PRINTFORMDW 清蘭は尻を撫でられ、艶のある声を発し始めた…
						CASE 2
							CALL HPH_PRINT,@"「あんっHPHんん…HPHあぅぅ…HPH ん…HPH%CALLNAME:MASTER%…HPH」","L"
							CALL HPH_PRINT,@"「お…お尻撫でるだけで……HPH いいの？HPH」","L"
							PRINTFORMDW 清蘭は少し恥ずかしそうにしつつも挑発的な笑みを浮かべた…
						CASE 3
							CALL HPH_PRINT,@"「ひゃあHPH あHPH ああっHPHし、尻尾ぉっ…HPH HPH」","L"
							CALL HPH_PRINT,@"「%CALLNAME:MASTER%っHPH もっとHPH もっと尻尾こすってぇぇHPH」","L"
							PRINTFORMDW 清蘭は尻尾を撫でられて甘い声を上げている……
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「ひっ…！？い、嫌ぁっ！！」
					PRINTFORMDW 清蘭に引っ叩かれた……
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:3
						CASE 0
							PRINTFORMW 「ちょっ…！何、してんのよぉっ…！」
						CASE 1
							PRINTFORMW 「んっ…！あぁっ…止めなさいってばぁ…！」
						CASE 2
							CALL HPH_PRINT,@"「ひゃんっHPH」","L"
							PRINTFORML 「～～～ッ！」
							PRINTFORMDW 清蘭に睨まれた…あまり尻尾は触られたくないようだ。
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「んん…HPH ま、まだ触るの！？ もういいでしょぉ…HPH」","L"
							PRINTFORMDW 清蘭の尻を堪能させてもらった。
						CASE 1
							PRINTFORML 「ちょ…ふふっ♪く、くすぐったい…！」
							CALL HPH_PRINT,@"「あははは！は…あっ…HPH んん…HPH」","W"
						CASE 2
							CALL HPH_PRINT,@"「ひゅうぅんっHPH し、尻尾はぁっ…HPH」","L"
							CALL HPH_PRINT,@"「はうぅ…！あ……んぅ…HPH」","L"
							PRINTFORMDW 清蘭は尻尾を撫でられるのが弱いようだ。
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

RETURN 1
