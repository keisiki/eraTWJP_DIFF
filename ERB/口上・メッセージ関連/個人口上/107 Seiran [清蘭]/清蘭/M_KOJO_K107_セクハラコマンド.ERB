					CASE 0
						CALL HPH_PRINT,@"「んん…HPH %CALLNAME:MASTER%…HPH お尻じゃなくて…その…HPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「あ…HPH んん…HPH お尻…直接触られてる…HPH」","W"
				ENDSELECT
			;素股
			CASE 52
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あぁっHPH な、なにも今触らなくてもぉっHPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「ちょっ…！今お尻触ったら危ないってばぁHPH」","W"
