@M_KOJO_MESSAGE_COM_K107_330_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMDL 清蘭の後ろに回り込み、スカートをめくった。
				PRINTFORMDW 童心に帰ったような、懐かしい気分がした…
			CASE 1
				;ダンスは…苦手だな
				PRINTFORMDL "な～にっかな？な～にっかな？今日のパンツはこれ！"
				PRINTFORMDW 一人虚しく謎のダンスを踊りながら、清蘭のスカートを捲り上げた…
		ENDSELECT
	ELSEIF CFLAG:うふふ == 2
		CALL HPH_PRINT,@"「えへへ…HPHおちんちん…全部、挿入っちゃった…HPH」","L"
		PRINTFORMDW スカートの下では素晴らしい光景が広がっていた…！
	ELSE
		SELECTCASE TFLAG:193
			;ビンタ
			CASE -5
				PRINTFORML 「いやあぁぁぁ！！」
				PRINTFORMDW パンッと乾いた音が鳴り響いたと思った直後、%CALLNAME:MASTER%の頬に鋭い痛みが走った…
			;つねられる
			CASE -3
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORMW 「%CALLNAME:MASTER%！！もうちょっとその…場所とか考えてっ！」
				ELSE
					PRINTFORMW 「悪い事しようとする手は私が"浄化"してあげよっか？」
				ENDIF
			;嫌がる
			CASE -1
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORMW 「ち…ちょっと！嫌だってば…！」
				ELSE
					PRINTFORML 「…………。」
					PRINTFORMDW "ドン引き"といった表情で%CALLNAME:MASTER%を見ている…
				ENDIF
			;嫌がる（嬉しげ）
			CASE 0
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORMW 「こ、ここじゃちょっと……恥ずかしいから……」
				ELSE
					PRINTFORMW 「――"スカートめくらせて"！？ダメに決まってんでしょ！」
				ENDIF
			;成功
			CASE 1
				IF ABL:露出癖 <= 2
					PRINTFORML 「～～～ッ！！」
					PRINTFORMDW 怒りか羞恥かはわからないが、清蘭は顔を真っ赤にしている…
					;穿いてる
					IF TEQUIP:0 > 1
						PRINTFORML 「い…いつまで見てんの！？もういいでしょ！？」
						PRINTFORMDW 短い時間だったが、清蘭のパンツを視姦させてもらった。
					;穿いてない
					ELSE
						PRINTFORMDL スカートの下には、そこにあるはずのパンツは無かった…
						PRINTFORMW 「何で履いてないのって…そんなのこっちが聞きたいわよぉ～…！」
					ENDIF
				ELSEIF ABL:露出癖 <= 4
					PRINTFORML 「ちょっ…ダメだってば……！」
					PRINTFORMDW 顔を赤らめてはいるが、満更でもなさそうだ…
					;穿いてる
					IF TEQUIP:0 > 1
						PRINTFORML 「……ねぇ…！もういいでしょ…？」
						PRINTFORML 「あとちょっとだけって…もう…！」
						PRINTFORMDW 清蘭の可愛らしいパンツをしっかりと目に焼き付けた。
					;穿いてない
					ELSE
						PRINTFORMDL スカートの下には、そこにあるはずのパンツは無かった…
						PRINTFORML 「……何よ！悪い！？私だって好きでパンツ履いてないわけじゃな――もがっ！？」
						PRINTFORMDW 誰かに気づかれる前に、慌てて清蘭の口を塞いだ……
					ENDIF
				ELSE
					CALL HPH_PRINT,@"「……えっちHPH」","L"
					PRINTFORMDW 清蘭はどこか嬉しそうだ……
					;穿いてる
					IF TEQUIP:0 > 1
						CALL HPH_PRINT,@"（%CALLNAME:MASTER%ったら…あんなに真剣に私のパンツ見ちゃって…HPH）","L"
						PRINTFORMDW 清蘭の可愛らしいパンツを思う存分視姦した。
					;穿いてない
					ELSE
						PRINTFORMDL スカートの下には、そこにあるはずのパンツは無かった…
						CALL HPH_PRINT,@"「え～？いいじゃない♪どうせエッチする時に脱いじゃうんだし…HPH」","W"
					ENDIF
				ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

