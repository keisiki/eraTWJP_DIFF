	ELSEIF CFLAG:睡眠
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「ん…おしり…がぁ……んん…」
			CASE 1
				PRINTFORML 「お尻…触んないでよぉ…むにゃ…」
				PRINTFORMDW 起きたかと思ったが、寝言だったようだ…
		ENDSELECT
	ELSEIF CFLAG:うふふ == 2
		SELECTCASE TCVAR:20
			;クンニ強制
			CASE 70
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「んあっHPH もう…今はお尻触らなくていいのっ…HPH」","W"
