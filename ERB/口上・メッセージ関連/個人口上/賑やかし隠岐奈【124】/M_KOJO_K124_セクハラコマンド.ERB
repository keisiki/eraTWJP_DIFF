;==================================================
@M_KOJO_MESSAGE_COM_K124_310
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K124_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K124_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		PRINTFORML 服の所為で分かりづらいが、隠岐奈の尻はなかなか触り甲斐のある大きさをしている。
		PRINTFORML よく座っている所為だろう。弾力よりも、もにゅっとした肉の柔らかさが印象的だ。
		PRINTFORML 少々だらしなさを感じる尻肉に、むしろ興奮を覚えながら、%CALLNAME:MASTER%は彼女の尻を飽きるまで撫でた…。
	ELSEIF CFLAG:睡眠
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「ん、ぅ……」
				PRINTFORMW もちっと柔らかな肉感が実に堪らない……。
			CASE 1
				PRINTFORML 「んうぅ……やめろ、まい……」
				PRINTFORML 隠岐奈はむにゃむにゃと寝言を言っている…。
			ENDSELECT
	ELSEIF CFLAG:うふふ == 2
		SELECTCASE TCVAR:20
			;アナル奉仕
			CASE 72
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「そ、そんなに熱心に……ひぅっ！」
					CASEELSE
						PRINTFORMW 「ほ、ほら、もっと丁寧に……ひぃんっ」
				ENDSELECT
			;スカートの中を見せる
			CASE 35
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「はぁ…はぁ…さ、触るだけでいい、のか？ふふっ%UNICODE(0x2764)%」
					CASEELSE
						PRINTFORMW 「そんな、いやらしい手つきで…ううっ」
				ENDSELECT
			;素股
			CASE 52
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「あっ、やっ！う、動きづらいだろうっ！？」
					CASEELSE
						PRINTFORMW 「す、少しは我慢できないのか…っ！」
				ENDSELECT
			CASEELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「ひゃっ！？や、やめろ…っ」
					CASEELSE
						PRINTFORMW 「お、お尻なんて触っても……うぅんっ%UNICODE(0x2764)%」
				ENDSELECT
		ENDSELECT
	ELSE
		IF TALENT:124:恋人 || TALENT:124:恋慕
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORMW 「ひゃっ！？こ、こら！やめんか！」
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「そ…そんな、いやらしい手つきで…へ、変態め……！」
						CASEELSE
							PRINTFORMW 「う、うう……やめろと言うに……」
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「なんだ？そんなに興奮して……ふふっ」
						CASEELSE
							PRINTFORMW 「…私のお尻なんて触って、楽しいか？」
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「うわあっ！？ お、お前、いきなり何をする！！」
					PRINTFORMW 隠岐奈は%CALLNAME:MASTER%の手をぱしんと叩いた。
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「ひっ！？や、やめろ！変態！！」
						CASEELSE
							PRINTFORMW 「馴れ馴れしく触るな…っ」
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「う、うぅ……」
							PRINTFORMW 隠岐奈は羞恥で顔を真っ赤にしている。
						CASEELSE
							PRINTFORMW 「こ、今回だけは許してやる……」
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

RETURN 1
