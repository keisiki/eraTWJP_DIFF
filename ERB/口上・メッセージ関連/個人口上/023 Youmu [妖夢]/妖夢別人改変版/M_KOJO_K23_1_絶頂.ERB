				;口からダラダラとこぼれでている
					SELECTCASE RAND:(2 + (ABL:精液中毒 >= 5) * 3)
						CASE 0
							PRINTFORML 「んぁ、あぷ……こ、こんなに……」
							CALL HPH_PRINT,@"「申し訳ありません、とんだ粗相を……HPH」","W"
						CASE 1
							PRINTFORML 「んっ、んむぅ……っ、ぷあ……！」
							PRINTFORMW 「い、一気には、飲みきれませんね……けほっ」
						CASE 2
							PRINTFORML 「ああ、たくさん零してしまいました……」
							CALL HPH_PRINT,@"「折角出して下さったのに、勿体ない……ちゅぷ、れろ、ぢゅるHPH」","W"
						CASE 3
							CALL HPH_PRINT,@"「ふぅ、はぁ……零してしまったのは私ですから、お掃除しないと……HPH」","L"
							PRINTFORMW 妖夢は肢体を汚した白濁を指先で掬い、じっくりと味わうように丁寧に舐め取っていった……
						CASE 4
							PRINTFORML 「はあぁ……臭くて、不味くて、喉に張り付くと知っているのに……」
							CALL HPH_PRINT,@"「これを見ていると、酷く渇いて……れろ、ん……じゅるるるるHPH」","W"
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ELSE
		IF (EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3) && EXP:精飲経験 >= 20
			IF ABL:精液中毒 >= 5 && RAND:3 == 0
				PRINTFORML 「んっ……んく、ふっ……こく……」
				PRINTFORMW （不味いのに……離せない……）
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「んぐ……うえぇ……相変わらず酷い味です……」
						PRINTFORMW （味自体には慣れてしまった……）
					CASE 1
						PRINTFORML 「うっ……むぐ……っ、はぁ……」
						PRINTFORMW 「……零したら周りが更に汚れるじゃないですか」
				ENDSELECT
			ENDIF
		ELSE
			IF TCVAR:TARGET:精飲経験 == 0
				PRINTFORML 「んぐ、むぅ！？　……うぇ、げほ、げほっ」
				PRINTFORMW 「なんですかこれ……飲ませようだなんて正気じゃありませんよ……」
			ELSEIF TCVAR:TARGET:精飲経験 < 20
				PRINTFORML 「げほっ、うえ……まずぅ……」
				PRINTDATAW
					DATAFORM （む、惨いとしか言えない後味……）
					DATAFORM （口移しででも飲ませてやりたい……ッ）
				ENDDATA
			ELSE
				IF ABL:精液中毒 >= 5 && RAND:3 == 0
					PRINTFORML 「ぐっ……じゅる……な、なんですかその目は」
					PRINTFORMW 「あれだけドロドロにされれば、嫌悪感だって麻痺しますよ……」
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「うぷ……ど、どこからこんな量が……？」
						CASE 1
							PRINTFORML 「む、んぅ……う、ごほっ、まっ、待って……ッ」
							PRINTFORMW （く、口だけじゃとても収まらない……）
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF
RETURN 1

