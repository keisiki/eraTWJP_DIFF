	SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			PRINTFORMW 
		;中
		CASE 2 TO 4
			PRINTFORMW 
		;弱
		CASE 1
			PRINTFORMW 
	ENDSELECT
ENDIF


