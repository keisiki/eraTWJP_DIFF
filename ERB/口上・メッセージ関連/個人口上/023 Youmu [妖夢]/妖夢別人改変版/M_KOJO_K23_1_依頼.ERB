;--------------------------------------------------
SELECTCASE SCENE
CASE "依頼提示時"
	PRINTFORMW 「昨日水やりを忘れてしまった鉢がありまして……」
	RETURN 1
CASE "依頼非受託時"
	
	RETURN 0
CASE "依頼受託時"
	PRINTFORML 「こんな些事を申し訳ありません……」
	PRINTFORMW 「土が乾いていると思いますので、どの鉢かはすぐ分かるかと」
	RETURN 1
CASE "依頼確認時"
	
	RETURN 0
CASE "依頼破棄時"
	
	RETURN 0
CASE "依頼実行直前"
;【◆口上】
;【W ［あなた］は［依頼主］の家の鉢植えや植木に水をあげた】
;【▼そのまま下の口上へ】
	IF ROLE == "CLIENT"
	;水やりした時、依頼主である口上主がTARGETの場合
		
		RETURN 0
	ELSEIF ROLE == "TARGET"
	;水やりした時、依頼主ではない口上主がTARGETの場合
		
		RETURN 0
	ENDIF
CASE "依頼実行直後"
;○この依頼は必ず実行後達成します
;【▲上の口上から続き】
;【◆口上】
	IF ROLE == "CLIENT"
	;水やりした時、依頼主である口上主がTARGETの場合
	;この場合はそのまま下の口上へ続きます
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「この鉢は本来そこまで水も要らず、世話も簡単なんですよ」
				PRINTFORMW 「観賞用としては優等生と評せるかもしれませんね」
			CASE 1
				PRINTFORML 「これは中々気難しい鉢で……枯れてなくて良かった」
				PRINTFORMW 「手塩にかけて育てた草木は、いつ見ても美しいものです」
			CASE 2
				PRINTFORML 「当然ながら、植物毎に適した土の湿り気は様々です」
				PRINTFORMW 「この鉢は……あ、これは大味でも大丈夫ですね」
		ENDSELECT
		RETURN 1
	ELSEIF ROLE == "TARGET"
	;水やりした時、依頼主ではない口上主がTARGETの場合
		
		RETURN 0
	ENDIF
CASE "成功報告時"
;報酬：\500
	
	RETURN 0
CASE "失敗報告時"
	
	RETURN 0
CASE "依頼報告不要"
	
	RETURN 0
ENDSELECT

