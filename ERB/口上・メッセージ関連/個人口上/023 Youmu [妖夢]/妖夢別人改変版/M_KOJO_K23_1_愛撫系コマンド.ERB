@M_KOJO_別人版_MESSAGE_COM_K23_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		PRINTFORMW
		RETURN 1
	;泥酔
	ELSEIF TCVAR:泥酔 == 1
		IF TALENT:恋慕
			CALL HPH_PRINT,@"「……えへへHPH %CALLNAME:MASTER%さんの手……」","L"
			CALL HPH_PRINT,@"「ふわふわします……もっと、もっとぉ……HPH」","W"
		ELSE
			PRINTFORML 「うぅ……なにを……？」
			PRINTFORMW 「さするなら、背中……やだ、そんなところ……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋慕
			CALL HPH_PRINT,@"「わ……ん、ぅ……HPH」","W"
			PRINTFORML %CALLNAME:MASTER%が妖夢の胸に摩るように手を沿わせると、こそばゆそうに吐息が漏れた
			PRINTFORMW 触れた掌から早鐘を打つ彼女の鼓動が伝わってくる
			PRINTFORML
			PRINTFORMW 「あはは……どれだけ緊張してるか、分かっちゃいますよね……」
			PRINTFORMW 恥ずかしそうにしつつも目は逸らさず、妖夢は%CALLNAME:MASTER%の手に自らの手を静かに重ねた
			PRINTFORML
			PRINTFORMW 「私の気持ち、ちゃんと伝わっていますか……？　あっ……ん……」
			PRINTFORMW 空いた手で妖夢を抱き寄せ口付ける
			PRINTFORMW 触れるだけの浅いキスを続けると、終える頃には彼女の肢体から強張りは抜けていた
			PRINTFORML
			PRINTFORMW 「ふぅ、は……ぁ……優しいんですね」
			PRINTFORMW 「%CALLNAME:MASTER%さん、続けて下さい……私ならもう大丈夫ですから」
		ELSE
			PRINTFORMW 「う……ん、ぅ……」
			PRINTFORML %CALLNAME:MASTER%が妖夢の小振りな胸に手を這わせると、くすぐったそうに身を捩った
			PRINTFORMW 触れている手の先から彼女の緊張がありありと感じ取れる
			PRINTFORML
			PRINTFORMW 「なんですか……そりゃ緊張してますよ、こんな経験ありませんし……」
			PRINTFORMW 頬を紅潮させた妖夢が%CALLNAME:MASTER%からプイと顔を逸らした
			PRINTFORML
			PRINTFORMW 「やっ、な……ひああ……！？」
			PRINTFORMW 妖夢の楚々とした肢体に丹念に指を滑らせる
			PRINTFORMW 愛撫を続けつつ反応を見ていくと、終える頃には彼女の息は大いに乱れていた
			PRINTFORML
			PRINTFORMW 「はあ、はぁ……くぅ……っ」
			PRINTFORMW 弱々しく睨んでくる妖夢へ、まだ余裕がありそうだと%CALLNAME:MASTER%は内心ほくそ笑んだ……
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;VA挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			IF TALENT:恋慕
				;V挿入ならV感覚5以上、A挿入ならA感覚5以上
				IF (TEQUIP:50 == PLAYER && ABL:Ｖ感覚 > 4) || (TEQUIP:51 == PLAYER && ABL:Ａ感覚 > 4)
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「き、あ、はぅ、んぁあ……ッ！　どこも、きもちいい……おﾞおぉHPH」","L"
						CASE 1
							CALL HPH_PRINT,@"「ふぅう、ひゃん、い、はっ……HPH あﾞぁあ……ぜんぶ、こすれてぇHPH」","L"
					ENDSELECT
					PRINTFORML 開発された妖夢の肉穴は最早腰を動かさずとも肉棒にしゃぶりついてくる
					PRINTFORMW 全身を弄られて乱れる妖夢の痴態が欲情を煽り、%CALLNAME:MASTER%は射精の瞬間剛直を奥へと押し込んだ
				ELSE
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「ふあ、ん、いぃ……HPH あぁ、ナカで……HPH」","L"
						CASE 1
							CALL HPH_PRINT,@"「あ、くぅHPH 手も、ナカもぉ……ぜんぶ……HPH」","L"
					ENDSELECT
					PRINTFORML 締まりの良い妖夢の肉穴は挿れているだけでも心地が好い
					PRINTFORMW %CALLNAME:MASTER%は愛撫を続けながら、込み上げる射精感に身を任せた
				ENDIF
			ELSE
				PRINTDATAW
					DATAFORM 「はぁ、あ……すこしは、遠慮を……えっ、な、中で……！？」
					DATAFORM 「ふう……くぅ、ん……？　なにか、膨らんで……」
				ENDDATA
			ENDIF
			RETURN 1
		ELSE
			IF TALENT:恋慕
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あ、はっ……ふぅ、うあ……んぅ……HPH」","L"
						PRINTFORML 腰の動きを緩めて暫し愛撫に没頭する
						PRINTFORMW 仄かに紅潮した柔肌を掌が撫でる度、媚肉が剛直を更に咥え込もうと蠕動した
					CASE 1
						CALL HPH_PRINT,@"「うぁ、ああ……っ！　だ、だめです、%CALLNAME:MASTER%さん……HPH」","L"
						CALL HPH_PRINT,@"「いっぺんにされたら、よすぎてHPH どこまでが……わたしなのかぁ、ひゃあああHPH」","L"
						PRINTFORMW 快楽で弛緩しきった妖夢の面持ちに普段の凛々しさは微塵も残っていない
				ENDSELECT
			ELSE
				PRINTFORML 「いっ、か、あ……やっ、て、手、とめてぇ……っ！」
				PRINTFORMW 妖夢は力の入らない手で必死に%CALLNAME:MASTER%の腕を除けようとしている……
			ENDIF
			RETURN 1
		ENDIF
	ENDIF

	;基本セット
	;恋慕
	IF TALENT:恋慕
		SELECTCASE RAND:6
			CASE 0
				CALL HPH_PRINT,@"「あっ、はぅ……んっ……ふぁHPH」","L"
				CALL HPH_PRINT,@"「もっと……もっと、触ってください……HPH」","W"
			CASE 1
				CALL HPH_PRINT,@"「きゃうぅ！　はぁ、%CALLNAME:MASTER%さん、すごいです……HPH」","L"
				PRINTFORMW 「貴方の手、気持ちよくて、ずっと……ひゃああん！」
			CASE 2
				CALL HPH_PRINT,@"「は、ふあぁ……そんな、おしりばっかりぃ……HPH」","L"
				PRINTFORMW 妖夢の程よく引き締まった尻肉をゆっくりと愛撫し、吸いつくような手触りを堪能した
			CASE 3
				CALL HPH_PRINT,@"「ひゃ……く、くすぐったいです……あんHPH」","L"
				PRINTFORMW 妖夢の後ろ髪を掻き上げてうなじに顔を埋め、幾度も唇を落としていく……
			CASE 4
				CALL HPH_PRINT,@"「ちゅ……んん！？　ぷぁっ……そ、そんな風に触られるとぉ……HPH」","L"
				PRINTFORMW 口付けながら内腿から鼠径部へと撫で上げると、思わず口を離した妖夢から情欲に塗れた吐息が零れた
			CASE 5
				PRINTFORML 「っや、そこ、また、いい……んんんっ！」
				PRINTFORMW 敏感な部分に%CALLNAME:MASTER%の手が触れる度、妖夢は身体を小さく震わせている
		ENDSELECT
	;それ以外
	ELSE
		PRINTDATAW
			DATAFORM 「ふあっ……んく、が、我慢なんて……」
			DATAFORM 「んぁ、ぅ……感じてなんか……っ」
			DATAFORM 「ひっ……うぁ、人の身体を、なんだと……」
		ENDDATA
	ENDIF
ENDIF
RETURN 1
