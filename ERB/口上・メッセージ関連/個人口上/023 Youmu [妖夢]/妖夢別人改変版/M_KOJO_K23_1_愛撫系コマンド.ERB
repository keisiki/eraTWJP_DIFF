					DATAFORM 「ふぅ、む……んっ、んん……っ」
				ENDDATA
				PRINTFORMW 突かれながらが息苦しいのか、妖夢は%CALLNAME:MASTER%の胸板を押し返そうとしてくる……
			ENDIF
		ENDIF

	;基本セット
	;恋慕
	ELSEIF TALENT:恋慕
		IF PALAM:欲情 > PALAMLV:4 && RAND:2 == 0
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ちゅ、ん、じゅる、ぐぷ、ぢゅうぅHPH ……ぷぁっ、ん、こく……はぁ……HPH」","L"
					PRINTFORML 舌を絡め合い互いの口内を貪り合う
					PRINTFORMW 口を離す間際、混ざり合った唾液を流し込むと、妖夢はそれを陶然と喉を鳴らして嚥下した
				CASE 1
					CALL HPH_PRINT,@"「ふ、んん、んっ……あむHPH ちゅ、れろ、じゅるる……HPH」","L"
					PRINTFORMW 唇に触れた%CALLNAME:MASTER%の舌先を妖夢が食むように咥え、己の口腔へと誘い入れた
				CASE 2
					CALL HPH_PRINT,@"「ちゅ、ふっ、ん、じゅるHPH ぢゅうう、んむ、ふ、ぁ……HPH」","L"
					CALL HPH_PRINT,@"「はぁあ、まだ……HPH あっ……はむ、ぅ、んんHPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「あむ、ちゅっ、んっ、ふっHPH んぅ……はっ、んむぅHPH」","L"
					PRINTFORMW 妖夢は%CALLNAME:MASTER%の啄むような口付けに夢中になって応えている
				CASE 1
					CALL HPH_PRINT,@"「んん、ふ、ちゅHPH ふぅ、くぷ、ん、ぅう……HPH」","L"
					PRINTFORML 口付けながら物欲しげに開いた唇の隙間を舌先でなぞる
					PRINTFORMW 焦らすような動きに妖夢はこそばゆそうに身を捩った
				CASE 2
					CALL HPH_PRINT,@"「ふぅ、んん……HPH ん、ふぅ……ぁ、はっHPH」","L"
					CALL HPH_PRINT,@"「優しいキス、好きです……HPH あっ、んん、ふあ、んぅHPH」","W"
			ENDSELECT
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		PRINTDATAW
			DATAFORM 「んむ、むぅ……うう……っ」
			DATAFORM 「む、く……ふぅ、う……」
			DATAFORM 「くっ、また……んん……っ」
		ENDDATA
		RETURN 1
	ENDIF
ENDIF
RETURN 1


