@M_KOJO_別人版_MESSAGE_COM_K23_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		PRINTFORMW
		RETURN 1
	;泥酔
	ELSEIF TCVAR:泥酔 == 1
		IF TALENT:恋慕
			CALL HPH_PRINT,@"「やんHPH おしりですか……ん？」","L"
			CALL HPH_PRINT,@"「っ！　あ、ゎ、%CALLNAME:MASTER%さん、そこ――ぉおﾞおおHPH」","W"
		ELSE
			PRINTFORML 「……ぅ？　はなし……」
			PRINTFORMW 「ッ！　へ、変なところに――ぃいﾞいい！？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋慕
			PRINTFORML 「ひっ、えぇ！？　%CALLNAME:MASTER%さん、それは……！」
			PRINTFORMW 肛門を摩る指に驚いたのか、妖夢の手が反射的に%CALLNAME:MASTER%の指先を制した
			PRINTFORML
			PRINTFORMW 「だ、だって、お尻の……え、間違ってないのですか……？」
			PRINTFORML 「感じるようになるって……ど、どう考えても無理ですよぉ……」
			PRINTFORMW 素質はありそうなものだが、確かに妖夢の尻穴は未だ固く、何かを挿れる事など到底望めないだろう
			PRINTFORMW 気長に解していく他なさそうだ……
		ELSE
			PRINTFORML 「んぃ！？　な、何を……やめて……ッ」
			PRINTFORMW 肛門を押し込む指に驚いたのか、妖夢の手が咄嗟に%CALLNAME:MASTER%の腕を払った
			PRINTFORML
			PRINTFORML 「あ、ありえないです！　そんなところまで侵すなんて……！」
			PRINTFORMW 余程屈辱だったのか、妖夢は涙目で%CALLNAME:MASTER%を睨み付けている
			PRINTFORMW 触れたのは数瞬でしかなかったが、これから時間をかけて解していけばいいだろう……
		ENDIF
		RETURN 1
	ENDIF

	;アナルバイブ
	IF TEQUIP:アナルバイブ
	;「小刻みに震えるアナルバイブを前後に抜き差しした…」
		IF TALENT:恋慕
			IF ABL:Ａ感覚 > 4
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ああ、あﾞぁぁHPH ふるえて、おひりのなかがぁ……ひっ、かっ、はぐ、あぁっHPH」","L"
						PRINTFORMW すっかり解れきった妖夢のアナルは激しいバイブの抽挿もまるで苦にしていない
					CASE 1
						CALL HPH_PRINT,@"「ひいい、ぎ、いぃHPH そ、そんなに、ぐりぐりされたら、なにもぉお……HPH」","L"
						PRINTFORML 抜き差しと共に腸壁をなぞるようにバイブで腸内を掻き回す
						PRINTFORMW バイブの挙動と絶え間ない振動が妖夢を殊更情事へと惑溺させる
					CASE 2
						CALL HPH_PRINT,@"「あっ、はっ、い――おﾞっ！？　こ、これ、ダメな、ところまで……ぇ、は、ひぃいHPH」","L"
						PRINTFORMW 抽挿の最中一際奥へとバイブを押し込むと、妖夢はだらしなく弛緩させた口元を一層蕩けさせた
				ENDSELECT
			ELSEIF ABL:Ａ感覚 > 2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「はぁ、か、あ……うそっ、気持ち、いい……HPH」","L"
					CASE 1
						CALL HPH_PRINT,@"「く、うﾞあ、ふっ、あぁ……ひ、引っ張られて……HPH」","L"
				ENDSELECT
				PRINTFORMW まだ違和感はありそうだが、妖夢は振動するバイブから快楽を得られ始めたようだ
			ELSE
				PRINTDATAL
					DATAFORM 「ぎ、うぅ……震えて、気持ち悪いです……」
					DATAFORM 「う、ぐ、ぁぁ……うっ、動かさないで……」
				ENDDATA
				PRINTFORMW 妖夢は苦痛よりも異物感に顔を顰めているように見える……
			ENDIF
		ELSE
			IF ABL:Ａ感覚 > 4
				PRINTDATAL
					DATAFORM 「が、はっ、うぁ、こ、こんな、道具に、い……っ！」
					DATAFORM 「ひっ、やっ、こ、れ以上は……抜いて、ぬいてぇ……！」
					DATAFORM 「あぁ、あﾞ、ど、どうして……気持ち、ぃぃ……っ」
				ENDDATA
				PRINTFORMW %CALLNAME:MASTER%に開発された尻穴はバイブの刺激を余す事無く官能へと置換する
			ELSE
				PRINTDATAL
					DATAFORM 「ぐうぅ、う……よ、よくもこんな無茶を……！」
					DATAFORM 「ぃ、がっ、ひ……す、少しはこちらの、事も……！」
				ENDDATA
				PRINTFORMW 目付きはこちらを射殺さんばかりだが、語気ほど苦痛を感じている訳ではなさそうだ……
			ENDIF
		ENDIF

	;V挿入
	ELSEIF TEQUIP:50 == PLAYER
		IF TALENT:恋慕
			IF ABL:Ａ感覚 > 4 && ABL:Ｖ感覚 > 4
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ふっ、あぐ、はっHPH まっ、前でも、ゆびが、ぁ……うぁあﾞあHPH」","L"
						PRINTFORML 尻穴に指を沈め陰茎の輪郭を確かめるように腸壁をなぞる
						PRINTFORMW 膣穴だけでは足りないとでも言いたげに、肛門は指先を咥え込もうと卑しく締め付けてくる
					CASE 1
						CALL HPH_PRINT,@"「ひぅっ、は、あ、ふぅ……んん、それだけ、ですかぁ……？HPH」","L"
						PRINTFORMW 肛門の周囲をぐにぐにと撫で回すと、剛直を収めた膣肉がもどかしそうに蠢いた
					CASE 2
						CALL HPH_PRINT,@"「ああ、ひぃあ、あﾞぁHPH おﾞ、おしり、までぇ……ふあ、ひゃあああHPH」","L"
						PRINTFORMW 開発されきったアナルは敏く刺激を感じ取り、収縮する媚肉が肉棒を締め上げる
				ENDSELECT
			ELSEIF ABL:Ａ感覚 > 2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「い、うぁ、はう……へ、変な感じ、なのに……HPH」","L"
						PRINTFORML 腰の動きを緩めて暫し愛撫に没頭する
