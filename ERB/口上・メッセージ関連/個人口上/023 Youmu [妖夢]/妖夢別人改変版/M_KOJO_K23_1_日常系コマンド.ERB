;下記の2行を忘れると定義できなかった
#DIMS 品名
#DIMS 属性
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;贈っていたら本体名と属性を取得しておく
	IF TFLAG:194 > 0
		CALL GIFTDATA_MAIN(GET_GIFTDATA(TCVAR:23:今日の贈り物, "本体"))
		品名 = %RESULTS%
		IF STRFIND(RESULTS:1, "握る") >= 0
			SIF STRFIND(RESULTS:1, "装飾") >= 0
				属性 = 身につける
			属性 = 握る
		ELSEIF STRFIND(RESULTS:1, "抱く") >= 0
			属性 = 抱く
		ELSEIF STRFIND(RESULTS:1, "身につける") >= 0
			属性 = 身につける
		ELSE
			属性 = 眺める
		ENDIF
	ENDIF

	SIF TCVAR:泥酔 == 1
		RETURN 1
	SELECTCASE TFLAG:194
		;ひやかし
		CASE -2
			;香霖堂内を定義したい
			IF MAIN_MAP != 5 && CFLAG:MASTER:現在位置 == 520
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「ぐぬぬ、あの店主……！」
						PRINTFORMW 「……いや、此度礼を欠いたのはこちらですか。抑えないと……」
					CASE 1
						PRINTFORML 「私達にも非はありますがあの店もあの店です！」
						PRINTFORMW 「非売品が多すぎるんですよ！」
				ENDSELECT
			ELSEIF TALENT:恋慕
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「あわわ……夢中になりすぎました」
						PRINTFORMW 「お店の方にも迷惑をおかけしてしまいましたね……」
					CASE 1
						PRINTFORML 「しまった、時間を忘れていました……」
						PRINTFORMW 「私も選ぶのに集中してしまって……あっ、き、気になさらないで下さい」
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「些か悩み過ぎましたね……」
						PRINTFORMW 「見計らうのが肝要とはいえ、お店の都合も考えないと」
					CASE 1
						PRINTFORML 「時間をかけ過ぎましたか……」
						PRINTFORMW 「随分拘っていたようですけど、一体何をお探しに？」
				ENDSELECT
			ENDIF
		;やっぱりやめる（見送り）
		CASE -1
			IF MAIN_MAP != 5 && CFLAG:MASTER:現在位置 == 520
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「道理に明るい判断です、%CALLNAME:MASTER%さん」
						PRINTFORMW 「どうせここにはガラクタしかありませんから」
					CASE 1
						PRINTFORML 「訳の分からない物ばかりと、あそこの店主に詳細を尋ねるのは早計でしょう」
						PRINTFORMW 「それ以上に訳の分からない屁理屈で貴重な時間を無駄にしますので」
				ENDSELECT
			ELSEIF TALENT:恋慕
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「お店の方には申し訳ないですけど、商品を見て回るだけでも楽しいですよね」
						PRINTFORMW 「%CALLNAME:MASTER%さんは何か気になる品はありましたか？」
					CASE 1
						PRINTFORML 「剣や刀を見付けるとついつい目を止めてしまいますよ」
						PRINTFORMW 「半霊を使えば四刀までは無理なく振れるので、いつか別々の得物でやってみたいですねぇ……」
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「あれ、よろしいのですか？」
						PRINTFORMW 「まあ成り行きで買って持て余しては勿体ないですからね」
					CASE 1
						PRINTFORML 「何かお探しでしたら私もお手伝いしますよ」
						PRINTFORMW 「こういった目利きにはそこまで自信はありませんけど……」
				ENDSELECT
			ENDIF
		;400点未満（BAD）
		CASE 1
			PRINTDATAL
				DATAFORM 「そ、それは私への……？　あ、いや、その……」
				DATAFORM 「%CALLNAME:MASTER%さんのご趣味はそういった……へ？　わ、私に？」
				DATAFORM 「んん……？　これを、私が……？」
			ENDDATA
			PRINTFORML 「あ、ありがとうございます……」
				IF TALENT:恋慕
					PRINTFORMW 妖夢は笑顔で贈り物を受け取ったものの、若干口元が引き攣っている……
						IF MAIN_MAP != 5 && CFLAG:MASTER:現在位置 == 520
							PRINTFORML （%CALLNAME:MASTER%さんにこんな駄物を売りつけるなんて……ッ）
							PRINTFORMW （……い、いけない。%CALLNAME:MASTER%さんの前で血を見せる訳には）
						ELSE
							PRINTFORMW （ど、どうしよう……私の好みで%CALLNAME:MASTER%さんに恥をかかせる訳には……）
						ENDIF
				ELSE
					PRINTFORMW あまりに趣向から外れた贈り物に妖夢は困惑しているようだ……
						IF MAIN_MAP != 5 && CFLAG:MASTER:現在位置 == 520
							PRINTFORMW （このガラクタ置き場に僅かでも期待した私が馬鹿だった……！）
						ELSE
							PRINTFORMW （もしやこれはお土産ではなく、何か重大な意図が隠されているのでは……？）
						ENDIF
				ENDIF
		;400点～699点（NOMAL）
		CASE 2
			PRINTDATAL
				DATAFORM 「これは先ほど買われていた……私がいただいてよろしいのですか？」
				DATAFORM 「少し興味のある品でしたが、こうして頂戴できるとは……」
				DATAFORM 「わわ、なんだか恐縮です……いえ、こういう時は違いますね」
			ENDDATA
			PRINTFORML 「ありがとうございます、%CALLNAME:MASTER%さん」
				IF TALENT:恋慕
					PRINTFORMW 妖夢は%CALLNAME:MASTER%から贈り物を受け取ると深く一礼した
						IF MAIN_MAP != 5 && CFLAG:MASTER:現在位置 == 520
							PRINTFORMW 「こんな廃材置き場から価値ある物を見付けてしまわれるなんて、お見事です！」
						ELSE
							PRINTFORMW 「気にかけていただけて嬉しいです、大切にさせていただきます」
						ENDIF
				ELSE
					PRINTFORMW 妖夢は%CALLNAME:MASTER%から贈り物を受け取ると一礼した
						IF MAIN_MAP != 5 && CFLAG:MASTER:現在位置 == 520
							PRINTFORMW 「……一応商品扱いできる物も紛れてはいるんですね」
						ELSE
							PRINTFORMW 「自分にこういった物を買う機会はそうないので、贅沢をさせていただけた気分です」
						ENDIF
				ENDIF
		;700点以上（GOOD）
		CASE 3
			;形容詞+本体名を取得
			CALL GIFTNAME(TCVAR:23:今日の贈り物)
			PRINTFORML 「なっ……これは、%RESULTS%……！？」
			PRINTFORML 「いやいやいやっ！　気に入らないなんてそんなっ、寧ろ逆ですよ……！」
				IF MAIN_MAP != 5 && CFLAG:MASTER:現在位置 == 520
					PRINTFORML 「このゴミの山からこれほどの傑作を掘り出してしまわれるとは、目利きの才もおありなんですね！」
				ELSEIF TALENT:恋慕
					PRINTFORML 「ちょっと、嬉しすぎて言葉が……なんとお礼を申し上げるべきか……」
				ELSE
					PRINTFORML 「私の好みをこうも的確に当ててしまわれるなんて……感激です！」
				ENDIF
			PRINTFORMW 妖夢は感極まった様子で贈り物を受け取り、%CALLNAME:MASTER%へ深々と頭を下げた
			PRINTFORML
				SELECTCASE 属性
					;懐剣、護刀はいずれも眺める
					CASE "眺める"
						IF 品名 == "懐剣" || 品名 == "護刀"
							IF TALENT:恋慕
								PRINTFORML 「部屋に戻ったら早速刀掛けに飾らせていただきますね！」
								PRINTFORMW 「――つ、使えませんよ！？　実戦で壊してしまったらと考えると、恐ろしくてとても……」
							ELSE
								PRINTFORML 「ああ、早く試し斬りしてみたい……」
								PRINTFORMW 「……や、やっぱり我慢です。手違いが起きてしまったら申し開きもできません」
							ENDIF
						ELSE
							PRINTFORMW 「物が少ない私の部屋なら尚更、どこに飾っても映えてくれる筈です」
						ENDIF
					CASE "抱く"
						PRINTFORMW 「何かの拍子に落としてしまわないように、しっかり抱えておかないと……」
					CASE "握る"
						PRINTFORMW 「元より私物だったと錯覚するほど手に馴染みます……思わず持ち歩きたくなってしまいますよ」
					CASE "身につける"
						IF TALENT:恋慕
							PRINTFORML 「え、着けた姿を見てみたいと？　そのぅ……い、今はご容赦を」
							PRINTFORMW 「実は相当いっぱいいっぱいなので、もしかしたら気絶してしまうかも……」
						ELSE
							PRINTFORML 「え、着けた姿が気になる？」
							PRINTFORMW 「……は、恥ずかしいのでまた後日に……」
						ENDIF
				ENDSELECT
			PRINTFORMW 「……本当にありがとうございます、%CALLNAME:MASTER%さん。このご恩には必ず報いますので」
	ENDSELECT
ENDIF
RETURN 1
