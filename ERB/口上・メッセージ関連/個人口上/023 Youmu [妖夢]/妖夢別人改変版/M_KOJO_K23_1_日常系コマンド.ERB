IF LOCAL
	;Activity_Type =0考え事/=1遊ぶ/=2食事/=3つまみ食い/=4掃除/=5運動/=6読書/=7料理
	;				=8セッション/=9採集/=10釣り/=11調合/=12休憩/=13晩酌/=14買い物/=15説教/=16落とし穴
	;ブチギレor反発刻印有
	IF CFLAG:318 == 1 || (MARK:23:反発刻印 == 1 && RAND:3 == 0) || (MARK:23:反発刻印 > 1 && RAND:3 != 0)
		PRINTFORML 「…………」
		PRINTFORMW 妖夢は%CALLNAME:MASTER%をいない者として見ているようだ……
	ELSE
		SELECTCASE Activity_Type:TARGET
			CASE 0
				;▼考え事［なに考えてるの］
				;TFLAG:193,-1=はぐらかされる
				IF TFLAG:193 == -1
					PRINTFORMW 「――へ？　あっ、こ、これは%CALLNAME:MASTER%さんだけには教えられないと言いますか……」
				ELSE
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「明日の夕飯の献立を考えていまして」
							PRINTFORMW 「気が早い？　いやいや、決してそんな事はないんです……」
						CASE 1
							PRINTFORML 「次はどんな形に庭木を剪定してみようかと……」
							PRINTFORMW 「そうです！　%CALLNAME:MASTER%さん、ご要望があれば可能な限りお応えしますよ？」
						CASE 2
							PRINTFORML 「次回の買い出しの確認をと思いまして」
							PRINTFORMW 「大仕事ですので事前の準備は幾らでもしておきたいですからね」
					ENDSELECT
				ENDIF
			CASE 2
				;▼食事［一緒に食事する］
				;TFLAG:193,-1=MASTERはお腹が空いてなかった
				IF TFLAG:193 == -1
					PRINTFORMW 「ええ……？　でしたら無理矢理詰め込まなくても……」
				ELSEIF TALENT:恋慕
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「ささやかな間食ではありますが……余分に持ち歩いていて正解でした」
							PRINTFORMW 「多少の空腹は繕えましたでしょうか？」
						CASE 1
							PRINTFORML 「普段いただいてばかりですし、お茶は私が用意しますね」
							PRINTFORMW 「……いかがでしょうか？　上手く淹れられていると良いのですが……」
					ENDSELECT
				ELSE
					PRINTFORML 「えっ、これを分けて欲しい？」
					PRINTFORMW 「構いませんけど……元々が少ないのであまり差し上げられませんよ？」
				ENDIF
			CASE 4
				;▼掃除［掃除を手伝う］
				;※[掃除係]持ちまたは汚臭耐性-1以下のみ
				;冬の屋外
				IF OUTROOF(CFLAG:現在位置) && DAY:2 == 4
					PRINTFORML 「こうも雪が積もっていては往来に支障が出ます！」
					PRINTFORMW 「誰かが転ぶ前に一緒に片付けてしまいましょう」
				;白玉楼庭
				ELSEIF MAIN_MAP == 6 && CFLAG:MASTER:現在位置 == 621
					PRINTFORML 「庭園の景観を保つためにも日々の清掃は欠かせません」
					PRINTFORMW 「まあ、結局こうしてお手を煩わせてしまっているのですが……お恥ずかしい限りです」
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「使用人のサガなのか、細かい汚れが気になってしまいまして」
								IF MAIN_MAP == 6 && INRANGE(CFLAG:MASTER:現在位置, 620, 629)
									PRINTFORMW 「%CALLNAME:MASTER%さんの手をお借りできましたが、他の幽霊は何をしているんでしょう……？」
								ELSE
									PRINTFORMW 「勝手に掃除していいのかちょっと不安ですが、綺麗になって気分を損ねる人はいませんよ……多分」
								ENDIF
						CASE 1
							PRINTFORML 「この辺りは良さそうですね。では次はあちらに……」
								IF ABL:MASTER:清掃技能 >= ABL:清掃技能
									PRINTFORM %CALLNAME:MASTER%は妖夢に指示を出しつつ
								ELSE
									PRINTFORM %CALLNAME:MASTER%は妖夢から指示を受けつつ
								ENDIF
							PRINTFORMW 二人で周囲の清掃に取り組んだ
					ENDSELECT
				ENDIF
			CASE 5
				;▼運動［一緒に運動する］
				;※[音楽知識]持ちは除く（演奏になる）
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「まだまだ鍛錬をし足りない気分でして」
							IF TALENT:恋慕
								PRINTFORMW 「%CALLNAME:MASTER%さんにお付き合いいただけるなんて、願ってもない僥倖です！」
							ELSE
								PRINTFORMW 「相手がいた方が内容に幅が出るので助かりますよ」
							ENDIF
					CASE 1
						PRINTFORML 「空いた時間で軽く身体を解しておくだけでも、いざという時に差が出るものです」
						PRINTFORMW 「熱中しすぎて困憊しては本末転倒なので、あくまで適度が大切ですけどね」
				ENDSELECT
			CASE 6
				;▼読書［本の感想を聞く］
				;TFLAG:193,-1=集中していて返事してくれない
				IF TFLAG:193 == -1
					PRINTFORMW 「…………%TEXTR("ふむふむ/むむむ/おお/")%」
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%さんもこの本に興味がおありですか？」
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「自分の知らない料理は紙面で見るだけでもワクワクしますね」
								PRINTFORMW 「この意欲のまま思い切った料理に挑戦してみたくなりますよ……決して見た目が危ない代物という意味ではなくて」
							CASE 1
								PRINTFORML 「外界には私も舌を巻くしかない庭園がこうも溢れているのですね……」
								PRINTFORMW 「うーん、今度紅魔館の門番と語り合ってみるのも……ほら、半分庭師みたいなものですし」
							CASE 2
								PRINTFORML 「幽々子様に稽古へ取り組んでいただくにはどうするべきか、書物を探っているのですが」
								PRINTFORMW 「食べ物でも効果が薄い時点で前途の暗さが拭えないんです……いえ、そう簡単に諦めてはいけませんよね」
						ENDSELECT
				ENDIF
			CASE 7
				;▼料理［料理を手伝う］→［料理を分けてもらう］
				;TFLAG:193,2=料理を手伝う,0=作った料理を食べる,-1==MASTERはお腹が空いてなかった（食べる時）
				IF TFLAG:193 == 2
					PRINTFORML 「手持ちの材料で何か作れないか試しているんですよ」
					PRINTFORMW 「これが段々楽しくなってきまして……%CALLNAME:MASTER%さんもご一緒にいかがですか？」
					PRINTFORMW %CALLNAME:MASTER%は妖夢と暫し料理の創作に熱中した
				ELSEIF TFLAG:193 == -1
					PRINTFORMW 「ええ……？　でしたら無理矢理詰め込まなくても……」
				ELSE
					IF TALENT:恋慕
						SELECTCASE RAND:2
							CASE 0
								PRINTFORML 「即席としては十分な物になりましたよ！　量は少ないのが残念ですけど……」
								PRINTFORMW 「多少の空腹は繕えましたでしょうか？」
							CASE 1
								PRINTFORML 「普段いただいてばかりですし、お茶は私が用意しますね」
								PRINTFORMW 「……いかがでしょうか？　上手く淹れられていると良いのですが……」
						ENDSELECT
					ELSE
						PRINTFORML 「有り合わせで作った割にはちゃんとした物にできましたね」
						PRINTFORMW 「元が少ないのであまりお裾分けはできそうにありませんけど……」
					ENDIF
				ENDIF
			CASE 9
				;▼採集［採集を手伝う］
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「これといって目当ての材料がある訳ではないんですけどね」
						PRINTFORMW 「単純に旬のものなんかが見付かればちょっと嬉しいかなぁ、なんて」
					CASE 1
						PRINTFORML 「漠然と探しているだけなのですが、強いて言えば食材になりそうな物が欲しいですね」
						PRINTFORMW 「自分で携帯してもいいですし、たくさん採れればお屋敷でも使えますので」
				ENDSELECT
			CASE 10
				;▼釣り［一緒に釣りをする］
				;TFLAG:193,-1=釣り竿がなくて見てるだけ
				IF TFLAG:193 == -1
					PRINTFORML 「う……見られているだけというのは落ち着きませんね」
					PRINTFORMW 「釣り竿が二本あれば良かったのですが……」
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「何をするでもなく釣り糸を垂らしているのも、息抜きには丁度いい暇です」
							PRINTFORMW 「ふわぁぁ……っと、流石に竿は持っていかれないようにしないと」
						CASE 1
							PRINTFORML 「よぉし、折角なので大物を狙っていきましょう！」
							PRINTFORMW 「立派な魚が釣れればその分次の買い出しで荷台が軽くなります！」
					ENDSELECT
				ENDIF
			CASE 11
				;▼実験［実験を手伝う］
				;PRINTFORMW
			CASE 12
				;▼のんびり［一緒にのんびりする］
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「あれ、%CALLNAME:MASTER%さんも休憩に？」
						PRINTFORMW 「気を張りすぎてもいけませんからね。休める時は休んでおきましょうか」
					CASE 1
						PRINTFORML 「のどかですねぇ……ん、どうされました？」
						PRINTFORMW %CALLNAME:MASTER%は妖夢の隣でだらけている半霊を抱き枕にしたい衝動に抗った……
					CASE 2
						PRINTFORML 「ふう……おや、お茶を淹れて下さったんですか」
						PRINTFORMW 「ありがとうございます。では私からは少ないですがお茶請けを……」
				ENDSELECT
			CASE 13
				;▼晩酌［晩酌に付き合う］
				;※酒耐性0以上のみ
				IF TALENT:恋慕 && RAND:3 == 0
					PRINTFORML （……こ、この雰囲気に酔っちゃいそう……）
					PRINTFORMW 妖夢は顔を赤らめつつ%CALLNAME:MASTER%にお酌をしてくれた
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「今日も一日頑張ったと、実感させてくれるこの時間は乙なものです」
							PRINTFORMW 「明日も頑張れるよう飲みすぎには注意しないと……」
						CASE 1
							PRINTFORML 「付き合っていただけますか？　私も話し相手がいた方が嬉しいですね」
							PRINTFORMW 「あっ……ですが、あまり荒唐無稽な冗談は吹き込まないでいただければと」
							PRINTFORMW 「酔った勢いで信じ切ってしまうかもしれませんので……」
					ENDSELECT
				ENDIF
			CASE 14
				;▼買い物［買い物を手伝う］
				IF TALENT:恋慕 && RAND:3 == 0
					PRINTFORML 「お手間を取らせるほどの買い物はできないのですが……」
					PRINTFORMW 「%CALLNAME:MASTER%さんは何かご入用でしょうか？　私が荷物をお持ちしますよ！」
				ELSE
					SELECTCASE RAND:2
						CASE 0
							IF MAIN_MAP == 2
								PRINTFORML 「買い出しの時に受け取り切れなかった物は、こうして別の機会に持ち帰っているんです」
								PRINTFORMW 「これくらいでしたら荷車は必要ありませんからね」
							ELSE
								PRINTFORML 「ちょっと屋敷で切らしてしまっている物がありまして、この機に買い足していこうと思ったのですが」
								PRINTFORMW 「馴染みの薄い店だとなかなか勝手が違いますね……%CALLNAME:MASTER%さんにはお手数をかけてしまいそうです」
							ENDIF
						CASE 1
							PRINTFORML 「むむむ……仕事以外となるとどうにも目移りしてしまいます」
							PRINTFORMW 「ですがあくまで必要な物だけ……無駄遣いはできませんので」
					ENDSELECT
				ENDIF
			CASE 15
				;▼説教［様子をうかがう］
				;※神霊・蓬莱人は除く
				;TFLAG:193,-1
				;PRINTFORMW 
			CASE 16
				;▼落とし穴［いたずらに付き合う］or［様子をうかがう］
				;※三月精・チルノ・クラウンピースのみ
				;それ以外は落ちる側
				;PRINTFORMW 
			CASE 17
				;▼雪だるま作り［一緒に雪作り］
				;※冬の月のみ
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「固めて遊ぶ分には雪も風情があるんですけどねぇ……」
						PRINTFORMW 「……集めて纏めているという意味では、これもれっきとした雪掻きなのでは？」
					CASE 1
						PRINTFORML 「どうせ作るなら気合を入れたものにしたいですね！」
						PRINTFORMW 「造形に拘るか、大きさに拘るか……悩ましいところです」
				ENDSELECT
			CASE 18
				;▼炬燵でのんびり［炬燵でくつろぐ］
				;※冬の月のみ
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「炬燵で温まるのは冬の醍醐味の一つですよねえ……」
						PRINTFORMW 「暫くは出たくなくなるのが難点ですけど……あぁ、そんな事さえ些細に……」
					CASE 1
						PRINTFORML 「たまに冥界で黄泉竈食を気にする方がいますが、私からすれば炬燵の方が余程恐ろしいですよ」
						PRINTFORMW 「蜜柑やアイスなんて口にしてしまったら、那岐命も這い出すのにはさぞ苦労されるでしょう」
					CASE 2
						PRINTFORML 「この至福の一時を他より多く享受できるのは、ちょっと優越感がありますね……」
						PRINTFORMW 妖夢は半霊と一緒に炬燵の温もりを堪能している……
				ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
