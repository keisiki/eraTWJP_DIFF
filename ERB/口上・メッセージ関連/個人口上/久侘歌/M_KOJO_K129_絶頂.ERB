			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:二重絶頂
			;Ｃ＆Ｖ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ絶頂
			CASE 2
