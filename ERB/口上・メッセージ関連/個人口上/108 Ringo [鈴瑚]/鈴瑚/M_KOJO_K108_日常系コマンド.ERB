LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怒り
	IF CFLAG:318 == 1
		SELECTCASE RAND:2
			CASE 0
				CALL COLORMESSAGE("「団子食べてるんだから邪魔しないで」", C_ORANGE, 0, 0)
