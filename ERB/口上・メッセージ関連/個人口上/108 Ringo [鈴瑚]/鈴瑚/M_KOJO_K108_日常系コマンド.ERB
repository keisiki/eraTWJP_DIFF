@M_KOJO_SUCCESS_COM_K108_300
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K108_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます。また、削除した場合は自動で口上色が付きません。
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K108_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K108_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;初回会話
	IF FIRSTTIME(SELECTCOM)
		CALL COLORMESSAGE("「ん～っ♪今日の団子も美味しい！幸せだなぁ～…♪」", C_ORANGE, 0, 0)
		SETCOLOR 0xFFFF00
		PRINTFORML MGMG
		CALL COLORMESSAGE("「―何さ？ジロジロ見て……もしかしてキミも食べたい？」", C_ORANGE, 0, 0)
		SETCOLOR 0xFFFF00
		PRINTFORML MGMG
		SETCOLOR 0xEE9900
		PRINTFORMDW 鈴瑚はいつも…と言うより、常に団子を食べている。体を壊さないか心配だ……
		PRINTFORML 「大丈夫大丈夫♪これ位で壊れる程、私はヤワじゃないよ」
		PRINTFORMW 「それに…これが私の"能力"だからね」
		PRINTFORMW 「教えてあげるよ%CALLNAME:MASTER%。私の能力は…"団子を食べる程に強くなる程度の能力"」
		PRINTFORMDW 冗談みたいな能力の内容に、%CALLNAME:MASTER%は思わず噴き出してしまった。
		PRINTFORML 「やれやれ……キミは、私の能力の恐ろしさに気づいて無いようだね」
		PRINTFORMW 「"団子を食べる程"…要するに、この能力に上限は無い…」
		CALL COLORMESSAGE("「いずれ私は、この幻想郷において最強の存在になるという事さ」", C_ORANGE, 0, 0)
		SETCOLOR 0xFFFF00
		PRINTFORML MGMG
		PRINTFORMDW 鈴瑚は見てるだけでお腹一杯になりそうな量の団子を新たに取り出し、食べ始めた……
	;怒り
	ELSEIF CFLAG:318 == 1
		CALL COLORMESSAGE("「…………。」", C_ORANGE, 0, 0)
		SETCOLOR 0xFFFF00
		PRINTFORML MGMG
		PRINTFORMDW こちらを視界に入れたくないのか、鈴瑚は明後日の方向を向きながら団子を食べている……
	;喋りすぎ
	ELSEIF TCVAR:302
		SELECTCASE RAND:3
			CASE 0
				IF TALENT:恋慕 || TALENT:思慕
					CALL COLORMESSAGE("「…ああ、ゴメン。何の話だっけ？」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 今の話には興味が無かったようだ……
				ELSE
					CALL COLORMESSAGE("「静かに。今は団子を味わうのに集中してるの」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 鈴瑚に話を遮られた……
				ENDIF
			CASE 1
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「……腹ごなしに何処か出掛けようか。気分転換も兼ねて」
					PRINTFORMDW 鈴瑚は、お喋りに飽きたようだ……
				ELSE
					CALL COLORMESSAGE("「そっかそっか。そりゃあ大変だ」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 鈴瑚は適当に相槌を打ちながら団子を食べている……
				ENDIF
			CASE 2
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「%CALLNAME:MASTER%ちょっと喋りすぎ。これでも食べて落ち着きなよ」
					PRINTFORMDW 口に団子をねじ込まれた……
				ELSE
					CALL COLORMESSAGE("「何で団子って、こんなに美味しいんだろう？」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 鈴瑚は%CALLNAME:MASTER%との話に興味を無くしたようだ……
				ENDIF
		ENDSELECT
	;酔っ払い時会話
	;ぐでぐで
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 8
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「も～……！キミとサシでのむと、いっつものみすぎちゃうんだよなぁ～！」
					PRINTFORMW 「まえはこんなんじゃなかったのに……ほんっと、キミってやつは…！」
					CALL HPH_PRINT,@"「……だいすきだよHPH」","W"
				CASE 1
					CALL COLORMESSAGE("「う～ん…おつまみなくなっちゃった……あ！そうだ」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					SETCOLOR 0xEE9900
					PRINTFORML 「かわりに%CALLNAME:MASTER%つまもう♪…ぐにぃ～……」
					PRINTFORML 「あっははは！よくのびる～♪」
					PRINTFORMDW よくわからないが、%CALLNAME:MASTER%の頬をつまむ鈴瑚はとても楽しそうだ……
				CASE 2
					PRINTFORML 「"ウイスキーボンボン"みたいに、おさけはいったダンゴとかありだとおもうんだよね」
					PRINTFORMDW 鈴瑚は目が据わっている……
				CASE 3
					PRINTFORML 「……%CALLNAME:MASTER%って、よくみたらおいしそうね……」
					PRINTFORML 「ちょっとあじみしてもいい？」
					PRINTFORMDW 鈴瑚は相当酔っているようだ……
				CASE 4
					PRINTFORML 「いへんのときもそうだったけどさー！みんなわたしが、どれだけくろうしてるのかわかってないよ！」
					CALL HPH_PRINT,@"「…%CALLNAME:MASTER%はわかってくれるのね？だからすきHPH」","L"
					PRINTFORMDW 鈴瑚は相当ストレスが溜まっているようだ……
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「の、のみすぎたぁ～…！ちょっときゅうけい……」
					PRINTFORMDW 鈴瑚はその場に横になった…
				CASE 1
					PRINTFORML 「…ん？おさけなくなっちゃった…」
					PRINTFORML 「しょうどくようのアルコールあるけど、のむ？」
					PRINTFORMDW それは駄目だろう……
				CASE 2
					PRINTFORML 「ほしがぁ…ぐるぐるしてるぅ……」
					PRINTFORMDW 鈴瑚はかなり酔ってしまったようだ……
				CASE 3
					PRINTFORML 「……Zzz」
					PRINTFORMW 「―はっ！…ねちゃうとこだった…」
				CASE 4
					PRINTFORML 「いや…おかわりはいいや。それよりもみずを…」
					PRINTFORMDW 鈴瑚に水を飲ませると、少し気分が良くなったようだ。
			ENDSELECT
		ENDIF
	;酔いどれ
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 5
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE RAND:5
				CASE 0
					CALL HPH_PRINT,@"「ふふ…♪ねぇ…今日はキミと朝まで飲みたいな…HPH」","L"
					PRINTFORMDW 鈴瑚が潤んだ瞳で見つめてくる…。
				CASE 1
					PRINTFORML 「んっ―……ふぅ…美味し…♪」
					PRINTFORMDW 酒に濡れた鈴瑚の唇が、妙に艶めかしく見える……
				CASE 2
					PRINTFORML 「%CALLNAME:MASTER%のお酒、美味しいからすぐ無くなっちゃう…♪」
					PRINTFORMDW "お代わりならある"と言うと、鈴瑚は嬉しそうに頷いた。
				CASE 3
					CALL HPH_PRINT,@"「ん？大丈夫……HPHまだ酔ってないよ…HPH」","L"
					PRINTFORMDW 鈴瑚は顔を赤らめている……
				CASE 4
					PRINTFORMDL ふと鈴瑚の方を見ると目が合った。
					PRINTFORML 「…？…ふふ♪な～に%CALLNAME:MASTER%？」
					PRINTFORMDW 頬を上気させながら、鈴瑚はふわりとほほ笑んだ…
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「いい感じに……酔いが回ってきたねぇ…♪」
					PRINTFORMDW 鈴瑚はゆらゆら揺れている。
				CASE 1
					PRINTFORML 「このお酒美味しいね。%CALLNAME:MASTER%、お代わりある？」
					PRINTFORMDW 鈴瑚は%CALLNAME:MASTER%の持ってきた酒を気に入ってくれたようだ。
				CASE 2
					PRINTFORML 「酒は飲んでも飲まれるなってね。大丈夫、私はまだいけるよ」
					PRINTFORMDW 鈴瑚は自分の限界をある程度理解しているようだ。
				CASE 3
					CALL COLORMESSAGE("「以外と酒にも合うなぁ」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 鈴瑚は団子をつまみにして酒を飲んでいる…
				CASE 4
					PRINTFORML 「む……%CALLNAME:MASTER%、グラスが減ってないじゃないか」
					PRINTFORML 「―あ！ひょっとして、私を酔わせていやらしい事しようとしてる？」
					PRINTFORMDW 誤解を解く為にも、%CALLNAME:MASTER%はグラスの酒を飲み干した……
			ENDSELECT
		ENDIF
	ELSEIF TALENT:恋人
		IF FLAG:デート相手 == 108
			SELECTCASE RAND:2
				CASE 0
					CALL RINGO_DATE
				CASE 1
					SELECTCASE RAND:4
						CASE 0
							CALL RINGO_KOIBITO
						CASE 1
							CALL RINGO_OTENKI
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_GENSAKU
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					CALL RINGO_GENSAKU
				CASE 1
					CALL RINGO_KOIBITO
				CASE 2
					CALL RINGO_SEASON
				CASE 3
					CALL RINGO_OTENKI
				CASE 4
					PRINTFORML 「%CALLNAME:MASTER%…今日は、デートしないの？」
					PRINTFORMDW 鈴瑚を誘って何処か出掛けようか……
			ENDSELECT
		ENDIF
	ELSEIF TALENT:恋慕
		IF FLAG:デート相手 == 108
			SELECTCASE RAND:2
				CASE 0
					CALL RINGO_DATE
				CASE 1
					SELECTCASE RAND:4
						CASE 0
							CALL RINGO_RENBO
						CASE 1
							CALL RINGO_OTENKI
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_GENSAKU
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE CFLAG:現在位置
			;人間の里
				CASE 201,202,203,204,205,206,230,240,210,220,230,240,250,270
					CALL RINGO_ODANGO
				CASEELSE
					SELECTCASE RAND:5
						CASE 0
							CALL RINGO_GENSAKU
						CASE 1
							CALL RINGO_RENBO
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_OTENKI
						CASE 4
							PRINTFORML 「%CALLNAME:MASTER%…今日は、デートしないの？」
							PRINTFORMDW 鈴瑚を誘って何処か出掛けようか……
					ENDSELECT
			ENDSELECT
		ENDIF
	ELSEIF TALENT:思慕
		IF FLAG:デート相手 == 108
			SELECTCASE RAND:2
				CASE 0
					CALL RINGO_DATE
				CASE 1
					SELECTCASE RAND:4
						CASE 0
							CALL RINGO_SHIBO
						CASE 1
							CALL RINGO_OTENKI
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_GENSAKU
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE CFLAG:現在位置
			;人間の里
				CASE 201,202,203,204,205,206,230,240,210,220,230,240,250,270
					CALL RINGO_ODANGO
				CASEELSE
					SELECTCASE RAND:5
						CASE 0
							CALL RINGO_GENSAKU
						CASE 1
							CALL RINGO_SHIBO
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_OTENKI
						CASE 4
							PRINTFORML 「ねえ…暇だし、何処か出掛けようよ」
							PRINTFORMDW 鈴瑚を誘って何処か出掛けようか……
