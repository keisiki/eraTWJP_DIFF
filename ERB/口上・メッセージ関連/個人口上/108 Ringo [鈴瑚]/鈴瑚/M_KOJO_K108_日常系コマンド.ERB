;301,お茶を淹れる
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K108_301
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K108_301
CALL M_KOJO_MESSAGE_COM_K108_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K108_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FIRSTTIME(SELECTCOM)
		PRINTFORML 「……ん？お茶くれるの？…ありがと♪じゃあ遠慮無く…」
		PRINTFORML （とりあえず、毒の類は入って無さそうだな……）
		PRINTFORMDW 少し警戒しながらも、鈴瑚はお茶を飲んでくれた。
	ELSEIF CFLAG:318 == 1
		PRINTFORML 「……どーも」
		PRINTFORMDW 鈴瑚は一言お礼を言うと、お茶を一気に飲み干した…
	ELSEIF TFLAG:193 == 1
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「おっ、茶柱だ！ラッキー！いいことありそう♪」
				PRINTFORMW 「…あぁ、美味しい…♪」
			CASE 1
				CALL COLORMESSAGE("「流石、キミは解ってるね…！やっぱり団子には緑茶が一番だよねぇ♪」", C_ORANGE, 0, 0)
				SETCOLOR 0xFFFF00
				PRINTFORML MGMG
				PRINTFORMDW 鈴瑚はお茶と団子に舌鼓を打っている。
			CASE 2
				CALL COLORMESSAGE("「凄く美味しいお茶だね…！団子にもよく合うし…」", C_ORANGE, 0, 0)
				SETCOLOR 0xFFFF00
				PRINTFORML MGMG
				SETCOLOR 0xEE9900
				PRINTFORML 「…%CALLNAME:MASTER%。このお茶、鈴瑚屋で出してくれないかな？」
				PRINTFORMDW 鈴瑚は%CALLNAME:MASTER%のお茶を気に入ってくれたようだ。
			CASE 3
				PRINTFORML 「美味しいお茶だ…疲れも吹っ飛ぶよ…♪」
				PRINTFORMDW 鈴瑚の顔から、疲労の色が消えた……気がする。
			CASE 4
				IF TALENT:恋慕 || TALENT:思慕
					CALL HPH_PRINT,@"「いつも美味しいお茶ありがとね♪%CALLNAME:MASTER%HPH」","L"
					PRINTFORMDW 鈴瑚の笑顔を見て、%CALLNAME:MASTER%も嬉しくなった。
				ELSE
					PRINTFORML 「美味しかったよ。ご馳走様」
					PRINTFORMDW 鈴瑚はお茶を全部飲んでくれた。
				ENDIF
		ENDSELECT
	ELSEIF TFLAG:193 == 0
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「茶柱は…無し。今日も平凡な一日になりそうだねぇ」
				PRINTFORMW 「ずず……美味しい♪」
			CASE 1
				PRINTFORML 「―んぐッ！？……ぐ…！」
				PRINTFORMDL 鈴瑚は団子を喉に詰まらせたようだ…%CALLNAME:MASTER%の手からお茶をひったくると、一気に飲み干した。
				PRINTFORMW 「%TEXTR("ふぅ～…死ぬかと思った…/ごくっ…ふぅ、今のは危なかったね…/いや～…ありがと、助かったよ/")%」
			CASE 2
				PRINTFORML 「うん、美味しい。結構なお手前で」
				PRINTFORML 「……って言うんだよね？地上の"茶道"ってヤツは」
				PRINTFORMDW 鈴瑚は地上の食文化に詳しいようだ。
			CASE 3
				PRINTFORML 「……！…%CALLNAME:MASTER%、このお茶…」
				PRINTFORML 「―私の店で出してみない？」
				PRINTFORMDW 鈴瑚はいつになく真剣な表情だ……
			CASE 4
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「…ん？%CALLNAME:MASTER%、お茶っ葉変えたの？」
					PRINTFORML 「いいね、これ。私気に入ったかも」
					PRINTFORMDW 鈴瑚の口に合うお茶だったようだ。
				ELSE
					PRINTFORML 「%CALLNAME:MASTER%…さしずめキミの能力は、"いつでもお茶を淹れられる程度の能力"ってトコかな？」
					PRINTFORMDW 違う。
				ENDIF
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「おお、お茶。ありがと―」
				PRINTFORML 「あ゛っつ！！…ふー…ふー…」
				PRINTFORMDW どうやら熱すぎたようだ……
			CASE 1
				CALL COLORMESSAGE("「このお茶…団子には合わないなぁ…」", C_ORANGE, 0, 0)
				SETCOLOR 0xFFFF00
				PRINTFORML MGMG
				PRINTFORMDW 鈴瑚は苦々しい顔をしている…
			CASE 2
				PRINTFORML 「%CALLNAME:MASTER%…このお茶、あんまり美味しくないね…」
				PRINTFORML 「悪いけど、これをお客さんにお出しするのは許可できないよ」
				PRINTFORMDW いつの間にそんな話になっていたのだろう……？
			CASE 3
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「…ん？%CALLNAME:MASTER%、お茶っ葉変えたの？」
					PRINTFORML 「前のお茶好きだったのに…」
					PRINTFORMDW 鈴瑚は少し残念そうだ……
				ELSE
					PRINTFORML 「う～ん…私はもう少し、濃い方が好きかな？」
					PRINTFORMDW このお茶は、鈴瑚の好みに合わなかったようだ…
				ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

