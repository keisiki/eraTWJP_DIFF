			ENDSELECT
		ENDIF
	ELSE
		IF FLAG:デート相手 == 108
			SELECTCASE RAND:2
				CASE 0
					CALL RINGO_NORMAL
				CASE 1
					CALL RINGO_OTENKI
			ENDSELECT
		ELSE
			SELECTCASE CFLAG:現在位置
			;人間の里
				CASE 201,202,203,204,205,206,230,240,210,220,230,240,250,270
					CALL RINGO_ODANGO
				CASEELSE
					SELECTCASE RAND:2
						CASE 0
