@M_KOJO_MESSAGE_COM_K108_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		PRINTFORMDW 時が止まった鈴瑚の尻を撫でた。安産型で丁度良い大きさだ。
	ELSEIF CFLAG:睡眠
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「んん…んぅ…？」
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%～…！何して……Zzz…」
				PRINTFORMDW 起きたかと思ったが、寝言だったようだ…
		ENDSELECT
	ELSEIF CFLAG:うふふ == 2
		SELECTCASE TCVAR:20
			;クンニ強制
			CASE 70
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「んんっHPH 今はお尻じゃなくて、おまんこでしょ…HPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「ひゃあっHPH HPH …もう、お返しっHPH」","L"
						PRINTFORMDW 鈴瑚が少し強めに秘部を押し付けてきた…！
					ENDSELECT
			;アナル奉仕
			CASE 72
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「ゆびぃっ…HPH お尻に…入って…あぁ…HPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「あぁっHPH お尻…もっと、おぉっHPH」","W"
				ENDSELECT
			;スカートの中を見せる
			CASE 35
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「んん…HPH ふふっ…HPH焦らしちゃってぇ…HPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「あ…HPH んん…HPH お尻…直接触られてる…HPH」","W"
				ENDSELECT
			;素股
			CASE 52
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あぁっHPH 私は気持ちよくしなくていいのぉっHPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「あんっHPH…お尻、スリスリしないでぇ…HPH」","W"
				ENDSELECT
			CASEELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「あぁっHPH わ、私は気持ちよくしなくていいのぉっHPH」","W"
					CASEELSE
						CALL HPH_PRINT,@"「あんっHPH…お尻、スリスリしないでぇ…HPH」","W"
				ENDSELECT
		ENDSELECT
	ELSE
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%～？　一体この手は何処を触ろうとしていたのかな～～？」
					PRINTFORMDW 鈴瑚に気付かれ、手を捻り上げられた……
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「んんっ…HPH %CALLNAME:MASTER%……ムラムラしちゃった？HPH」","W"
						CASE 1
							CALL HPH_PRINT,@"「ひゃあぁ…HPH ん、ぐっ……HPH」","L"
							PRINTFORMW （%CALLNAME:MASTER%……！まさか、こんな所で…！？）
						CASE 2
							CALL HPH_PRINT,@"「んん……！んっ……HPH ふ……HPH んぅぅ…HPH」","L"
							PRINTFORMDW 敏感な尻尾を触られ、鈴瑚は必死に嬌声を我慢している…！
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ひゃっHPHん…HPH ふふ…♪く、くすぐったい…HPH」","W"
						CASE 1
							CALL HPH_PRINT,@"「んっ…HPHはぁ…HPHはぁ…HPH %CALLNAME:MASTER%…HPHもっと…HPH」","L"
							PRINTFORMDW 鈴瑚は尻を撫でられ、昂り始めてきたようだ…！
						CASE 2
							CALL HPH_PRINT,@"「ひゃあHPH あHPH ああっHPHし、尻尾ぉっ…HPH HPH」","L"
							CALL HPH_PRINT,@"「%CALLNAME:MASTER%っHPH もっとHPH それ、もっとぉっHPH」","L"
							PRINTFORMDW 鈴瑚は尻尾を撫でられて甘い声を上げている……
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「――ッ！？…何すんの！」
					PRINTFORMDW 鈴瑚に引っ叩かれた……
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「や……やだぁ…HPHやめ、てぇ……HPH」","W"
						CASE 2
							CALL HPH_PRINT,@"「ひゃああっHPH HPH」","L"
							PRINTFORML 「―ッ！」
							PRINTFORMDW 尻尾を触ったら、鈴瑚に手を抓られた……
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「んん…HPH うぅ……ねぇ…まだ触るの？」","W"
						CASE 1
							CALL HPH_PRINT,@"「ひゅうぅんっHPH し、尻尾はぁっ…HPH」","L"
							CALL HPH_PRINT,@"「や、やめ…！あ……あぁっ…HPH」","L"
							PRINTFORMDW 鈴瑚は尻尾を撫でられるのが弱いようだ。
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

RETURN 1
