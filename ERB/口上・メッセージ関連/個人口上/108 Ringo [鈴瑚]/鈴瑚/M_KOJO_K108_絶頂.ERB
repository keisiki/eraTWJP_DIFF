;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな変数などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:Ｃ絶頂 NOWEX:Ｖ絶頂 NOWEX:Ｂ絶頂 NOWEX:Ａ絶頂 NOWEX:Ｍ絶頂
;各部位の絶頂強度。まず0~3が割り当てられ、多重絶頂により最大16倍される。つまり単部位なら最大3、5重絶頂で最大48
;NOWEX:TotalEX
;多重絶頂補正を受ける前の各部位絶頂強度の合計値。1~15。このテンプレにおける分岐の基準。

;TEQUIP:50
;Ｖに誰の竿が入ってるか。TEQUIP:50 = MASTERであなたが挿入中となる。
;TEQUIP:51
;Ａに誰の竿が入ってるか。TEQUIP:51 = MASTERであなたが挿入中となる。
;NOWEX:射精
;射精量、最大3。たとえばIF NOWEX:MASTER:射精 > 1だと、あなたが大量射精以上した場合となる。
;SYNCED_ORGASM(TARGET)
;相手とあなたが同時に絶頂した場合真となる。絶頂の部位は問わない。

;-------------------------------------------------
;絶頂時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K108
#DIM TotalEX
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	CALL HPH_PRINT,@"「く…ぅっ…HPH　おっぱい、たくさん出てっ…HPH」","W"
ENDIF
;-------------------------------------------------
;射精した
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 「～～ッ！……お願い、見ないで……！」
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;合計絶頂数で分岐
;これだけではどの部位が絶頂したかは区別されないため、書き分けたければ任意で分岐を加筆されたしHPH
IF LOCAL
	SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			SELECTCASE RAND:6
				CASE 0
					CALL HPH_PRINT,@"「イックうぅぅぅぅっHPH HPH HPH HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「～～～～ッHPH HPH HPHか……は……っHPH HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「ま、まって……！おっきいのきちゃ―んんんぅぅぅーーっHPH HPH HPH HPH」","W"
				CASE 3
					CALL HPH_PRINT,@"「～～～～ッHPH HPH HPH」","L"
					CALL HPH_PRINT,@"「はっ……HPHはっ……HPHちょっ…まっへ……HPHいき、できな……HPH HPH」","W"
				CASE 4
					CALL HPH_PRINT,@"「あ゛あ゛あぁぁぁぁーーーッHPH HPH HPH HPH」","W"
				CASE 5
					CALL HPH_PRINT,@"「またイクっHPH イクぅっHPH ああぁぁぁーーっHPH HPH HPH」","W"
			ENDSELECT
		;弱
		CASE 1 TO 4
			SELECTCASE RAND:6
				CASE 0
					CALL HPH_PRINT,@"「あぁっHPHイクっHPHイクうぅぅぅっHPH HPH HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「んんっ……HPHんんぅーっHPH HPH HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「イクHPHイッ―んん～～～っHPH HPH HPH」","W"
				CASE 3
					CALL HPH_PRINT,@"「あぁ……HPHふぁああぁぁぁっHPH HPH HPH」","W"
				CASE 4
					CALL HPH_PRINT,@"「ああぁぁーーーっHPH HPH HPH…はぁ…HPH はぁ…HPH」","W"
				CASE 5
					CALL HPH_PRINT,@"「ふぁっ……あぁっ……HPHあたま、チカチカするぅ……HPH HPH」","W"
			ENDSELECT
	ENDSELECT
ENDIF

