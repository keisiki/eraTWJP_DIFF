;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K108_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K108_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K108_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			CALL HPH_PRINT,@"「んっHPHんっHPHぢゅ……HPHん…♪」","L"
			PRINTFORMDL 鈴瑚が一所懸命に頭を動かして、ぺニスを奉仕してくれているが、もう少し刺激が欲しい……
			PRINTFORMDL %CALLNAME:MASTER%の目に、鈴瑚の垂れ下がったウサ耳が目に入った。
			PRINTFORML 「……？ ―んぶっ！？ん゛…ぐっ……！ん～～っ！！」
			PRINTFORMDW %CALLNAME:MASTER%は両手でウサ耳を掴むと、ぺニスを喉奥まで突き刺した……！
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「んぶっ！ん゛っ！ん゛っ！…ん゛んーっ！ん゛うぅーっ！」
					PRINTFORMDL 腰の動きが加速し、射精が近い事を察したのか…鈴瑚は"止めて"と懇願するように叫ぶ。
					PRINTFORMDW %CALLNAME:MASTER%はそれを無視するかのように、ぺニスを喉奥へとねじ込んだ…！
				CASE 1
					PRINTFORML 「ん゛……ん……ぐ……っ……」
					PRINTFORMDL 放心しているのか、鈴瑚の反応が薄くなってきた……
					PRINTFORMDW ぺニスを勢いよく喉奥へ捩じ込むと、虚ろだった目が大きく見開かれた―
				CASE 2
					PRINTFORML （もう嫌……早く終わって……）
					PRINTFORMDL 鈴瑚の頭ウサミミを掴み、喉で肉棒を扱いていた%CALLNAME:MASTER%だったが、射精感が近づいてきた。
					PRINTFORMDW "射精そうだ"と鈴瑚に告げ、更に喉奥へと肉棒を押し込んだ…！
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ん゛んぅっ…！ん……ぐぅ……っ…！」
					PRINTFORML （苦しいっ……！息、できな……！）
					PRINTFORMDW 鈴瑚は目尻に涙を浮かべながら、%CALLNAME:MASTER%のピストンに耐えている……
				CASE 1
					PRINTFORML 「んぶっ…！ん…！ごほっ！ごほっ！」
					PRINTFORMDW 喉奥をぺニスで刺激され、鈴瑚は込み上げてきた嘔吐感を必死に堪えているようだ……
				CASE 2
					PRINTFORML 「ん゛んっ……！ん゛っ…！ん、ぶっ……。ぐぅ……っ！」
					PRINTFORMDL 鈴瑚が%CALLNAME:MASTER%のぺニスを咥えこんだまま、睨みつけてくる……
					PRINTFORMDW 言い様の無い興奮を覚えた%CALLNAME:MASTER%は、更に激しく腰を突き出した…！
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

