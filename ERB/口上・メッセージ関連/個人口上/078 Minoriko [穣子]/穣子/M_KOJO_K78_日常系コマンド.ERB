		LOCAL:1 = GETCOLOR()
		SETCOLOR GETDEFCOLOR()
		CALL ASK_M(@"ついでに%CALLNAME:TARGET%のお尻の手入れや乳首についても願掛けする",1,"願掛けはこれだけ",1)
		IF RESULT == 0
			PRINTL
			PRINTFORMDL %CALLNAME:MASTER%はついでに%CALLNAME:TARGET%のお尻のお手入れと乳首についてお願いすることにした。

			$PRAYER_LOOP
			VARSET LOCALS

			SELECTCASE CFLAG:78:1001
			CASE 0
				LOCALS:0 = 変化しない
			CASE 1
				LOCALS:0 = 出産前後のみ変化する
			CASE 2
				LOCALS:0 = 妊娠にかかわらず母乳体質によって変化する
			CASEELSE
				LOCALS:0 = ※CFLAG:78:1001の値が不正※
			ENDSELECT

			SELECTCASE CFLAG:78:1002
			CASE 0
				LOCALS:1 = スカトロなし
			CASE 1
				LOCALS:1 = スカトロあり
			CASEELSE
				LOCALS:1 = ※CFLAG:78:1002の値が不正※
			ENDSELECT

			PRINTL
			PRINTFORMDL 乳首変化の現在の設定：%LOCALS:0%　　　スカトロ表現の現在の設定：%LOCALS:1%
			PRINTL
			CALL ASK_M("乳首変化の設定を変更する",1,"スカトロ表現の設定を変更する",1,"これでいい",1)
			SELECTCASE RESULT
			CASE 0
				IF CFLAG:78:1001 >= 2
					CFLAG:78:1001 = 0
				ELSE
					CFLAG:78:1001 += 1
				ENDIF
				PRINTL
				GOTO PRAYER_LOOP
			CASE 1
				CFLAG:78:1002 = !CFLAG:78:1002
				PRINTL
				GOTO PRAYER_LOOP
			CASE 2
				PRINTFORMDL %CALLNAME:TARGET%の乳首が%LOCALS:0%ようになった気がする。
				PRINTFORMD %CALLNAME:TARGET%が
				IF CFLAG:78:1002 == 0
					PRINTFORMD お尻をきれいにしてアナルセックスの準備を怠らない
				ELSE
					PRINTFORMD アナルセックスの前でもお尻をそのままにしておく
				ENDIF
				PRINTFORMDW ようになった気がする。
				PRINTL
			ENDSELECT
		ENDIF
		SETCOLOR LOCAL:1
	ENDIF
	IF K78_CONSCIOUS_AND_TOLERANT()
		SELECTCASE RAND:9
		CASE 1 TO 4
			PRINTFORMW 「なにをお願いしたの？」
		CASE 5 TO 8
			PRINTFORMW 「ずいぶん熱心にお祈りしてたわね」
		CASEELSE
			PRINTFORMW 「うーん… あなたの拝んでる神さま、なんとなくいかがわしいものを感じるような…」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

