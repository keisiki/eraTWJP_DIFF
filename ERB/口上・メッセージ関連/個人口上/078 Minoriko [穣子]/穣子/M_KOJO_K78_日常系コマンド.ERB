;TFLAG:193(1=大成功 0=成功 -1=失敗)
;ABL:親密(6以上=通常成功時分岐
;==================================================
@M_KOJO_MESSAGE_COM_K78_309
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_309_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_309_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_CONSCIOUS_AND_TOLERANT()
		$HEAD_PATTING_LOOP
		SELECTCASE TFLAG:193
		CASE 1
			IF ( TALENT:恋慕 || TALENT:愛欲 ) && RAND:2
				PRINTFORMDL %CALLNAME:MASTER%が頭を撫でると、%CALLNAME:TARGET%はうれしそうに体を密着させてきた。
				CALL K78_HPH_PRINT,@"「んふふHPH もっと撫でてHPH」","W"
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%のなめらかでつやのある金髪に感嘆した。
				PRINTFORMW 「私の髪、そんなにいいかな？ えへへ」
			ENDIF
		CASE 0
			IF ABL:親密 >= 6
				IF TALENT:恋慕 || TALENT:愛欲
					SELECTCASE RAND:3
					CASE 1
						IF TALENT:MASTER:性別 == 2 && TALENT:MASTER:体型 >= 0
							CALL K78_HPH_PRINT,@"「%CALLNAME:MASTER%の手、大きいのね。やっぱり男の人だものねHPH」","W"
						ELSE
							GOTO HEAD_PATTING_LOOP
						ENDIF
					CASE 2
						PRINTFORML 「も、もう終わり？」
						PRINTFORMDW %CALLNAME:TARGET%は物足りなさげな表情で見上げてきた。
					CASEELSE
						PRINTFORML 「あなたに撫でられてると、なんだか安心する…」
						PRINTFORMDW %CALLNAME:TARGET%は目をとじておだやかな笑みをうかべている。
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMW 「私子供じゃないのに…… まあ…%CALLNAME:MASTER%ならいっか」
					CASEELSE
						PRINTFORML 「言っておくけど、私だっていちおう神様なんだからね！」
						PRINTFORMDW そう言って威厳があるところを見せようとするが、撫でられたままだ。
					ENDSELECT
				ENDIF
			ELSE
				PRINTFORMW 「ちょ、ちょっと。恥ずかしいじゃないの。もうっ」
			ENDIF
		CASE -1
			PRINTFORMW 「いきなり頭をさわるなんて失礼ね！」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
