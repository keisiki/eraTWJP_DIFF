;305,膝枕してもらう
;TFLAG:193(-5=MASTERがTARGETよりも巨体の場合(TALENT:MASTER:体型 > TALENT:体型 + 2) 1=大成功 0=成功 -1=失敗)(-5以外は地の文分岐無し)
;ABL:親密(6以上=顔をのぞき込みながら頭を撫でてくれる 3以上=ただ頭を撫でてくれる 2以下=縮こまっている)
;==================================================
@M_KOJO_MESSAGE_COM_K78_305
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_305_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_305_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_CONSCIOUS_AND_TOLERANT() && TFLAG:193 > -5
		IF TALENT:恋慕 || TALENT:愛欲
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「はいはい、また膝枕ねHPH お疲れさまHPH」","W" 
			CASE 2
				CALL K78_HPH_PRINT,@"「疲れちゃったの？ いいわよ。はい、どうぞHPH」","W" 
			CASEELSE
				PRINTFORMDL 甘い香りにつつまれながら横になっていると眠たくなってきた。
				CALL K78_HPH_PRINT,@"「眠い？ しばらく眠ってていいわよ。ずっとこうしててあげるからHPH」","W" 
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
			CASE IS >= 0
				SELECTCASE ABL:親密
				CASE IS >= 6
					PRINTFORML 「またベタベタして…… あなたって誰にでもこういうことしてるの？」
					PRINTFORMDW 口ではそう言うものの、%CALLNAME:TARGET%は微笑をうかべている。
				CASE 3 TO 5
					IF TALENT:MASTER:幼稚
						PRINTFORML 「子供だし、しょうがないか」
					ELSE
						PRINTFORML 「%CALLNAME:MASTER%ってときどき子供みたい…」
					ENDIF
					PRINTFORMDW %CALLNAME:TARGET%は呆れながらも頭をなでてくれた。
				CASE IS <= 2
					PRINTFORMW 「こ、これはさすがに恥ずかしすぎるんだけど…」
				ENDSELECT
			CASE -1
				PRINTFORMW 「…あし痛いし恥ずかしいし、もうやめっ」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
