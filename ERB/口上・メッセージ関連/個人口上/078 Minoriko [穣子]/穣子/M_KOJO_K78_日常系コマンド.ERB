;300,会話
;TFLAG:193(1=大成功 0=成功 -1=失敗 -2=大失敗（TARGETがMASTERより教養が4以上高い)
;TCVAR:302(0=会話可能 1>=非恋慕時会話不能)
;MARK:反発刻印(3以上=会話不能)(4以上はバグ
;TCVAR:泥酔(1=通常成功or大失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K78_300
;現在地が浴場で恋慕or愛欲でしらふのときは体毛指定関数を呼び出す
IF BATHROOM() && (TALENT:恋慕 || TALENT:愛欲) && SHIRAHU(TARGET)
	CALL K78_HAIR_TREATMENT
	;体毛関係のイベントを選んだ場合は通常の会話口上無し
	SIF RESULT == 1
		RETURN 1
ENDIF
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:ブチギレ
		PRINTFORMDW 怒った%CALLNAME:TARGET%はそっぽを向いている。
	ELSEIF TCVAR:泥酔
		IF TALENT:機嫌 >= 0
			PRINTFORMDL 気分がいいのか%CALLNAME:TARGET%はけらけらと笑っている。
			PRINTFORMW 「なぁに？ よっれないわよぉー　らいじょぶらいじょぶ」
		ELSE
			PRINTFORML 「ちょっとぉ、わらひのはなし聞いてるぅ？ 聞きなさいったらぁ！」
			PRINTFORMDW どうやらよくない酔い方をしているようだ。
		ENDIF
	ELSE
		SELECTCASE TFLAG:193
		CASE 1
			IF ( TALENT:恋慕 || TALENT:愛欲 ) && RAND:3
				SELECTCASE RAND:2
				CASE 1
					CALL K78_HPH_PRINT,@"「ご飯がいっぱい食べられて、こうしてあなたと一緒にいられる…… 私って幸せ者ね…HPH」","W"
				CASEELSE
					PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の美しさを褒めたたえた。
					CALL K78_HPH_PRINT,@"「もうっ　おだてたってなにも出ないわよHPH」","L" 
					SIF CFLAG:既成事実 & 2
						CALL K78_HPH_PRINT,@"「でも、今夜はちょっとサービスしてあげてもいいかなHPH」","L" 
					WAIT
				ENDSELECT
			ELSE
				PRINTFORML 「あははっ♪ %CALLNAME:MASTER%ってばおもしろいわね」
				PRINTFORMDW とても受けたようだ。
			ENDIF
		CASE 0
			;天気の話題
			IF GET_MAPNAME(GET_MAPID(CFLAG:MASTER:現在位置)) != "地底" && !RAND:5
				SELECTCASE TIME:5
				;晴れ
				CASE 0
					;１９時以降
					IF TIME:0 >= 1140
						PRINTFORMW 「今日は星がとっても綺麗」
					;冬
					ELSEIF DAY:2 == 4
						;７～１７時
						IF BETWEENTIME(420,1020)
							PRINTFORMW 「日が出てるおかげで過ごしやすいわ」
						ELSE
							PRINTFORMW 「冬晴れの乾燥には注意よ」
						ENDIF
					ELSE
						PRINTFORMW 「うん、いい天気。作物もぐんぐん育つといいわね」
					ENDIF
				;快晴
				CASE 1
					;夏
					IF DAY:2 == 2
						;起床～７時
						IF BETWEENTIME(CFLAG:起床時間,420)
							PRINTFORMW 「今日は暑くなりそうだから、涼しいあいだに仕事を片付けた方がいいわよ」
						;７～１９時						
						ELSEIF BETWEENTIME(420,1140)
							PRINTFORMW 「日射しが強いわねー。暑さでやられないよう気を付けなきゃだめよ」
						;１９時以降
						ELSE
							PRINTFORMW 「日が沈んでもまだまだ暑くていやになっちゃう」
						ENDIF
					;冬
					ELSEIF DAY:2 == 4
						PRINTFORMW 「天気はとってもいいけど、霜が降りないか心配だわ」
					ELSE
						PRINTFORMW 「雲ひとつ無いいい天気ね。気分がいいわ」
					ENDIF
				;薄曇、曇り
				CASE 2,3
					;曇りで１９時以降
					IF TIME:5 == 3 && TIME:0 >= 1140
						PRINTFORMW 「今日は曇ってて星がよく見えないのが残念…」
					;冬の起床～１７時
					ELSEIF DAY:2 == 4 && BETWEENTIME(CFLAG:起床時間,1020)
						PRINTFORMW 「寒いわね。お日様が出てくれないかな」
					;夏の起床～１７時
					ELSEIF DAY:2 == 2 && BETWEENTIME(CFLAG:起床時間,1020)
						PRINTFORMW 「雲が出てくれたわ。農作業にいい日和ね」
					;曇り
					ELSEIF TIME:5 == 3
						PRINTFORMW 「雲が濃いわね。雨になるかも」
					ELSE
						PRINTFORM 「あの雲、
						SELECTCASE RAND:4
						CASE 1
							PRINTFORM たけのこ
						CASE 2
							PRINTFORM ナスビ
						CASE 3
							PRINTFORM おにぎり
						CASEELSE
							PRINTFORM 焼き芋
						ENDSELECT
						PRINTFORMW みたいでおいしそう…」
					ENDIF
				;雨、霧雨、みぞれ
				CASE 4,6,12
					;夏
					IF DAY:2 == 2
						PRINTFORMW 「ムシムシして気持ち悪い…」
					ELSE
						PRINTFORMW 「天気が悪いわね。お家でのんびりするのもいいかも」
					ENDIF
				;大雨
				CASE 5
					;台風
					IF FLAG:異常気象 == 9
						PRINTFORMW 「ひどい風と雨…… 作物は大丈夫かな…」
					ELSE
						PRINTFORMW 「すごい雨。出歩かない方がいいわよ」
					ENDIF
				;霧
				CASE 7
					PRINTFORMW 「霧が深いわ。出かけるなら気を付けてね」
				;雪、細雪、霧雪
				CASE 8,10,11
					PRINTFORMW 「雪がふってるのね。寒いはずだわ」
				;吹雪、あられ
				CASE 9,13
					PRINTFORMW 「ひどい天気ねー。今日は家にいた方がいいわよ」
				ENDSELECT
			;季節の話題
			ELSEIF RAND:2
				SELECTCASE DAY:2
				CASE 1
					SELECTCASE RAND:4
					CASE 1
						PRINTFORMW 「春と言えばたけのこよね。たけのこご飯♪ たけのこご飯♪」
					CASE 2
						PRINTFORML 「山菜の季節よ。ぜんまい、たらの芽、ふきのとう、野蒜」
						PRINTFORMW 「……お酒が飲みたくなってきちゃった」
					CASE 3
						PRINTFORMW 「春になると心がうきうきしてこない？」
					CASEELSE
						PRINTFORMW 「そろそろ田植えね。『苗半作』っていうくらいこの時期の苗づくりが大事なのよ」
					ENDSELECT
				CASE 2
					SELECTCASE RAND:4
					CASE 1
						PRINTFORML 「夏といえば、トマト、ナス、キュウリ、ピーマンなんかの美味しい野菜！」
						PRINTFORMW 「健康にもいいからしっかり摂らなきゃだめよ」
					CASE 2
						PRINTFORML 「夏の田んぼの仕事は草とり、虫の駆除、水の管理ね」
						PRINTFORMW 「あつくて大変だけど、あつさも冷害よりはずっとマシだもの」
					CASE 3
						PRINTFORMW 「あついわねー。かき氷でも食べたいわ」
					CASEELSE
						PRINTFORMW 「夏は台風に注意よ。一日中家にとじこめられちゃうんだもん」
					ENDSELECT
				CASE 3
					SELECTCASE RAND:4
					CASE 1
						PRINTFORML 「すっかり秋ね。名にし負う、私の季節よ」
						PRINTFORMDW %CALLNAME:TARGET%は胸をはっている。
					CASE 2
						PRINTFORML 「収穫の時期ね。今年も初穂のお米をいっぱい食べられるわ」
						PRINTFORMDW %CALLNAME:TARGET%はホクホク顔だ。
					CASE 3
						PRINTFORMW 「黄金色の稲穂だって紅葉に劣らず美しいと思わない？」
					CASEELSE
						PRINTFORMW 「秋が旬といえばキノコ。炒めてよし、揚げてよし、煮てよしでとっても重宝するわ」
					ENDSELECT
				CASE 4
					SELECTCASE RAND:4
					CASE 1
						PRINTFORMW 「冬は気分が落ち込むの。はやく春が来ないかなぁ」
					CASE 2
						PRINTFORMW 「寒い冬には焼き芋。甘くていい匂いで大好き♪」
					CASE 3
						PRINTFORMW 「うー、寒いわ。こたつにずっと入っていたい…」
					CASEELSE
						PRINTFORMW 「冬はやっぱり根菜よね。おいしい煮物にしてあったまりたいわ」
					ENDSELECT
				ENDSELECT
			;その他の話題
			ELSE
				IF ( TALENT:恋慕 || TALENT:愛欲 ) && RAND:2
					SELECTCASE RAND:4
					CASE 1
						PRINTFORML 「ね、あなたの好きな食べ物ってなにがある？」
						PRINTFORMDL %CALLNAME:TARGET%に唐突に質問された。
						PRINTFORMW 「ふーん、じゃあその材料を育ててみよっかな」
					CASE 2
						PRINTFORMW 「ん？ この匂い？ ふふっ、豊穣の神特製の香水よ♪」
					CASE 3
						PRINTFORML 「でねでね、この前おもしろいことがあったのよ」
						PRINTFORMDW %CALLNAME:TARGET%はからだが触れ合うほど近くに寄ってきて楽しそうに話を続けた。
					CASEELSE
						PRINTFORMW 「あ、もうこんな時間。あなたとしゃべってたら時間がすぐすぎるわ」
					ENDSELECT
				ELSE
					IF FLAG:なりきり != 77 && !RAND:4
						PRINTFORML 「紅葉なんて食べられないじゃないって姉さんに言ったら、もみじの天ぷらがあるって返されたの」
						PRINTFORMW 「おいしいのかな？」
					ELSEIF FLAG:なりきり != 77 && !RAND:3
						PRINTFORML 「あのね、姉さんったら%TEXTR("すごい/ひどい/おもしろい")%のよ。この前なんてね……」
						PRINTFORMDW %CALLNAME:77%の話をする%CALLNAME:TARGET%はとても楽しそうだ。
					ELSEIF RAND:2
						PRINTFORML 「ねえ%CALLNAME:MASTER%、どの季節がいちばん好き？」
						PRINTFORMDW 秋と答えざるをえない雰囲気だ。
					ELSE
						;農民でなく園芸用品をもっていない
						IF !FLAG:地主 && !ITEM:園芸用品一式
							PRINTFORMW 「土いじりはいいわよー。あなたも家庭菜園やってみたら？」
						ELSE
							PRINTFORMW 「畑の調子はどう？ 豊作には日々の手入れあるのみよ」
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		CASE -1
			PRINTFORML 「ふーん…」
			PRINTFORMDW %CALLNAME:TARGET%には興味のない話題だったようだ。
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
