;-------------------------------------------------
;奴隷の絶頂時の口上
;NOWEX:0～4 CVABM絶頂
;NOWEX:6 二重絶頂
;NOWEX:7 三重絶頂
;NOWEX:8 四重絶頂
;NOWEX:9 五重絶頂
;NOWEX:10 噴乳
;NOWEX:11 射精
;NOWEX:12 放尿
;適当に組み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K78
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_CONSCIOUS_AND_TOLERANT() && !TALENT:無知
		;-------------------------------------------------
		;五重絶頂
		;-------------------------------------------------
		IF NOWEX:五重絶頂
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「おっ、んぉっHPH んはぁHPH あぁ、あっ、ああ゛あ゛あぁっHPH い、い゛ぐううぅぅHPH」","L"
				PRINTFORMDL %CALLNAME:TARGET%は大きな叫び声をあげたあと、糸が切れたように全身をだらりと弛緩させ、ぜーぜーと肩で息をしている。
				PRINTFORMW 「フヒュー、フゥー、フゥー ……も…もう…ゆるして…」
			CASE 2
				CALL K78_HPH_PRINT,@"「ひゃっ、あぁっ、あっHPH んああぁっHPH んぃ、んひいぃぃHPH」","L"
				PRINTFORMDL %CALLNAME:TARGET%は涙や鼻水、涎にまみれた顔もそのままに、びくびくと痙攣しながらもだえている。
				CALL K78_HPH_PRINT,@"「はぁ、はっ、はぇHPH ぉ、ぅあ、はぁHPH ふぅ、ふぅHPH んぅHPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「死ぬっ、死んじゃうっHPH ダメぇっHPH だめえぇえっHPH ほぉっ、んほおお゛お゛ぉぉHPH」","L"
				PRINTFORMDL 悲鳴のような声をあげた%CALLNAME:TARGET%は、ぐったりとして%CALLNAME:MASTER%にもたれかかった。
				CALL K78_HPH_PRINT,@"「はぁ、はぁ…… す、すごすぎよぉHPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;四重絶頂
		;-------------------------------------------------
		IF NOWEX:四重絶頂
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「やあぁっ、だめえぇHPH もうっ、もう許してぇHPH はっ、んはああぁっHPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「もうムリっHPH キモチよすぎておかしくなるっHPH あたま真っ白になっちゃうからぁHPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「やめてぇっHPH もうイったからぁHPH きもちよすぎてっHPH とぶのっHPH とんじゃうのっHPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;三重絶頂
		;-------------------------------------------------
		IF NOWEX:三重絶頂
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「やぁっHPH これすごいのっHPH あっ、んあぁHPH あひ、んひぃッHPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「んっ、んぉHPH だ、だめぇHPH イクっHPH いっぱいイっちゃうっHPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「んひィッHPH ひぃっHPH だ、だめっHPH んぃ、ィッ～～っHPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;二重絶頂
		;-------------------------------------------------
		IF NOWEX:二重絶頂
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「あっ、はっ、んはぁHPH キちゃうっ キちゃううぅHPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「はぁ、いいよぉHPH はっ、あっ、ふあぁっ　イっちゃうっHPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「んはぁっ、はぁんっ、イイっHPH %CALLNAME:MASTER%っ　キモチイイよぉHPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;単部位の絶頂
		;-------------------------------------------------	
		IF NOWEX:Ｃ絶頂
			;超強絶頂Ｃ
			IF NOWEX:Ｃ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「\@ TALENT:性別 & 2 ?おちんちん #クリ \@きもちいいっHPH あんっHPH ん、んぃっHPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「んふぅHPH んんぅHPH おっ、\@ TALENT:性別 & 2 ?おちんちん #お豆 \@キちゃうぅHPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「\@ TALENT:性別 & 2 ?おちんちん #お豆 \@すごっ、すごいのぉっHPH あぁ、はぁっ、はんぅっHPH」","W"
				ENDSELECT
			;強絶頂Ｃ
			ELSEIF NOWEX:Ｃ絶頂 == 2
				CALL K78_HPH_PRINT,@"「ふあっ　はっ、いっ、イクうぅっHPH」","W" 
			;絶頂Ｃ
			ELSE
				PRINTFORMW 「あっ、んぁっ、はっ　そこ、いいっ、いいのっ」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ｖ絶頂
			;超強絶頂Ｖ
			IF NOWEX:Ｖ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「んぅっ、おまんこすごいっHPH んあっ、んはぁHPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「すごいよぉHPH おまんこがイっちゃうのぉHPH んくっ、っ～～ッHPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「うねってキモチイイっHPH おまんこもっとグリグリしてぇっHPH」","W"
				ENDSELECT
			;強絶頂Ｖ
			ELSEIF NOWEX:Ｖ絶頂 == 2
				CALL K78_HPH_PRINT,@"「あふっ、あっ、ふあっ　なか、キモチイイHPH」","W"
			;絶頂Ｖ
			ELSE
				PRINTFORMW 「んっ、あっ、いいところっ、あたってるっ」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ａ絶頂
			;超強絶頂Ａ
			IF NOWEX:Ａ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「んぉっHPH クるよぉHPH おしりですごいのがキちゃうっHPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「んっ、んひぃHPH おしりまんこっHPH おしりまんこイクううぅHPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「あっ、やぁっHPH うんちの穴でイクのHPH はぁん、んあぁっHPH」","W"
				ENDSELECT
			;強絶頂Ａ
			ELSEIF NOWEX:Ａ絶頂 == 2
				CALL K78_HPH_PRINT,@"「あっ、おしりがっ、ふあ、あはぁっHPH」","W"
			;絶頂Ａ
			ELSE
				PRINTFORMW 「あふっ、ん、んくっ　あっ、はあっ」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ｂ絶頂
			;超強絶頂Ｂ
			IF NOWEX:Ｂ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「あはぁHPH き、きもちっHPH おっぱいキモチイイのっHPH 」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「乳首ジンジンってぇHPH すごいよぉっHPH んひぃ、ぃ～～っHPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「胸がっ、おっぱいが熱いよHPH イクっ、おっぱいでイクっHPH」","W"
				ENDSELECT
			;強絶頂Ｂ
			ELSEIF NOWEX:Ｂ絶頂 == 2
				CALL K78_HPH_PRINT,@"「んぁっ　あっ、胸いいっ、いいっHPH」","W"
			;絶頂Ｂ
			ELSE
				PRINTFORMW 「んっ、はぁ、ん、あぁん」
			ENDIF
			RETURN 1
		ENDIF
		[SKIPSTART]
		IF NOWEX:Ｍ絶頂
			;超強絶頂Ｍ
			IF NOWEX:Ｍ絶頂 == 3
				PRINTFORMW 
			;強絶頂Ｍ
			ELSEIF NOWEX:Ｍ絶頂 == 2
				PRINTFORMW 
			;絶頂Ｍ
			ELSE
				PRINTFORMW 
			ENDIF
		ENDIF
		[SKIPEND]
	ENDIF
ENDIF
RETURN 1
