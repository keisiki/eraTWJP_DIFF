;==================================================
;45,アナルバイブ
;==================================================
@M_KOJO_MESSAGE_COM_K78_45
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_45_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_45_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	IF K78_BASIC_CONDITION_IN_MATING()
		;装着
		IF TEQUIP:アナルバイブ
			IF TALENT:恋慕 || TALENT:愛欲
			
			ELSE
			
			ENDIF
		;解除
		ELSE
			IF 充填率(TARGET,2) >= 60 && !RAND:3
				IF CFLAG:1002
					IF ( TALENT:恋慕 || TALENT:愛欲 ) && !TCVAR:351 && !RAND:5
						PRINTFORMDL 額に汗をうかべながら便意に耐えていた%CALLNAME:TARGET%だったが、ついに限界が来た肛門は一気に口をひらくやいなやブチブチという激しい音とともに精液まじりの糞便をまき散らした。
						CALL DIALOGUE_K78_DEFECATION_IN_MATING
					ELSEIF !TCVAR:351 && RAND:2
						PRINTFORMDL あふれんばかりの精液を直腸に流し込まれたためか、%CALLNAME:TARGET%の腹がギュルギュルと鳴った。
						PRINTFORMDL 便意をもよおしながらも必死に我慢しているようだ。
					ELSE
						PRINTFORMDL \@ 充填率(TARGET,2) >= 100 ?腹がふくれるほど #大量の \@精液を注ぎ込まれた%CALLNAME:TARGET%は、腸液がまじって独特のにおいを放つ%TEXTR("精液/ザーメン/子種汁/白濁液")%を%TEXTR("肛門/アヌス/尻の穴")%から垂れ流している。
					ENDIF
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%の肛門からバイブを引き抜くと、直腸に大量に注ぎ込まれた%TEXTR("精液/ザーメン/子種汁/白濁液")%がゆっくりと垂れ落ちてきた。
				ENDIF
			ELSE
				IF CFLAG:1002 && ( TALENT:恋慕 || TALENT:愛欲 ) && !TCVAR:351 && !RAND:20
					PRINTFORMDL %CALLNAME:TARGET%は不意にモジモジしだしたかと思うと、大きな放屁音とともにビチャビチャとした下痢ぎみの便を肛門から噴き出した。
					CALL DIALOGUE_K78_DEFECATION_IN_MATING
				ELSEIF CFLAG:1002 && !RAND:5
					PRINTFORMDL バイブの振動に刺激されて腸が蠕動しはじめたのか、%CALLNAME:TARGET%は%TEXTR("ぷすぅー/ブウゥ/ボバッ/ぶぴっ")%と屁をひってしまった。
					CALL DIALOGUE_K78_FARTING_IN_MATING
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%の%TEXTR("肛門/アヌス/尻の穴")%は\@ TALENT:MASTER:性別 & 2 ?%TEXTR("肉棒/陰茎/ペニス/イチモツ")% #張り形 \@が抜かれてもなお%TEXTR("ぽっかり/ぱくぱく/ヒクヒク")%と口をあけ、%TEXTR("肛内の赤い肉/ぬらぬらとした粘膜")%をさらしている。
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

