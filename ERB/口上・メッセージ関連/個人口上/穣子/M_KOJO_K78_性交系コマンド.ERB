;### ＶＡ共通騎乗位口上用関数 ################
@DIALOGUE_K78_VA_COWGIRL,ARGS

IF K78_BASIC_CONDITION_IN_MATING()
	$VA_COWGIRL_LOOP
	IF TALENT:恋慕 || TALENT:愛欲
		SELECTCASE RAND:9
		CASE 1
			IF !TEQUIP:縄 && !TEQUIP:拘束 && !TEQUIP:継続キス && ( ( ABL:欲望 >= 8 && ABL:露出癖 - TALENT:羞恥心 * 2 >= 4 ) || ( !( CFLAG:1007 && K78_ARMPIT_EXPOSURE() ) && !K78_SWEATY_UNDERARMS() && ABL:欲望 >= 4 && ABL:露出癖 - TALENT:羞恥心 >= 1 ) )
				IF ABL:欲望 >= 8 && ABL:露出癖 - TALENT:羞恥心 * 2 >= 4
					IF CFLAG:1007 && K78_ARMPIT_EXPOSURE()
						PRINTFORMDL %CALLNAME:TARGET%は両手を頭のうしろで組み、%K78_ARMPIT_HAIR_MODIFIER()%腋を見せつけながら腰をふった。
						CALL K78_HPH_PRINT,@"「はいっHPH %CALLNAME:MASTER%の好きな腋毛どうぞっHPH」","W"
					ELSEIF K78_SWEATY_UNDERARMS()
						PRINTFORMDL 両手を頭のうしろに組んで腰をふる%CALLNAME:TARGET%の腋の下には大きな汗ジミができていた。
						CALL K78_HPH_PRINT,@"「やぁんHPH すごい汗でっHPH 恥ずかしいっHPH」","W"
					ELSE
						PRINTFORMDL %CALLNAME:TARGET%は両手を頭のうしろで組み、脚を大きくひらいた下品な格好で激しく腰をふった。
						IF ABL:欲望 >= 20 && RAND:2
							CALL K78_HPH_PRINT,@"「んぉっ、ほぉっHPH はしたない私を見てぇっHPH」","W"
						ELSE
							CALL K78_HPH_PRINT,@"「はぁっ、はぁんHPH はしたない私を見てぇっHPH」","W"
						ENDIF
					ENDIF
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%は両手を頭のうしろで組んだまま腰をふった。
					CALL K78_HPH_PRINT,@"「こういうイヤラシイ格好、好きでしょ？HPH」","W"
				ENDIF
			ELSE
				GOTO VA_COWGIRL_LOOP
			ENDIF
		CASE 2
			IF ( EQUIP:スカート || EQUIP:着物 || EQUIP:ワンピース ) && !TEQUIP:縄 && !TEQUIP:拘束 && !TEQUIP:継続キス && ABL:露出癖 - TALENT:羞恥心 >= 2
				PRINTFORMD %CALLNAME:TARGET%はみずから裾をつまんで持ち上げ、
				IF CFLAG:ノーパン
					IF CFLAG:1003 && RAND:3
						PRINTFORMD %K78_GENITAL_HAIR_MODIFIER()%股間を
					ELSE
						PRINTFORMD \@ TALENT:MASTER:性別 & 2 ?ペニス #張り形 \@をくわえこんだ\@ ARGS == "V" ?性器 #排泄穴 \@を
					ENDIF
				ELSE
					PRINTFORMD パンツを穿いたままつながった二人の結合部を
				ENDIF
				PRINTFORMDL 見せてきた。
				CALL K78_HPH_PRINT,@"「ね、見て見てHPH えっちでしょHPH」","W"
			ELSE
				GOTO VA_COWGIRL_LOOP
			ENDIF
		CASE 3
			IF ABL:腰 >= 3 && ( ( ARGS == "V" && ABL:膣 >= 3 ) || ( ARGS == "A" && ABL:アナル >= 3 ) ) && TALENT:MASTER:性別 & 2 && !TEQUIP:アイマスク
				IF ARGS == "A"
					PRINTFORMDL %CALLNAME:TARGET%は腰をふりながら括約筋を緊張させ、すぼまった肛門管でたくみにカリ首を刺激してきた。
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%の膣は腰ふりに合わせてうねうねと絶妙に蠕動し、%CALLNAME:MASTER%の子種をしぼりとろうとしているようだ。
				ENDIF
				CALL K78_HPH_PRINT,@"「ふふっHPH 感じてる顔、かわいいHPH」","W"
			ELSE
				GOTO VA_COWGIRL_LOOP
			ENDIF
		CASE 4
			IF !TEQUIP:縄 && !TEQUIP:拘束 && !TEQUIP:継続キス && !TEQUIP:アイマスク
				PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%の手をとると、指をからめながら嬉しそうに顔をのぞきこんできた。
				CALL K78_HPH_PRINT,@"「あなたの顔をよく見たいのHPH やんHPH 恥ずかしがらないでちゃんとこっち見てHPH」","W"
			ELSE
				GOTO VA_COWGIRL_LOOP
			ENDIF
		CASE 5
			IF !TEQUIP:継続キス && !TEQUIP:胸揉み && !TEQUIP:乳首吸い
				PRINTFORMDL %CALLNAME:TARGET%に向こうを向かせ、%CALLNAME:MASTER%の上で腰をふる様子を背面から見て楽しむことにした。
				CALL K78_HPH_PRINT,@"「あっHPH うしろ向きだとっ、いつもと違うところにあたってっ、んぁ、んんっHPH」","L"
				PRINTFORMD %CALLNAME:TARGET%の形のいい尻をなでまわしつつ
				IF TEQUIP:下半身着衣状況 == 0 && ARGS == "V" && !RAND:3
					PRINTFORMDW 、ときおり尻たぶを開いてかわいらしい肛門を鑑賞した。
