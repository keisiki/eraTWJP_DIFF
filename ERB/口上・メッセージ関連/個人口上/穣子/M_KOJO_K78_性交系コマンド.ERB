;### ＶＡ共通対面座位口上用関数 ################
@DIALOGUE_K78_VA_FACE_TO_FACE_SITTING,ARGS

IF K78_BASIC_CONDITION_IN_MATING()
	$VA_FACE_TO_FACE_SITTING_LOOP
	IF TALENT:恋慕 || TALENT:愛欲
		SELECTCASE RAND:6
		CASE 1
			IF !TEQUIP:縄 && !TEQUIP:拘束 && !TEQUIP:継続キス && !TEQUIP:乳首吸い && ( K78_ARMPIT_EXPOSURE() || K78_SWEATY_UNDERARMS() )
				IF K78_ARMPIT_EXPOSURE()
					PRINTFORMD 膝の上で腰をふる%CALLNAME:TARGET%に腕をあげさせ、
					IF K78_ARMPIT_SWEAT() && RAND:10 >= 7
						PRINTFORMD 汗でじっとりと湿った
					ELSE
						PRINTFORMD %K78_ARMPIT_HAIR_MODIFIER()%
					ENDIF
					PRINTFORMDL 腋に舌を這わせた。
					CALL K78_HPH_PRINT,@"「んっHPH くすぐったいけどっ、キモチイイかもHPH んぁ、はぁHPH」","W"
				ELSE
					PRINTFORMDL 膝の上にのせた%CALLNAME:TARGET%に片腕をあげさせ、汗ジミのできた腋に顔をつっこみながら腰を動かした。
					CALL K78_HPH_PRINT,@"「ちょ、やぁんHPH 本当に変態っぽいわよHPH や、におい嗅ぎすぎっHPH」","W"
				ENDIF
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 2
			IF TEQUIP:継続キス
				PRINTFORMDL %CALLNAME:TARGET%はクネクネと腰をふりながら%CALLNAME:MASTER%に体を密着させ、しきりに口づけを求めてきた。
				CALL K78_HPH_PRINT,@"「はむっ、ん…… んちゅ… はぁHPH ちゅっHPH ちゅぱ…あむ…… ん、もっとぉHPH」","W"
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 3
			IF !TEQUIP:継続キス
				CALL K78_HPH_PRINT,@"「はぁっ はぁっHPH んっ、あはっ、はぁ、はぁんHPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%の唇からいつにもまして甘い香りのする吐息がもれ、%CALLNAME:MASTER%の顔にふきかかる。
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 4
			IF !TEQUIP:縄 && !TEQUIP:拘束
				PRINTFORMDL %CALLNAME:MASTER%が%CALLNAME:TARGET%の腰に腕を回してギュッと抱きしめると、それにこたえて%CALLNAME:TARGET%は腕を首にからめてきた。
				CALL K78_HPH_PRINT,@"「えへへ♪ ギュッてするのいいねHPH %CALLNAME:MASTER%の心臓がドキドキしてるのまで感じられるものHPH」","W"
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 5
			PRINTFORMDL 互いにあまり腰を動かせないもどかしさが余計に熱情を高め、耳もとでしきりに愛をささやき合った。
			CALL K78_HPH_PRINT,@"「はぁ、はぁっ、好き、大好きHPH 愛してるわHPH あなたとずっとこうしていたいHPH」","W"
		CASEELSE
			PRINTFORMDL クイクイと腰をグラインドさせる%CALLNAME:TARGET%の煽情的な姿が、%CALLNAME:MASTER%の欲情を高ぶらせる。
			CALL K78_HPH_PRINT,@"「えへHPH この動きどう？ 気持ちよくなってくれるとうれしいなHPH」","W"
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			IF ABL:腰 >= 3
				PRINTFORMDL %CALLNAME:TARGET%はゆっくりとおだやかに動きつつも確実に%CALLNAME:MASTER%の快感を高めていく。
				PRINTFORMW 「こういうゆったりしたエッチも悪くないわね」
			ELSE
				PRINTFORMW 「私も動くの？ う、うん。でもなかなか難しいのよね…」
			ENDIF
		CASE 2
			PRINTFORML 「大丈夫？ お、重くないかな？」
			PRINTFORMDW %CALLNAME:TARGET%は年頃の女の子らしく体重を気にしているようだ。
		CASEELSE
			PRINTFORMW 「こ、この格好だと顔が近くて…ちょっと恥ずかしい…」
		ENDSELECT
	ENDIF
ENDIF


