;==================================================
;311,抱き付く
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 60=ディープキス 73=ぱふぱふ 74=乳合わせ 77=乳を吸わせる ＶＡ性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K78_311
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_311_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_BASIC_CONDITION_IN_MATING()
		IF CFLAG:うふふ == 2
			
		ELSEIF CHK_DATENOW(CFLAG:LOCAL:デート中) && CFLAG:MASTER:現在位置 != OMANEKIBEYA()
			
		ELSE
			IF BASE:ムード >= 500 && BASE:理性 < 975
				;嬉しそうに抱き返してきた…
				IF TALENT:恋慕
					SELECTCASE RAND:2
					CASE 1
						CALL K78_HPH_PRINT,@"「私もギュってしてあげるHPH」","W"
					CASEELSE
						CALL K78_HPH_PRINT,@"「んふふHPH なんだか安心する…HPH」","W"
					ENDSELECT
				;照れくさそうにしながらも、満更でもないようだ…
				ELSEIF TALENT:思慕
					SELECTCASE RAND:2
					CASE 1
						PRINTFORML 「う、動けないんだけど…」
						PRINTFORMDW そう言いながらも%CALLNAME:TARGET%のほおは弛んでいる。
					CASEELSE
						PRINTFORMW 「またべたべたして… は、恥ずかしいじゃないっ」
					ENDSELECT
				;恥ずかしそうにしている…
				ELSE
					IF TALENT:愛欲
						SELECTCASE RAND:2
						CASE 1
							CALL K78_HPH_PRINT,@"「さびしいなら慰めてあげようかHPH」","W"
						CASEELSE
							CALL K78_HPH_PRINT,@"「このまま押し倒されちゃうのかしらHPH」","W"
						ENDSELECT
					ELSE
						PRINTFORMW 「い、いきなりなによ……恥ずかしい…」
					ENDIF
				ENDIF
			ELSE
				IF TALENT:恋慕 || TALENT:愛欲
					PRINTFORMW 「急にどうしたの？」
				ELSEIF TALENT:思慕 || TALENT:セフレ
					PRINTFORMW 「むぐ……あつくるしいわ」
				ELSE
					PRINTFORMW 「や、やめてよっ　あつくるしいっ」
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
;==================================================
;312,キスする
;TFLAG:193(99=拒絶(キス不可) 1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 50=手淫 60=キス 性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K78_312
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_312_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_BASIC_CONDITION_IN_MATING()
		IF CFLAG:うふふ == 2
			IF TALENT:恋慕 || TALENT:愛欲
				SELECTCASE RAND:3
				CASE 1
					PRINTFORMDL %CALLNAME:MASTER%と%CALLNAME:TARGET%はまるで軟体動物の交尾のように舌をからませ合い、とめどなく分泌される唾液でびちゃびちゃの口内を貪り合った。
					CALL K78_HPH_PRINT,@"「ちゅ…ぴちゃ……ちゅぱ……ちゅる…んはぁHPH %CALLNAME:MASTER%、好き、愛してるHPH」","W"
				CASE 2
					PRINTFORMDL ひたすらに甘い%CALLNAME:TARGET%の吐息にあてられ、なかば朦朧としながら舌を動かしつづけた。
					CALL K78_HPH_PRINT,@"「はむ…んちゅっ、ん、んふぅ…れろ……ちゅぷ…はぁHPH まだ、もっとぉHPH」","W"
				CASEELSE
					PRINTFORMDL 果実のように甘い%CALLNAME:TARGET%の唾液のとりこになった%CALLNAME:MASTER%は、時間のたつのも忘れて無心に味わいつづけた。
					CALL K78_HPH_PRINT,@"「んむ…じゅぷ…ちゅHPH …じゅる、じゅるるっ……ちゅぱHPH」","W"
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「ちゅっ……んっ……はぁ……んむ…ちゅっ…んぅ」
				CASEELSE
					PRINTFORMW 「ん…ちゅぷ……んちゅ……んはっ……はむ…ちゅ」
				ENDSELECT
			ENDIF
		;キス拒絶ではない
		ELSEIF TFLAG:193 != 99
			IF BASE:ムード >= 650 && BASE:理性 < 875
				IF TALENT:恋慕 || ( TALENT:愛欲 && ( ABL:親密 >= 5 || ABL:欲望 >= 5 ) )
					SELECTCASE RAND:2
					CASE 1
						CALL K78_HPH_PRINT,@"「ちゅっ、ちゅぱ……んちゅ、んむ……んふHPH もう終わり？HPH」","W"
					CASEELSE
						CALL K78_HPH_PRINT,@"「あむっ……んちゅ…ぺろ……ちゅ…ちゅぱHPH えへへ、キスっていいわねHPH」","W"
					ENDSELECT
				ELSEIF ABL:親密 >= 5 || ABL:欲望 >= 5
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMW 「んっ……んん…ちゅ……はぁ…」
					CASEELSE
						PRINTFORMW 「んちゅ……ちゅ…んむ……ん…」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 1
						PRINTFORML 「んむっ、ん……ちゅ……んはっ、はぁ……」
						PRINTFORMDW %CALLNAME:TARGET%は無言のまま顔を真っ赤にしている。
					CASEELSE
						PRINTFORML 「んんっ……んちゅ…ん……ちゅ…」
						PRINTFORMDW %CALLNAME:TARGET%は一瞬抵抗しようとしたが、%CALLNAME:MASTER%が肩を抱くとじっとしたまま受け入れた。
					ENDSELECT
				ENDIF
			ELSE
				IF TALENT:恋慕 || ( TALENT:愛欲 && ( ABL:親密 >= 5 || ABL:欲望 ) )
					PRINTFORMW 「んっ……ちゅぷ…ちゅ……ん、がっつきすぎよ」
				ELSE
					PRINTFORMW 「んぐ！？ きゅ、急になにするのよっ」
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
