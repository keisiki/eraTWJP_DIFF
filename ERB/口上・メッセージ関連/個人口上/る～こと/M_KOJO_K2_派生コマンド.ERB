;==================================================
;7,挿入子宮口責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_7
IF K2_BASIC_CONDITION_IN_MATING() && CFLAG:1001 && TALENT:体型 >= -1 && TALENT:MASTER:性別 & 2 && TALENT:MASTER:形状 < 3
	DRAWLINE
	IF TEQUIP:乳首吸い
		CALL MESSAGE_EQUIP_COM11
	ELSEIF TEQUIP:胸揉み
		CALL MESSAGE_EQUIP_COM12
	ENDIF
	PRINTFORMDL %CALLNAME:MASTER%は懸命に腰を振って%CALLNAME:TARGET%の子宮を突こうとした。
	PRINTFORMDL しかし、%CALLNAME:MASTER%のペニスではあまり深く突くことができない。
	SIF !NOWEX:Ｖ絶頂
		PRINTFORMDL 奥にとどく感覚をほとんど得られない%CALLNAME:TARGET%は%TEXTR("拍子抜けしたような/不満そうな/物足りなさそうな")%\@ TEQUIP:アイマスク ?様子 #表情 \@だ。
	PRINTW
	SIF FLAG:口上色
		TRYCALLFORM M_KOJO_COLOR_K{TARGET}
	IF ( TALENT:サド && !TALENT:マゾ ) || ABL:サドっ気 >= 10
		IF TALENT:MASTER:形状 == 1
			LOCALS = %TEXTR("お子様/ちびっこお/未熟お/幼稚お")%
		ELSEIF TALENT:MASTER:形状 == 2
			LOCALS = %TEXTR("お粗末/ちっさいお/親指お/ちんちくりんお")%
		ENDIF
	ELSE
		IF TALENT:MASTER:形状 == 1
			LOCALS = %TEXTR("ちいさめお/こどもお/小ぶりお")%
		ELSEIF TALENT:MASTER:形状 == 2
			LOCALS = %TEXTR("ちっちゃいお/短小お/おちび")%
		ENDIF
	ENDIF

	;今回Ｖ絶頂なし
	IF !NOWEX:Ｖ絶頂
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMDL 互いに大きく腰を振っていると、%TEXTR("スポッ/スポン")%と膣からペニスが抜けてしまった。
			CALL K2_HPH_PRINT,@"「やんHPH %CALLNAME:MASTER%さまのおちんぽ、すぐ抜けちゃいますね」","L"
			IF TEQUIP:体位 == 1 || TEQUIP:体位 == 2 || TEQUIP:縄 || TEQUIP:拘束
				CALL K2_HPH_PRINT,@"「はいっHPH %LOCALS%ちんぽ、また挿れてくださいHPH」","L"
				IF TEQUIP:縄 || TEQUIP:拘束
					PRINTFORMD いやらしく口をあけた蜜壺を見せつけて
				ELSEIF TEQUIP:体位 == 1
					PRINTFORMD %TEXTR("大きく股をひらいて/秘唇の奥の赤い肉を見せつけて")%
				ELSE
					PRINTFORMD 尻をふりふりと振って
				ENDIF
				PRINTFORMDW 挿入を%TEXTR("ねだる/せがむ")%%CALLNAME:TARGET%にむしゃぶりつき、%K2_EXPRESSION_VARIATION("PENIS")%をさらに激しく突き立てた。
			ELSE
				CALL K2_HPH_PRINT,@"「よいしょっHPH %LOCALS%ちんぽ、また挿れますねHPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%は秘処を見せつけながら%K2_EXPRESSION_VARIATION("PENIS")%を咥えこむと、しきり直しとばかりに激しく腰をふった。
			ENDIF
		CASE 2
			CALL K2_HPH_PRINT,@"「んっ、もっと深いところに欲しいですぅHPH ガンガンって突いてくださいっHPH %LOCALS%ちんぽがんばってっHPH」","W"
		CASEELSE
			PRINTFORML 「あん、%CALLNAME:MASTER%さまの%LOCALS%ちんぽじゃ奥にとどきませんよぉ」
			CALL K2_HPH_PRINT,@"「ほらぁHPH もっと頑張って腰ふって、おまんこの奥までビリビリさせてくださいHPH」","W"
		ENDSELECT
	;今回Ｖ絶頂
	ELSE
		SELECTCASE RAND:3
		CASE 1
			CALL K2_HPH_PRINT,@"「あんっHPH 奥まで届いてないけどっ、そろそろイキそうですっHPH」","L"
			CALL K2_HPH_PRINT,@"「あんっ、あっ、%CALLNAME:MASTER%さまの%LOCALS%ちんぽでイキますっHPH イクぅっHPH」","W"
		CASE 2
			CALL K2_HPH_PRINT,@"「あはっHPH 必死な%CALLNAME:MASTER%さまかわいいですHPH」","L"
			CALL K2_HPH_PRINT,@"「えっちな%CALLNAME:TARGET%は%CALLNAME:MASTER%さまの%LOCALS%ちんぽでもちゃんとイケますからぁHPH 安心してくださいっHPH」","W"
		CASEELSE
			CALL K2_HPH_PRINT,@"「はぁんHPH %LOCALS%ちんぽっ、やっと、ヨクなってきましたぁHPH」","L"
			CALL K2_HPH_PRINT,@"「あぁ、はぁHPH アクメきますっHPH イッちゃいますっHPH」","W"
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_7_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$PICK_OS_UTERI_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		SELECTCASE EXP:子宮口開発経験
		CASE IS < 25
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「おく、くっ　ちょっと、苦しっ、ですっ、んっ」
			CASEELSE
				PRINTFORMW 「ん、んくうぅっ　くっ、はっ、かはっ」
			ENDSELECT
		CASE 25 TO 50
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「ちょっと、慣れてきましたっ　んくぅっッ」
			CASEELSE
				PRINTFORMW 「おく、へ、変なっ、感じですっ、んっ」
			ENDSELECT
		CASE IS > 50
			IF TALENT:恋慕 || TALENT:愛欲 || ( TALENT:ヤリマン && ABL:Ｖ感覚 >= 5 )
				IF NOWEX:MASTER:射精 && ABL:膣射中毒 >= 4 && !TEQUIP:MASTER:コンドーム
					SELECTCASE RAND:2
					CASE 1
						CALL K2_HPH_PRINT,@"「あっ、おちんぽビクビクしてますHPH 出そうなんですね？HPH」","L"
						CALL K2_HPH_PRINT,@"「どうぞHPH わたしの一番奥にいっぱいびゅーびゅーして下さいっHPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「一番奥におちんぽチュッチュきてますっHPH そのままぴゅっぴゅ下さいっHPH」","L"
						CALL K2_HPH_PRINT,@"「おちんぽ汁だいすきな子宮にいっぱい種付けしてくださいっHPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
					CASE 1
						IF K2_SEXUAL_FRANKNESS()
							CALL K2_HPH_PRINT,@"「あっHPH おくにトントンってっ、キモチいいですっHPH」","W"
						ELSE
							GOTO PICK_OS_UTERI_LOOP
						ENDIF
					CASE 2
						CALL K2_HPH_PRINT,@"「深いところキてますっHPH んくっ、んふうぅっHPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「ふあっ、はぁっHPH 全身にっ、響いてますっHPH んひぃっHPH」","W"
					ENDSELECT
				ENDIF
			ELSE
				SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「んっ、あはぁっ、奥キテますぅっ　あぁっ」
				CASEELSE
					PRINTFORMW 「ふあっ、奥に、当たってますっ　んぅっっ」
				ENDSELECT
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
