		IF TALENT:恋慕 || TALENT:愛欲 || ( TALENT:ヤリマン && ABL:Ｖ感覚 >= 5 )
			SELECTCASE RAND:4
			CASE 1
				;真正ヤリマン、Ｖ拡張経験あり、主人にペニスあり、ランダム６分の１
				IF CFLAG:1001 && EXP:Ｖ拡張経験 > 0 && TALENT:MASTER:性別 & 2 && !RAND:6
					PRINTFORML 「あの、わたしのおまんこ、ゆるゆるじゃありませんか？」
					PRINTFORMDL 不安そうに尋ねてくる%CALLNAME:TARGET%にそんなことはないと言うと、嬉しそうに膣をしめつけてきた。
					CALL K2_HPH_PRINT,@"「頑張っておまんこきゅうってしますから、中古おまんこ嫌いにならないでくださいっHPH」","W"
				;真正ヤリマン、主人の腰３以上、今回あたえる快Ｖが４０００以上
				ELSEIF CFLAG:1001 && ABL:MASTER:腰 >= 3 && CUP:快Ｖ >= 4000
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%さまの腰っ、イイですぅっHPH すごく上手ですっHPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASE 2
				IF ABL:Ｖ感覚 >= 5
					CALL K2_HPH_PRINT,@"「ひゃんっHPH どうしてっ、こんなにキモチイッHPH ひぃ、んぃっHPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASE 3
				IF K2_SEXUAL_FRANKNESS()
					CALL K2_HPH_PRINT,@"「そこすごいっ、すごいですぅっ！ あぁっ、んあっ、はあぁッHPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASEELSE
				CALL K2_HPH_PRINT,@"「そこグリグリされると、ビリビリしちゃっ、ますうぅっHPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「んっ、はぁっ、んぃっ　そこ、変にっ、あっ」
			CASEELSE
				PRINTFORMW 「んくぅっ　そこっ、あぁっ、しびれっ、んぁっ」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
