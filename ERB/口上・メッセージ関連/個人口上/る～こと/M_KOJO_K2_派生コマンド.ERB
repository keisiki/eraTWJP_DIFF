;==================================================
;5,手淫フェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_5_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	$HAND_BLOWJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:恋慕 || TALENT:愛欲
			SELECTCASE RAND:6
			CASE 1
				IF ABL:技巧 >= 4
					CALL K2_HPH_PRINT,@"「ちゅるっ、んふっHPH たまたまも触りますねHPH」","L"
					CALL K2_HPH_PRINT,@"「きゅってなるたまたま可愛いですHPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 2
				IF ABL:舌 >= 3 && ABL:指 >= 2
					PRINTFORMDL %CALLNAME:TARGET%は、亀頭を頬の内側にグイグイとこすりつけ、栗鼠のように頬をふくらませている。
					CALL K2_HPH_PRINT,@"「ほむ……ふぁ……ぴちゃ……んふHPH …ん…もが……んぁ………ぉぐ…じゅるっHPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%の口からはあふれた涎が大量に垂れて、落ちた先に染みをつくっている。
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 3
				IF !TEQUIP:MASTER:コンドーム
					CALL K2_HPH_PRINT,@"「先っぽからおつゆが溢れてびちゃびちゃですHPH ちゅるるっHPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 4
				IF !TEQUIP:MASTER:コンドーム
					CALL K2_HPH_PRINT,@"「はむ……ちゅ………んふふHPH おひんぽ、おいひいえふHPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 5
				PRINTFORMDL %CALLNAME:TARGET%は夢中で%CALLNAME:MASTER%のイチモツにしゃぶりついている。
				CALL K2_HPH_PRINT,@"「じゅるっ……ちゅぷ……ん……じゅるるっHPH じゅぽっ…じゅぽっHPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「んむ、じゅぽっ、ん、おちんぽの根元しこしこ、キモチいいですか？HPH」","W"
			ENDSELECT
			IF NOWEX:MASTER:射精 && ( TCVAR:精飲経験 > 0 || TEQUIP:MASTER:コンドーム )
				PRINTL
				PRINTFORMDL キュッと収縮した陰嚢から射精が近いことを察した%CALLNAME:TARGET%は、激しく手を動かして射精へと導いていく。
				CALL K2_HPH_PRINT,@"「あはっHPH わたしの手と%CALLNAME:MASTER%さまのおちんぽ、くちゅくちゅくちゅってとってもエッチな音たててますよHPH」","L"
				CALL K2_HPH_PRINT,@"「えいっHPH えいっHPH \@ !TEQUIP:MASTER:コンドーム && RAND:2 ?おちんぽ汁、わたしの舌に出してくださいっ #えっちな白いお汁だしちゃってくださいっ \@HPH」","W"
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 1
				IF ABL:技巧 >= 4
					PRINTFORMW 「んちゅっ、ん、陰嚢もいっしょに、ですか？ わかりました。はむっ」
				ELSE
					PRINTFORMW 「んぷっ、はぁ……手と口を同時に動かすのは難しいです」
				ENDIF
			CASEELSE
				PRINTFORMW 「あむ……ちゅぱ…ぴちゃ……んっ…ふぁ」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K2_{SELECTCOM}_1
RETURN 1

