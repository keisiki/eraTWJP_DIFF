;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K2_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	SELECTCASE RAND:2
	CASE 1
		PRINTFORMDL %CALLNAME:TARGET%は額に汗をうかべ、時折むせながら、必死に%CALLNAME:MASTER%の%K2_EXPRESSION_VARIATION("PENIS")%を咥えこんでいる。
		PRINTFORMW 「んぐっ……ぶほぉっ、ごほっ ……はぁ…はぁ……っ…ごふっ……ぐぽっ」
	CASEELSE
		PRINTFORMDL 鼻汁と涎を垂れ流し、美しい顔を下品にゆがませる%CALLNAME:TARGET%の喉を、%CALLNAME:MASTER%はさらに激しく突いた。
		PRINTFORMW 「ぐっ……… ぐぽ……がっ、がはっ、けほっ、はぁ……はぁ…… んぐっ……ぐぷっ…」
	ENDSELECT
ENDIF
RETURN 1

