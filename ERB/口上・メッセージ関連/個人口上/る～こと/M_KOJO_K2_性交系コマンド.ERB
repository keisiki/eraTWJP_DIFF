			ENDSELECT
		ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:思慕 || TALENT:セフレ || TALENT:ヤリマン
			IF !TEQUIP:継続キス && RAND:2
				IF TALENT:恋慕 || TALENT:愛欲 || TALENT:ヤリマン
					CALL K2_HPH_PRINT,@"「あっ、はっ、んっHPH %CALLNAME:MASTER%さまぁHPH %TEXTR("キス/ちゅー")%も欲しいですHPH」","W"
				ELSE
					CALL K2_HPH_PRINT,@"「キ、キスして、ほしいです…HPH」","W"
				ENDIF
			ELSE
				PRINTFORMDL %CALLNAME:TARGET%は仰向けになると、\@ TALENT:恋慕 || TALENT:思慕 ?はにかんだ微笑で少しうなずき、%CALLNAME:MASTER%を受け入れる意志をしめした。 #欲情をたたえた目でこちらを見つめ、%CALLNAME:MASTER%をさそった。 \@
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「抱いてくださいHPH」","W"
				CASEELSE
					CALL K2_HPH_PRINT,@"「きてくださいHPH」","W"
