;81,フェラチオ
;==================================================
@M_KOJO_MESSAGE_COM_K2_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$BLOWJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:恋慕 || TALENT:愛欲
			SELECTCASE RAND:4
			CASE 1
				SELECTCASE ABL:舌
				CASE 0,1
					PRINTFORML 「んちゅっ……ぷはぁ……あの、わたし、ちゃんとできてますか？」
					PRINTFORMDL 口での奉仕に慣れていない%CALLNAME:TARGET%が、不安そうにたずねてきた。
					PRINTFORMW 「もっと%CALLNAME:MASTER%さまを気持ちよくして差し上げたいです」
				CASE 2,3
					CALL K2_HPH_PRINT,@"「ちゅぽ……ちゅぷ…… えへHPH イイところがわかってきましたHPH」","W"
				CASE IS >= 4
					PRINTFORMDL %CALLNAME:TARGET%は唇でカリを刺激しながら、同時に陰茎小帯を舌でチロチロと責めている。
					CALL K2_HPH_PRINT,@"「ぐぽっ…じゅっ、ちゅぽHPH ……じゅる……ぺろ……じゅぱ……れろろっHPH」","W"
				ENDSELECT
			CASE 2
				;フル勃起
				IF CFLAG:MASTER:勃起度 >= 1500
					CALL K2_HPH_PRINT,@"「カチカチおちんぽ、このままおクチ奉仕でいいんですか？ おまんこでドクドクぴゅっぴゅでもいいですよHPH」","W"
				;挿入可能な勃起
				ELSEIF CFLAG:MASTER:勃起度 >= 1000
					IF !TEQUIP:MASTER:コンドーム
						CALL K2_HPH_PRINT,@"「あっ、えっちなおつゆが漏れてきましたよHPH」","W"
					ELSE
						GOTO BLOWJOB_LOOP
					ENDIF
				;半勃ち
				ELSEIF CFLAG:MASTER:勃起度 > 500
					CALL K2_HPH_PRINT,@"「おクチをつかって、おちんぽもっと固くして差し上げますHPH」","W"
				;ふにゃちん
				ELSE
					CALL K2_HPH_PRINT,@"「ふにゃふにゃおちんぽ、おクチのなかでコロコロするの楽しいですHPH」","W"
				ENDIF
			CASE 3
				IF TEQUIP:MASTER:コンドーム
					PRINTFORML 「%CALLNAME:MASTER%さまのおちんぽ、ちゃんとじかに味わいたいです……」
					PRINTFORMDW %CALLNAME:TARGET%はゴム越しでの口淫に不満なようだ。
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%は竿を舐めながら裏筋のあたりに鼻を押し当て、しきりにスンスンと匂いを嗅いでいる。
					CALL K2_HPH_PRINT,@"「はぁ…HPH すっごくいやらしい匂い…… ずっと嗅いでいたいですHPH」","W"
				ENDIF
			CASEELSE
				CALL K2_HPH_PRINT,@"「ぷちゅ……れろ……ちゅぱ……ぐぽっ……ちゅるっHPH」","W"
			ENDSELECT
			IF NOWEX:MASTER:射精
				PRINTL
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「おちんぽピクピクしてますっHPH イッちゃうんですねHPH どうぞっHPH」","L"
					IF EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3 && !TEQUIP:MASTER:コンドーム && ABL:精液中毒 >= 5 && TCVAR:精飲経験 >= 20
						CALL K2_HPH_PRINT,@"「おちんぽ汁だいすきなメイドにごっくんさせてくださいHPH」","W"
					ELSEIF !TEQUIP:MASTER:コンドーム && TCVAR:精飲経験 >= 20 
						CALL K2_HPH_PRINT,@"「%CALLNAME:TARGET%のおクチまんこに精液びゅーびゅーってしてくださいHPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「わたしのおクチでイッてくださいHPH」","W"
					ENDIF
				CASEELSE
					PRINTFORMDL %CALLNAME:MASTER%の%K2_EXPRESSION_VARIATION("PENIS")%はもう爆発寸前だ。
					IF EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3 && !TEQUIP:MASTER:コンドーム && ABL:精液中毒 >= 5 && TCVAR:精飲経験 >= 20
						CALL K2_HPH_PRINT,@"「ごきゅごきゅっていっぱい飲ませてくださいっHPH いいですよね？ ね？HPH おちんぽ汁ほしくてたまらないんですっHPH」","W"
					ELSEIF !TEQUIP:MASTER:コンドーム && TCVAR:精飲経験 >= 20 
						CALL K2_HPH_PRINT,@"「あむ、じゅぽ、じゅるっ、んぷっ、んっHPH びゅっびゅっれ、らいへっ、うらはいっHPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「んちゅっ、んぱっ、ちゅっHPH このままおクチで最後までしちゃいますねHPH」","W"
					ENDIF
				ENDSELECT
			ENDIF
		ELSEIF TALENT:思慕 || TALENT:セフレ
			SELECTCASE RAND:4
			CASE 1
				IF ABL:舌 < 2
					PRINTFORMW 「ちゅ……ぺろ……んっ…むずかしいです……」
				ELSE
					GOTO BLOWJOB_LOOP
				ENDIF
			CASE 2
				PRINTFORMW 「んっ、じゅるっ……ぴちゃ………ちゅぷっ……ちゅぽっ…」
			CASE 3
				PRINTFORMW 「んちゅっ……はぁっ、どうですか？ 気持ちいいですか？」
			CASEELSE
				PRINTFORMW 「んふっ、ピクピクしてます。イキそうになったら言ってくださいね」
			ENDSELECT
		ELSE
			IF ABL:奉仕精神 >= 2
				;主人のペニスに、精液かアナルか粘液か血の汚れあり、かつコンドームなし
				IF STAIN:MASTER:Ｐ & 108 && !TEQUIP:MASTER:コンドーム
					PRINTFORML 「……よごれてて…くさいです……ん、はむ……ちゅる」
					PRINTFORMDW %CALLNAME:TARGET%はくさいと言いながらも淡々と奉仕を続けた。
				ELSE
					PRINTFORMW 「はむ、ん……ぴちゃ……ぴちゅ……れろ………ちゅる」
				ENDIF
			ELSE
				PRINTFORMW 「な、舐めるんですか？ うぅ……」
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

