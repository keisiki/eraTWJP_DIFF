;奉仕系コマンド
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K2_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K2_BASIC_CONDITION_IN_MATING() && CFLAG:1001 && FIRSTTIME(SELECTCOM)
		PRINTFORM 「ふふっ、
		SELECTCASE TALENT:MASTER:形状
		CASE 1
			PRINTFORM かわいらしい
		CASE 2
			PRINTFORM ずいぶんちっちゃな
		CASE 3
			PRINTFORM たくましい
		CASE 4
			PRINTFORM まぶしい
		CASE 5
			PRINTFORM 変わったかたちの
		CASE 6
			PRINTFORM 立派な
		CASE 10
			PRINTFORM すごく大きい
		CASEELSE
			PRINTFORM えっちな
		ENDSELECT
		PRINTFORMW おちんぽですね」
		PRINTFORMDW %CALLNAME:TARGET%はいやらしい笑みを浮かべながら、片手で陰茎をもてあそんでいる。
		PRINTFORMW 「それじゃあ、おちんぽご奉仕しますね」
		RETURN 1
	ENDIF
	
	$HANDJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:恋慕 || TALENT:愛欲
			SELECTCASE RAND:6
			CASE 1
				IF ABL:技巧 >= 4 && !NOWEX:MASTER:射精
					CALL K2_HPH_PRINT,@"「お手てでこしこし気持ちいいですか？ いつでもぴゅっぴゅしていいですからねHPH」","W"
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 2
				IF ABL:指 >= 4
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%さまはここをシコシコするとすぐイッてしまうんですよねHPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%は、巧みな手の動きで%CALLNAME:MASTER%の弱点を責めている。
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 3
				IF !TEQUIP:アイマスク && !TEQUIP:乳首吸い && !TEQUIP:継続キス && !TEQUIP:シックスナイン && ABL:マゾっ気 >= 3
					PRINTFORMDL ひざまずいた%CALLNAME:TARGET%は手コキをしながら、%CALLNAME:MASTER%の顔を上目遣いに見つめている。
					PRINTFORMW 「%CALLNAME:MASTER%さまに見下ろされながらこうしていると、ご奉仕してるって感じがします」
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 4
				IF ABL:指 >= 2 && ABL:サドっ気 >= 4
					PRINTFORMDL %CALLNAME:TARGET%の激しい責めに、%CALLNAME:MASTER%は思わず腰を引いてしまった。
					CALL K2_HPH_PRINT,@"「あん、逃げちゃだめです。もっとヨクして差し上げますよHPH」","L"
					SIF !TEQUIP:乳首吸い && !TEQUIP:胸揉み && !TEQUIP:拘束 && !TEQUIP:シックスナイン
						PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%の後ろに回ると、逃げられないように抱きつき、背後からさらに激しくしごいた。
					WAIT
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 5
				CALL K2_HPH_PRINT,@"「おちんぽがびくんびくんって脈動してるの感じますHPH」","W"
			CASEELSE
				SELECTCASE ABL:指
				CASE 0
					PRINTFORMW 「ええっと、こんな感じでしょうか？」
				CASE 1,2
					PRINTFORMW 「どうですか？ 気持ちよくできてますか？」
				CASE IS >= 3
					PRINTFORMDL %CALLNAME:TARGET%は巧みに緩急をつけながら、%CALLNAME:MASTER%を射精にみちびいている。
					;フル勃起
					IF CFLAG:MASTER:勃起度 >= 1500
						CALL K2_HPH_PRINT,@"「あはっ、すっごく固いですHPH どうしますか？ このままお手てで射精しますか？ それともおまんこしましょうか？HPH」","W"
					;挿入可能な勃起
					ELSEIF CFLAG:MASTER:勃起度 >= 1000
						CALL K2_HPH_PRINT,@"「あんHPH 固くて素敵ですHPH でも、%CALLNAME:MASTER%さまはまだもっと固くなりますよね？HPH」","W"
					;半勃ち
					ELSEIF CFLAG:MASTER:勃起度 > 500
						CALL K2_HPH_PRINT,@"「んしょっ、んしょっHPH ほらぁHPH もっとおっきくしないと、おまんこできませんよぉHPH」","W"
					;ふにゃちん
					ELSE
						CALL K2_HPH_PRINT,@"「まだふにゃふにゃですね。ほら、がんばってHPH がんばってHPH」","W"
					ENDIF
				ENDSELECT
			ENDSELECT
			IF NOWEX:MASTER:射精 && ABL:技巧 >= 4
				PRINTL
				PRINTFORMDL %CALLNAME:TARGET%の手でしごかれた%K2_EXPRESSION_VARIATION("PENIS")%はいまにも射精しそうだ。
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「あHPH イッちゃいそうなんですか？HPH お手てでドピュドピュってしちゃうんですね？HPH」","W"
				CASEELSE
					CALL K2_HPH_PRINT,@"「あっ、もう出そうなんですね？ いいですよHPH わたしのお手てでお汁ぴゅっぴゅしてくださいHPH」","W"
				ENDSELECT
			ENDIF
		ELSEIF TALENT:思慕 || TALENT:セフレ
			IF ABL:指 >= 2
				SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「どこがイイか、段々わかってきました」
				CASEELSE
					PRINTFORML 「んっ……んっ…… 大丈夫ですか？ 痛くないですか？」
					PRINTFORMDW %CALLNAME:TARGET%はクチュクチュと音を立てながら、速い動きで%CALLNAME:MASTER%に快感を与えている。
				ENDSELECT
			ELSE
				PRINTFORMW 「うぅ……腕が疲れてしまいました」
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「手でこすればいいんですね。わかりました」
			CASEELSE
				PRINTFORML 「…………」
				PRINTFORMDW %CALLNAME:TARGET%は無言でひたすら手を動かしている。
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

