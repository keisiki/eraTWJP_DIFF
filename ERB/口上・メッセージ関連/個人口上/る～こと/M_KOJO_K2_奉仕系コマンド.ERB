;==================================================
;83,素股
;==================================================
@M_KOJO_MESSAGE_COM_K2_83
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_83_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_83_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	$FEMORAL_SEX_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:恋慕 || TALENT:愛欲 || TALENT:ヤリマン
			SELECTCASE RAND:5
			CASE 1
				IF !TEQUIP:継続キス && !( TALENT:性別 & 2 ) && TALENT:MASTER:形状 != 2
					PRINTFORMDL %CALLNAME:TARGET%を立たせ、その股間に背後から%K2_EXPRESSION_VARIATION("PENIS")%をこすりつけた。
					CALL K2_HPH_PRINT,@"「ふふHPH おちんぽが生えてきたみたいでおもしろいですHPH」","L"
					SIF !TEQUIP:縄 && !TEQUIP:拘束
						PRINTFORMDL %CALLNAME:TARGET%は興味ぶかそうに%CALLNAME:MASTER%の%K2_EXPRESSION_VARIATION("PENIS")%をいじっている。
					WAIT
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 2
				IF !( CFLAG:育児人数 > 0 && !TEQUIP:MASTER:コンドーム && !TCVAR:ピル && CFLAG:生理周期 >= 3 )
					IF CFLAG:溜まってる度 >= 500 && PALAM:潤滑 >= PALAMLV:3
						PRINTFORML 「あれ？ 挿れないんですか？ わたしのおまんこ準備万端なのに…」
						PRINTFORMDW %CALLNAME:TARGET%は何だかうらめしそうな\@ TEQUIP:アイマスク ?様子だ。 #顔で見てきた。 \@
					ELSE
						PRINTFORMW 「本当に挿れないでいいんですか？ おまんこにズポズポしてぴゅっぴゅしなくていいんですか？」
					ENDIF
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 3
				IF K2_SEXUAL_FRANKNESS()
					CALL K2_HPH_PRINT,@"「んあぁHPH おちんぽでこすれてますHPH おちんぽしゅっしゅイイですっHPH」","W"
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 4
				CALL K2_HPH_PRINT,@"「ぬるぬるお股と%CALLNAME:MASTER%さまのおちんぽでピチャピチャってぇHPH あはぁHPH すごい音ですHPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「わたしの腰どうですかぁHPH ちゃんとキモチヨクできてますか？HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMDL %CALLNAME:TARGET%に上に乗ってもらって素股をしてもらうことにした。
				PRINTFORMW 「大丈夫でしょうか… お、おちんちんを潰してしまわないか心配で…」
			CASE 2
				PRINTFORMW 「あっ…ん…… こ、こすれてっ……んっ」
			CASEELSE
				PRINTFORMW 「挿れないんですか？ わかりました」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

