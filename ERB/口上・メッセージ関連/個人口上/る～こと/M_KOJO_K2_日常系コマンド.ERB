;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K2_CONSCIOUS_AND_TOLERANT() && TFLAG:193 != -5
		IF TALENT:恋慕 || TALENT:愛欲
			SELECTCASE RAND:3
			CASE 1
				CALL K2_HPH_PRINT,@"「いいですよ～　気持ちいい枕になって差し上げますHPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%の腿はほどよい肉付きでとても気持ちよかった。
			CASE 2
				CALL K2_HPH_PRINT,@"「ふふHPH 仕事つかれちゃいましたね」","L"
				PRINTFORMDW %CALLNAME:TARGET%は微笑みながらずっと頭をなでてくれた。
			CASEELSE
				CALL K2_HPH_PRINT,@"「お疲れですか？ それとも甘えたいんですか？ ふふっHPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
			CASE 1
				IF ABL:親密 >= 6
					PRINTFORMW 「いつもお仕事でお疲れですからね。どうぞ♪」
				ELSE
					PRINTFORMW 「わ、わかりました。%CALLNAME:MASTER%さまにはお世話になってますから」
				ENDIF
			CASE 0
				SELECTCASE ABL:親密
				CASE IS >= 6
					PRINTFORMW 「お疲れ様です。少し休憩ですね」
				CASE 3 TO 5
					PRINTFORML 「こ、これもメイドのお仕事なのでしょうか……」
					PRINTFORMDW %CALLNAME:TARGET%は困惑しつつも膝枕をしてくれた。
				CASE IS <= 2
					PRINTFORML 「ど、どうすればいいのでしょうか……？」
					PRINTFORMDW %CALLNAME:TARGET%は困惑している。
				ENDSELECT
			CASE -1
				PRINTFORMW 「うぅ……あしがジンジンします……」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
