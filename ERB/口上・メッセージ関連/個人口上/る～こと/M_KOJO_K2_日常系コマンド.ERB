;==================================================
;300,会話
;TFLAG:193(1=大成功 0=成功 -1=失敗 -2=大失敗（TARGETがMASTERより教養が4以上高い)
;TCVAR:302(0=会話可能 1>=非恋慕時会話不能)
;MARK:反発刻印(3以上=会話不能)(4以上はバグ
;TCVAR:泥酔(1=通常成功or大失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K2_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K2_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:ブチギレ
		PRINTFORMDW %TEXTR("怒った/けわしい表情の")%%CALLNAME:TARGET%はほとんど返事をしてくれなかった。
	ELSEIF TCVAR:泥酔
		PRINTFORMDL %CALLNAME:TARGET%は赤ら顔でぼーっとしている。話がよくわからないようだ。
		PRINTFORMW 「え？ なんですか～？」
	ELSEIF K2_CONSCIOUS_AND_TOLERANT()
		;博麗神社が拠点マップでその台所で仕事中のばあい
		IF GET_MAPNAME(MAIN_MAP) == "博麗神社" && 仕事中チェック(TARGET) && CFLAG:MASTER:現在位置 == 台所
			IF TALENT:恋慕 || TALENT:愛欲
				SELECTCASE ABL:料理技能
				CASE IS >= 4
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%さまのために美味しいお料理つくって差し上げますからねHPH」","W"
				CASE 2,3
					CALL K2_HPH_PRINT,@"「味見なさいませんか？ 愛情たっぷりですよHPH」","W"
				CASE IS <= 1
					PRINTFORMW 「%CALLNAME:MASTER%さまのために美味しいお料理つくれるようになりたいです」
				ENDSELECT
			ELSE
				SELECTCASE ABL:料理技能
				CASE IS >= 4
					PRINTFORML 「そうなんです。そしてでですね～」
					PRINTFORMDW %CALLNAME:TARGET%は器用に料理をこなしながら会話している。
				CASE 2,3
					PRINTFORMW 「味見なさいますか？」
				CASE IS <= 1
					PRINTFORML 「ええっと、お味噌はどこでしたっけ～？」
					PRINTFORMDW %CALLNAME:TARGET%は料理で忙しいようだ。
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE TFLAG:193
			CASE 1
				IF TALENT:恋慕 || TALENT:愛欲
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%に体を寄せてきた。
						PRINTFORML 「お仕事は毎日たのしいですが、あまり%CALLNAME:MASTER%さまとお出かけできないのが残念です」
						CALL K2_HPH_PRINT,@"「なのでこういうふたりの時間、大事にしましょうねHPH」","W"
					CASEELSE
						;拠点が博麗神社
						IF GET_MAPNAME(MAIN_MAP) == "博麗神社"
							PRINTFORML 「いつも家事を手伝ってくださって、本当にありがとうございます」
							PRINTFORMW 「%CALLNAME:MASTER%さまと一緒にお仕事ができて、とっても幸せです」
						ELSE
							PRINTFORML 「お仕事たのしいか、ですか？ はい。とってもたのしいです」
							PRINTFORMW 「それに、いつ%CALLNAME:MASTER%さまがいらしてもいいように、きれいにしておかないと」
						ENDIF
					ENDSELECT
				ELSE
					PRINTFORMW 「なるほど～。%CALLNAME:MASTER%さまは物知りなんですね」
				ENDIF
			CASE 0
				SELECTCASE RAND:7
				CASE 1
					IF FLAG:なりきり != 1 && FLAG:なりきり != 9
						PRINTFORML 「そうなんです。そのとき夢美教授と霊夢さまが――」
						PRINTFORMDW %CALLNAME:TARGET%は昔のはなしをしてくれた。
					ELSE
						PRINTFORML 「そんなこともありましたね～」
						PRINTFORMDW 昔のはなしで%CALLNAME:TARGET%と盛り上がった。
					ENDIF
				CASE 2
					PRINTFORMW 「放っておくとすぐにお部屋が汚れていくので大変です」
				CASE 3
					SELECTCASE TIME:5
					CASE 0,1
						PRINTFORML 「天気がいいですね」
						PRINTFORMW 「洗濯日和でうれしいです♪」
					CASE 2,3
						PRINTFORML 「曇り空ですね」
						PRINTFORMW 「お洗濯物かわくかしら？」
					CASE IS >= 4
						PRINTFORML 「うーん、天気が悪いですね」
						PRINTFORMW 「洗濯ができなくて残念です……」
					ENDSELECT
				CASE 4
					PRINTFORML 「お掃除する円盤なんてあるんですか？」
					PRINTFORMW 「うーん、わたしも負けないようがんばらないと」
				CASE 5
					IF FLAG:なりきり == 9
						PRINTFORMW 「わたしはご飯を食べなくても大丈夫なようですが、動力源はどうなっているんでしょう？ 」
					ELSE
						PRINTFORML 「夢美教授がわたしを作ってくださったんです」
						PRINTFORMW 「え？ 動力源ですか？ さあ、どうなってるんでしょう？」
					ENDIF
				CASE 6
					;清掃技能と料理技能の合計が４未満
					IF ABL:清掃技能 + ABL:料理技能 < 4
						PRINTFORML 「う～。どうしてわたしはお仕事がへっぽこなんでしょうか」
						PRINTFORML 「このままでは倉にしまわれてしまうのでは……」
						PRINTFORMDW %CALLNAME:TARGET%は想像して落ち込んでいる。
					;清掃技能と料理技能の合計が４以上
					ELSE
						PRINTFORML 「お仕事も少しずつできるようになってきました」
						PRINTFORMW 「%CALLNAME:MASTER%さまのおかげです♪」
					ENDIF
				CASEELSE
					IF TALENT:恋慕 || TALENT:愛欲
						;拠点が博麗神社
						IF GET_MAPNAME(MAIN_MAP) == "博麗神社"
							;午後6時以前
							IF TIME < 1080
								PRINTFORMW 「今日の晩ご飯は何がいいですか？ %CALLNAME:MASTER%さまのお好きな物にしますよ」
							;午後6時以降
							ELSE
								PRINTFORMW 「あしたの献立はどうしようかしら。%CALLNAME:MASTER%さまのお好きな物にしましょうか」
							ENDIF
						ELSE
							;午後6時以前
							IF TIME < 1080
								PRINTFORMW 「今日の晩ご飯は何がいいかな～♪」
							;午後6時以降
							ELSE
								PRINTFORMW 「あしたの献立はどうしようかな～♪」
							ENDIF
						ENDIF
					ELSE
						;午後6時以前
						IF TIME < 1080
							PRINTFORMW 「今日の晩ご飯どうしましょう」
						;午後6時以降
						ELSE
							PRINTFORMW 「あしたの献立どうしましょう」
						ENDIF
					ENDIF
				ENDSELECT
			CASE -1
				PRINTFORMW 「うーん。そのおはなし、わたしには難しいです……」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
