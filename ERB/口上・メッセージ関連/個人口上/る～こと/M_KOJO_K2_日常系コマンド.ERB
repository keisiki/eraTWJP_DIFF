;60,耳かき
;前ターン膝枕後スキンシップコマンド
;TFLAG:193(1=大成功 0=成功 -1=失敗)(失敗するのはTARGET
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_60
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_60_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K2_60_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K2_CONSCIOUS_AND_TOLERANT()
		IF ( TALENT:恋慕 || TALENT:愛欲 ) && TFLAG:193 >= 0
			SELECTCASE RAND:2
			CASE 1
				CALL K2_HPH_PRINT,@"「お役に立ててると実感できるので、耳かき好きですHPH」","L"
				CALL K2_HPH_PRINT,@"「はいっ、終わりです。でももっとしていたいです。またさせてくださいねHPH」","L"
				PRINTFORMDW やりすぎに注意しなくては。
			CASEELSE
				PRINTFORML 「%CALLNAME:MASTER%さまのお耳、きれいにして差し上げますね～」
				PRINTFORMDL 頼んでいないのに、%CALLNAME:TARGET%は自分から耳かきを申し出てきた。
				PRINTFORMW 「あ、大きいのとれた。ふふ、楽しいです」
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
			CASE 1
				PRINTFORMDL とても気持ちよかったと伝えると、%CALLNAME:TARGET%は嬉しそうに笑った。
				PRINTFORMW 「お役に立ててよかったです」
			CASE 0
				PRINTFORMW 「なかなか……難しいです。い、痛くないですか？」
			CASE -1
				PRINTFORMDL %CALLNAME:MASTER%が痛いと声をあげると、%CALLNAME:TARGET%は申しわけなさそうに謝った。
				PRINTFORMW 「うぅ……ごめんなさい……メイド失格です……」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
