			SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「えへへ。これでいい夢見れそう…」
				CASE 2
					CALL HPH_PRINT("「おやすみのちゅー、ありがとー。私からもお返しのちゅーHPH」","W")
