;-------------------------------------------------
;奴隷の絶頂時の口上
;NOWEX:Ｃ絶頂～3 CVAB絶頂
;NOWEX:Ｍ絶頂 二重絶頂
;NOWEX:二重絶頂 四重絶頂
;NOWEX:噴乳 噴乳
;NOWEX:射精 射精
;NOWEX:放尿 放尿
;適当に組み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K56
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	PRINTFORMW 「うそっ…!? おっぱいっ 出てるっ%UNICODE(0x2665)%」
ENDIF
;-------------------------------------------------
;射精
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;五重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:五重絶頂
		PRINTFORMW 「はぅ…!? お゛っ%UNICODE(0x2665)% っほお゛お゛お゛ぉ゛ぉ゛ぉ゛お゛っ」
		PRINTFORMW 獣じみた悲鳴を上げ絶頂を迎えた神子は、あらゆる体液を垂れ流して崩れ落ちた。
		PRINTFORMW 「あ゛ー…%UNICODE(0x2665)% あ゛ー…%UNICODE(0x2665)%」
		PRINTFORMW 思考を奪われるほどの強い快楽を味わい、半開きの口からは理性を失った喘ぎ声や涎がだらしなく溢れている
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:四重絶頂
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:四重絶頂
			;Ｃ＆Ｖ＆Ａ＆Ｂ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ａ＆Ｍ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ＆Ｍ絶頂
			CASE 3
				PRINTFORMW 「やぁっ…%UNICODE(0x2665)%くるっ…くるっ%UNICODE(0x2665)% あっ あっ%UNICODE(0x2665)%」
				PRINTFORMW 膣内にを迎え入れながら敏感な部分をねっとりと撫でられ、神子は体をガクガク震わせた。
				PRINTFORMW 「あひゃぁああっ!? っぁはっ、っ あぁぁぁぁぁん%UNICODE(0x2665)%」
				PRINTFORMW 最後にしっかりと抱きしめ、膣奥にペニスを勢いよく突き立てると結合部から潮を吹き出しよがり狂った
				RETURN 1
			;Ｃ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;三重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:三重絶頂
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:三重絶頂
			;Ｃ＆Ｖ＆Ａ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ｍ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｖ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;二重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:二重絶頂
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:二重絶頂
			;Ｃ＆Ｖ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｂ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ絶頂
		;超強絶頂Ｃ
		IF NOWEX:Ｃ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｃ
		ELSEIF NOWEX:Ｃ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ絶頂
		;超強絶頂Ｖ
		IF NOWEX:Ｖ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｖ
		ELSEIF NOWEX:Ｖ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ａ絶頂
		;超強絶頂Ａ
		IF NOWEX:Ａ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ａ
		ELSEIF NOWEX:Ａ絶頂 == 2
			PRINTFORMW 
		;絶頂Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ絶頂
		;強絶頂Ｂ
		IF NOWEX:Ｂ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｂ
		ELSEIF NOWEX:Ｂ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｂ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｍ絶頂
		;超強絶頂Ｍ
		IF NOWEX:Ｍ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｍ
		ELSEIF NOWEX:Ｍ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｍ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


