@M_KOJO_MESSAGE_COM_K100_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:時間停止
		PRINTFORML 
		RETURN 1
	;押し倒され
	ELSEIF CFLAG:うふふ == 2
		PRINTFORML
		CALL AUTO_AEGI
		RETURN 1
	ELSEIF CFLAG:睡眠
		IF TALENT:恋慕
			CALL HPH_PRINT,@"「んぅ……はぁ……HPH」","L"
			PRINTFORMDL 眠っているにも関わらず、尻を撫でられている%CALLNAME:TARGET%は頬を上気させ艶めかしく腰をくねらせている
			PRINTFORMDW 『……本当は起きてる？』%CALLNAME:MASTER%が耳元でそう囁いてみると、%CALLNAME:TARGET%の身体がピクッと跳ねた……
		ELSE
			PRINTFORML 「……んん」
			PRINTFORMDW %CALLNAME:TARGET%は寝苦しそうにしている……
		ENDIF
	;合意無し
	ELSEIF !CFLAG:既成事実
		IF TALENT:恋慕
			PRINTFORML 「んんっ……ダメだよ、こんなこと……」
			PRINTFORMDL %CALLNAME:TARGET%が抵抗しないのを良い事に、その柔らかさを存分に堪能した……
			PRINTFORMW （でも……%MASTERNAME:100%になら……）
		ELSEIF TALENT:思慕
			PRINTFORML 「やぁ……ダメ、ですよぉ……」
			PRINTFORMDL 口ではそう言うが、%CALLNAME:TARGET%は抵抗する素振りすら見せない
			PRINTFORMW 「私のお尻……そんなに好きなんですか……？」
		ELSE
			PRINTFORML 「ひゃんっ！？」
			PRINTFORMDL %CALLNAME:TARGET%の可愛らしい尻に手を伸ばし、ガシッと鷲掴みにしてやる
			PRINTFORMDL 突然の出来事に%CALLNAME:TARGET%は硬直し、されるがままになっている……
			PRINTFORMW 「っ、ぅぅ……やめてください……」
		ENDIF
	;通常
	ELSE
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「や、ぁ……尻尾は、だめ……HPH」","L"
					PRINTFORMDL 尻と尻尾を同時に撫でられ、%CALLNAME:TARGET%は悩まし気に腰をくねらせている
					PRINTFORMDL %CALLNAME:MASTER%は、『どうして？』と耳元で囁いてみた
					CALL HPH_PRINT,@"「だ、だって……その……気持ち良く、なっちゃうから……HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「あっ……%MASTERNAME:100%……HPH」","L"
					CALL HPH_PRINT,@"「ん、ん……っHPH エッチな触り方、しないで……HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%にセクハラされてとても嬉しそうだ……
				CASE 2
					PRINTFORML 「ひゃんっ♪ もう、しょうがないなぁ……」
					PRINTFORMDL %CALLNAME:TARGET%は困ったように、しかし少しだけ嬉しそうに%CALLNAME:MASTER%の手を振り払った
					PRINTFORMW 「こういうこと…私以外にしちゃダメだよ？」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ぁっ……触りたくなっちゃったんですか……？」
					PRINTFORMDL %CALLNAME:MASTER%が頷くと、%CALLNAME:TARGET%は頬を赤らめ尻を差し出してきた
					PRINTFORMW 「……少しだけなら……いいですよ」
				CASE 1
					PRINTFORMDL %CALLNAME:MASTER%は痴漢のように、手の甲でスリスリと%CALLNAME:TARGET%の尻を撫で上げた……
					PRINTFORMW 「触り方が……んっ、いやらしいです……」
				CASE 2
					PRINTFORML 「ひゃうっ！？ ぅぅ……またですか……？」
					PRINTFORML 「そ、その……ちゃんと言ってくれたら、触っていいですから……」
					PRINTFORMW 「だから、いきなりはダメです……ビックリしちゃいます……」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「んん……%MASTERNAME:100%、痴漢みたいです……」
					PRINTFORMDW %CALLNAME:TARGET%は少しだけ嫌そうにしている……
				CASE 1
					PRINTFORML 「ひぁっ！？ ぅぅ……その、いきなりはビックリしちゃいます……」
					PRINTFORMDW %CALLNAME:TARGET%の可愛らしい悲鳴が聞けて、%CALLNAME:MASTER%は満足した
				CASE 2
					PRINTFORML 「私のお尻なんて、触っても楽しくないですよ……？」
					PRINTFORMDL 執拗に尻を撫でようとする%CALLNAME:MASTER%を%CALLNAME:TARGET%は困惑の表情で見つめている
					PRINTFORMW 「いや、いいからって……ん、くぅ……っ」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
