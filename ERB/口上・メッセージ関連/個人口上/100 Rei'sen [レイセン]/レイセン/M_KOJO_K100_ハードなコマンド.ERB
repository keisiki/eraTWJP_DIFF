;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K100_140
CALL M_KOJO_MESSAGE_COM_K100_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K100_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;舌技術５以上
	IF ABL:舌 >= 5
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL %CALLNAME:MASTER%は予告もせずに、%CALLNAME:TARGET%の喉奥で射精した
					CALL HPH_PRINT,@"「ぐぷ、んっHPH ごく……ごく……っHPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%はトロンと顔を蕩けさせ、流し込まれる精液を胃に収めている……
				CASE 1
					CALL HPH_PRINT,@"「っふ、ぷっHPH ぢゅぷっHPH ぐぽっHPH」","L"
					PRINTFORMDL 後背位で犯すときのように、%CALLNAME:TARGET%の顔を掴み腰を打ち付ける
					PRINTFORMDW %CALLNAME:TARGET%は一切文句を言わずに、その行為を受け入れている……
				CASE 2
					CALL HPH_PRINT,@"「こぷ、ふっHPH ぇっHPH んぐっHPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%の喉の奥に、大量の熱い精液がドクドクと注がれる
					PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%の腰にしっかりと手を回し、一滴も零さずそれを飲んでいる……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ふぅっHPH ふぅっHPH んぶ、んんっHPH」","L"
					PRINTFORMDL 多少乱暴に扱っても壊れないところが%CALLNAME:TARGET%の良いところだ
					PRINTFORMDW %CALLNAME:MASTER%は欲望のまま、%CALLNAME:TARGET%の喉を『使った』
				CASE 1
					CALL HPH_PRINT,@"「っ、けぽっ……はぁ……はぁ……HPH」","L"
					PRINTFORMDL 涙と鼻水、そして唾液で顔をグチャグチャにしながらも、こちらに微笑む%CALLNAME:TARGET%
					PRINTFORMDW もし彼女を奴隷として売ったなら、きっとかなりの額になるだろう……
				CASE 2
					CALL HPH_PRINT,@"「んふぅ、ぢゅるっHPH ふぅ、ぐぷっHPH」","L"
					PRINTFORMDL 乱暴に喉を犯されながらも、%CALLNAME:TARGET%は奉仕を忘れない
					PRINTFORMDW %CALLNAME:MASTER%が気持ち良くなれるように、最大限の技量でペニスをもてなした
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ぶっ！？ ぉ、ん、ぐぅ……っ！」
					PRINTFORMDL 喉の奥でペニスが痙攣し、直接食道に精液を注ぎ込む
					PRINTFORMDW 酸欠でダランと脱力する%CALLNAME:TARGET%を、%CALLNAME:MASTER%はオナホのように扱った
				CASE 1
					PRINTFORML 「ふぶっ！ んっ！ んんっ！ ――っっ！！！」
					PRINTFORMDL ただ自分が気持ち良くなる為だけに、%CALLNAME:MASTER%は%CALLNAME:TARGET%の喉をレイプする
					PRINTFORMDW %CALLNAME:TARGET%は諦めたように、ただ終わるのを待っている……
				CASE 2
					PRINTFORMDL 限界が来た%CALLNAME:MASTER%は%CALLNAME:TARGET%の頭を引き寄せると、そのまま喉奥で射精した
					PRINTFORML 「はぶ、ぅっ！ ん、ぐっ、げほっ！ げほっ！」
					PRINTFORMDW ペニスを引き抜くと、%CALLNAME:TARGET%は鼻から精液を垂らしながら激しく咽込んだ……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「んぶっ！？ ごっ！ ふぶぅっ！？」
					PRINTFORMDL %CALLNAME:TARGET%の頭をガッシリと掴み、負担を考えず腰を突き入れる
					PRINTFORMDW %CALLNAME:TARGET%は完全に混乱し、目を白黒させている……
				CASE 1
					PRINTFORML 「ぉご、ぇっ！？ ぇうっ！ うぅっ！」
					PRINTFORMDL 苦しそうに脚を叩いてくる%CALLNAME:TARGET%を無視して、欲望のままに腰を振る
					PRINTFORMDW %CALLNAME:TARGET%は涙と鼻水で顔をグチャグチャにしている……
				CASE 2
					PRINTFORML 「っ、っ、げほっ！ ぇほっ！ はぁ……はぁ……」
					PRINTFORML 「どう、して……こんな……けほっ……」
					PRINTFORMDW %CALLNAME:TARGET%は虐待されたペットのように%CALLNAME:MASTER%を見上げている……
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

