;==================================================
;82,パイズリ
;==================================================
@M_KOJO_長短髪_MESSAGE_COM_K59_82
CALL TRAIN_MESSAGE
CALL M_KOJO_長短髪_MESSAGE_COM_K59_82_1
RETURN RESULT

@M_KOJO_長短髪_MESSAGE_COM_K59_82_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「おっぱいでおちんちん、ぎゅーってしてほしいんですか?ふふっ、おっぱい、好きなんですね」
		PRINTFORM %NICKNAME:59%は、%CALLNAME:MASTER%の
		SIF TALENT:MASTER:形状 <= 2
			PRINTFORM 小さな
		PRINTFORMW ペニスを、大きな胸で挟む
		PRINTFORML
		SIF TALENT:MASTER:形状 <= 2
			PRINTFORMW 「あらあら、%MASTERNAME:59%の、恥ずかしがり屋さんなんですね♡おっぱいに隠れちゃって♡」
		PRINTFORMW 「それじゃ、おっぱいでおちんちん、ぎゅーってしてあげますね♡」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 
				PRINTFORMW 
			CASE 0
				IF TALENT:MASTER:形状 <= 2 && RAND:2
					PRINTFORML 
					PRINTFORMW 
					PRINTFORMW 
					SIF NOWEX:PLAYER:射精
						PRINTFORMW 
				ELSE
					PRINTFORML 「」
					PRINTFORMW 
					SIF NOWEX:PLAYER:射精
						PRINTFORMW 
				ENDIF
			CASEELSE
				;かわいいおちんちん＆半分の確率
				IF TALENT:MASTER:形状 <= 2 && RAND:2
					PRINTFORML 「%MASTERNAME:59%のおちんちん、%NICKNAME:59%のおっぱいでかくれんぼするのが大好きなんですね♡」
					PRINTFORMW 「ふふっ♪どこにいるのかなー?隠れてないで出てきてもいいんですよー♡」
					PRINTFORMW %NICKNAME:59%はそう言って、%CALLNAME:MASTER%のペニスを胸でもみくちゃにする
					SIF NOWEX:PLAYER:射精
						PRINTFORMW 「…おや?出てきちゃいそうですか?恥ずかしがり屋さんだけど、寂しがり屋さんなんですね♡」
				ELSE
					PRINTFORML 「んっ、おっぱいでぎゅーってされるの、どうですか?」
					PRINTFORMW 「%MASTERNAME:59%、%NICKNAME:59%のおっぱいが気持ちよかったら、遠慮せずイってくださいねー♡」
					SIF NOWEX:PLAYER:射精
						PRINTFORMW 「…ビクビクってして、わかりやすい♡」
				ENDIF
		ENDSELECT
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

