		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;20,キスする
;==================================================
@M_KOJO_MESSAGE_COM_K103_20
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K103_20_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K103_20_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
;	ELSEIF TALENT:恋慕
;		PRINTFORMW 
;		RETURN 1
	;屈服3
;	ELSEIF MARK:不埒刻印 == 3
;		PRINTFORMW 
;		RETURN 1
	;それ以外
	ELSE
		LOCAL:1 = M_KOJO_K103_KANKEI()
		SELECTCASE LOCAL:1
		CASE 10, 11, 12, 13, 21, 22, 23, 24
			;特別な感情は抱いていない時
			SELECTCASE M_KOJO_K103_RANDNR(4,315)
			CASE 1
				PRINTFORMW 「んん……ぷぁ…………ふぅ…」
			CASE 2
				PRINTFORMW 「ん…ちゅ……ねぇ、キスは要らなくない？」
			CASE 3
				PRINTFORMW 「…ちゅ………も、もうおわりっ！」
			CASEELSE
				PRINTFORMW 「…ん……んん…ぷはっ……息できないじゃない…んんっ…」
			ENDSELECT
			PRINTFORML
		CASE 31,32,33,34,61,62,63,64
			;思慕ORセフレ
			SELECTCASE M_KOJO_K103_RANDNR(5,315)
			CASE 1
				PRINTFORMW 「…んっ…ちゅ…はぁ……ん、おわり…？…あ…んっ…」
			CASE 2
				PRINTFORMW 「ん……んん…ちゅ…ぷぁ……んっ…」
			CASE 3
				PRINTFORMW 「ちゅ…ちゅっ………」
			CASE 4
				PRINTFORMW 「…れるっ……んちゅ……」
			CASEELSE
				PRINTFORMW 「ちゅ…やっ、唇吸わないで……ん…」
			ENDSELECT
			PRINTFORML
		CASE 41,42,43,44,51,52,53,54,71,72,73,74
			;恋慕OR愛欲
			SELECTCASE M_KOJO_K103_RANDNR(5,315)
			CASE 1
				PRINTFORMW 「ちゅ…んちゅ…はぁ……んんっ…%M_KOJO_K103_HEART()%」
			CASE 2
				PRINTFORMW 「ぷはっ……はー、はー…まだ…んっ…ちゅっ…%M_KOJO_K103_HEART()%」
			CASE 3
				PRINTFORMW 「れるっ…んちゅ……はぁ……まだ足らないの…んっ…%M_KOJO_K103_HEART()%」
			CASE 4
				PRINTFORMW 「んちゅ……ちゅっ…ぷぁ……んん…ちゅっ%M_KOJO_K103_HEART()%」
			CASEELSE
				PRINTFORMW 「ちゅっ…唇も……あむ…歯の裏も……んちゅ…%M_KOJO_K103_HEART()%」
			ENDSELECT
			PRINTFORML
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1


