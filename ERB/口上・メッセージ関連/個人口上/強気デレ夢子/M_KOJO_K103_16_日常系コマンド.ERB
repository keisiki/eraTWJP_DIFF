@M_KOJO_SUCCESS_COM_K103_300
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K103_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます。また、削除した場合は自動で口上色が付きません。
SIF TCVAR:350 < 6 || TCVAR:350 > 10
	CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K103_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K103_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TCVAR:350 >= 6 && TCVAR:350 < 10
		;初対面で時止め状態だった
		
		PRINTFORML
		;邂逅処理を行う
		CALL M_KOJO_K103_MEET
		;初対面の挨拶を強制的に呼び出す
		CALL M_KOJO_ENCOUNTER_K103
		;時止めフラグ初期化
		CALL M_KOJO_K103_RESETTW
		RETURN 1
	ELSEIF TCVAR:350 >= 2 && TCVAR:350 < 10
		;今日初めて会った時に時止め状態だった
		
		;邂逅処理を行う
		CALL M_KOJO_K103_MEET
		SELECTCASE TCVAR:350
		CASE 2
			;何もしてない
			PRINTFORML 「わっ、…な、なに……って%CALLNAME:MASTER%か…」
			PRINTFORML 「どこから出てきたのよ……%M_KOJO_K103_AISATSU()%」
		CASE 3
			;パンツが無い
			PRINTFORML 「えっ、…あれ？ なんか………あ、%CALLNAME:MASTER%…」
			PRINTFORML 「…その……%M_KOJO_K103_AISATSU()%」
		CASE 4
			;快感が走ってる
			PRINTFORML 「あうっ！？ ………あ、%CALLNAME:MASTER%…」
			PRINTFORML 「…ううん、なんでもない、何でもないわ……%M_KOJO_K103_AISATSU()%」
		CASE 5
			;絶頂した
			PRINTFORML 「ひぐっ…………、あ…%CALLNAME:MASTER%…」
			PRINTFORML 「………%M_KOJO_K103_AISATSU()%…」
		ENDSELECT
		;時止めフラグ初期化
		CALL M_KOJO_K103_RESETTW
		RETURN 1
	ENDIF
	LOCAL:1 = M_KOJO_K103_KANKEI()
	SELECTCASE LOCAL:1
	CASE 11, 12, 21, 22
		;関係が普通もしくは同行許可かつ機嫌がいい
		IF ALCOHOL_FACE(TARGET) == "酔いどれ" || ALCOHOL_FACE(TARGET) == "ぐでぐで"
			;酔ってる
			PRINTFORML 「うう…ひっく……聞いてますよ～、…聞いてます……はい」
			PRINTFORML 「で、何の話でしたっけ…？」
		ELSEIF TFLAG:193 == 0
			$KAIWA01
			;会話成功時
			IF TCVAR:103:350 == 11 || TCVAR:103:350 == 15
				;時止め解除：通常
				PRINTFORML 「わっ、びっくりした！」
				PRINTFORMW 「\@ LOCAL:1 > 20 ? あなた # %CALLNAME:MASTER% \@今突然現れなかった？」
			ELSEIF TCVAR:103:350 == 12 || TCVAR:103:350 == 16
				;時止め解除：パンツ
				PRINTFORML 「スースーする……ああもう、なんなのよ一体…」
				PRINTFORMW 「…あ、ごめん何か言った？」
			ELSEIF TCVAR:103:350 == 13 || TCVAR:103:350 == 17
				;時止め解除：快感
				PRINTFORML 「んんっ…、…なにかしら」
				PRINTFORMW 「…用事なら後にしてほしいわ」
			ELSEIF TCVAR:103:350 == 14 || TCVAR:103:350 == 18
				;時止め解除：絶頂
				PRINTFORML 「…んっ……ふっ……っ！」
				PRINTFORMW 「…え？何でもないわ、なんでも」
			ELSE
				IF FLAG:デート相手 == 103 || CFLAG:同行
					;未実装らしいのでこっちで回収 実装後は要修正
					CALL M_KOJO_MESSAGE_COM_K103_600_1
				ELSE
					SELECTCASE M_KOJO_K103_RANDNR(5,300)
					CASE 1
						PRINTFORML 「私とお喋りしても楽しくは無いと思うけれど…」
						PRINTFORMW 「ま、暇つぶしくらいなら付き合うわ」
					CASE 2
						PRINTFORML 「一応私もメイドの端くれなんだけれど…」
						IF LOCAL:1 > 20 
							PRINTFORML 「掃除がどうしても苦手なのよね…」
							PRINTFORMW 「そういえば%CALLNAME:MASTER%は掃除が仕事なんですってね」
						ELSE
							PRINTFORMW 「主に荒事担当なのよね…」
						ENDIF
					CASE 3
						PRINTFORML 「その時の魔理沙の顔ったらなかったわ」
						IF CFLAG:102:現在位置 != CFLAG:103:現在位置
							PRINTFORML 「そうそう、あの頃の魔理沙はね…」
							PRINTFORML
							IF CFLAG:11:現在位置 == CFLAG:103:現在位置
								PRINTFORMDL 後ろから物凄い視線を感じる…
								PRINTFORMDW 冷たいものを感じて、%CALLNAME:MASTER%は慌てて話題を切り替えた
							ELSE
								PRINTFORMDL これは聞いてもいい話なのだろうか…
								PRINTFORMDW %CALLNAME:MASTER%は何となく話題を逸らした
							ENDIF
						ELSE
							;神綺が同じ場所にいる場合
							PRINTFORMW 「神綺様もそう思いますよね？」
						ENDIF
					CASE 4
						PRINTFORML 「なんだか最近パンツが無くなる騒ぎが起きてるらしいわよ？」
						PRINTFORML 「異変…なのかしらねぇ………ん？ どうかした？」
						PRINTFORMW 「そ、なんでもないならいいんだけど」
					CASEELSE
						PRINTFORML 「んー、これといって話題は無いんだけど」
						PRINTFORML 「ああ、そういえば神綺様が食べたいものがあるそうなのだけど」
						PRINTFORMW 「オムライスって知ってる？」
					ENDSELECT
				ENDIF
			ENDIF
		ELSEIF TFLAG:193 == 1
			;会話大成功時
			SIF TCVAR:103:350 > 0 && TCVAR:103:350 != 15
				GOTO KAIWA01
