CALL SPEVENT_MESSAGE_1(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	PRINTFORML 「楽しかったね！　%CALLNAME:MASTER%、ありがとう！」
	PRINTFORML デートの内容を思い返して、リグルはご機嫌のようだ
	PRINTW 
	IF TALENT:MASTER:体型 > TALENT:体型 + 2
		PRINTFORML 「ねえ、ちょっとかがんでくれるかな？」
		PRINTFORML 「いーから。早くかがんでよ。あと目は閉じててね」
		PRINTW 
		PRINTFORML リグルに言われるがまま、訝しみながらも目線を彼女と合わせ、瞳を閉じる
		PRINTFORML リグルの細い指が、%CALLNAME:MASTER%の頬に添えられ──
	ELSE
		PRINTFORML 「ねえ、ちょっとだけじっとしててくれる？」
		PRINTFORML 「いーから。あと目は閉じててね」
		PRINTW 
		PRINTFORML リグルに言われるがまま、訝しみながらも瞳を閉じる
		PRINTFORML リグルの細い指が、%CALLNAME:MASTER%の頬に添えられ──
	ENDIF
	PRINTW 
	PRINTFORML 「えへへ、似合わないことしちゃった…かな？」
	PRINTFORML 唇に柔らかい感触が触れたことに気付き目を開けると、眼前には頬を赤らめたリグルがいた
	IF TALENT:MASTER:度胸 == -1
		PRINTFORML 「捕食されるかと思ったって？　失敬なー！」
		PRINTFORML 「ふんだ。そういうこと言うなら、頭から食べてやろうかなー」
		PRINTW 
		PRINTFORML 「なーんてね。冗談だよ。それは後にとっておこうね」
		PRINTFORML 冗談とも本気ともつかない表情でリグルが微笑む
		PRINTW 
	ENDIF
	PRINTFORML 「遊びに連れて行ってくれたお礼。こういうことしたの、%CALLNAME:MASTER%が初めてだよ」
	PRINTFORML 「また、一緒に遊びに行って、一緒に帰ろうね。約束だよ」
	PRINTW 
	;キスをキャンセルしたい場合は以下を開放
	;TCVAR:キスキャンセル = 1
ENDIF
RETURN 1

;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K27_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:1000 == 1
			PRINTFORML 「お出かけ、楽しかったね♪」
			PRINTFORML 「私、%CALLNAME:MASTER%とお出かけするの好きだよ♪」
			PRINTW 
			PRINTFORML 「あの…あの、ね」
			PRINTFORML 「%CALLNAME:MASTER%は、私のこと、好き…？」
			PRINTFORML 「えっと、その、虫的な意味じゃなくて、女の子、として…」
			PRINTW 
			PRINTL 
			PRINTFORML 「私と、交尾したいって思える…？」
			PRINTW 
		ELSE
			PRINTFORML 「ねぇ、虫たちのことは前より好きになった？」
			CALL ASK_YN("好きになった","いやあんまり")
			IF !RESULT
				PRINTFORML 「本当！？　嬉しいなあ！」
				PRINTFORML 「私の啓発が%CALLNAME:MASTER%に浸透したんだね。えへへ」
			ELSE
				PRINTFORML 「あはは…正直だねえ」
			ENDIF
			PRINTFORML 「じゃあ…じゃあ、さ」
			PRINTFORML 「私のことは、好きになった…？」
			PRINTFORMW 「えっと、その、虫的な意味じゃなくて、女の子、として…」
			PRINTL 
			PRINTFORML 「私と、交尾したいって思える…？」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF FIRSTTIME("返答")
			PRINTFORML 「本当に？　嬉しい！」
			PRINTFORML 「もう取り消しはできないからね！　取り消ししたら、毎朝大量のゲジゲジをけしかけるから！」
			PRINTW 
			PRINTFORML 「絶対に離さないよ。私と、たくさんの子どもを作ってもらうんだから！」
			PRINTFORML 「私たちの仲間をたくさん増やそうね。大好きだよ、%CALLNAME:MASTER%」
		ELSE
			PRINTFORML 「本当に？　…嬉しい！」
			PRINTFORML 「諦めなくてよかった…君とならつがいになれるって思ったから」
			PRINTW 
			PRINTFORML 「絶対に離さないよ。私と、たくさんの子どもを作ってもらうんだから！」
			PRINTFORML 「私たちの仲間をたくさん増やそうね。大好きだよ、%CALLNAME:MASTER%」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF FIRSTTIME("返答")
			PRINTFORML 「そっか…やっぱり、弱い虫の妖怪なんかとは付き合えないよね」
			PRINTFORML 「それとも、私が人間だったら君は受け入れてくれたのかな」
			PRINTFORML 「それとも、君が蟲だったら私に惹かれてくれたのかな」
			PRINTW 
			PRINTFORML 「あーあ。やっぱり同族の妖怪が現れるのを待つしかないかなぁ」
			PRINTW 
			PRINTFORML 「来ないで！」
			PRINTW 
			PRINTFORMW 「今の私の顔、見ないでよ。きっと、不細工だから」
			PRINTFORML 「でも、そこにいて。ちょっとだけ。ちょっとだけでいいから」
			PRINTL 
			PRINTFORML 背を向けてすすり泣くリグルを、%CALLNAME:MASTER%は黙って見守った…
			PRINTW 
		ELSE
			PRINTFORML 「うん。分かってたよ。分かってたけど──」
			PRINTFORML 「それでも、やっぱり君のことがまだ好きだったみたい」
			PRINTFORML 「君が優しくしてくれるから、ひょっとしたらって勘違いしちゃうんだよ」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF

ENDIF
RETURN 1

