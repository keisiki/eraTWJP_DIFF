		CASE IS >= 2
			SELECTCASE RAND:5
			CASE 0
				CALL HPH_PRINT,@"「えへへ、上手になってきたでしょ？HPH」","L"
				CALL HPH_PRINT,@"「君のために頑張ったんだよ…HPH　だから、後でたくさん君のが欲しいなHPH」","L"
				PRINTW 
			CASE 1
				PRINTFORML 「ねえ、君のここ、気持ちいい？」
				CALL HPH_PRINT,@"「私のことも、たくさん気持ちよくしてほしいな…HPH」","L"
				PRINTW 
			CASE 2
				CALL HPH_PRINT,@"「あは、すごく硬いね…HPH」","L"
				CALL HPH_PRINT,@"「それに、強い光。これなら私も強い子を産めそうだよ…HPH」","L"
				PRINTW 
			CASE 3
				CALL HPH_PRINT,@"「手で出されたいの？HPH　いいよHPH」","L"
				CALL HPH_PRINT,@"「その代わり、同じ量だけ私に射精してくれればねHPH」","L"
