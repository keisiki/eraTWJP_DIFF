			CASE 4
				CALL HPH_PRINT,@"「ねえ、出したい？HPH」","L"
				CALL HPH_PRINT,@"「このまま出しちゃう？HPH　それとも、私と子作りしちゃう？HPH」","L"
			ENDSELECT
		CASEELSE
			SELECTCASE RAND:5
			CASE 0
				CALL HPH_PRINT,@"「わあ…HPH　素敵な%TEXTR("生殖器/おちんちん")%だね…HPH」","L"
				CALL HPH_PRINT,@"「私、一生懸命ご奉仕するねHPH」","L"
				PRINTW 
			CASE 1
				CALL HPH_PRINT,@"「君のここ…すっごく光ってるよHPH」","L"
				PRINTW 
			CASE 2
				PRINTFORML 「こんな素敵な%TEXTR("生殖器/おちんちん")%に巡り合えるなんて、私ってすっごくラッキー♪」
				PRINTFORML 「一生懸命ご奉仕するけど、気持ちよくなかったらごめんね？」
				PRINTW 
