				CALL HPH_PRINT,@"「私ね、頑張って気持ちよくさせるから…HPH」","L"
				CALL HPH_PRINT,@"「気持ちよくなったら、私と交尾してくれるかな…HPH」","L"
			ENDSELECT
		ENDSELECT
	ELSEIF TALENT:恋慕 || TALENT:恋人
		SELECTCASE ABL:指
		CASE IS >= 5
			SELECTCASE RAND:6
