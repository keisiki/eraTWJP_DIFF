			CASE 8
				CALL HPH_PRINT,@"「ねえ、早くこれで交尾しようよおHPH」","L"
			CASE 9
				CALL HPH_PRINT,@"「今すぐ交尾するって言わないと、出させてあげない…HPH」","L"
