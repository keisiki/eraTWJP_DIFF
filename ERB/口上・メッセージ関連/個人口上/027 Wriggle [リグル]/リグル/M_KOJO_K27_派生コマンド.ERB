		IF CFLAG:K27C_恋の季節 == 1
			SELECTCASE EXP:子宮口開発経験
			;開発され切った
			CASE IS > 50
				SELECTCASE RAND:11
				CASE 0
					CALL HPH_PRINT,@"「\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/子袋")% # 奥 \@突いて\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/子袋")% # 奥 \@突いて\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/子袋")% # 奥 \@突いてっっHPHHPH」","L"
					CALL HPH_PRINT,@"「そこにっっHPHHPH　%TEXTR(@"%CALLNAME:MASTER%/君")%の精子っHPH　たっぷり注いでぇっHPHHPH」","L"
				CASE 1
					CALL HPH_PRINT,@"「孕ませてぇっHPH　孕みたいのっHPH　%TEXTR(@"%CALLNAME:MASTER%/君")%の赤ちゃんっHPH」","L"
					CALL HPH_PRINT,@"「中で射していいからっHPH　一番奥で出してえっHPH」","L"
				CASE 2
					CALL HPH_PRINT,@"「こじ開けてぇっHPH　そこっHPH　私の一番大事なところぉっHPH」","L"
					CALL HPH_PRINT,@"「私にっHPH　強い子産ませてぇっHPHHPH」","L"
				CASE 3
					CALL HPH_PRINT,@"「あ゛っHPH　あ゛だっでるぅHPH　いちばん奥のっHPH　その中ぁっHPHHPH」","L"
					CALL HPH_PRINT,@"「%TEXTR("せーしょくき/おちんちん")%が直接っHPH　当たってるよぉっHPH」","L"
				CASE 4
					CALL HPH_PRINT,@"「きたHPH　きたHPH　キたHPH　キたぁっHPHHPH」","L"
					CALL HPH_PRINT,@"「一番奥っHPH　コリコリするところぉっHPH　届いたぁっHPHHPH」","L"
				CASE 5
					CALL HPH_PRINT,@"「子どもっHPH　作るところぉっHPH　%TEXTR("生殖器/おちんちん")%でっHPH　キスっHPH　いぃっHPH」","L"
				CASE 6
					CALL HPH_PRINT,@"「そっ、そんなっ、乱暴にっ、突かれたらぁっ」","L"
					CALL HPH_PRINT,@"「降りちゃうっHPH　\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/赤ちゃんの部屋/子袋")% # 子袋 \@降りちゃうよおっHPHHPH」","L"
				CASE 7
					CALL HPH_PRINT,@"「そこっHPH　そこっ、いいよおっHPH」","L"
					CALL HPH_PRINT,@"「溜め込んだせーしHPH　全部私の一番奥にっHPH　出してぇっHPHHPH」","L"
				CASE 8
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%…HPHHPH」","L"
					CALL HPH_PRINT,@"「はやく…HPH　\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/赤ちゃんの部屋/子袋/膣内/おまんこ")% # 中 \@に出して…HPH　ねえ…HPH」","L"
				CASE 9
					CALL HPH_PRINT,@"「すごぉ…いHPH　%TEXTR(@"%CALLNAME:MASTER%/君")%の%TEXTR("生殖器/おちんちん")%…っHPH」","L"
					CALL HPH_PRINT,@"「お腹の中、ぐちゃぐちゃになってるよぉ…HPH」","L"
				CASE 10
					CALL HPH_PRINT,@"「あ゛ーーっHPH　あ゛ーーっHPH　あ゛ーーっHPH　あ゛ーーっHPH」","L"
				ENDSELECT
				PRINTW 
			;綻び始めた
			CASE 25 TO 50
				SELECTCASE RAND:6
				CASE 0
					CALL HPH_PRINT,@"「お゛う゛っ！？HPH　お゛っHPH　う゛っHPH」","L"
					CALL HPH_PRINT,@"「苦しいのにっHPH　これっHPH」","L"
				CASE 1
					CALL HPH_PRINT,@"「\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/赤ちゃんの部屋/子袋")% # おなかっ \@HPH　つぶれるっHPH」","L"
				CASE 2
					CALL HPH_PRINT,@"「奥っっHPH　%TEXTR(@"%CALLNAME:MASTER%/君")%のがゴリゴリしてるうっHPH」","L"
				CASE 3
					CALL HPH_PRINT,@"「あっHPH　うHPH　うっHPH　あHPH　アHPH　アッHPH　アァッHPH」","L"
					PRINTFORML リグルは子宮口を一突きされるたび、甲高い声を上げて全身を戦慄かせた…
				CASE 4
					CALL HPH_PRINT,@"「お腹の奥までっHPH　お腹の奥までっっHPH」","L"
					CALL HPH_PRINT,@"「%TEXTR(@"%CALLNAME:MASTER%/君")%のがHPH　届いてるっHPH」","L"
				CASE 5
					CALL HPH_PRINT,@"「だめっHPH　だめっHPH」","L"
					CALL HPH_PRINT,@"「精液っHPH　欲しくなるっHPH　\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/赤ちゃんの部屋/子袋/膣内/おまんこ")% # 中 \@に欲しくなるっHPHHPH」","L"
				ENDSELECT
				PRINTW 
			;異物感
			CASEELSE
				SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「いっHPH　ぎっHPH」","L"
					CALL HPH_PRINT,@"「苦しいのにっHPH　これっHPH」","L"
				CASE 1
					CALL HPH_PRINT,@"「\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/赤ちゃんの部屋/子袋")% # おなかっ \@HPH　つぶれるっHPH」","L"
				CASE 2
					CALL HPH_PRINT,@"「だめぇっHPH　それ以上っ、入らないよおっHPH」","L"
				ENDSELECT
				PRINTW 
			ENDSELECT
		ELSE
			SELECTCASE EXP:子宮口開発経験
			;開発され切った
			CASE IS > 50
				IF TALENT:恋慕 || TALENT:恋人
					SELECTCASE RAND:7
					CASE 0
						PRINTFORML 「来てっ…%TEXTR(@"%CALLNAME:MASTER%/君")%のなら、強くしてもいいからっ…♥」
					CASE 1
						PRINTFORML 「お腹にっ、響く、うっ♥　」
						PRINTFORML リグルは異物感とは別に、時折ズシンと奥に響く快楽に振り回されているようだ…
					CASE 2
						PRINTFORML 「はあ、あっ、その、優しくトントンされるの、嫌いじゃない、かも…♥」
						PRINTFORML 「うんっ、いいっ、いいよっ、あっ、ああっ♥」
					CASE 3
						PRINTFORML 「もっとトントンしてっ♥　トントンっ♥」
						PRINTFORML 「入っちゃうっ♥　あー♥　入っちゃうっ♥」
					CASE 4
						PRINTFORML 「%TEXTR(@"%CALLNAME:MASTER%/君")%の%TEXTR("生殖器/おちんちん")%と私のが密着してるよおっ♥」
					CASE 5
						PRINTFORML 「くるしっ♥　けど、気持ち、いいっ、のおっ♥」
						PRINTFORML 「なんで、だろっ♥　%TEXTR(@"%CALLNAME:MASTER%/君")%だからっ♥　かなあっ♥」
					CASE 6
						PRINTFORML 「だめっ♥　なのにっ♥」
						PRINTFORML 「%TEXTR("グリグリ/ゴツゴツ/")%されるの好きっ♥」
					ENDSELECT
				ELSEIF TALENT:愛欲 || TALENT:セフレ
					SELECTCASE RAND:7
					CASE 0
						PRINTFORML 「一番奥っ♥　%TEXTR(@"%CALLNAME:MASTER%/君")%専用の穴になっちゃったぁっ♥」
					CASE 1
						PRINTFORML 「これっ♥　やばいっ♥　奥グポグポされるのハマッちゃったっ♥」
					CASE 2
						PRINTFORML 「重いのくるっ♥　奥にっ♥　ズンズンってえ♥」
					CASE 3
						PRINTFORML 「当たるっ♥　押しつぶされるっ♥　ふぎゅっ♥　ふぎゅっ♥」
					CASE 4
						PRINTFORML 「あっ♥　お゛っ♥　奥っ♥　つぶれるっ♥」
					CASE 5
						PRINTFORML 「\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/赤ちゃんの部屋/子袋")% # 奥 \@もっと突いてっ♥」
					CASE 6
						PRINTFORML 「\@ CFLAG:1006 >= 1 ? %TEXTR("子宮/赤ちゃんの部屋/子袋")% # 奥 \@当たっ♥　お゛お゛っっ♥」
					ENDSELECT
				ELSEIF TALENT:思慕
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「ふぐううっ、うううっ、ううっ」
						SELECTCASE RAND:3
						CASE 0
							PRINTFORML （奥にっ、重いのズシンってくるっ…）
						CASE 1
							PRINTFORML （私の中っ、奥まで%CALLNAME:MASTER%の生殖器の形にされちゃうよおっ）
						CASE 2
							PRINTFORML （これっ、赤ちゃんの部屋に当たってるのっ！？　だ、駄目えっ…♥）
						ENDSELECT
					CASE 1
						PRINTFORML 「ふうっ！？♥　ちがっ、気持ちよくないよお…」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「ああっっ！！？♥　入ってるっ！？　入ってるよおっ！！」
					CASE 1
						PRINTFORML 「だ、駄目ぇっ！　奥グポグポしたらっ、おかしくなるぅっ！」
					ENDSELECT
				ENDIF
				PRINTW 
			;綻び始めた
			CASE 25 TO 50
				IF TALENT:恋慕 || TALENT:恋人
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「来てっ…%TEXTR(@"%CALLNAME:MASTER%/君")%のなら、受け入れられるよっ…」
					CASE 1
						PRINTFORML 「んっ、んっ、んっ、んっ、んうっ！？♥」
						PRINTFORML リグルは異物感とは別に、時折ズシンと奥に響く快楽に振り回されているようだ…
					CASE 2
						PRINTFORML 「はあ、あっ、その、優しくトントンされるの、嫌いじゃない、かも…♥」
						PRINTFORML 「うんっ、いいっ、いいよっ、あっ、ああっ♥」
					ENDSELECT
				ELSEIF TALENT:愛欲 || TALENT:セフレ
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「そんなっ奥までみっちり突かれたらあぁっ♥　%TEXTR(@"%CALLNAME:MASTER%/君")%専用の穴になっちゃうっ♥」
					CASE 1
						PRINTFORML 「これっ♥　やばいっ♥　ハマッちゃうかもっ♥」
					CASE 2
						PRINTFORML 「重いのくるっ♥　奥にっ♥　ズンズンってえ♥」
					ENDSELECT
				ELSEIF TALENT:思慕
					SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「ふぐううっ、うううっ、ううっ」
						SELECTCASE RAND:3
						CASE 0
							PRINTFORML （奥にっ、重いのズシンってくるっ…）
						CASE 1
							PRINTFORML （私の中っ、奥まで%CALLNAME:MASTER%の生殖器の形にされちゃうよおっ）
						CASE 2
							PRINTFORML （これっ、赤ちゃんの部屋に当たってるのっ！？　だ、駄目えっ…♥）
						ENDSELECT
					CASE 1
						PRINTFORML 「ふうっ！？♥　ちがっ、気持ちよくないよお…」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「ぐうう、奥まで、入ってるう…！」
					CASE 1
						PRINTFORML 「一番奥っ、当たってるよお…！」
					ENDSELECT
				ENDIF
				PRINTW 
			;異物感
			CASEELSE
				IF TALENT:恋慕 || TALENT:恋人
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「お願い…突いてもいいから、優しくして…」
