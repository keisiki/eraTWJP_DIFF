@M_KOJO_SUCCESS_COM_K30_300
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
;裁判中会話は強制失敗扱い。代わりに会話累積値を打ち消して「気まずい空気」を発生させない
IF 仕事中チェック(30) && BETWEENTIME(540, 719) && (CFLAG:30:現在位置 == CFLAG:30:職場) && CFLAG:30:職種 == 43
	TFLAG:192 = -1
	TCVAR:301 = 0
ELSE
	TFLAG:192 = 0
ENDIF

@M_KOJO_MESSAGE_COM_K30_300
;TRAIN_MESSAGEを消す条件並べる。TFLAG:193を借りてフラグ立てる
;初回or変Ｔで初対面日
IF !STRCOUNT(CSTR:30:0, "300/") || (CFLAG:30:衣装一時変更 == 62 && STRCOUNT(CSTR:30:1, "初対面"))
	TFLAG:193 = 4
;フェラ抜き
ELSEIF TFLAG:193 == -999
;変Ｔ
ELSEIF K30_CHECK_HEN_T() && !GROUPMATCH(CFLAG:現在位置, 連れ込み宿, ラブホ, OMANEKIBEYA())
	TFLAG:193 = 2
;バイブデート中
ELSEIF CHK_DATENOW(CFLAG:30:デート中) && TCVAR:30:364 > 0
	TFLAG:193 = 5
;裁判中
ELSEIF (仕事中チェック(30) && BETWEENTIME(540, 719) && (CFLAG:30:現在位置 == CFLAG:30:職場) && CFLAG:30:職種 == 43) 
;キス魔取得時確率
ELSEIF TALENT:キス魔 && TALENT:恋慕 && RAND:(50 - TALENT:機嫌 * 25 - (CFLAG:溜まってる度 / 50)) == 0 && TALENT:機嫌 != -1 && MARK:30:反発刻印 == 0 && (K30_BE_SEEN() == 0 || RAND:2 == 1) && TFLAG:193 == -999
	TFLAG:193 = 3
;他は通常どおり
ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K30_300_1
	RETURN RESULT
ENDIF
;TRAIN_MESSAGE抜きで口上メイン呼出
CALL K30_D_LINE
CALL M_KOJO_MESSAGE_COM_K30_300_1
RETURN RESULT

