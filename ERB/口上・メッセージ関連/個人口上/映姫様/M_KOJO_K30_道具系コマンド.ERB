;==================================================
;44,バイブ
;==================================================
@M_KOJO_MESSAGE_COM_K30_44
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K30_44_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K30_44_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;時姦中・睡眠中
	IF CFLAG:MASTER:イタズラ == 2 || FLAG:70
		RETURN 1
	ENDIF
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF CFLAG:1801 == 1
			IF TCVAR:破瓜 != 0
				PRINTFORMW 「なんですか、その道具は……えっ、まさか」
				PRINTFORMW 「だめです！　絶対にだめ！　そんな道具でなんてふざけてるの！？」
				PRINTFORMW 「……本気、なの？　…待って……いや、お願い…いやっ！」
				PRINTFORMW 「やめてっ！　…あああぁっ！！」
				TCVAR:30:待たぬ = 1
				TCVAR:30:無理矢理 = 1
				;フラグに残す
				SIF TCVAR:破瓜 == 1
					SETBIT CFLAG:1800, 2
				RETURN 1
			ELSE
				PRINTFORMW 「なんですか、その道具は……ひっ！」
				PRINTFORMW 「やだぁ…中……震えてる……」
				RETURN 1
			ENDIF
		ELSE
			IF TCVAR:破瓜 != 0
				PRINTFORML 「なんですか、その道具は……えっ、まさか」
				PRINTFORMW 「ちょっと、ちょっと待ってください！……ひっ！」
				PRINTFORMW 「私は…道具に初めてを………いえ、何でもないです……」
				RETURN 1
			ELSE
				PRINTFORMW 「なんですか、その道具は……ひっ！」
				PRINTFORMW 「やだぁ…中……震えてる……」
				RETURN 1
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;マグロスイッチオン
	IF CFLAG:1501 || TCVAR:30:無理矢理 || TCVAR:30:泥酔
		CALL K30_GASPING_MAGURO
		RETURN 1
	ENDIF
	;基本セット
	;バイブ解除時
	IF !TEQUIP:バイブ
		IF EXP:Ｖ経験 > 70
			IF RAND:2 == 1
				PRINTFORML 「あっ、抜いちゃうの……だったら、次は…」
				PRINTFORMW 「%K30_C_NAME(MASTER)%に、愛してもらいたいの」
			ELSE
				PRINTFORML 「……抜かないでぇ」
				PRINTFORMW 「もっと貴方に、いじめてほしいんです…♥」
			ENDIF
		;Ｖ経験40以上
		ELSEIF EXP:Ｖ経験 > 40
			PRINTFORML 「はぁ、はぁ……」
			PRINTFORMW 「あっ……終わっちゃう、の？」
		;Ｖ経験20以上
		ELSEIF EXP:Ｖ経験 > 20
			PRINTFORMW 「うぅっ……やっぱり、慣れないわ…」
		;それ以外
		ELSE
			PRINTFORMW 「やだっ、まだ……あそこに、何かささってる感じが…」
		ENDIF
	;%TINKO%引き抜いてからすぐ入れた
	ELSEIF TCVAR:100 == MASTER
			IF EXP:Ｖ経験 > 100
				IF RAND:2 == 1
					PRINTFORML 「はぅ……んっ♥　ちがうとこ、当たってるっ！」
					PRINTFORMDW バイブで身体の奥から掻き回され、映姫の表情もすぐに蕩けだした…
				ELSE
					PRINTFORML 「あはああっ♥　すごいっ！　こっちも、すごいっ♥」
					PRINTFORMDL %CALLNAME:MASTER%がバイブから手を離すと、映姫の身体と同様にバイブがひくひくと動いている
					PRINTFORMDW 映姫の膣がバイブを呑み込もうと咀嚼しているようにも見える…
				ENDIF
			ELSEIF EXP:Ｖ経験 > 20
				PRINTFORMW 「あぅ…♥　道具でなんて、そんなぁ……」
			ELSE
				PRINTFORMW 「や、だ…道具じゃなくて、貴方のが…いいのに…」
			ENDIF
	ELSE
		IF ABL:Ｖ感覚 > 2
			IF RAND:2 == 1
				PRINTFORML 「入って……奥、届いてる…♥」
				PRINTFORMW 「ああっ♥　だめっ、だめっ！　これっ…だめっ♥」
				PRINTFORMDW バイブを膣奥まで飲み込み、映姫は悲鳴ともつかぬ悦びの声を上げている…
			ELSE
				PRINTFORML 「お願い、早く挿れて…ください……♥」
				PRINTFORMDW 馴染ませてるのを焦らしと勘違いし、映姫がおねだりをしてきた…
				PRINTFORMDL %CALLNAME:MASTER%はリクエストに応えるべく映姫の濡れそぼった肉壺にバイブを押し込んだ
				PRINTFORMW 「ふああああぁっ♥　おくぅ、きもちいいのっ」
			ENDIF
		ELSEIF PALAM:潤滑 <= PALAMLV:1
			PRINTFORML 「ちょっと待って……待って！　無茶なこと、しないで…」
			PRINTFORMDW まだほとんど濡れてない映姫の割れ目にバイブがいかにもキツそうだ…
		ELSEIF EXP:Ｖ経験 > 20
			IF RAND:2 == 1
				PRINTFORML 「ぁ……あ…、どんどん…奥まで……はあああんっ♥」
				PRINTFORMDW バイブを全部受け入れたところで、映姫は大きな吐息を吐いた
			ELSE
				PRINTFORML 「ああぁっ♥　やだっ、こんなの…入ってきたら……」
				PRINTFORMDW 不安そうな口ぶりとは裏腹に、映姫の膣はバイブをすんなり受け入れた…
			ENDIF
		ELSE
			IF RAND:2 == 1
				PRINTFORMW 「その道具、ちょっと怖いのよ……痛くしないでね？」
			ELSE
				PRINTFORML 「動くだけならいいんですが、その…震えるのが、あまり…」
				PRINTFORMDW 映姫はバイブの微妙な振動にまだ慣れないようだ…
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

