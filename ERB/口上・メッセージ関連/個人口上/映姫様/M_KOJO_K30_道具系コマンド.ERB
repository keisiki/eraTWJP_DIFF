@M_KOJO_MESSAGE_COM_K30_45_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	IF FLAG:70
		RETURN 1
	ENDIF
	;マグロスイッチオン
	IF CFLAG:1501 || TCVAR:30:無理矢理 || TCVAR:30:泥酔
		CALL K30_GASPING_MAGURO
		RETURN 1
	ENDIF
	;基本セット
	;解除
	IF !TEQUIP:アナルバイブ
		SELECTCASE ABL:Ａ感覚
			CASE IS > 4
				IF NOWEX:Ａ絶頂 && !NOWEX:二重絶頂
					CALL HPH_PRINT("「あっHPH　あっ、あああっHPH　ぬけちゃう！　ぬけちゃうぅHPH」", "W")
				ELSE
					CALL HPH_PRINT("「あっHPH　あっ、あああっHPH　……はぁ…はぁ」", "W")
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「……おしりぃ、抜いちゃ、いやです」
							PRINTFORMW 「ねぇ……もっと……♥」
						CASE 1
							PRINTFORML 「今、抜いてしまったら、その…切なくて」
							PRINTFORMW 「今度は……貴方のが、欲しいの…♥」
						CASE 2
							PRINTFORML 「はぁ、はぁ……すごかった、です……♥」
					ENDSELECT
				ENDIF
			CASE 2, 3, 4
				CALL HPH_PRINT("「んっHPH　抜け…る……あんっHPH」", "W")
				IF RAND:2 == 1
					PRINTFORMW 「んんっ…終わ、り？　……なのね…」
				ELSE
					PRINTFORMW 「…ひっ、拡がったままとか、恥ずかしいこといわないでっ！」
				ENDIF
			CASE 1
				PRINTFORML 「だめっ！　だめっ！　見ないでっ！　出ちゃう！　…あっ、ああっ♥」
				PRINTFORMW 「…本当に、恥ずかしいの、見ないで…お願い…」
			CASE 0
				PRINTFORMW 「う、ぐぐぅ…ううぅ…ああっ！！」
				PRINTFORML 「はぁ、はぁ、はぁ………」
				PRINTFORMDW 映姫は肩で息をしたまま、しばらく目をあわせてくれなかった…
		ENDSELECT
	;装着
	ELSE
		;二本差し
		IF TEQUIP:50 == PLAYER || TEQUIP:バイブ
			SELECTCASE ABL:Ａ感覚
				CASE IS > 4
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「あぁ…\@ TEQUIP:バイブ ? 前 # 貴方の \@だけでも\@ ABL:Ｖ感覚 > 2 ? すごい # いっぱいな \@のに…お尻、にも…♥」
							CALL HPH_PRINT("「ぁっ…あっ、あああっHPH　入ってくるうぅHPH」", "W")
						CASE 1
							CALL HPH_PRINT(@"「ああぁっHPH　いやっ、いやっHPH　いっしょに、うご\@ TEQUIP:バイブ ? かし # い \@ちゃ、だめぇHPH」", "L")
							IF TEQUIP:50 == PLAYER
								PRINTFORMDW アナルバイブを咥え込むと映姫の膣内はさらに熱さと蠢きを増していった…
							ELSE
								PRINTFORMDW 二穴バイブがもたらす無機質な快感に映姫は翻弄されよがり声を上げ続ける
							ENDIF
						CASE 2
							CALL HPH_PRINT("「あっ、ああっHPH　だめっ、だめぇHPH　こんなのっ、だめぇHPH」", "L")
							PRINTFORMD %CALLNAME:MASTER%は何がダメなのかとわざと意地悪く囁き
							IF TEQUIP:バイブ
								PRINTFORMDW 両方のバイブを抜き差しした…
							ELSE
								PRINTFORMDW 腰を\@ TEQUIP:体位 < 3 ? ゆっくり前後させ # 軽く突き上げ \@た…
							ENDIF
							CALL HPH_PRINT("「やあぁHPH　なか、あたってぇHPH　こすれてるぅHPH　だめぇHPH」", "W")
					ENDSELECT
				CASE 2, 3, 4
					IF RAND:2 == 1
						CALL HPH_PRINT("「あっ…両方、入っちゃってる…ん、んんっHPH」", "W")
						CALL HPH_PRINT("「これ…だめっ、癖になったら…だめっHPH」", "W")
					ELSE
						PRINTFORML 「こんなに、簡単に入って、気持ち…良くなってる…」
						CALL HPH_PRINT(@"あっHPH　だめっ…今\@ TEQUIP:バイブ ? 動かし # 貴方が動い\@たら…HPH」", "W")
					ENDIF
				CASE 1
					PRINTFORML 「ぁ、あぅう、きつい、けど……ぅうう」
					PRINTFORMW 「あっ、待って…動か\@ TEQUIP:バイブ ? さ # \@ないで、そのまま…」
					PRINTFORMW 「あっ……♥」
				CASE 0
					PRINTFORMW 「ううっ…苦し、い…の…さすがに、無理…」
			ENDSELECT
		ELSE
			;アナルバイブのみ
			SELECTCASE ABL:Ａ感覚
				CASE IS > 4
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT("「あっHPH　すごい、おしりの奥まで…んっHPH　んんっHPH」", "W")
							CALL HPH_PRINT("「おくぅHPH　おなか、おくまでHPH　ふるえてるのっHPH」", "W")
						CASE 1
							PRINTFORMW 「…早く、挿れて…ください…我慢出来ないんです、お尻に…早く…♥」
							PRINTFORMDL 尻を切なげに揺らし挿入をねだる映姫には、えもいわれぬ色香が漂っている
							PRINTFORMDW %CALLNAME:MASTER%は窄まりにあてがったバイブを映姫の奥深くまで押し込んだ…
							CALL HPH_PRINT("「―――っ！　いいっHPHHPH　これ、すごいいぃHPHHPH」", "W")
						CASE 2
							PRINTFORMW 「準備、出来てますよ…早く、それで…掻き回してください…♥」
							PRINTFORMDL バイブを押し込むと映姫の肛門は柔軟に口を開き、するりとバイブを飲み込んでいく…
							CALL HPH_PRINT("「あっHPH　あっHPH　はぁぁHPH　おしりぃ、はいってきたぁHPH」", "W")
					ENDSELECT
				CASE 2, 3, 4
					IF RAND:2 == 1
						PRINTFORML 「こんな恥ずかしいこと…不浄の穴に物を入れるだなんて…」
						PRINTFORMDW 恨みがましくつぶやく映姫だが、あてがったバイブの振動にはぴくぴくと反応している…
						PRINTFORMDL %CALLNAME:MASTER%は窄まりにあてがったバイブをゆっくりと押し込んだ…
						CALL HPH_PRINT("「う…ぁ…あ…あっ、あっHPH　おなか、押されてる…HPH」", "W")
					ELSE
						PRINTFORML 「………ぅ…ぁ、うぅっ…ああぁっ♥」
						PRINTFORMDW アナルバイブを挿れられた映姫はなんとか快感をこらえようとしていた
						PRINTFORMDL だが弱点を的確に苛む振動は、その覚悟をあっさりと破ってしまった…
						CALL HPH_PRINT("「いいのHPH　私……お尻が、気持ちいいのっ…HPH」", "W")
					ENDIF
				CASE 1
					PRINTFORML 「恥ずかしい…見ないで、見ないでください…あっ！」
					PRINTFORMW 「ぁ…ぅぅ…、やっぱり…苦しい…」
				CASE 0
					PRINTFORMW 「う…ううぅ、苦し…いの、お願い…抜いて…」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
