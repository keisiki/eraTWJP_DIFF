IF LOCAL
	IF FLAG:70
		RETURN 1
	;マグロスイッチ
	ELSEIF CFLAG:1501 || TCVAR:30:無理矢理 || TCVAR:30:泥酔
		IF TEQUIP:PLAYER:コンドーム
			ONEDARI = 0
			CALL K30_GASPING_MAGURO
			RETURN 1
		ELSEIF TCVAR:30:泥酔
		ELSEIF TCVAR:30:無理矢理
			PRINTFORMW 「何が、したいのよ…」
		ELSEIF CFLAG:1501
			PRINTFORMW 「外すんですか…？　できればそのままが…」
		ENDIF
	;装着時
	ELSEIF TEQUIP:PLAYER:コンドーム
		ONEDARI = 0
		;中出しOK時
		IF !K30_CREAMPIE()
			IF (TALENT:妊娠願望 || GETBIT(CFLAG:1502, 4))
				PRINTDATAW
					DATAFORM 「そんなの……いりますか？」
					DATAFORM 「なんで着けてるの……？」
					DATAFORM 「……外してもらえますか」
				ENDDATA
				PRINTFORMDW 映姫は不満そうに%CALLNAME:MASTER%を見上げている
				ONEDARI = 3
				RETURN 1
			ELSEIF ABL:膣射中毒
				PRINTDATAW
					DATAFORM 「子どもを、つくること……やっぱり、迷ってます？」
					DATAFORM 「……出される時の、あの感覚……私も、嫌いじゃないですが」
					DATAFORM 「……そうするべき、なんでしょうか……」
				ENDDATA
			ELSE
				PRINTDATAW
					DATAFORM 「その方がいいですね」
					DATAFORM 「そうするべき、ですかね…」
					DATAFORM 「そっちの方が、いいのでしょうか…」
					DATAFORM 「そっちの方が安心できますね」
				ENDDATA
			ENDIF
		;中出しNG時
		ELSE
			PRINTDATAW
				DATAFORM 「ありがとう。安心できるわ」
				DATAFORM 「よかった。ちゃんとしてくれて」
				DATAFORM 「そうね。できてしまったら困りますから」
			ENDDATA
		ENDIF
		;Ａ挿入慣れてて、Ａの方が好きか見る
		SIF !ABL:肛射中毒 || ABL:Ａ感覚 < 2 || (ABL:膣射中毒 >= ABL:肛射中毒 && !RAND:5)
			RETURN 1
		SIF K30_CREAMPIE()
			PRINTFORMD 口ではそう言いながら
		PRINTFORMDW 映姫はゴムに覆われた%TINKO()%を残念そうに撫で回す…
		;0にも100にもならない適当な判定。他人の前では自重
		IF MIN(ABL:肛射中毒, 7) < NOISE3(GETTIME(), 9) || GET_TARGETNUM() > 1 || K30_CREAMPIE() == 1
			PRINTFORMW 「…ごめんなさい。気にしないで」
			ONEDARI = 1
		ELSE
			PRINTFORML 
			PRINTFORMW 「…………………」
			PRINTFORML 「ねえ、%K30_C_NAME(MASTER)%」
			IF ABL:Ａ感覚 > 4 && ABL:肛射中毒 > 4 && ABL:露出癖 > 4
				PRINTFORMW 「ゴムなんていまさら必要ですか…？」
				PRINTDATA
					DATAFORM 「ここに出せば、二人とも…気持ちいいですよ
					DATAFORM 「貴方は、おしりに出すのも…お好みでしょ
					DATAFORM 「そんなところに出すより…こっちを、使ってほしいの
				ENDDATA
				CALL HPH_PRINT, @"HPH」", "W"
				PRINTFORMDL 映姫は自分から尻穴を拡げ生挿入をおねだりをしてきた…
				PRINTFORMDW 肛射の快感にすっかりハマったようで、もはや取り繕う様子もない
				ONEDARI = 2
			ELSE
				PRINTFORMW 「…………………」
				PRINTDATAW
					DATAFORM 「……おしりに、なら…いいのよ…」
					DATAFORM 「…おしり、なら…大丈夫だから…」
					DATAFORM 「……おしりに、どうですか…」
				ENDDATA
				PRINTFORMDL …どうやら肛射の感覚が忘れられないらしい
				PRINTFORMDW 自分で言ってから映姫は耳まで真っ赤にしている
				ONEDARI = 1
			ENDIF
		ENDIF
	;取り外し時
	ELSE
		SELECTCASE ONEDARI
			CASE 0
				SIF K30_CREAMPIE() > 1 && RAND:2 == 1
					PRINTFORMW 「…わかってますか？　しないとダメな日ですよ？」
				PRINTDATAW
					DATAFORM 「…一枚無駄になったわね」
					DATAFORM 「…付けたり外したりで、忙しそうね」
				ENDDATA
			CASE 1
				PRINTDATAW
					DATAFORM 「…ありがとう」
					DATAFORM 「…して、くれるのね」
				ENDDATA
			CASE 2
				IF RAND:2 == 1
					PRINTFORML  「ふふっ、やっとその気になった♪」
					CALL HPH_PRINT, @"「こっちで、ちゃんと気持ちよくしてあげますよHPH」", "W"
				ELSE
					PRINTFORML 「そうでしょ。最初からそうすれば良かったのよ」
					CALL HPH_PRINT, @"「こんな気もちのよいところで…しないなんて、もったいないわHPH」", "W"
				ENDIF
			CASE 3
				IF RAND:2 == 1
					SIF ESTRUS_CYCLE(30) >= 100
						PRINTFORML 「今日は、ちょうど…出来やすい日なのよ。だから…」
					PRINTFORMW 「作りましょうか…私たちの、赤ちゃん」
					PRINTFORMW 「頑張って…いっぱい、愛し合いましょ♥」
				ELSE
					PRINTFORML 「ごめんなさい、わがまま言ってしまって。でも…」
					SIF ESTRUS_CYCLE(30) < 70
						PRINTFORML 「期待できる日じゃなくても、可能性があるのなら…無しで、したいの」
					PRINTFORMW 「どうしても、欲しいんです。貴方との…子どもが」
				ENDIF
		ENDSELECT
		ONEDARI = 0
	ENDIF
ENDIF
RETURN 1

