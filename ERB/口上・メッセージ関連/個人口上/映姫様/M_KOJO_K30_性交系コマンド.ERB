		SOURCE:快Ｂ = 250
		TEQUIP:30:Ｂ接触部位 = 3
		TFLAG:193 = 2
	CASE 3
		TEQUIP:30:Ｃ接触部位 = 3
		SOURCE:快Ｃ = 250
		SOURCE:快Ｖ += 50
		TFLAG:193 = 3
ENDSELECT
RETURN

