;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K30_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;押し倒されフラグ消す
	TCVAR:30:376 = 0
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
