	;疲れた
		PRINTFORMW 「はぁ、はぁ……私が、もう限界……付き合ってくれて、ありがとうね……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		IF (TALENT:妊娠願望 || GETBIT(CFLAG:1502, 4)) && 充填率(30, 4) > 20 && !TCVAR:ピル
			PRINTFORML 「はぁ、はぁ………ありがとう。すごく…良かったの」
			PRINTFORML 「ここも、こんなに満たされて…とっても、幸せ…♥」
			PRINTFORMDW 映姫はしっかり種付けされた下腹部を大切そうに撫でている
			PRINTFORMW 「ちゃんと、出来ると…いいですね」
		ELSEIF INRANGE(TCVAR:30:363, -4, -2)
			PRINTFORML 「はぁ、はぁ……すごく…良かったの♥　朝から…お疲れさま♥」
			PRINTFORML 「ああ、でも…貴方も私も…べとべとのどろどろね…」
			PRINTFORMW 「お風呂にでも入ってゆっくりしませんか？」
			TCVAR:30:363 = -100
		ELSEIF TALENT:恋慕
			PRINTFORML 「はぁ、はぁ………ふふふっ、ありがとう。満足させてもらったわ♪」
			PRINTFORMW 「%K30_C_NAME(MASTER)%もこういうの、嫌いじゃないですよね」
		ELSE
			PRINTFORML 「はぁ、はぁ……今日はこれでお終いにしましょう」
			PRINTFORMW 「……付き合わせてごめんなさい。ありがとうございます……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	TCVAR:30:376 = 1
	;満足したところを押し倒した
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「……%K30_C_NAME(MASTER)%が特別？　他の人ってどうなのかしら……」
					PRINTFORMDW まだまだやる気の%CALLNAME:MASTER%に映姫は若干引き気味だ……
				CASE 1
					PRINTFORML 「……火をつけただけかしら」
					PRINTFORMW 「でもそうね…こうなったら私も責任をとらなきゃね」
				CASE 2
					PRINTFORML 「そうよね、普段の貴方ならこれで満足なんてしないわね……」
					PRINTFORMW 「なら、今度は私が…満足させる番……」
			ENDSELECT
		ELSE
			PRINTFORML 「え、ええ？　まだ…しないとだめなんですか……？」
			PRINTFORML 「もう、わたしはそこまで………きゃっ！？」
			PRINTFORMDW 先程とは逆に%CALLNAME:MASTER%が映姫を若干強引に押し倒した……
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;押し倒されフラグ再点灯
	TCVAR:30:376 = 2
	;壊れるまでしてあげる
		PRINTFORML 「……言ったわね。なら試してみましょうか」
		PRINTFORMW 「地獄の閻魔様が本気で攻めてあげるわよ………どこまで持つかしら♥」
		RETURN 1
	ENDIF
ENDIF
RETURN 1


;==================================================
;一日の終了時(AFTERTRAニーの前)、面識がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K30_31(ARG,ARG:1)
;口上主はTARGET
;ARGは会ってない日数
;何日以上会ってなかったら愚痴を言うとかできます
;CFLAG:オナバレで分岐入れてもよさそう
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
VARSET LOCALS
IF LOCAL
	;日記フラグ：ぱんつの日、恋慕取得、妊娠願望取得
	SIF DIARY:30:11 == 0 && FLAG:異常気象 == 7
		CALL K30_WRITE_DIARY(11)
	SIF DIARY:30:30 == 0 && TALENT:30:恋慕
		CALL K30_WRITE_DIARY(30)
	SIF DIARY:30:32 == 0 && GETBIT(CFLAG:1502, 3)
		CALL K30_WRITE_DIARY(32)
	;口上外うふふ強制中断のフォロー
	CFLAG:1501 = 0
	CFLAG:1600 = 0
	CFLAG:1601 = 0
	;縁結びフラグ立てor折り
	IF FLAG:願掛け内容 == 30 && CFLAG:翌日来訪 == 1 && GET_MAPID(GET_CHARAHOME(30)) != MAIN_MAP
		CFLAG:1603 = 1
	ELSE
		CFLAG:1603 = 0
	ENDIF
	;うふふがまん日数記録
	IF TCVAR:30:380
		CFLAG:1603 = 0
	ELSE
		CFLAG:1603 ++
	ENDIF
	PRINTFORML 
	CALL M_KOJO_COLOR_K30
	;媚薬でいじった素質戻す
	IF TCVAR:30:379 > 1000
		TALENT:30:淫乱 = 0
		PRINTFORMDL 映姫に盛った薬の効果が切れた…
		PRINTFORMDW 映姫は[淫乱]を失った 
		PRINTFORML 
	ENDIF
	;関数使うと呼び名安定しないので先に固定
	LOCALS = %CSTR:30:80%
	IF LOCALS == ""
		LOCALS = %MASTERNAME:30%
		SIF !TALENT:恋人
			LOCALS += "さん"
	ENDIF
	;同棲中におはよう口上通してるなら最後に会った日をリセット
	[SKIPSTART]
	IF CFLAG:MASTER:初期位置 == CFLAG:30:初期位置 && TCVAR:30:373
		TCVAR:30:今日会ったか = 1
		CFLAG:30:最後に会った日 = DAY
	ENDIF
	[SKIPEND]
	;今日会っていなければおやすみ口上フラグ折っておく
	SIF TCVAR:30:今日会ったか == 0
		CFLAG:1605 = 0
	;詫びおぱんつ処理
	IF TCVAR:30:377
		IF TCVAR:30:377 < 0
			;貰うおぱんつを抽選。ノーパン/トランクスを渡さないように最低値は2
			LOCAL = MAX(今日のぱんつ(30), 2)
			PRINTFORMDL 枕元に丁寧に畳まれた%PANTSNAME(LOCAL, 30)%が置いてあった
			PRINTFORMDL あんなくだらない約束でもきちんと守るとはさすが閻魔様…
			PRINTFORML 
		ELSE
			LOCAL = TCVAR:30:377
		ENDIF
		PRINTFORMDW %CALLNAME:MASTER%は\@ TCVAR:30:377 < 0 ? 隠しておいた # \@映姫の%PANTSNAME(LOCAL, 30)%をコレクションに加えた
		CFLAG:30:(パンツ管理 + LOCAL) ++
		PRINTFORML
	ENDIF
	;同棲相手が無断外泊とか普通はキツい。幻想郷の子、ドライすぎない？
	;それとは別に映姫様は重い女の方が良い
	IF CFLAG:MASTER:初期位置 == CFLAG:30:初期位置 && CFLAG:MASTER:現在位置 != CFLAG:30:初期位置
		SELECTCASE RAND:3
			CASE 0
				LOCALS:1 = 「%LOCALS%……私を、ひとりに…しないで……%LOCALS%…」
			CASE 1
				LOCALS:1 = 「もしかして…%LOCALS%の身に何かあった？　……まさか、そんな…」
			CASE 2
				LOCALS:1 = 「%LOCALS%…何で帰ってきてくれないの…ねぇ、なんで…？」
		ENDSELECT
		;イキ顔を泣き顔と言い張る暴挙
		IF CFLAG:差し替え適用 && 顔絵付きメッセージ
			CALL K30_SPTALK, "絶頂", @"%LOCALS:1%"
		ELSE
			PRINTFORML %LOCALS:1%
		ENDIF
		PRINTFORMDW 帰ってこない%CALLNAME:MASTER%を案じて映姫は憔悴している…
		RESETCOLOR
		RETURN 1
	ENDIF
	;口上通って無くても同棲中の部屋戻ってたらおやすみ口上フラグ立てる
	SIF CFLAG:MASTER:初期位置 == CFLAG:30:初期位置 && CFLAG:MASTER:初期位置 == CFLAG:MASTER:現在位置
		CFLAG:1605 = 1
	;妊娠願望取得
	IF !EXP:出産経験 && !TALENT:妊娠願望 && CFLAG:1502 == 7
		PRINTFORML 
		LOCALS:1 = （言い訳ばかりじゃだめ…もっと素直にならなきゃ）
		LOCALS:2 = （%MASTERNAME:30%と一緒に生きた証…愛した、愛された証が私は欲しい）
		LOCALS:3 = （二人の愛の結晶をこのお腹に…）
		IF CFLAG:差し替え適用 && 顔絵付きメッセージ
			CALL K30_SPTALK, "LOVE", @"%LOCALS:1%/%LOCALS:2%/%LOCALS:3%"
			PRINTFORML 
		ELSE
			PRINTFORML 
			PRINTFORML %LOCALS:1%
			PRINTFORML %LOCALS:2%
			PRINTFORMW %LOCALS:3%
		ENDIF
		PRINTFORMDL 映姫は%CALLNAME:MASTER%との子どもを心から望むようになった
		PRINTFORMDW 映姫は[妊娠願望]を得た
		TALENT:妊娠願望 = 1
		SETBIT CFLAG:1502, 3
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;会わない日に一言
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1
		;陥落無しならしっかり間隔を開ける。攻略対象外が出しゃばってはいけません。ついでに顔も出しません
		IF ARG % 14 == 0 && ARG != 0 && !TALENT:30:恋慕 && !TALENT:30:思慕
			PRINTFORMDL 閻魔様は書類仕事に精を出している…
			PRINTFORMDL 一段落したところで、ふとおかしな外来人の事を思い出した
			PRINTDATAW
				DATAFORM 「%K30_C_NAME(MASTER)%…しばらく顔を見てないような」
				DATAFORM 「%K30_C_NAME(MASTER)%…やっぱり、私の方には寄ってこないわね」
				DATAFORM 「%K30_C_NAME(MASTER)%…このところ教えを説いていない気がするわ」
			ENDDATA
			PRINTFORML 「…………………」
			PRINTFORML 「まあ…いつものことですね」
			PRINTFORMDW 映姫は特に気にすることなく仕事に戻った
		;思慕は７日に１回＠映姫様ここからが固いので放置しがちだよね
		ELSEIF ARG % 7 == 0 && ARG != 0 && TALENT:30:思慕
			SELECTCASE RAND:3
				CASE 0
					LOCALS:1 = 「%K30_C_NAME(MASTER)%…きちんと善行を積んでるかしら…」
				CASE 1
					LOCALS:1 = 「%K30_C_NAME(MASTER)%…あれで女性にだらしなくなければいい人なのに…」
				CASE 2
					LOCALS:1 = 「%K30_C_NAME(MASTER)%…変なことしていないか心配ね…」
			ENDSELECT
			IF CFLAG:差し替え適用 && 顔絵付きメッセージ
				CALL K30_SPTALK, "通常", @"%LOCALS:1%"
			ELSE
				PRINTFORML %LOCALS:1%
			ENDIF
			PRINTFORMDL しばらく顔を見てない%CALLNAME:MASTER%の事を映姫はふと思い出した
			PRINTFORMDW なぜかはわからないが心がざわめくのを感じた…
		;恋慕なら３日会わない毎に一言
		ELSEIF ARG % 3 == 0 && ARG != 0
			IF TALENT:30:恋人
				IF RAND:2 == 1
					LOCALS:1 = 「こんなに恋人を放っておくなんて……次に会ったら、絶対に…」
				ELSE
					LOCALS:1 = 「%LOCALS%ったら…どうせまた他の女の子と…」
				ENDIF
				IF CFLAG:差し替え適用 && 顔絵付きメッセージ
					CALL K30_SPTALK, "反発", @"%LOCALS:1%"
				ELSE
					PRINTFORML %LOCALS:1%
				ENDIF
				PRINTFORMDW 逢いに来ない恋人の事を考え映姫はやきもきとしている…
			ELSEIF TALENT:30:恋慕
				IF RAND:2 == 1
					LOCALS:1 = 「……私から急に会いに行ったら、変かしら」
				ELSE
					LOCALS:1 = 「来るまで待つ……それよりも会いに行ったほうが…でも……」
				ENDIF
				IF CFLAG:差し替え適用 && 顔絵付きメッセージ
					TFLAG:192 = -6
					CALL K30_SPTALK, "赤面", @"%LOCALS:1%"
				ELSE
					PRINTFORML %LOCALS:1%
				ENDIF
				PRINTFORMDW なかなか会えない想い人の事を考え映姫はもやもやとしている…
			ENDIF
		ENDIF
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会っている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
	ENDIF
ENDIF
RESETCOLOR
RETURN 1

