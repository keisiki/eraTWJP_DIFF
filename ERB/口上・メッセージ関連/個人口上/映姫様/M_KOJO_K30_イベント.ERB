				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「いいわよ……はい、どうぞ」
					SIF GETBIT(CFLAG:1699, 0)
						PRINTFORMW 「だから…もう私のだけで我慢してくださいね」
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「……変なことに使わないでくださいよ……」
					SIF GETBIT(CFLAG:1699, 0)
						PRINTFORMW 「そんなに集めてどうするつもりなんですか…まったく」
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「……そんなこと許すと思いますか？」
					SIF GETBIT(CFLAG:1699, 0)
						PRINTFORMW 「前にも注意したのにまだパンツ集めなんてやってるんですか？」
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

