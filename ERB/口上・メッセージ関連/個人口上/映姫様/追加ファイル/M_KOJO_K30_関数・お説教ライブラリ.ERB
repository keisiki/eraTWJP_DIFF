SIF TALENT:キス魔 && SELECTCOM == 312
	RETURNF 1
LOCAL = RAND:110
LOCAL:1 = 25 + GET_SUCCESS_RATE() / 8
SIF LOCAL:1 > 109
	LOCAL:1 = 109
;キャップをかけた後に視線が通ってたら-33%、違ったら+20%
IF K30_BE_SEEN() == 0
	LOCAL:1 += 20
