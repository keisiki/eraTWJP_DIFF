	G_ID = 0
	FOR COUNT, 0, 5 + (TALENT:恋慕 > 0)
		CALL 画像合成(G_ID, SET_RESOURCE:COUNT)
	NEXT
	SPRITECREATE RESOURC_NAME, G_ID
ENDIF

