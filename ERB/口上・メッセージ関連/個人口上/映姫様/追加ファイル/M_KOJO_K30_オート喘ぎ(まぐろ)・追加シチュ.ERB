;溜まってると嗅ぎつけてくる
SIF TALENT:MASTER:濃厚精液
	S_VAL += 20
;裁判の日は自重
SIF DAY % 2 == 0
	S_VAL -= 30
;最低保証3%
SIF S_VAL < 2
