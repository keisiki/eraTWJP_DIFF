IF DAY % 2 == 1 && (CFLAG:30:溜まってる度 > 500 || GROUPMATCH(S_VAL, 1000, 999)) && !CFLAG:30:弱み握り&& ABL:30:精液中毒 > 4 && !K30_CREAMPIE()
