IF (TALENT:妊娠願望 || GETBIT(CFLAG:1502, 4)) && ESTRUS_CYCLE(30) >= 100 && TALENT:MASTER:濃厚精液
	S_VAL = 1000
	GOTO BEG
ENDIF
