;オート喘ぎ声口上（まぐろ）
;口上どうしても思い浮かばないのでAUTO_AEGI改変関数
;部位・感度等無視して矛盾しない喘ぎ声をつくるのを目標に
@K30_GASPING_MAGURO
;--------------------------------------------------
#DIM 状況
#DIM 心神喪失
#DIM ひどい
#DIM TEMP
#DIM TEMP2
心神喪失 = 0
ひどい = 0
TEMP = 0
TEMP2 = 0
SIF !CFLAG:30:うふふ
	RETURN 0
;ノーマルまぐろ
状況 = 0 
;セフレ
SIF TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
	状況 = 4
;泥酔まぐろ
IF TCVAR:泥酔
	状況 = 5
	心神喪失 =1
ENDIF
;睡眠まぐろ
IF CFLAG:睡眠
	状況 = 9
	心神喪失 =1
ENDIF
;キス時
SIF GROUPMATCH(SELECTCOM, 19, 20)
	状況 = 1
;フェラ時
SIF GROUPMATCH(SELECTCOM, 81, 88, 89, 140, 501, 505, 508, 509, 512)
	状況 = 2
;個別処理時(薬
SIF GROUPMATCH(SELECTCOM, 50, 51, 180, 181, 182, 192)
	状況 = 6
;SM系(SOURCE入らない)時
SIF GROUPMATCH(SELECTCOM, 100, 104, 105, 107)
	状況 = 7
;奉仕(ちゅぱ音無し)時
SIF GROUPMATCH(SELECTCOM, 80, 82, 83, 84, 85, 510, 511)
	状況 = 8
;Ａ関連
SIF GROUPMATCH(SELECTCOM, 4, 5, 13, 45, 62, 63, 66, 69, 70, 71, 74, 75, 514, 515)
	状況 = 3
PRINTFORM 「
SIF TCVAR:無理矢理
	PRINTFORM %TEXTR("やだ/いや/だめ/やめて")%っ！　…
;セフレ、心神喪失、薬、ＳＭは必ず最初に一言
IF (RAND:2 == 0 || GROUPMATCH(状況, 4, 6, 7) || 心神喪失) && !TCVAR:無理矢理 
	CALL K30_GASPING_RAND(状況)
	;ＳＭは一言で処理終了
	SIF 状況 == 7
		RETURN 1
	TEMP = 1
ENDIF
;行中の処理
REPEAT 5 + RAND:2
	SELECTCASE 状況
	;キス・フェラはちゅぱ音
	;心神喪失・無理矢理時は頻度半分
		CASE 1, 2
			IF (RAND:2 == 1 && !(心神喪失 || TCVAR:無理矢理)) || (RAND:4 == 1 && (心神喪失 || TCVAR:無理矢理))
				IF 状況 == 1
					PRINTFORM …%TEXTR("んんっ/れろ/あむっ/ちゅる/んむ")%
				ELSE
					PRINTFORM …%TEXTR("んぷっ/じゅぽっ/じゅるっ/ずぶっ/じゅぷっ")%
				ENDIF
				SIF PALAM:30:欲情 > PALAMLV:7 && !TCVAR:無理矢理 && RAND:3 != 1
					PRINTFORM ♥　
			ELSE
				PRINT …
			ENDIF
	;通常は拒否っぽい声	、準強制わいせつは寝言っぽいのを、セフレ・恋人同伴は肯定をそれぞれ2回まで
		CASE 0, 4, 5, 9
			IF TEMP2 < 3 && !TCVAR:無理矢理
				IF RAND:3 == 1
					TEMP2 ++
					IF TCVAR:泥酔
						PRINTFORM ……%TEXTR("んふふ/う～ん/あんっ/んっんん/ひっく")%
					ELSEIF CFLAG:睡眠
						PRINTFORM ……%TEXTR("むにゃ/あふぅん/んっ/あんっ/んぅん")%
					ELSEIF 状況 == 4 || TALENT:恋慕
						PRINTFORM ……%TEXTR("きもちいいの/そこ/ああ/いいの/あん/やん")%
						IF (PALAM:30:欲情 > PALAMLV:3 && RAND:3 == 1) || (PALAM:30:欲情 > PALAMLV:5 && RAND:3 != 1)
							PRINTFORM っ
						ENDIF
						IF PALAM:30:欲情 > PALAMLV:6
							SIF RAND:2 == 1
								CALL HPH_PRINT(@"HPH　")
						ELSEIF PALAM:30:欲情 > PALAMLV:3 && RAND:3 != 1
							PRINTFORM ♥　
						ENDIF
					ELSE
						PRINTFORM ……%TEXTR("ちがうの/だめ/へんなの/かんじてなんか/いや/おかしいの")%
						IF (PALAM:30:欲情 > PALAMLV:2 && RAND:3 == 1) || PALAM:30:欲情 > PALAMLV:6
							PRINTFORM っ
						ENDIF
						IF PALAM:30:欲情 > PALAMLV:9
							SIF RAND:4 == 1
								CALL HPH_PRINT(@"HPH　")
						ELSEIF PALAM:30:欲情 > PALAMLV:5 && RAND:3 == 1
							PRINTFORM ♥　
						ENDIF
					ENDIF
				ELSE
					PRINT …
				ENDIF
			ELSE
				PRINT …
			ENDIF
	;他は無言
		CASEELSE
			PRINT …
	ENDSELECT
REND
IF TEMP == 0
	;無理矢理・薬時は語尾に一言だけ、後はどっちかに一言だけ CFLAG:1600 無理矢理口上反映フラグ
	IF TCVAR:無理矢理
		CFLAG:1600 = 1
	;無理矢理時に避妊
		SIF (SELECTCOM == 51 || TEQUIP:PLAYER:コンドーム) && TCVAR:30:待たぬ == 1
			PRINTFORM %TEXTR("何がしたいの…/意味がわからない…/ふざけてるの…/何考えてるの…")%
		PRINTFORM %TEXTR("…もう…やめてっ/…ひどいっ/…もういいでしょっ/…信じてたのにっ/…いい加減にしてっ/…触らないでっ")%
	ELSE
		CALL K30_GASPING_RAND(状況)
	ENDIF
ENDIF
PRINTFORMW 」
PRINTFORML 
;MASTER射精直前
IF NOWEX:MASTER:11 && !心神喪失 
	;無理矢理避妊無しで中出し
	IF TCVAR:無理矢理 && !TCVAR:30:ピル && TEQUIP:PLAYER:コンドーム == 0 && TCVAR:PLAYER:2 == 1
		;閻魔様の罰は準備中
		SETBIT CFLAG:1800, 0
		ひどい ++
		PRINTDATA
			DATAFORM 「待って…お願い……
			DATAFORM 「いやっ、いやだぁ……
			DATAFORM 「やめて！！　抜いてよぉ！！　……
			DATAFORM 「中は！　中はだめっ！！　……
		ENDDATA
		SELECTCASE ひどい
			CASE 1
				PRINTFORM %TEXTR("もう…やめて/できちゃうから…やめて/本当に…やめて")%………
			CASEELSE
				PRINTFORM %TEXTR("もう…許してぇ…/…もう…いやなのぉ…/おねがい…やめてぇ…")%………
		ENDSELECT
		PRINTFORML %TEXTR("やめてえええぇ/いやああああぁ！！/だめえええぇ！！/あああああぁ！")%！」
	ELSEIF 状況 == 2
		SIF EXP:30:精飲経験 < 20 && !TALENT:恋慕
			PRINTFORMW （%TEXTR("後で吐き出そう…/飲みたくないな…/外に出してよ…")%）
	ENDIF
;絶頂直前
ELSEIF (NOWEX:Ｃ絶頂 || NOWEX:Ｖ絶頂 || NOWEX:Ａ絶頂 || NOWEX:Ｂ絶頂 ) && !心神喪失 && !GROUPMATCH(状況, 1, 2, 8) 
	PRINTFORMDW 映姫は絶頂を堪えるように%TEXTR("身体を震わせる/首を振っている/口を押さえている/胸を掻き抱いている/目を固く閉じている")%……
	;セフレにはなんとなく悔し(ryな煽りを入れる
	IF 状況 == 4
		PRINTDATADW
			DATAFORM だが肉欲に溺れた身体は絶頂へと容易く流されていく……
			DATAFORM しかし身体に刻み込まれた雌の喜びが僅かな抵抗を押し流す……
			DATAFORM そんな抵抗を嘲笑うかのように、抗えない快楽が映姫の内側から湧き上がる……
			DATAFORM だが絶頂にあらがう事で、より甘美な快楽が映姫に呼び寄せられる……
			DATAFORM だがそれも悪あがきに過ぎず、抑えつけた事でむしろ膨れ上がった快感が映姫を襲う……
		ENDDATA
	ENDIF
ENDIF
RETURN 1
