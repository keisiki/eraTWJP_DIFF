;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K30_100
CALL TRAIN_MESSAGE
TCVAR:30:355 = TIME:1
CALL K30_PRINT_BUTT_IMAGE(SELECTCOM)
CALL M_KOJO_MESSAGE_COM_K30_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K30_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	ELSEIF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	;マグロスイッチオン
	ELSEIF CFLAG:1501 || TCVAR:30:無理矢理 || TCVAR:30:泥酔
		CALL K30_GASPING_MAGURO
		RETURN 1
	;基本セット
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

