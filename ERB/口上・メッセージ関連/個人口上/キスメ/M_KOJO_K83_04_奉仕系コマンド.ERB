;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K83_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K83_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[キスメ]])
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:時間停止
		;未成熟,粗チン
		IF TALENT:MASTER:形状 == 1 || TALENT:MASTER:形状 == 2
			PRINTFORMW 「握ってこするの……？」
		ELSE
			PRINTFORMW 「お、おっきい……。握ってこするの……？」
		ENDIF
		PRINTFORMW 「こ、こうかな……？ どう……？」
		RETURN 1
	ENDIF
	IF FLAG:時間停止
	;イきそう
	ELSEIF NOWEX:MASTER:射精
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「すごい膨らんでる……イきそうなんだね……」
			CALL HPH_PRINT(@"「いいよ……いっぱいぴゅっぴゅして……HPH」", "W")
		CASEELSE
			PRINTFORML 「イきそう？ じゃあ、もっと早くするね……」
			CALL HPH_PRINT(@"「いいよ……いっぱいぴゅっぴゅして……HPH」", "W")
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
		CASE 1
			PRINTFORML 「ど、どうかな……？」
			PRINTFORMW 「いっぱいきもちよくなってね……」
		CASE 2
			PRINTFORML 「なでなでしてあげる……」
			PRINTFORMW 「ふふ……かわいい……」
		CASE 3
			PRINTFORML 「うまくできてる？ ほんと？」
			PRINTFORMW 「よかった……。もっとがんばる……」
		CASE 4
			PRINTFORML 「しこしこ、きもちいい……？」
			PRINTFORMW 「いいよ、もっときもちよくなって……」
		CASEELSE
			;未成熟,粗チン
			IF TALENT:MASTER:形状 == 1 || TALENT:MASTER:形状 == 2
				PRINTFORML 「%CALLNAME:MASTER%のおちんちん……ちっちゃくてかわいい……」
				PRINTFORMW 「大丈夫、わたしの身体にはぴったりだよ……」
			;発光,イボ付き
			ELSEIF TALENT:MASTER:形状 == 4 || TALENT:MASTER:形状 == 5
				PRINTFORML 「%CALLNAME:MASTER%のおちんちん……その……」
				PRINTFORMW 「へ、へんだと思う……。やっぱり……」
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%のおちんちん……おっきいね……」
				PRINTFORMW 「こんなにおっきいのが……わたしの中に……ゴクリ」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

