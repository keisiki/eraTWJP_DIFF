		PRINTFORML 「む、胸……」
		PRINTFORMW 「%CALLNAME:MASTER%……あてつけ？」
		RETURN 1
	ENDIF
	IF FLAG:時間停止
	;イきそう
	ELSEIF NOWEX:MASTER:射精
		PRINTFORML 「ふふ、%CALLNAME:MASTER%、イきそうなんだね……」
		CALL HPH_PRINT(@"「すごく気持ちよさそう……嬉しいな……HPH」", "W")
	ELSE
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「うぅ……む、むずかしい……」
		CASE 2
			PRINTFORMW 「はぁ…はぁ……熱い……」
		CASEELSE
			IF TALENT:[[キスメ]]:バストサイズ < 0
				PRINTFORMW 「……もっとおっきかったら良かったのに……ごめんね……」
			ELSE
				PRINTFORMW 「どうかな……わたし、うまくできてる？」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

