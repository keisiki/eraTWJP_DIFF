@M_KOJO_MESSAGE_PALAMCNG_A_K83_20(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;手淫フェラ
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[キスメ]])
;-------------------------------------------------
IF LOCAL
	;精飲
	IF EXP_UP(15) == NOWEX:PLAYER:射精 * 3
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「んんっ！ んぐんぐ……ごくんっ」
			CALL HPH_PRINT(@"「ぷぁ……全部飲めた……HPH」", "W")
		CASE 2
			PRINTFORML 「んーっ！ んぐぐ……じゅるる……」
			CALL HPH_PRINT(@"「ぷぁ……おいしかったHPH」", "W")
		CASEELSE
			PRINTFORML 「んっ！ んー……んぐ……ごくんっ」
			CALL HPH_PRINT(@"「ぷぁ……ごちそうさまHPH」", "W")
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			CALL HPH_PRINT(@"「んぐっ…！ ぷぁ……いっぱい出たね……HPH」", "W")
		CASE 2
			PRINTFORMW 「んんっ…！ ぷぁ……%CALLNAME:MASTER%、気持ちよさそう……」
		CASEELSE
			PRINTFORMW 「んーっ…！ ぷぁ……ちょっとにがい……」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
RETURN 1
