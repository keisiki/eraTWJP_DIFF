;==================================================
;一日の終了時(AFTERTRAニーの前)、面識がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K75_31(ARG,ARG:1)
;口上主はTARGET
;ARGは会ってない日数
;何日以上会ってなかったら愚痴を言うとかできます
;CFLAG:オナバレで分岐入れてもよさそう
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL

;==================================================
;メディスン特殊イベント枠
;==================================================

	;禁断のポワゾン
	IF (TALENT:恋慕 && TALENT:恋人) ||  (TALENT:恋慕 || TALENT:恋人) && CFLAG:75:好感度 >= 10000 && CFLAG:75:信頼度 >= 5000 && EXP:75:愛情経験 >= 1000
		CALL K75_POISON_XXX
	ENDIF

	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;今日会っていない
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG
		IF !TALENT:75:恋慕 || !TALENT:75:恋人
			IF ARG == 30
				CALL PRINT_FACE, [[メディスン]], "通常", "服", "", ""
				PRINTFORML 「………………」
				PRINTW 
			ELSEIF ARG == 20
				CALL PRINT_FACE, [[メディスン]], "発情", "裸", "", ""
				PRINTFORML （%CALLNAME:MASTER%…きっと、私のことなんて忘れちゃったのね）
				PRINTFORML （やっぱり、人間なんて信じられない）
				PRINTW 
			ELSEIF ARG == 10
				CALL PRINT_FACE, [[メディスン]], "通常", "裸", "", ""
				PRINTFORML （%CALLNAME:MASTER%、全然会いに来ない）
				PRINTFORML 「嫌な気持ちだね、スーさん」
				PRINTW 
			ELSEIF ARG == 5
				CALL PRINT_FACE, [[メディスン]], "怒り", "服", "", ""
				PRINTFORML 「あいつったら、もう何日も私に会いに来ないわ」
				PRINTFORML 「本当に失礼な奴よね、スーさん」
				PRINTW 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会っている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		RETURN 1
	ENDIF
ENDIF
RETURN 1

