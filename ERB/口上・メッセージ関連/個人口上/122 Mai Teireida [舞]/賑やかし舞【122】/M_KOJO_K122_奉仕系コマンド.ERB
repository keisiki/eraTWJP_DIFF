@M_KOJO_MESSAGE_COM_K122_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K122_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K122_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:狂気
			PRINTFORML 「ふふん、そういう事は僕に任せなよ！」
			PRINTFORMW 舞は情熱的な目つきで%CALLNAME:MASTER%に奉仕を始めた……。
		ELSE
			PRINTFORML 「うーん、どのくらいの強さで握ればいいの…？」
			PRINTFORMW 舞は困惑しながら、恐る恐る手を上下させている。
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		IF TALENT:狂気
			PRINTFORML 「それそれっ、頑張れ頑張れ♪　いつでも出していいからねー♪」
			PRINTFORMW 舞は積極的な奉仕を続けている……。
		ELSE
			PRINTFORML 「えへへ、上手にできてる？」
			PRINTFORMW 舞は少し恥ずかしそうにしながらも、にこにこと笑顔で手を動かしている。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

