@M_KOJO_MESSAGE_COM_K122_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:狂気
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「そう、そんな感じで……ふふっ、%CALLNAME:MASTER%も上手だねー%UNICODE(0x2764)%」
			PRINTFORMW 舞は%CALLNAME:MASTER%の愛撫に恍惚とした笑みを浮かべている。
		CASE 1
			PRINTFORML 「君の好きにしていいんだよ？ほら%UNICODE(0x2764)%」
			PRINTFORMW 舞は少し体を動かして、%CALLNAME:MASTER%が触りやすいようにしてくれた。
		ENDSELECT
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「%CALLNAME:MASTER%に触られると、すごくドキドキする……%UNICODE(0x2764)%」
			PRINTFORMW 舞は息を荒げながら、%CALLNAME:MASTER%から与えられる快楽を享受している。
		CASE 1
			PRINTFORML 「うっ、んんっ%UNICODE(0x2764)% %CALLNAME:MASTER%、気持ちいい…%UNICODE(0x2764)%」
			PRINTFORMW 舞は%CALLNAME:MASTER%の愛撫に恍惚とした笑みを浮かべている。
		ENDSELECT
	RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「ん……ふふ、悪くはないかな…%UNICODE(0x2764)%」
			PRINTFORMW 舞は%CALLNAME:MASTER%の愛撫に満足気な笑みを浮かべた。
		CASE 1
			PRINTFORML 「もっと、していいよ？%UNICODE(0x2764)%」
			PRINTFORMW 舞は%CALLNAME:MASTER%を誘うように身体を密着させた。
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
