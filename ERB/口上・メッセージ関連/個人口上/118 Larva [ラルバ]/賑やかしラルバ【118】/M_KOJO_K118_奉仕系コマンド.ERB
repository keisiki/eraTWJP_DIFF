;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K118_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K118_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K118_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「こ、こういう遊びをするの……？ 誰かに見られてない……？」
		PRINTFORML 「……で、でもこれ、ちょっと私の角に似てるかも………」
		PRINTFORMW ラルバは意を決すると、おそるおそる%CALLNAME:MASTER%の肉棒に手を触れた……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMW %CALLNAME:MASTER%は動かないラルバの腋に気の済むまで肉棒を擦り付けた……。
		RETURN 1
	;それ以外
	ELSE
		IF NOWEX:PLAYER:射精
			PRINTFORML 「%CALLNAME:MASTER%、きもちいの……？」
			PRINTFORMW ラルバは不意に手淫のペースを上げた……。
		ELSE
			PRINTFORML 「これ、なんだか握ってると安心する……」
			PRINTFORMW ラルバは耳を赤くしながら手淫を続けている……。
		ENDIF
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,フェラチオ
;==================================================
@M_KOJO_MESSAGE_COM_K118_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K118_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K118_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「こ、これを舐める遊び……？ 私の角よりもヘンなにおい……」
		PRINTFORMW ラルバは戸惑いながらもチロチロと亀頭を舐め始めた……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORML 物言わぬラルバの口内に肉棒を突き入れた……。
		PRINTFORMW 小さな歯がこりこりと気持ち良い……。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「ちゅるちゅる…… なんらかこれ、蜜すってるみらい……」
		PRINTFORMW ラルバは精子管に吸い付くようにフェラチオを続けている……。
		IF NOWEX:PLAYER:射精
				PRINTFORML 「！！…………んん……%UNICODE(0x2764)%」
				PRINTFORMW ラルバは精液の苦い味が好みのようだ……。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

