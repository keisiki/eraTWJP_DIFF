;特殊コマンド
;=================================================
;-------------------------------------------------
;180,ローション
;-------------------------------------------------
@M_KOJO_MESSAGE_COM_K44_180
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K44_180_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K44_180_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL

	;CFLAG:1500 == 十進数
	;0.イベントがオフだよ　選択肢が出るよ　　初期値の事情だよ
	;1.イベントがオンだよ　選択肢が出るよ
	;2.イベントがオフだよ　選択肢を出さないよ
	;3.イベントがオンだよ　選択肢を出さないよ
	;4.イベントがオフだよ　選択肢が出るよ　　bit2が真だよ
	
	;1=オン（bit0）　2=選択肢を出さないよ（bit1）
	;4=簡単に条件を付け足せるよ（bit2）
	
	;GETBIT (CFLAG:1500,1)　ここの1は二進数の二桁目の事　間違いやすいよ
	;(CFLAG:1500,1)の数値真(0以外主に1)偽(0)を見るよ
	
	;SETBIT STAIN:1,7　二進数の八桁目を真(主に1)にするよ
	;十進数で言えば　+128されていない場合+128するよ
	
	;CLEARBIT STAIN:1,7　二進数の八桁目を偽(0)にするよ
	;十進数で言えば　既に+128されている場合に限り-128するよ
	
	;INVATEBIT STAIN:1,7　二進数の八桁目を反転させるよ
	;十進数で言えば　+128されていない場合+128　既に+128されている場合-128するよ


	;時間停止フラグがかつ参照する事のbit1(選択肢を出さないよ)が偽なら選択肢が出る
	SIF !FLAG:70 && !GETBIT(CFLAG:1500,1)
		CALL M_KOJO_MESSAGE_COM_K44_180_選択肢

	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:時間停止
			;時間稼働中に始動している場合の矛盾解消
			IF GETBIT(STAIN:1,7)
				PRINTFORML 時間稼働中に始動している場合の矛盾解消　初
				RETURN 0
			;それ以外　時間停止ローション
			;時間停止塗り塗り分岐は無い　きっとそれが正義
			ELSE
				PRINTFORML 時間停止ローション　初
				RETURN 0
			ENDIF

		;以降!FLAG:時間停止　「発言」は概ねここから

		;分岐条件　チョコ塗れの手ペロペロ分岐
		;足の汚れは無いので手を手足として代用
		;別の形でチョコを塗った時もここに来るので一応イベントフラグも条件に追加
		ELSEIF GETBIT(STAIN:1,7) && GETBIT(CFLAG:1500,0)
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 恋慕ペロペロ　初
				;お手々のチョコ舐め取ったよ
				;CLEARBIT STAIN:1,7
				RETURN 0
			;それ以外　ゲームバランス次第で恋慕無し恋人有りなんてケースも有り得る
			ELSE
				PRINTFORML ペロペロ　初
				RETURN 0
			ENDIF
		;分岐条件　CFLAG:1500のbit0の値これ真ならイベント発生　塗り塗り分岐
		ELSEIF GETBIT(CFLAG:1500,0)
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 恋慕塗り塗り　初
				;お手々べとべと
				SETBIT STAIN:1,7
				RETURN 0
			;それ以外　ゲームバランス次第で恋慕無し恋人有りなんてケースも有り得る
			ELSE
				PRINTFORML 塗り塗り　初
				;お手々べとべと
				SETBIT STAIN:1,7
				RETURN 0
			ENDIF
		;それ以外　ローション分岐
		ELSE
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 恋慕ローション分岐　初
				RETURN 0
			;それ以外　ゲームバランス次第で恋慕無し恋人有りなんてケースも有り得る
			ELSE
				PRINTFORML それ以外ローション分岐　初
				RETURN 0
			ENDIF
		ENDIF

	;以降!FIRSTTIME　時間停止中に初回を迎えた場合との兼ね合いに注意
	ELSE
		IF FLAG:時間停止
			;時間稼働中に始動している場合の矛盾解消
			IF GETBIT(STAIN:1,7)
				PRINTFORML 時間稼働中に始動している場合の矛盾解消
				RETURN 0
			;それ以外　時間停止ローション
			ELSE
				PRINTFORML 時間停止ローション
				RETURN 0
			ENDIF

		;以降!FLAG:時間停止　「発言」は概ねここから

		;分岐条件　チョコ塗れの手ペロペロ分岐
		;足の汚れは無いので手を手足として代用
		;別の形でチョコを塗った時もここに来るので一応イベントフラグも条件に追加
		ELSEIF GETBIT(STAIN:1,7) && GETBIT(CFLAG:1500,0)
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 恋慕ペロペロ
				;お手々のチョコ舐め取ったよ
				;CLEARBIT STAIN:1,7
				RETURN 0
			;それ以外　ゲームバランス次第で恋慕無し恋人有りなんてケースも有り得る
			ELSE
				PRINTFORML ペロペロ
				RETURN 0
			ENDIF
		;分岐条件　CFLAG:1500のbit0の値これ真ならイベント発生　塗り塗り分岐
		ELSEIF GETBIT(CFLAG:1500,0)
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 恋慕塗り塗り
				;お手々べとべと
				SETBIT STAIN:1,7
				RETURN 0
			;それ以外　ゲームバランス次第で恋慕無し恋人有りなんてケースも有り得る
			ELSE
				PRINTFORML 塗り塗り
				;お手々べとべと
				SETBIT STAIN:1,7
				RETURN 0
			ENDIF
		;それ以外　ローション分岐
		ELSE
			;ここに恋慕有無を問わない描写も書けるよ
			;PRINTFORML 
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 恋慕ローション分岐
				RETURN 0
			;それ以外　ゲームバランス次第で恋慕無し恋人有りなんてケースも有り得る
			ELSE
				PRINTFORML ローション分岐
				RETURN 0
			ENDIF
			;ここに恋慕有無を問わない描写を書いても二行上六行上のRETURN 0で引き返しているから表示されないよ
			;PRINTFORML 
		ENDIF
	ENDIF
ENDIF
;リターンしないとそのまま次まで突っ切って行っちゃうよ
;1は7行目のCALL M_KOJO_MESSAGE_COM_K44_180_1に送られ8行目のRETURN RESULTに引き渡されるよ
RETURN 1
