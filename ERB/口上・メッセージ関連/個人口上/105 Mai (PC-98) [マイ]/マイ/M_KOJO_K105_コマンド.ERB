;==================================================
;181,媚薬,
;==================================================
@M_KOJO_MESSAGE_COM_K105_181
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K105_181_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K105_181_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;こういうのが好き、というのがあるんだけど上手いこと書けてる気がまったくしない
IF LOCAL
	IF TCVAR:泥酔 == 1
		PRINTFORML 
	ELSEIF CFLAG:うふふ
		IF FIRSTTIME("うふふ中媚薬")
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「媚薬ねぇ……探せば色々出てきそうではあるけど、ほんとに効果あるの？」
				PRINTFORML 「せいぜい、血行が良くなるとかその程度じゃないかな」
				PRINTFORMW 「効かなくっても、わたしのせいじゃないからね」
				PRINTFORML 
				PRINTFORML 「ん……」
				PRINTFORMW （ま、夢を壊しても悪いし……乗ってあげようかな）
				PRINTFORMW （ほんとに効いたら、面白いし）
				PRINTFORMW （案外、プラセボ効果で……）
				PRINTFORML 
				PRINTFORMW 「……あ、あれ？　なんか、気分が……ぁ」
				PRINTFORMW 「%CALLNAME:MASTER%……この、薬」
				PRINTFORMW 「あ、あぁ……%UNICODE(0x2665) *1%　これぇ……多分、効くわ……%UNICODE(0x2665) *1%」
				PRINTFORML 
				PRINTFORMW 「とりあえず……続き、しよ……%UNICODE(0x2665) *1%」
				CALL RECOVER(TARGET,300,"気力")
				TCVAR:105:351 += 1
			ELSE
				PRINTFORML 「媚薬……？」
				PRINTFORMW 「……そう、そんなものが」
				PRINTFORMW 「たぶん、効かないと」
				PRINTFORMW 「思う……」
				PRINTFORMW 「け、ど……？」
				PRINTFORML 
				PRINTFORML 「っ……こ、これぇ……まさか」
				PRINTFORMW 「あああぁ……こんな、に……%UNICODE(0x2665) *1%」
				CALL RECOVER(TARGET,300,"気力")
				TCVAR:105:351 += 1
			ENDIF
		ELSE
			IF TALENT:恋人 || TALENT:恋慕
				SELECTCASE TCVAR:105:351
				CASE 0
					PRINTFORML 「う、媚薬かぁ……間違いなく、気持ち良いと思うんだけど」
					PRINTFORMW 「記憶が曖昧で……わたし、変なことしてないよね？」
					PRINTFORMW 「まぁいいわ、とりあえず……ん……」
					PRINTFORML 
					PRINTFORMW 「……ふぅっ、効いてきたぁ……%UNICODE(0x2665) *1%」
					PRINTFORMW 「じゃ、続きね%UNICODE(0x2665) *1%」
					CALL RECOVER(TARGET,300,"気力")
					TCVAR:105:351 += 1
				CASE 1
					IF CFLAG:105:1002 == 0
						PRINTFORML 「ふふ、薬だぁ……%UNICODE(0x2665) *1%　もう一本、イっちゃう？」
						PRINTFORML 「……ん……%UNICODE(0x2665) *1%」
						PRINTFORML 
						PRINTFORMW 「あああぁ、効く……%UNICODE(0x2665) *1%　%CALLNAME:MASTER%……早く、続きしよ……%UNICODE(0x2665) *1%」
						CALL RECOVER(TARGET,200,"気力")
						TCVAR:105:351 += 1
					ELSEIF CFLAG:105:1002 == 1
						PRINTFORML 「薬っ、足りないぃ……%UNICODE(0x2665) *1%　ああ、もう……まどろっこしいわ」
						PRINTFORML 「わたしも持ってきたの……%UNICODE(0x2665) *1%　%CALLNAME:MASTER%の媚薬と……飲み合わせの、悪そうなやつ%UNICODE(0x2665) *1%」
						PRINTFORMW 「さぁっ、実験してみよ……ふふ、ふふふ……%UNICODE(0x2665) *1%」
						PRINTFORMDW マイは懐から小さな瓶を取り出し、中身を一気に飲み干した……
						PRINTFORML 
						TCVAR:105:351 += 3
						CFLAG:105:1002 = 2
						SETCOLOR 0xFF99EE
						PRINTFORMW 「あ、あ、あ……来、たぁぁ%UNICODE(0x2665) *1%」
						PRINTFORMW 「こっ、れぇ……まえのやつ……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						PRINTFORMW 「あぇへへ……%CALLNAME:MASTER%……せっくすぅ、つづきしよ……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						CALL RECOVER(TARGET,200,"気力")
					ELSEIF CFLAG:105:1002 == 2
						PRINTFORML 「お薬、まだ追加しちゃう？　じゃ、今日もそろそろ飛ばしていこっか%UNICODE(0x2665) *1%」
						PRINTFORML 「ちゃーんと、改良してるんだから……ばっちり効くわ……%UNICODE(0x2665) *1%」
						PRINTFORMW 「いや、改悪かな……ふふふふ、ふふ……%UNICODE(0x2665) *1%」
						PRINTFORMDW マイは懐から小さな瓶を取り出し、中身を一気に飲み干した……
						PRINTFORML 
						TCVAR:105:351 += 3
						SETCOLOR 0xFF99EE
						PRINTFORML 
						PRINTFORMW 「……あ゛ぁ、来た、来たぁ……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						CALL RECOVER(TARGET,200,"気力")
					ELSE
						PRINTFORML
						TCVAR:105:351 += 1
					ENDIF
				CASE 2
					PRINTFORML 「えぇ、また？　やばいよ、これ以上は……たぶん……」
					PRINTFORMW 「ちょっとだけ、こわくなって」
					PRINTFORMW 「でもぉ……いいかぁ、きもちいいし……んっ」
					PRINTFORML 
					SETCOLOR 0xFF99EE
					PRINTFORMW 「ん゛っ、あぁ……%UNICODE(0x2665) *1%　いい……%UNICODE(0x2665) *1%　まだ、まだいける%UNICODE(0x2665) *1%」
					PRINTFORMW 「もっ、と……もっと、媚薬、ぅ……」
					SETCOLOR 0xFFDDEE
					PRINTFORML 
					PRINTFORMW 「っ……ふ、ふふふふふ……%UNICODE(0x2665) *1%」
					PRINTFORMDW マイの様子がおかしい……これ以上媚薬は飲ませないほうが良いだろう。
					CALL RECOVER(TARGET,200,"気力")
					TCVAR:105:351 += 1
				CASE 3
					IF FIRSTTIME("媚薬OD")
						PRINTFORML 「くすりぃ……まだ、あるの？　じゃあ、もっかい……%UNICODE(0x2665) *1%」
						PRINTFORMW 「これぇ……あはは……飲めば、飲むだけ、効、く」
						PRINTFORML 
						TCVAR:105:351 += 1
						CFLAG:105:1002 = 1
						SETCOLOR 0xFF99EE
						PRINTFORMW 「ぁ、れ……これ、やばっ、あ゛……っ」
						PRINTFORMW 「んお゛ぉ……なに、これぇ%UNICODE(0x2665) *1%　あたま、おかしくなる……ぅ%UNICODE(0x2665) *1%」
						PRINTFORMW 「ぎもちいぃ゛……%CALLNAME:MASTER%……あぇ、ぅふふ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					ELSE
						PRINTDATAW
							DATAFORM 「ああぁ、これいじょうはぁ……し、しんじゃう……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
							DATAFORM 「ま、だ……？　まだ、きもちよくぅ、なれるの……じゃ、じゃあ……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
							DATAFORM 「あはぁぁ、またっ……%UNICODE(0x2665) *1%　これぇ……もうっ、やめられないぃ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						ENDDATA
						CALL RECOVER(TARGET,100,"気力")
						TCVAR:105:351 += 1
					ENDIF
				CASE IS >= 4
					PRINTDATAW
						DATAFORM 「ああぁ、これいじょうはぁ……し、しんじゃう……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						DATAFORM 「ま、だ……？　まだ、きもちよくぅ、なれるの……じゃ、じゃあ……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						DATAFORM 「あはぁぁ、またっ……%UNICODE(0x2665) *1%　これぇ……もうっ、やめられないぃ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					ENDDATA
					CALL RECOVER(TARGET,100,"気力")
					TCVAR:105:351 += 1
				CASEELSE
					PRINTFORML 
				ENDSELECT
				RETURN 1
			ELSE
				SELECTCASE TCVAR:105:351
					CASE 0
						PRINTFORML 「あぁ、媚薬ね……」
						PRINTFORMW 「……まぁ、少しなら……気持ち良いし……」
						TCVAR:105:351 += 1
					CASEELSE
						PRINTFORML 「だっ、ダメ……これ以上、は……」
						PRINTFORML 「なにか……まずい気がするの……」
						PRINTFORMDW マイは媚薬の殆どを吐き出してしまった……
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ELSE
		PRINTFORML 
	ENDIF
ENDIF
RETURN 1


