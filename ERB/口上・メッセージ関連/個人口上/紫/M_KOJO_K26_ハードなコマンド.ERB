;==================================================
;150,胸の谷間を犯す
;==================================================
@M_KOJO_MESSAGE_COM_K26_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K26_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K26_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「あら？そんなにペニスを突き出すだなんて私の胸はそんなに魅力的かしら%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;胸3以上
	ELSEIF ABL:胸 > 2
		IF RAND:3 == 0
			PRINTFORMW 「なんだか胸を犯されていると興奮してくるわね%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んっ%UNICODE(0x2665) *1%駄目よ、胸がチンカス臭くなってしまうわ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%は私よりも私のおっぱいが好きなのね%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん……私の胸にペニスを擦り付けるだなんて見る目があるわね」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「早く射精しないと、私の胸が解放されないわ」
		ELSE
			PRINTFORMW 「なかなか立派でしょう？私の胸も%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
ENDIF
RETURN 1
