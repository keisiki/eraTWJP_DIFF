			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%んほぉっ%UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:三重絶頂
			;Ｃ＆Ｖ＆Ａ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ｍ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｖ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;二重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:二重絶頂
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で紫は絶頂した。
			ELSEIF RAND:5 == 0
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%駄目っ%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「んおっ%UNICODE(0x2665) *1%あっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「おおっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んあっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%」
			ELSE
