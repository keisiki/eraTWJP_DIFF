			ELSEIF RAND:4 == 0
				PRINTFORMW 「あんっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
