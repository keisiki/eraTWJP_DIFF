			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:二重絶頂
			;Ｃ＆Ｖ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｂ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ絶頂
		;時姦中
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で紫は絶頂した。
		ELSEIF RAND:5 == 0
			PRINTFORMW 「んんっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「駄目っ%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「んあっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「そこっ%UNICODE(0x2665) *1%弱いのっ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「やぁっ%UNICODE(0x2665) *1%駄目っ%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:Ｖ絶頂
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で紫は絶頂した。
		ELSEIF TEQUIP:50 == MASTER && TEQUIP:MASTER:コンドーム == 0
			IF RAND:5 == 0
				PRINTFORMW 「生ハメちんぽっ%UNICODE(0x2665) *1%好きぃっ%UNICODE(0x2665)*1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%生ちんぽっ%UNICODE(0x2665) *1%頂戴っ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んほっ%UNICODE(0x2665) *1%あﾞっ%UNICODE(0x2665) *1%いぐぅ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「生ハメっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%もっとっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「ふぁっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
				PRINTFORMW 「……生は気持ちいいわね……%UNICODE(0x2665) *1%」
			ENDIF
		ELSEIF TEQUIP:50 == MASTER
			IF RAND:5 == 0
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%っ%UNICODE(0x2665)*1%駄っ%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665) *1%待ってぇっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んほっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%ちんぽ頂戴っ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「っ%UNICODE(0x2665) *1%んおっ%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「ぁっ%UNICODE(0x2665) *1%んあっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:5 == 0
				PRINTFORMW 「そこっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665)*1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%もっとっ%UNICODE(0x2665) *1%っ%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%おﾞっ%UNICODE(0x2665) *1%イグっ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%あっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んっ%UNICODE(0x2665) *1%んんっ%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	ENDIF
	IF NOWEX:Ａ絶頂
		;時姦中
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で紫は絶頂した。
		ELSEIF RAND:5 == 0
			PRINTFORMW 「んんっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「アナルっ%UNICODE(0x2665) *1%弱いのにっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「アナル弱いのっ%UNICODE(0x2665) *1%優しくしてっ%UNICODE(0x2665) *1%」
