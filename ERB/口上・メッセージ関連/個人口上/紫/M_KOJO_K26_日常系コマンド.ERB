;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怒り
	IF CFLAG:318 == 1
		PRINTFORMW 「よくもまぁ人の中にずけずけと入ってくるわね」
		PRINTFORMW 紫は怒気をはらんだ声で返事した。
	;会話不能
	ELSEIF TCVAR:302
		PRINTFORMW 「………」
		PRINTFORMW 紫は押し黙ってしまった。
	ELSEIF TFLAG:193 == -2
		IF RAND:5 == 0
			PRINTFORMW 「もう少し教養をつけてくださるかしら？」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「妖精と話した方が楽しいかもしれないわ」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「つまらないわ、いやどん詰まりね」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「私が勉強を見てあげましょうか？」
		ELSE
			PRINTFORMW 「悪いけど、もう少し賢くなって頂戴」
		ENDIF
	;ミニマップ時のみ
	;条件をメインマップ時にできなくもないけど面倒
	;現在位置が墓
	ELSEIF CFLAG:MASTER:現在位置 == 150
		IF RAND:5 == 0
			PRINTFORMW 「墓荒らしの真似事なんて、一人で十分よね」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「ここからも冥界に繋がるのかしら？」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「何か視えるかしら？私には色々見えるけど」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「葬式不要戒名不要、ではここの僧侶に嫌われるかもね」
		ELSE
			PRINTFORMW 「あまり長居しても趣味が悪いわね」
		ENDIF
	;現在位置が竹林入り口
	ELSEIF CFLAG:MASTER:現在位置 == 410
