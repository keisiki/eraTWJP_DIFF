;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K26_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K26_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K26_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「まるでウロボロスね、永遠に陰と陽を繰り返すのよ」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「ん～～%UNICODE(0x2665) *1%」
				PRINTFORMW 紫は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「早くチンカスザーメンを飲ませなさい%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……%UNICODE(0x2665) *1%ちゅっ%UNICODE(0x2665) *1%……もう出そうなの？ザーメンの匂いが奥から漂ってくるわ%UNICODE(0x2665) *1%」
				PRINTFORMW 紫は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%」
				PRINTFORMW 紫は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「今にもザーメンをぶちまけたいってペニスが言っているわ……%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「ん～」
				PRINTFORMW 紫は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「早く飲ませて%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……もう少しね」
				PRINTFORMW 紫は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっむせ返るほどの雄の味がしてきたわ%UNICODE(0x2665) *1%」
				PRINTFORMW 紫は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「ん……%UNICODE(0x2665) *1%れろっ%UNICODE(0x2665) *1%」
				PRINTFORMW 「チンカス臭くて好きになっちゃいそう……%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「……もっとペニスの味を刷り込んでっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%れろっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「んん～」
				PRINTFORMW 「慣れてきたわ、舌だけで射精させるのにね%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……こうかしら%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んんっれろっ……」
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん……れろっ……%UNICODE(0x2665) *1%」
			PRINTFORMW 「あまりペニスを舐めたことが無いから、下手なのは我慢しなさい」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……れろっ……んっ……」
		ELSE
			PRINTFORMW 「……んっ……ちゅっ……」
			PRINTFORMW 紫はどこかぎこちなく陰茎を口に含んだ。
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K26_{SELECTCOM}_1
RETURN 1

