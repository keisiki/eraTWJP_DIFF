;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
CALL M_KOJO_COLOR_K134
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「んHPH」","L"
		CALL HPH_PRINT,@"「……美宵とのキス、忘れないでね……HPH」","L"
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「んっHPH」","L"
			CALL HPH_PRINT,@"「もっと……くださいHPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「んHPH」","L"
		ELSE
			CALL HPH_PRINT,@"「んっHPH」","L"
			CALL HPH_PRINT,@"「キスだけじゃ物足りないですHPH」","L"
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「んHPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「んんHPHもっと……HPH」","L"
		ELSE
			CALL HPH_PRINT,@"「んんっ……HPH」","L"
		ENDIF
	ENDIF
ENDIF
RETURN 1


