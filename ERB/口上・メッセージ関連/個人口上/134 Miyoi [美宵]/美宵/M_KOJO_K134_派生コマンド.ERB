;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K134_1
CALL M_KOJO_MESSAGE_SCOM_K134_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K134_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K134
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		CALL HPH_PRINT,@"「どっちが先にイかせられるか勝負しましょうHPH」","L"
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「ん？んんん～HPH」","L"
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				CALL HPH_PRINT,@"（もう少しで美味しいぷりぷりザーメンいっぱい出てきそうね……HPH）","L"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「んHPHんちゅっHPH」","L"
				CALL HPH_PRINT,@"「我慢できないぷりぷりのおちんぽ汁、美宵の口の中にいっぱい射精してくださいHPH」","L"
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				CALL HPH_PRINT,@"「んんっHPHおちんちんどんどん固くなってきましたねHPH」","L"
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				CALL HPH_PRINT,@"（濃厚なおちんちん汁早く飲みたいなぁHPH）","L"
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「んん」
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				CALL HPH_PRINT,@"「もう射精しちゃうぐらいカチカチのおちんちんになってきましたねHPH」","L"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「んんHPHんむっHPH」","L"
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				CALL HPH_PRINT,@"「んんっHPHもうでちゃいそうなんですかぁHPH」","L"
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「んんHPHんんんHPH」","L"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「んんHPHHPHHPH」","L"
				CALL HPH_PRINT,@"「大きくて固くて逞しいおちんちんですねぇ咥えるのも大変ですよぉHPH」","L"
			ELSE
				CALL HPH_PRINT,@"「んちゅHPHれろっHPH」","L"
			ENDIF
		ELSE
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「ん～HPH」","L"
				CALL HPH_PRINT,@"「美宵のお口は気持ちいいですかぁHPHもっと気持ちよくしてあげますからねぇHPH」","L"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「んんHPH美宵に任せてくださいねHPH」","L"
			ELSE
				CALL HPH_PRINT,@"「んんっHPHおちんちんさんいっぱい気持ちよくしてあげますからねぇHPH」","L"
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「んHPHちゅるHPH」","L"
			CALL HPH_PRINT,@"「こんなにおちんちんビクビクさせて可愛いですねぇHPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「れろっHPHんんHPH」","L"
		ELSE
			CALL HPH_PRINT,@"「んん……HPH」","L"
			PRINTFORMW 美宵はどこかぎこちなく陰茎を口に含んだ。
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K134_{SELECTCOM}_1
RETURN 1

