;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K134_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K134_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K134_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「どっちが先にイかせられるか勝負しましょう%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「ん？んんん～%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW （もう少しで美味しいぷりぷりザーメンいっぱい出てきそうね……%UNICODE(0x2665) *1%）
			ELSEIF RAND:2 == 0
				PRINTFORMW 「ん%UNICODE(0x2665) *1%んちゅっ%UNICODE(0x2665) *1%」
				PRINTFORMW 「我慢できないぷりぷりのおちんぽ汁、美宵の口の中にいっぱい射精してください%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%おちんちんどんどん固くなってきましたね%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW （濃厚なおちんちん汁早く飲みたいなぁ%UNICODE(0x2665) *1%）
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「んん」
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「もう射精しちゃうぐらいカチカチのおちんちんになってきましたね%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん%UNICODE(0x2665) *1%んむっ%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%もうでちゃいそうなんですかぁ%UNICODE(0x2665) *1%」
				PRINTFORMW 美宵は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「んん%UNICODE(0x2665) *1%んんん%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「大きくて固くて逞しいおちんちんですねぇ咥えるのも大変ですよぉ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んちゅ%UNICODE(0x2665) *1%れろっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「ん～%UNICODE(0x2665) *1%」
				PRINTFORMW 「美宵のお口は気持ちいいですかぁ%UNICODE(0x2665) *1%もっと気持ちよくしてあげますからねぇ%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん%UNICODE(0x2665) *1%美宵に任せてくださいね%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%おちんちんさんいっぱい気持ちよくしてあげますからねぇ%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん%UNICODE(0x2665) *1%ちゅる%UNICODE(0x2665) *1%」
			PRINTFORMW 「こんなにおちんちんビクビクさせて可愛いですねぇ%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「れろっ%UNICODE(0x2665) *1%んん%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「んん……%UNICODE(0x2665) *1%」
			PRINTFORMW 美宵はどこかぎこちなく陰茎を口に含んだ。
		ENDIF
	ENDIF
ENDIF
