				CALL HPH_PRINT,@"「明日は会えるかなぁ……%CALLNAME:MASTER%さんHPH」","L"
				PRINTFORML 美宵は一人でそう呟いた。
			ELSEIF RAND:2 == 0
				PRINTFORML （%CALLNAME:MASTER%さんは何をしてるんだろう）
				PRINTFORML 美宵は一人でそう呟いた。
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%さん……寂しいよぅ……」
				PRINTFORML 美宵は一人でそう呟いた。
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会っている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		RETURN 1
	ENDIF
	IF CFLAG:134:1002 == 1
		CFLAG:134:1002 = 0
	ENDIF
	IF CFLAG:134:1003 == 1
		CFLAG:134:1003 = 0
	ENDIF
ENDIF
RETURN 1

