;==================================================
;150,胸の谷間を犯す
;==================================================
@M_KOJO_MESSAGE_COM_K134_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K134_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K134
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「ちょっと、いきなり突っ込むのは良くないですよHPH」","L"
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「おっぱいにいっぱい中出ししてくださいねぇHPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「おっぱいオナホに興奮しちゃったんですかHPHどんどんおっきくなってますよぉHPH」","L"
		ELSE
			CALL HPH_PRINT,@"「もう！乱暴すぎますよHPH」","L"
		ENDIF
	;胸3以上
	ELSEIF ABL:胸 > 2
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「美宵のおっぱいで気持ちよくなってくださいねぇHPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「んHPHおっぱい気持ちいいですかぁHPH」","L"
		ELSE
			CALL HPH_PRINT,@"「いっぱい射精してくださいねHPH」","L"
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「んんHPH気持ちいいですかぁHPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「いっぱいおっぱい犯してくださいねHPH」","L"
		ELSE
			CALL HPH_PRINT,@"「あんまり乱暴にしないでくださいねHPH」","L"
		ENDIF
	ENDIF
ENDIF
RETURN 1
