;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K134_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K134_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K134
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「ん！んん～～～！」
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「んんHPH」","L"
			PRINTFORMW 美宵は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「んHPHんHPH」","L"
			PRINTFORMW 美宵は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ELSE
			CALL HPH_PRINT,@"「んんっHPHんんHPH」","L"
			PRINTFORMW 美宵は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「んん～！」
			PRINTFORMW 美宵は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んんん～～～！！！」
			PRINTFORMW 美宵は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ELSE
			PRINTFORMW 「んん！！！」
			PRINTFORMW 美宵は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ENDIF
	ENDIF
ENDIF
RETURN 1

