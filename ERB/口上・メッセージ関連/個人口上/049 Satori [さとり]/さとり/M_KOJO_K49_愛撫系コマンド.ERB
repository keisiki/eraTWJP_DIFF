CALL M_KOJO_MESSAGE_COM_K49_0_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			IF !FLAG:70 && (TALENT:2 & 3) == 3
				CALL 渧泣()
			ELSEIF !FLAG:70
				CALL 渧泣()
			ENDIF
			PRINTFORMW 
			RETURN 1
		ELSE
			IF !FLAG:70 && (TALENT:2 & 3) == 3
				CALL 渧泣()
			ELSEIF !FLAG:70
				CALL 渧泣()
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			IF !FLAG:70 && (TALENT:2 & 3) == 3
				CALL 渧泣()
			ELSEIF !FLAG:70
				CALL 渧泣()
			ENDIF
			PRINTFORMW 
			RETURN 1
		ELSE
			IF !FLAG:70 && (TALENT:2 & 3) == 3
				CALL 渧泣()
			ELSEIF !FLAG:70
				CALL 渧泣()
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		IF !FLAG:70
			CALL 渧泣()
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		IF !FLAG:70
			CALL 渧泣()
		ENDIF
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF !FLAG:70
			CALL 渧泣()
		ENDIF
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		IF !FLAG:70
			CALL 渧泣()
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1
