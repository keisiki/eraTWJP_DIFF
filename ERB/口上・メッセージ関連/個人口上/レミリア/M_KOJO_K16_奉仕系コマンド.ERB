	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;技巧不足
	;技量が低いときにこそ[無知]が活きる気がします
	;ELSEIF ABL:舌 < 2
	
	;勃起しきってない
	ELSEIF BASE:MASTER:勃起 < 1000
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「…何かしら、これは」
				PRINTFORML 「私がこうして舐めているのだから、硬くできるはずでしょう？」
				PRINTFORMW 「あむ…んぅ…そう、それでいいのよ…\@ TALENT:恋慕 ? %UNICODE(0x2764)% # \@」
			CASE 1
				PRINTFORML 「…この私が奉仕してあげているのよ？それに見合った反応があるべきだわ」
				PRINTFORMW レミリアは上目遣いに%CALLNAME:MASTER%をねめつけながら逞しい怒張に舌を這わせている
		ENDSELECT
	;恋慕
	ELSEIF TALENT:恋慕 && RAND:2
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「んん、ちゅる、くぷっ…くちゅる、ぅ…」
				PRINTFORML レミリアは%CALLNAME:MASTER%の先端を小さな口内に迎え入れて舐めまわしている
				PRINTFORML 唇の端から唾液が溢れるのにも構わず一心に奉仕を続ける様は如何にも健気で愛おしく
				PRINTFORML 否応なしに快感を高められてしまう
				PRINTFORML 「…むぐ、ちゅ、はぁ…本当に、硬いものね…」
				PRINTFORMW 「まあ、貴方にとって私がそれだけ魅力的だということなら…吝かではないわ…%UNICODE(0x2764)%」
			CASE 1
				PRINTFORML 「………」
				PRINTFORML 突然レミリアの動きが止まったかと思うと、亀頭に何か硬いものをあてがわれる
				PRINTFORML 「…これは、獲物の喉笛に食らいつき血を啜る吸血鬼の牙」
				PRINTFORMW 「あなたは自分がどれほど恐ろしい事をしているか、自覚しているのかしら」
				PRINTFORML 
				PRINTFORML 「……ちょっとした冗談よ」
				PRINTFORML レミリアはくすりと笑って口奉仕を再開する
				PRINTFORML 鋭利な、それでいてどこか優しい感触が雁首の縁へ時折引っかかり、ゾクゾクとした性感が湧きあがる
				PRINTFORMW 「くちゅ、ぐぷっ、ちゅる…牙を抜かれたような気分とは、こういうことを言うのかしらね」
			CASE 2
				PRINTFORML 「ぁむ、ちゅ…くぷっ…ひもふぃい…」
				PRINTFORML 「…ぷふっ、こほん…気持ちいいかしら？」
				PRINTFORML こんな時にも体面を取り繕おうとするレミリアが少し可笑しかったが
				PRINTFORML %CALLNAME:MASTER%はおくびにも出さず彼女の頭を撫でてあげた
				PRINTFORMW 「……うー…」
		ENDSELECT
		RETURN 1
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「%TEXTR("相変わらず、/ふぅ…/")%すごいニオイ…」
				;精液中毒高い
				IF ABL:精液中毒 > 4 && RAND:2
					PRINTFORML 「私を、んん…%UNICODE(0x2764)%こんなモノに…奉仕させる、なんて…んむ、ちゅ…%UNICODE(0x2764)%不敬、だわ…%UNICODE(0x2764)%」
					PRINTFORMW レミリアはどこか嬉しそうに%CALLNAME:MASTER%の陰茎を舐めしゃぶっては、内腿を擦り合わせている…
				ELSEIF TALENT:恋慕 && RAND:2
					PRINTFORML 「それでも、あなたの一部だと思えばどこか可愛らしく感じられさえする」
					PRINTFORMW 「不思議なものね…」
				ELSE
					PRINTFORML 「こんなモノを舐めさせるなんて、酷い人ね…ん、ちゅ…」
					PRINTFORML レミリアは悪態をつきつつも先端を咥え、舌全体をにゅるにゅると絡ませ快楽を与えてくる
					PRINTFORML 笠のまわりや尿道といった弱点を柔らかな感触が撫で上げるたびに押し寄せる性感の波に、%CALLNAME:MASTER%は思わず呻き声を上げた
					PRINTFORMW 「まあ、あなたのそういう反応を見るのは楽しい\@ TALENT:恋慕 ? わ…%UNICODE(0x2764)% # かしらね \@」
				ENDIF
			CASE 1
				PRINTFORML 「んっ、ちゅ、ちゅく…ぷぁ…まったく、理解しがたい行為ね…」
				IF ABL:精液中毒 > 4 && RAND:2
					;精液中毒高い
					PRINTFORML 「こんなものに舌を這わせて…ん、れるっ…じっくりと舐め上げて…」
					PRINTFORML 「溢れてくる露を…んく…っ、呑み込ん、で…」
					PRINTFORML 「あなたの昂りを感じながら、ふっ、んん…%UNICODE(0x2764)%口の中で、たくさん…ちゅく…%UNICODE(0x2764)%いじめて…%UNICODE(0x2764)%」
					PRINTFORML 「最後は…%UNICODE(0x2764)%ちゅ、れぷっ…%UNICODE(0x2764)%そのまま…%UNICODE(0x2764)%」
					PRINTFORMW レミリアはうわごとのようにつぶやきながら、恍惚とした表情で口淫を続けている…
				ELSEIF TALENT:恋慕
					PRINTFORML 「あなたでなければ、噛み千切ってやっているところよ…は、ぁむ、れるれる…%UNICODE(0x2764)%」
					PRINTFORMW レミリアの真っ赤な舌がちろちろと踊り、%CALLNAME:MASTER%の快感を高めてくる
				ELSE
					PRINTFORML レミリアは少しむくれながら口奉仕を続けている…
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1

