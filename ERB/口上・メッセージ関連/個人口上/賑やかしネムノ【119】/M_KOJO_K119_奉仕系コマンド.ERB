;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K119_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K119_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K119_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF FLAG:70
		PRINTFORMW %CALLNAME:MASTER%は動かないネムノに気の済むまで肉棒を擦り付けた……。
		RETURN 1
	ENDIF
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「なんだ、ちんぽを擦ればいいのか？」
		PRINTFORML 「よくわからんけど、別にいいぞ。変な趣味だな。」
		PRINTFORMW ネムノは実にあっけらかんと%CALLNAME:MASTER%の肉棒を上下した……。
		RETURN 1
	ENDIF
	IF NOWEX:PLAYER:射精
		PRINTFORML 「なんだ、ぶるぶるしてるぞ。うちが怖いのか？」
		PRINTFORMW ネムノはなにか勘違いしているようだ……。
	ENDIF
	PRINTFORML 「しっかし、山芋でも擦ってるみたいだなあ。」
	PRINTFORMW %CALLNAME:MASTER%はおろしがねに擦られる様を想像してしまった……。
ENDIF
RETURN 1

;==================================================
;81,フェラチオ
;==================================================
@M_KOJO_MESSAGE_COM_K119_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K119_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K119_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「咥えるのはいいけどうちの歯、ぎざぎざしてて危ないぞ。」
		PRINTFORMW ネムノの鋭利な犬歯が肉棒を掠めた……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORML 物言わぬネムノの口内に肉棒を突き入れた……。
		PRINTFORMW 歯に刺さらないよう細心の注意を払わなければ……。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「んぐんぐ……間違って齧ったらごめんな。」
		PRINTFORMW ラネムノはやりにくそうにフェラチオを続けている……。
		RETURN 1
	ENDIF
	IF NOWEX:PLAYER:射精
		PRINTFORML 「むぐ、んむむ…………。……とろろ？」
		PRINTFORMW ネムノの口内に熱い精液をぶち撒けた……。
	ENDIF
ENDIF
RETURN 1

