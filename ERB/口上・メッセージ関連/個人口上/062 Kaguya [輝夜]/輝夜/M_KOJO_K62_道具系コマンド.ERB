;==================================================
;50,ゴム装着
;==================================================
@M_KOJO_MESSAGE_COM_K62_50
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K62_50_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K62_50_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF FLAG:70
	ELSEIF ABL:膣射中毒
					PRINTFORML
			IF TALENT:恋人 || (TALENT:妄信 && TALENT:MASTER:追加種族 == 4)
				;ゴムハメ嫌いな輝夜様。
				SELECTCASE RAND:10
					CASE 9
						PRINTFORML 「旦那さま、輝夜、出来ればそういうの着けないでしたいのですが……」
						PRINTFORML
					CASE 8
						PRINTFORML 「旦那さま、ゴム着けるんですか……？」
						PRINTFORMW
					CASE 7
						PRINTFORML 「どうしてそんなものを着けてるんです……？」
						PRINTFORML
					CASE 6
						PRINTFORML 「……輝夜はゴムじゃなくて、旦那さまと触れ合いたいのですが」
						PRINTFORML
					CASE 5
						PRINTFORML 「……輝夜を欲求不満にしようと思ってらっしゃるのでしょうか？」
						PRINTFORML
					CASE 4
						PRINTFORML 「旦那さま、輝夜は子宮に直接、びゅーって出されたいのですが……」
						PRINTFORML
					CASE 3
						PRINTFORML 「輝夜に旦那さまの精液をお預けするおつもりですか……？」
						PRINTFORML
					CASE 2
						PRINTFORML 「旦那さま、そんなものを着けたら旦那さまが気持ちよくないですよ……？」
						PRINTFORML
					CASE 1
						PRINTFORML 「それは輝夜への当てつけでしょうか……？」
						PRINTFORML
					CASE 0
						PRINTFORML 「旦那さまは輝夜じゃなくて、ゴムで気持ちよくなりたいんですか……？」
						PRINTFORML
				ENDSELECT
						PRINTFORML 輝夜ははじーっと男根に装着されるゴムを睨んでいる。
						PRINTFORMW
			ELSE
				SELECTCASE RAND:8
					CASE 7
						PRINTFORML 「……ふぅん」
						PRINTFORML
					CASE 6
						PRINTFORML 「私、それ嫌いなの。……一応言っておくけど」
						PRINTFORML
					CASE 5
						PRINTFORML 「……嫌がらせ？」
						PRINTFORML
					CASE 4
						PRINTFORML 「…………いいけど」
						PRINTFORML
					CASE 3
						PRINTFORML 「うぅ……何でそんなの」
						PRINTFORML
					CASE 2
						PRINTFORML 「ふぅん……着けるのね」
						PRINTFORML
					CASE 1
						PRINTFORML 「……ゴム」
						PRINTFORML
					CASE 0
						PRINTFORML 「……着けるの？」
						PRINTFORML
				ENDSELECT		
						PRINTFORML 輝夜ははじーっと男根に装着されるゴムを睨んでいる。
						PRINTFORMW
			ENDIF
	ELSEIF TALENT:恋人 || (TALENT:妄信 && TALENT:MASTER:追加種族 == 4)
						PRINTFORML 輝夜は何やら不満げに男根に装着されるゴムを見ている。
						PRINTFORMW
	ENDIF
ENDIF
RETURN 1