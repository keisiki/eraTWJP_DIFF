;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K125_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K125_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K125_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORMW 「手でしろって？」
			PRINTFORMW 「ま、任せなさいよ」
			PRINTFORMW 女苑はたどたどしい手付きで%CALLNAME:MASTER%の肉棒に手を這わせた
			PRINTFORMW 「うわぁ…か、硬い…%UNICODE(0x2764)%」
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋人
	ELSEIF TALENT:恋人 || TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「えーっと、こんな感じかな…」
			PRINTFORMW 「ど、どう？ きもちいいでしょ」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「これが私の中に…」
			PRINTFORMW 「そ、想像なんかしてない！」
		ELSE
			PRINTFORMW 「あわわ…びくびくして…」
			PRINTFORMW 「び、ビビってなんかないわよ！」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

