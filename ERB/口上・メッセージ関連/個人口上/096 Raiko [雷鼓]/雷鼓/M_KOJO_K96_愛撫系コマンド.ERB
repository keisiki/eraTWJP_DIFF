RETURN RESULT

@M_KOJO_MESSAGE_COM_K96_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:時間停止
			PRINTFORMW 時の流れが止まった状況の中、雷鼓の体を愛撫する。
			PRINTFORMW 反応はないが、どこか体を震わせているように感じられた。
		ELSE
			PRINTFORMW 「ん…そんなに触りたいの…？」
			PRINTFORMW 「いいわよ、私の体、好きにして頂戴…」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 止まった時の中で雷鼓の体を愛撫する。
		PRINTFORMW 色っぽい体からは仄かに香水の臭いがする。
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 「ん…もっと…触って…」
		PRINTFORMW 「ひゃん…ん…あんっ…」
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 「この体は…ん…別に貴方に触られるためのものじゃ…あ…」
		PRINTFORMW 「ん…もう…いっぱい触られちゃった…♪」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「あ…ん…」
		PRINTFORMW 「もっと…優しく…ん…」
		RETURN 1
	ENDIF
ENDIF
RETURN 1
