;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K96_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K96_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K96_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で雷鼓の尻を叩き始めた。
				RETURN 1
			ELSE
				PRINTFORMW 「…んっ！？」
				PRINTFORMW 「ちょっ…ちょっと！？…んあ%UNICODE(0x2665) *1%」
				RETURN 1
			ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 凍てついた時の中で雷鼓の尻を叩き始めた。
		RETURN 1
	;V挿入スパンキング
	ELSEIF LOCAL:1 && TEQUIP:50 == PLAYER
		IF RAND:3 == 0
			PRINTFORMW 「んっ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…やだ…これ…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あっ%UNICODE(0x2665) *1%…んん%UNICODE(0x2665) *1%」
			PRINTFORMW 「…挿れながら…ん%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 「んあ%UNICODE(0x2665) *1%…ああっ%UNICODE(0x2665) *1%」
			PRINTFORMW 「ん…もう…%UNICODE(0x2665) *1%」
			RETURN 1
		ENDIF
	;V挿入スパンキング
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		IF RAND:3 == 0
			PRINTFORMW 「ん%UNICODE(0x2665) *1%…っ…」
			PRINTFORMW 「…お尻…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あっ%UNICODE(0x2665) *1%…ん…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…これ…気持ちいい…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 「あっ%UNICODE(0x2665) *1%…んっ%UNICODE(0x2665) *1%」
			PRINTFORMW 「もう…%UNICODE(0x2665) *1%…馬鹿ぁ…%UNICODE(0x2665) *1%」
			RETURN 1
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…%UNICODE(0x2665) *1%あっ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…お尻…駄目になっちゃう…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あっ…%UNICODE(0x2665) *1%んっ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…これ…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 「んあっ…%UNICODE(0x2665) *1%こんなの…%UNICODE(0x2665) *1%」
			PRINTFORMW 「お尻馬鹿になっちゃう…%UNICODE(0x2665) *1%」
			RETURN 1
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「ん%UNICODE(0x2665) *1%」
			PRINTFORMW 「…あ%UNICODE(0x2665) *1%…お尻%UNICODE(0x2665) *1%」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あっ%UNICODE(0x2665) *1%…ん…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…これ…あんまり…んっ%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 「…んあ%UNICODE(0x2665) *1%…ひぁ%UNICODE(0x2665) *1%」
			RETURN 1
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん…もう…」
			PRINTFORMW 「…私のおしりは叩くものじゃないんだけど…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あんまり…叩かないでよ…」
			PRINTFORMW 「…痛いのよ…これでも」
			RETURN 1
		ELSE
			PRINTFORMW 「…ん…」
			PRINTFORMW 「痛い…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

