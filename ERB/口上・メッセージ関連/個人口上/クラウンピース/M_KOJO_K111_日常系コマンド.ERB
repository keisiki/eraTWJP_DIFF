LOCAL = 1
;-------------------------------------------------
;地の文をカットしてるのでMESSAGE処理を内蔵する
IF LOCAL
	CALL M_KOJO_COLOR_K111
	SELECTCASE TFLAG:193
	;釣れない
	CASE 0
		PRINTFORMDW 何も釣れなかった……
		IF TALENT:111:恋慕
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「あ～～釣れない～～っ！」
			CASE 2
				PRINTFORMW 「さかなさかなさかな～♪」
			CASEELSE
				PRINTFORMW 「%CALLNAME:MASTER%、タイクツ～」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「……ひまー！！」
			CASE 2
				PRINTFORMW 「……これ、楽しい？」
			CASEELSE
				PRINTFORMW 「……なんにも釣れないね……」
			ENDSELECT
		ENDIF
		RETURN 1
	;木片
	CASE 610
		IF TFLAG:194 > 1000
			PRINTFORMW 「見て！ なんか釣れた！！」
			PRINTL 
			PRINTFORMDL %CALLNAME:TARGET%が木片を釣った…
			PRINTW
			PRINTFORMW 「ん～～～！！」
		ELSE
			PRINTFORMDL 木片が釣れた……
			PRINTW 
			PRINTFORMW 「きゃははは！ はずれ～！」
		ENDIF
		RETURN 1
	;紙切れ
	CASE 131
		IF TFLAG:194 > 1000
			PRINTFORML 「見て！ なんか釣れた！！」
		ELSE
			PRINTFORMDL 紙切れが釣れた
		ENDIF
		PRINTDW 
		PRINTDL ┌─────────────┐
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　守矢の守札　　　　　　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　五枚揃えると　　　　　｜
