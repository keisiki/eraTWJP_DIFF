@M_KOJO_MESSAGE_COM_K111_350_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K111
	;押し倒され値	ENDURE(111) >= RAND:300 + 300で真,しかしRANDの値は変化するのでここでは400を超えたら押し倒されることにする
	IF FLAG:時間停止 == 1
	;野外失敗
	ELSEIF TFLAG:194 == 2
		IF ENDURE(111) > 400
			PRINTFORML 「えっ\/\/\/ そんな……\/\/\/」
			PRINTFORMW 「こ、ここはお外だよ……\/\/\/」
		ELSE
			PRINTFORML 「わっ！ なにっ！？」
			PRINTFORMW 「こ、ここはお外だよ……？」
		ENDIF
	;人目失敗
	ELSEIF TFLAG:194 == 1
		IF ENDURE(111) > 400
			PRINTFORML 「えっ\/\/\/ そんな……\/\/\/」
			PRINTFORMW 「人が見てるよ……\/\/\/」
		ELSE
			PRINTFORML 「わっ！ なにっ！？」
			PRINTFORMW 「人が見てるよ……？」
		ENDIF
	;乱交失敗
	ELSEIF TFLAG:194 == 3
		PRINTFORMW 「えっ、いや……%CALLNAME:MASTER%と二人きりがいい……」
	;ムード失敗
	ELSEIF TFLAG:194 == 4
		PRINTFORML 「うわっ！ やめてっ！」
		PRINTFORMW 「もう！ ムードってのを考えてよね」
	;泥酔失敗
	ELSEIF TFLAG:194 == 5
		PRINTFORMW 「んん～～？ %CALLNAME:MASTER%、どうひたのぉ～～？？」
		PRINTL 
		PRINTFORMDW 素面の目撃者がいるのに酔姦するわけにいかない
	;泥酔
	ELSEIF TCVAR:111:泥酔 == 1
		PRINTFORMW 「ふわぁ～～にゃ～～にぃ～～？ おねんねぇ～～？？」
	;乱交
	ELSEIF GET_TARGETNUM() > 1
		IF FIRSTTIME("乱交")
			PRINTFORMW 「えっ！ ほ、他の子と一緒にするの……？」
			PRINTFORMW 「ま、まぁいっか……」
		ELSE
			PRINTFORML 「%TOFULL(TOSTR(GET_TARGETNUM()))%ぴーってやつ？」
			PRINTFORMW 「ちゃんとびょーどーに相手してよ？」
		ENDIF
	;お泊り
	ELSEIF (CFLAG:111:現在位置 == 連れ込み宿 || CFLAG:111:現在位置 == OMANEKIBEYA()) && CFLAG:TARGET:うふふ != 2
		PRINTFORMW 「きゃっ\/\/\/」
	;それ以外
	ELSEIF !TCVAR:351
		PRINTFORMW 「あっ\/\/\/」
	ENDIF
	;先に押し倒された
	;うふふフラグは押し倒した時点で変更されるんでした
	;押し倒し返しは小傘口上さんを参考にして専用TCVARを作ることに
;	IF !FLAG:70 && !TFLAG:194 && CFLAG:111:うふふ == 2
	IF TCVAR:351
		PRINTFORML 
		PRINTFORMW 「きゃはっ♪ その気になってくれた？」
		PRINTFORMW 「うれしいっ%UNICODE(0x2764)% 好きにしていいよ%UNICODE(0x2764)%」
	ELSEIF !FLAG:70 && !TFLAG:194
		PRINTFORML 
		SELECTCASE TCVAR:111:抱き寄せ初回
		;普通の初回,キスがまだ,クラウンピースはキスが先だとか後だとか大して気にしてない
		CASE 1, 2
			PRINTFORML 「え……%CALLNAME:MASTER%……？」
			PRINTFORMW （こ、これってひょっとして、そういうこと、だよね……？）
			PRINTFORML 「うん%UNICODE(0x2764)% いいよ、%CALLNAME:MASTER%%UNICODE(0x2764)%」
			PRINTFORMW 「やさしくしてね%UNICODE(0x2764)%」
		;媚薬or発情
		CASE 3
			PRINTFORML 「ふわぁ……%CALLNAME:MASTER%、なんだかあたい、頭がぼうっとして……」
			PRINTFORMW 「はぁ……もう、好きにして……」
		;時姦
		CASE 4
			PRINTFORML 「なにするの！ いや！」
			PRINTL 
			PRINTFORMDW 口ではそういいつつも%CALLNAME:TARGET%は%CALLNAME:MASTER%に抱きついてきた
			PRINTL 
			PRINTFORMW 「えっ！？ なんで……どうして……？」
;		CASE 5
;			PRINTFORML 押しの弱い%CALLNAME:TARGET%は、%CALLNAME:MASTER%を拒むことができなかった…
		;もうちょっと
		CASE 6
			PRINTFORML 「ふぇ……そんな……」
			PRINTFORMW 「いやだ……ごめんね%CALLNAME:MASTER%……」
		;ダメ
		CASE 7
			PRINTFORML 「んわぁっ！ なにするんだよう！」
			PRINTFORMW 「そーゆーのいけないんだよ？」
		;早すぎ
		CASE 8
			PRINTFORML 「きゃぁあっ！！！ なにするの！ ヘンタイ！ ヘンタイ！」
			PRINTFORMW 「ご主人様ーーっ！！ こいつ地獄に堕としてくださーい！！」
		;初回じゃない
		CASEELSE
			SELECTCASE RAND:6
			CASE 1
				PRINTFORML 「んもうっ♪ %CALLNAME:MASTER%のえっち%UNICODE(0x2764)%」
				PRINTFORMW 「ふふ%UNICODE(0x2764)% 好きにしていいよ%UNICODE(0x2764)%」
			CASE 2
				PRINTFORMW 「きゃはっ♪ もう、しょうがないにゃぁ♪」
			CASE 3
				PRINTFORMW 「ふふっ♪ いいよ%UNICODE(0x2764)% 一緒にえっちな遊びしよっ%UNICODE(0x2764)%」
			CASE 4
				PRINTFORMW 「いやー%UNICODE(0x2764)% %CALLNAME:MASTER%のケダモノー！」
			CASE 5
				PRINTFORMW 「ん……、%CALLNAME:MASTER%、今日は甘えてもいーい？」
			CASEELSE
				IF TALENT:111:妊娠
					PRINTFORMW 「きょ、今日はちょっと、ゆっくりして……%UNICODE(0x2764)%」
				ELSE
					PRINTFORMW 「きょ、今日はちょっと、激しくして……%UNICODE(0x2764)%」
				ENDIF
			ENDSELECT
			;妊娠中
			IF TALENT:111:妊娠 && CFLAG:111:妊娠自覚状態 == 1
				PRINTFORML 
				PRINTFORMW 「あ！ お腹の子もいるんだから、やさしくしてよ？」
			;危険日
			ELSEIF CFLAG:111:生理周期 == 7 && !TALENT:111:妊娠 && CFLAG:111:生解禁 == 2
				PRINTFORML 
				PRINTFORMW （今日はひょっとして……チャンスだわ♪）
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
