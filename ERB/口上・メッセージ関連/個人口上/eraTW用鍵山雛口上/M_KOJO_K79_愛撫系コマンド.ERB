@M_KOJO_MESSAGE_COM_K79_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;初めて
	IF FIRSTTIME(SELECTCOM)
		IF FLAG:時間停止
			PRINTFORML 時が止まった世界で、%CALLNAME:MASTER%は雛の身体を撫で回していく。
			PRINTFORMW 当然、雛が抵抗できるはずもなく、無意識のまま%CALLNAME:MASTER%の愛撫を受け入れた……。
		ELSEIF TALENT:恋慕
			PRINTFORML 「その……、優しく、してね……？」
			PRINTFORMW %CALLNAME:MASTER%は頷くと、雛の身体を優しく撫で始めた……。
		ELSE
			PRINTFORML 「うっ……、ううっ……」
			PRINTFORMW %CALLNAME:MASTER%の愛撫を受ける雛の身体は硬直している……。
		ENDIF
		RETURN 1
	ENDIF
	;二回目以降
	;V挿入中
	IF TEQUIP:50 == PLAYER
		IF TALENT:恋慕
			PRINTFORML 「あっ%UNICODE(0x2764)% ひゃあっ、%CALLNAME:MASTER%っ……%UNICODE(0x2764)% ふぅんっ、あぁんっ%UNICODE(0x2764)%」
			PRINTFORMW %CALLNAME:MASTER%は腰を動かしながら、雛の身体を優しく愛撫した……。
			PRINTFORML 「ひゃっ、そっちはっ…%UNICODE(0x2764)% あぁっ、らめっ%UNICODE(0x2764)% きもちっ、いいっ%UNICODE(0x2764)% ふあぁんっ！」
			PRINTFORMW 身体のあらゆる部位から快感が押し寄せ、堪らず雛は嬌声をあげている……。
		ELSE
			PRINTFORML 
		ENDIF
		RETURN 1
	ENDIF
	
	;A挿入中
	IF TEQUIP:51 == PLAYER
		IF TALENT:恋慕
			PRINTFORML 
		ELSE
			PRINTFORML 
		ENDIF
		RETURN 1
	ENDIF
	
	;ふたなり
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		IF TALENT:恋慕
			PRINTFORML 
		ELSE
			PRINTFORML 
		ENDIF
		RETURN 1
	ENDIF

	;基本セット
	;時姦中
	IF FLAG:時間停止
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:5
		CASE 0
			PRINTFORML 「ふぅんっ、ひゃんっ…%UNICODE(0x2764)%、あっ、ふぁああっ%UNICODE(0x2764)%%UNICODE(0x2764)%」
			PRINTFORMW 軽い愛撫でも雛の身体は敏感に反応し、快感をもたらしている……。
		CASE 1
			PRINTFORML 「んんっ…%UNICODE(0x2764)%　はぁっ、あっ…%UNICODE(0x2764)% んっ、%CALLNAME:MASTER%っ……%UNICODE(0x2764)%%UNICODE(0x2764)%」
			PRINTFORMW 雛の身体をゆっくりと弄ると、雛は息を荒げて身体をくねらせている……。
		CASE 2
			PRINTFORML 「んっ、あぁっ、あっ…%UNICODE(0x2764)% そこっ……、きもちいいっ……%UNICODE(0x2764)%」
			PRINTFORMW 全身を優しく包みこむような愛撫に、雛の表情は蕩けきっている……。
		CASE 3
			PRINTFORML 「あぁっ、%CALLNAME:MASTER%……%UNICODE(0x2764)% %CALLNAME:MASTER%っ……%UNICODE(0x2764)%」
			PRINTFORMW 雛は%CALLNAME:MASTER%の愛撫を求め、自ら身体を寄せてきた……。
		CASE 4
			PRINTFORML 「はああぁっ……%UNICODE(0x2764)% %CALLNAME:MASTER%っ、もっと…、もっとしてっ……%UNICODE(0x2764)%」
			PRINTFORMW 雛は%CALLNAME:MASTER%の愛撫にすっかり酔いしれているようだ……。
		ENDSELECT
	;不埒3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORML 「あっ……、んんっ…、あっ、はあぁっ……」
		PRINTFORMW 雛は%CALLNAME:MASTER%の愛撫を受け入れ、甘い声を漏らしている……。
		PRINTFORML 「…ひゃぁっ！ あぁっ、そこは……、だ、だめぇっ……！」
		PRINTFORMW %CALLNAME:MASTER%が雛の秘裂に手を伸ばすと、雛は思わず身をよじらせた。
	;それ以外
	ELSE
		PRINTFORML 「んんっ…、んっ、うっ……、んっ……」
		PRINTFORMW %CALLNAME:MASTER%の愛撫に雛は身体を硬直させている……。
		PRINTFORML 「ん、あぁっ……、んっ、んあぁっ……」
		PRINTFORMW しかし、秘裂や乳首の刺激には敏感な反応を示しており、快楽の兆しが見えつつある。
	ENDIF
	;絶頂判定
	CALL M_KOJO_MESSAGE_EVENT_K79_EX_ORGASM
ENDIF
RETURN 1
