				CALL HPH_PRINT,@"「典は面倒なので、御自分で腰をヘコヘコ振って勝手にイってくださいHPH」","L"
				PRINTL 
				PRINTFORML 典は舌をだらりと出し、挑発的にレロレロと動かしてみせる
				PRINTFORML 生意気な態度をとる典の頭を掴み、肉棒を一気に喉奥まで突き入れた
				PRINTW 
				CALL HPH_PRINT,@"「んふっ！？　ん～～～～～～！？HPH」","L"
				PRINTFORML 典は肩をビクビクと震わせながらも、肉棒の暴力を口腔で受け止める
				PRINTFORML 唾液が溜まっていた口腔は温かくぬめっており、容易な抽挿を可能としていた
				PRINTL 
				PRINTFORML じゅぼっ！　じゅろろろろっ！　じゅぼっ！　じゅろろろろぉっ
				CALL HPH_PRINT,@"「おっごHPH　お゛え゛っHPHHPH　お゛っHPH　うぶっHPHHPH」","L"
				PRINTL 
				PRINTFORML 典は止めてという風に%CALLNAME:MASTER%の体を叩くが、口はペニスに吸い付いて離そうとしない
				PRINTFORML 無意識に叩いたのか、それとも、こちらの嗜虐心を煽るためにわざとやっているのか…
				IF ABL:舌 >= 2
					PRINTFORML 極上の舌使いとともに、%CALLNAME:MASTER%は典の口腔を堪能した
				ELSE
					PRINTFORML 舌使いは上手ではないが、なかなか楽しめそうな口淫を%CALLNAME:MASTER%は堪能した
				ENDIF
				PRINTW 
			CASE 1
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%どのHPH　典のお口まんこと交尾してください…HPH」","L"
				PRINTFORML 典は拘束具のように指を口に引っ掛け、肉棒をおねだりしている…
				PRINTW 
				PRINTFORML 望み通り、バックから犯すように、典の顔を尻に見立ててグイっと引き寄せる
				PRINTFORML そして一息に唾液の蜜を湛えた媚唇を貫いた
				PRINTL 
				CALL HPH_PRINT,@"「おごっHPH　んぼっHPH　んごっHPHHPH」","L"
				PRINTFORML 自らの意思で腰を振る口淫はとても心地よく、ただひたすら射精へと突き進んでいく
				CALL HPH_PRINT,@"（これっHPH　口で本気交尾されてるっHPH　口で孕んじゃうっHPHHPH）","L"
				PRINTW 
			CASE 2
				PRINTFORML ずろろろろぉ～～～～…
				CALL HPH_PRINT,@"「んHPH　んHPH　んHPH　んHPH　んHPH　んHPH」","L"
				CALL HPH_PRINT,@"（くるHPHくるHPHくるっHPHHPH）","L"
				PRINTL 
				PRINTFORML ずぱぁんっ！！
				CALL HPH_PRINT,@"「おごげぇっ！？HPHHPH」","L"
				CALL HPH_PRINT,@"（きたぁっHPH　容赦ない腰振りっHPH　穴扱いされちゃってるHPH）","L"
				PRINTW 
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

