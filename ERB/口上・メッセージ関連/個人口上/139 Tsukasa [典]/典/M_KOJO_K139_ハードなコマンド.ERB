			CASE 1
				CALL HPH_PRINT,@"「ほら…お姉ちゃんのここを使うんですよ…HPH」","L"
				CALL HPH_PRINT,@"「お口だけど、今は『お口まんこ』っていうの…HPH」","L"
				PRINTL 
				CALL HPH_PRINT,@"「まんこなんだから、手加減しちゃ駄目ですよ…HPH」","L"
				CALL HPH_PRINT,@"「自分の腰を使って、パンパン犯して、お姉ちゃんのここに気持ちいい%TEXTR("射精/びゅっびゅ/ぴゅっぴゅ")%するんですHPH」","L"
				PRINTFORML 典は舌をだらりと下げ、右手で輪っかをつくると、何かをしごくような真似をしてみせる
				PRINTW 
				CALL HPH_PRINT,@"ちゅ…ずるるるるぅ～～～～～～HPH","L"
				IF TALENT:MASTER:形状 == 6 || TALENT:MASTER:形状 == 10
					CALL HPH_PRINT,@"「おぼっHPH　おごえっHPHHPH」","L"
