				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「ろうろ（どうぞ）…HPH」","L"
				PRINTW 
				RESETCOLOR
				PRINTFORML にゅるんっ！！
				CALL HPH_PRINT,@"ぱちゅHPHじゅぼHPHじゅぼHPHぢゅぼHPH","L"
				PRINTL 
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「んぶっHPH　んごっHPHHPH　おぶっHPH　んぼっHPHHPH」","L"
				RESETCOLOR
				PRINTFORML 典は乱暴にペニスを口腔内に突き入れられているにも関わらず、その表情は喜びに満ち溢れている…
				PRINTL 
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"（嬉しいっHPH　お姉ちゃんのお口でそんなに気持ちよくなってるんですねっHPHHPH）","L"
				CALL HPH_PRINT,@"（いいんですよっHPH　あなただけのお口まんこHPHHPH　好きに犯してHPH）","L"
				CALL HPH_PRINT,@"（お姉ちゃんはあなたのものだってマーキングしてHPHHPH）","L"
				RESETCOLOR
				PRINTW 
			ELSE
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML ごっ！　ごっ！　ごっ！　ごっ！　ごっ！　
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"「%TEXTR("おげっHPH/おごっHPH/うぼっHPH/んぶっHPH/お゛っHPH")%%TEXTR("おげっHPH/おごっHPH/うぼっHPH/んぶっHPH/お゛っHPH")%%TEXTR("おげっHPH/おごっHPH/うぼっHPH/んぶっHPH/お゛っHPH")%」","L"
					CALL HPH_PRINT,@"（喉の奥っHPH　容赦なく突かれてるっHPHHPH　苦しいっHPHHPH）","L"
					CALL HPH_PRINT,@"（苦しいのにっHPH　発情中の私喜んでるっHPHHPH　一突きごとに喉でイクッHPHHPH）","L"
					PRINTL 
					RESETCOLOR
					PRINTFORML 典の手が秘所に延びる。そこはすでにぐしょぐしょに濡れそぼっていた
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"「ふーーーーーっHPH　ふーーーーーっHPH　ふーーーーーーっHPHHPH」","L"
					RESETCOLOR
					PRINTFORML 典は乱暴に膣内を指でかき混ぜる。そうした方がより感じるといわんばかりに
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"（乱暴でもいいっHPH　もっと愛してっHPHHPH　典の体で気持ちよくなってっHPHHPH）","L"
					RESETCOLOR
					PRINTW 
				CASE 1
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"「典を仰向けにして、これから何をなさろうとしているんですか…おぐえぇっ！？HPH」","L"
					RESETCOLOR
					PRINTFORML 典の問いかけを無視し、\@ TALENT:MASTER:形状 == 6 || TALENT:MASTER:形状 == 10 ? 凶悪な # \@肉棒を口腔に突き立てた
					PRINTW 
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"「んぶっHPH　んごっHPHHPH　おぶっHPH　んぼっHPHHPH」","L"
					CALL HPH_PRINT,@"（激しっHPH　のどマンコっHPHHPH　ハメ壊されるっHPH）","L"
					PRINTL 
					RESETCOLOR
					PRINTFORML 典は顔を手で固定され、肉のオナホ扱いされている
					PRINTFORML それにも関わらず、愛する者が夢中で腰を振っている様子に、苦痛より多幸感が勝っているようだった
					PRINTW 
				ENDSELECT
			ENDIF
		ELSE
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
				CALL HPH_PRINT,@"「はぁいHPH　%CALLNAME:MASTER%どののおちんちんはこちらにどうぞHPHHPH」","L"
				PRINTFORML 典は口の前で輪っかをつくり、唾液に塗れた舌を垂らして肉棒の挿入を待ちわびている
				PRINTW 
				CALL HPH_PRINT,@"ちゅ…にゅにゅにゅ…にゅるんっHPH","L"
				CALL HPH_PRINT,@"「んふふHPH　ひぇんふ、はいっひゃいらひはねぇ…HPHHPH」","L"
				PRINTFORML 典は舌で%CALLNAME:MASTER%のペニスを優しく、丹念に愛撫する
				PRINTL 
				CALL HPH_PRINT,@"「ふほいへHPH　ほへーひゃん、らいひょーふらひゃら…HPH」","L"
				PRINTFORML 典に言われるがまま、腰を前後に動かす
				PRINTL 
				CALL HPH_PRINT,@"ぱちゅっHPHぱちゅっHPHぱちゅっHPHぱちゅっHPH","L"
				CALL HPH_PRINT,@"「%TEXTR("うぶっHPH/おぼっHPH/んぼっHPH/んむっHPH")%%TEXTR("うぶっHPH/おぼっHPH/んぼっHPH/んむっHPH")%%TEXTR("うぶっHPH/おぼっHPH/んぼっHPH/んむっHPH")%」","L"
				PRINTL 
				CALL HPH_PRINT,@"（ふふHPH　気持ちよさそうに腰を振って…HPH　そんなに私の口が気に入ったのね…HPH　可愛い子HPH）","L"
				PRINTFORML 口虐されながらも、典は慈母のような笑みを浮かべて%CALLNAME:MASTER%の行為を見守っている…
				PRINTW 
			ELSE
				CALL HPH_PRINT,@"「はぁいHPH　おしゃぶりですねHPH　喜んでHPHHPH」","L"
				PRINTFORML 典は嬉々として%CALLNAME:MASTER%の肉棒を口腔に受け入れようとする
				PRINTL 
				PRINTFORML がしっ！
				PRINTFORML 「はえ？」
				PRINTFORML 典が素っ頓狂な声を上げた次の瞬間には、%CALLNAME:MASTER%の肉棒は典の口腔奥深くに突き入れられていた
				PRINTW 
				PRINTFORML ぶっちゅ！　ぶっちゅ！　ぶっちゅ！　ぶっちゅ！　ぶっちゅ！　ぶっちゅ！
				PRINTFORML 「おごっ！？　んぶっ！　んぼっ！　んぼぉっ！」
				PRINTFORML 典は目を白黒させつつ、それでもペニスに歯を立てまいと必死に口を開いて凶行を受け入れる
				PRINTL
				CALL HPH_PRINT,@"（%CALLNAME:MASTER%どのっ、なんて、情熱的なぁっHPH）","L"
				CALL HPH_PRINT,@"（頭がクラクラするHPH　口の中、おちんぽの臭いでいっぱいになるHPH）","L"
				PRINTFORML 典にとっては、物のように扱われる行為ですら愛おしく、悦びに感じるようだった…
				PRINTW 
			ENDIF
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
