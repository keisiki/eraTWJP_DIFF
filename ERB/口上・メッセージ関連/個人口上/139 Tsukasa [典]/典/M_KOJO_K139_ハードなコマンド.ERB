					CALL HPH_PRINT,@"（こんなの突っ込まれたら、ほとんどの女は雌になっちゃうわ…HPH）","L"
				ELSEIF TALENT:MASTER:形状 == 1 || TALENT:MASTER:形状 == 2
					CALL HPH_PRINT,@"「んふHPH　んっHPHHPH」","L"
					PRINTL 
					CALL HPH_PRINT,@"（ふふ…HPH　ちっちゃいオチンチン、一生懸命奥まで入れようとして可愛い…HPH）","L"
					CALL HPH_PRINT,@"（僕は雄なんだぞって、必死に自己主張しようとしてるHPH）","L"
				ELSE
					CALL HPH_PRINT,@"「んもっHPH　おぼっHPHHPH」","L"
					PRINTL 
					CALL HPH_PRINT,@"（ふふ、一人前に勃起しちゃって…HPH）","L"
					CALL HPH_PRINT,@"（私に必死におちんちんの味を教え込もうとグイグイ押し込んでる…HPH）","L"
				ENDIF
				PRINTL 
				CALL HPH_PRINT,@"ぱちゅっHPHぱちゅっHPHぱちゅっHPHぱちゅっHPH","L"
				CALL HPH_PRINT,@"「うぶっHPH　おぼっHPH　んぼっHPH　んむっHPH」","L"
				CALL HPH_PRINT,@"（幼い子に無理やり口を犯される感覚…意外と、悪くないわHPH）","L"
				PRINTW 
			CASE 2
				CALL HPH_PRINT,@"「ふーーーーっHPH　ふーーーーっHPH」","L"
				PRINTFORML 典は口の端から涎を垂らしながら、%CALLNAME:MASTER%の亀頭に吸い付いている…
				PRINTL 
				IF TCVAR:MASTER:射精我慢 > 0
					CALL HPH_PRINT,@"（こんなに必死に腰を振っているのに、射精を我慢して、なんていじらしいHPH）","L"
					CALL HPH_PRINT,@"（少しでも、お姉ちゃんの気持ちいい口まんこ、味わいたいんですよねHPH）","L"
					IF TCVAR:MASTER:イきそう == 1
						PRINTL 
						CALL HPH_PRINT,@"（でも、イっちゃいそうなんですよね？HPH　出そうなんですよね？HPH）","L"
						CALL HPH_PRINT,@"（だって、あなたの先っぽから精液の味が混じってきているんですものHPH）","L"
						CALL HPH_PRINT,@"（いいんですよHPH　お姉ちゃんの口まんこの奥で、中出ししてHPH　喉を孕ませてHPHHPH）","L"
					ENDIF
					PRINTW 
				ELSE
					IF TCVAR:MASTER:イきそう == 1
						CALL HPH_PRINT,@"（ふふ、責められてるのは私なのに、余裕がないのは%CALLNAME:MASTER%だなんて…HPH）","L"
						CALL HPH_PRINT,@"（先走りに精液の味が濃くなってきたHPH　出ちゃうんでしょHPH　出したいんだよねHPH）","L"
						CALL HPH_PRINT,@"（いいんですよHPH　我慢しないでHPH　プリプリの精液HPH　お姉ちゃんの中に出してHPHHPH）","L"
						PRINTW 
					ELSE
						IF ABL:舌 >= 2
							CALL HPH_PRINT,@"（ふふ、自分が攻めてるはずなのに、段々余裕がなくなってきているのが分かりますよHPH）","L"
							CALL HPH_PRINT,@"（喉の締め方を変えたり、舌を擦り付けてるのにも気付かず…HPH）","L"
							PRINTW 
						ELSE
							CALL HPH_PRINT,@"（こういう乱暴な%CALLNAME:MASTER%も、新鮮で悪くないわ…HPH）","L"
							CALL HPH_PRINT,@"（後で〝反撃〟してあげるのも悪くないわね…HPH）","L"
							PRINTW 
						ENDIF
					ENDIF
				ENDIF
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
