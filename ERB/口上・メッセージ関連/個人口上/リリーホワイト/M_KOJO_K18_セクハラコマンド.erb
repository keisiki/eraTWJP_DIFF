;==================================================
;310,尻を撫でる
;TFLAG:193(1=不快(TALENT:臆病なら涙目) 2&&3=恥ずかしがる(TALENT:男嫌い兼MASTERが男なら、身をすくめる) 4=されるがまま)
;CFLAG:うふふ==2&&TCVAR:20(パンツ無し顔面騎乗(70=クンニ強制 71=フェラチオ強制 72=アナル奉仕) 35=スカート内見せつけ 52=素股)
;CFLAG:うふふ==2&&TEQUIP:シックスナイン(パンツありTCVAR:20(70～72)はこちら)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_SUCCESS_COM_K18_310
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K18_310
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K18_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K18_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
	ELSEIF CFLAG:うふふ == 2
		SELECTCASE TCVAR:20
			CASE 70
			;クンニ強制
				PRINTFORML 
			CASE 35
			;スカート内見せつけ
				PRINTFORML 
			CASE 52
			;素股
				PRINTFORML 
			CASEELSE
				PRINTFORML 
		ENDSELECT
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「んっ…くすぐったいですー…」
				PRINTFORMW 
			CASE 0
				PRINTFORML 「キャ…そんなにさわるの楽しいですかー？」
				PRINTFORMW 「それなら…もっとしてもいいですよー%UNICODE(0x2665) *1%」
			CASEELSE
				PRINTFORML 「そんなにじっくり撫でないでくださいぃ…」
				PRINTFORMW 「春のキモチになっちゃう…%UNICODE(0x2665) *1%」
		ENDSELECT
	ELSEIF TALENT:思慕
		SELECTCASE RAND:3
			;CASE 1
				;PRINTFORML 
				;PRINTFORMW 
			CASE 0
				PRINTFORML 「だ…ダメですよー…」
				PRINTFORMW 
			CASEELSE
				PRINTFORML 「ひゃっ！？え、エッチー！」
				PRINTFORMW 
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「服にひっつき虫がついてたんですかー？」
				PRINTFORML 「なんだー、とってくれようとしてくれたんですねー！」
				PRINTFORML 「………ホントですかぁー？」
			CASE 0
				PRINTFORML 「春になるとこうゆう人がでるのだけでは苦手ですー…」
				PRINTFORMW 
			CASEELSE
				PRINTFORML 「頭だけ春なのはキライですよー！」
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
;==================================================
;311,抱き付く
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 60=ディープキス 73=ぱふぱふ 74=乳合わせ 77=乳を吸わせる ＶＡ性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_SUCCESS_COM_K18_311
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K18_311
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K18_311_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K18_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CHK_DATENOW(CFLAG:LOCAL:デート中)
	ELSEIF CFLAG:睡眠
	ELSEIF CFLAG:うふふ == 2
		SELECTCASE TCVAR:20
			CASE 32
			;クンニ強制
				PRINTFORML 
			CASE 60
			;スカート内見せつけ
				PRINTFORML 
			CASE 73
			;素股
				PRINTFORML 
			CASE 85
			;V挿入する
				PRINTFORML 
			CASE 88
			;腰振りV
				PRINTFORML 
			CASE 86
			;だいしゅきホールド
				PRINTFORML 
			CASEELSE
				PRINTFORML 
		ENDSELECT
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%から春の匂いがしますよー♪」
				PRINTFORMW リリーは%CALLNAME:MASTER%の胸元に頬を摺り寄せている。
			CASE 0
				PRINTFORML 「んぅ…」
				PRINTFORML リリーは%CALLNAME:MASTER%の腕の中でもぞもぞと身体を動かしている。
				PRINTFORMW 「…あ、えと…えと、なんでもないですよー！」
			CASEELSE
				PRINTFORML 「もっと強く抱きしめてもダイジョウブ…ですよー♪」
				
		ENDSELECT
	ELSEIF TALENT:思慕
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「ハグですよー」
				PRINTFORMW %CALLNAME:MASTER%が抱きしめるとリリーも抱きしめ返してくれた。
			CASE 0
				PRINTFORML %CALLNAME:MASTER%は小さなリリーを軽々と抱き上げた。
				PRINTFORMW 「遠くも見えますよー！%CALLNAME:MASTER%は力持ちですー！」
			CASEELSE
				PRINTFORML 「んふふー春の匂いがしますよー」
				PRINTFORMW リリーからは桜の甘い香りがした。
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「んんー！」
				PRINTFORMW リリーはバタバタもがいている。
			CASE 0
				PRINTFORML 「んむ～動きにくいですよぉー」
				PRINTFORMW 「早く離して欲しいですー！」
			CASEELSE
				PRINTFORML 「わぁ！」
				PRINTFORML 「…どうしたんですかー？」
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
