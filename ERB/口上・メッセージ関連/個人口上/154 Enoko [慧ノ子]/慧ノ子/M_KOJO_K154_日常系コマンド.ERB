@M_KOJO_MESSAGE_COM_K154_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怒り
	IF CFLAG:318 == 1
			PRINTFORMW 「ふん！話しかけないでよね！」
	;会話不能
	ELSEIF TCVAR:302
		IF RAND:5 == 0
			PRINTFORMW 「えーあっそっかー」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「ぽかーん」
			PRINTFORMW 慧ノ子は口を開けて困惑している。
		ELSEIF RAND:3 == 0
			PRINTFORMW 「もっとお話しようよ～」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「おばあちゃん若い子の話題わかんなくて……」
		ELSE
			PRINTFORMW （もうちょっと面白い話ないかな～）
		ENDIF
	;ぐでぐで
	ELSEIF TALENT:恋慕 && BASE:酒気 > (MAXBASE:酒気 / 10) * 8
		IF RAND:5 == 0
			PRINTFORMW 「がおー！噛み噛みしちゃうぞ～？」
			PRINTFORMW 慧ノ子は酔っ払いながら絡んでくる。
		ELSEIF RAND:4 == 0
			PRINTFORMW 「おばあちゃん酔ってきちゃった……%UNICODE(0x2665) *1%
			PRINTFORMW 慧ノ子は酔いながら絡んでくる。
		ELSEIF RAND:3 == 0
			PRINTFORMW 「八岐大蛇は酔い潰れて退治されちゃったけどケルベロスはどうなのかな？」
			PRINTFORMW 慧ノ子は顔を上気させながら話しかけてくる。
			PRINTFORMW 「まあいいや！%CALLNAME:MASTER%になら……ね%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「%CALLNAME:MASTER%……えへへへへ～ッ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%……いい臭いするね、食べちゃいたい%UNICODE(0x2665) *1%」
		ENDIF
	;酔いどれ
	ELSEIF TALENT:恋慕 && BASE:酒気 > (MAXBASE:酒気 / 10) * 5
		IF RAND:5 == 0
			PRINTFORMW 「ん～あまり酔わない体質なんだけどね～」
			PRINTFORMW 「年を取るとアルコールの効きも悪くなるのかなぁ～？」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「%CALLNAME:MASTER%はさぁ、死にたいと思ったことある？」
			PRINTFORMW 慧ノ子は酔いながら絡んでくる。
			PRINTFORMW 「私はあるよ、でも今は――ないかも」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「……ん？」
			PRINTFORMW 「どこ見てたのかなぁ～えっちなやつめ～%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「%CALLNAME:MASTER%～呼んだだけ～%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「ふふふふ～ん、酔うにはいい日だね%UNICODE(0x2665) *1%」
		ENDIF
	;ほろ酔い
	ELSEIF TALENT:恋慕 && BASE:酒気 >= 1
		IF RAND:5 == 0
			PRINTFORMW 「お酒って美味しくはないよね～%CALLNAME:MASTER%の方が美味しいかも」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「万が一の時は介抱してね～」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「ん……お酒は苦手かも……」
			PRINTFORMW 「酔いが回って水瓶に嵌まるとしんどいからさ～」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んん～～～ほろ酔い気分ってやつ～？」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%のこともっと聞かせて～～～」
		ENDIF
	;ぐでぐで
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 8
		IF RAND:5 == 0
			PRINTFORMW 「魔理沙はね～よく泣いてたんだよ～」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「あの子も立派になって私も感動したね～」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「お酒は美味しいけど飲み過ぎて死なないようにね～私は死なないけど」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「竹林のオオカミさんもいい毛並みしてそうだよね～」
		ELSE
			PRINTFORMW 「早鬼様はね！すっごく強くてかっこいいんだよ～～～！！！」
		ENDIF
	;酔いどれ
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 5
		IF RAND:5 == 0
			PRINTFORMW 「私はもう早く走れないからさぁ」
			PRINTFORMW 「早鬼様がいっぱい走ってるの見ると嬉しくなるの！」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「うう……お酒慣れない……」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「こうやって人と飲むのは不思議な感じするね～」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「トラバサミは外の世界だと禁止なんだって？なんだか悲しいなぁ」
		ELSE
			PRINTFORMW 「結構飲んだねぇ～楽しくなってきた！」
		ENDIF
	;ほろ酔い
	ELSEIF BASE:酒気 >= 1
		IF RAND:5 == 0
			PRINTFORMW 「ちょっと～どこ見てるの～？」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「がおー！ケルベロスだぞ～！」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「地獄の番犬たるもの頑張って番人しないとねぇ～」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あー死にたくなってきたかも」
		ELSE
			PRINTFORMW 「昔の事なんて覚えてないから酒に酔うのかな？」
		ENDIF
	;風呂場
	ELSEIF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
		IF RAND:3 == 0
			PRINTFORMW 「ちゃーんとお風呂に入ってないと臭っちゃうからね～」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「折角だから洗いっこしよっか～？」
		ELSE
			PRINTFORMW 「尻尾が三つもあると洗うのも大変なんだ～」
		ENDIF
	ELSE
		SELECTCASE RAND:4
		CASE 0
			IF DAY:2 == 1
				IF RAND:3 == 0
					PRINTFORMW 「春うららってやつだね～ぽかぽかだ～」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「桜も見飽きたかもって思ってたけど飽きないね～」
				ELSE
					PRINTFORMW 「春になるとちょっと毛が暑くなってくるんだよね～」
				ENDIF
			ELSEIF DAY:2 == 2
				IF RAND:3 == 0
					PRINTFORMW 「あっついね～私は涼しい格好してるけどね～」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「夏場は水浴びしたいね～犬かきは得意だよ～」
				ELSE
					PRINTFORMW 「夏は開放的になっちゃうね～元から開放的？……えっちだ！」
				ENDIF
			ELSEIF DAY:2 == 3
				IF RAND:3 == 0
					PRINTFORMW 「紅葉を見るとしんみりするね～」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「秋の味覚もいっぱい食べたいな～」
				ELSE
					PRINTFORMW 「芸術の秋だからこそ罠を張るのよね～」
				ENDIF
			ELSEIF DAY:2 == 4
				IF RAND:3 == 0
					PRINTFORMW 「うう……寒くなってきたね……そんな格好してるから？そうかもね～」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「早鬼様とお鍋食べたいな～」
				ELSE
					PRINTFORMW 「魔法の森のお地蔵さんは元気にしてるかなぁ」
				ENDIF
			ENDIF
		CASEELSE
			IF TALENT:恋人
				IF RAND:5 == 0
					PRINTFORMW 「君になら殺されてもいいかも、なんてね～%UNICODE(0x2665) *1%」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「いっぱい食べて大きくなりなよ、そしたら食べてあげるからね～%UNICODE(0x2665) *1%」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「%CALLNAME:MASTER%も私のお肉食べたら不老不死になるかな」
					PRINTFORMW 「試してみる？大丈夫、減るもんじゃないよ～」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「こうして恋人みたいに話すだなんて思ってもみなかったね～」
				ELSE
					PRINTFORMW 「まるで夢みたい」
					PRINTFORMW 「好きな人とこうして語らうなんてさ～幸せだよね～」
				ENDIF
			ELSEIF TALENT:恋慕
				IF RAND:5 == 0
					PRINTFORMW 「%CALLNAME:MASTER%は食べたいものとかある？残無様のお肉はおすすめしないな～」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「魔理沙に見られたらからかわれちゃうかもね～」
					PRINTFORMW 「……あの子、こういうの好きなんだよね」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「いい毛艶してるって？照れちゃうよ、そんなこと言ったら……%UNICODE(0x2665) *1%」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「早鬼様のお肉も美味しそうだよね～ちょっと分けてもらおっか？」
				ELSE
					PRINTFORMW 「君に首輪付けられちゃった感じだね～あまり引っ張っちゃダメだよ？」
				ENDIF
			ELSEIF TALENT:思慕
				IF RAND:5 == 0
					PRINTFORMW 「ふんふんその服は寒くないかって？」
					PRINTFORMW 「……ちょっとセクハラだよ～」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「早鬼様は足速いから追いつくの大変なんだ～」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「いつもはのんびりしてるけどこうやって人とお話しするのも楽しいかもね～」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「君って色んなお話を知ってるんだね～私もいっぱいお話聞きたいな～」
				ELSE
					PRINTFORMW 「昔のことはあんまり覚えてないから君とお話しするの楽しいんだ～」
				ENDIF
			ELSEIF CFLAG:2 >= 500
				IF RAND:5 == 0
					PRINTFORMW 「君のお肉？美味しくなさそう～」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「この宝玉？あんまり触らない方がいいよ～」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「好きなお肉の話？……実は菜食主義者だって言ったら信じる？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「地獄は番犬だから鶏さんとは同僚になるのかな？あっでも私達の敵かも？」
				ELSE
					PRINTFORMW 「残無様の考えてることは難しいよね～」
				ENDIF
			ELSE
				IF RAND:5 == 0
					PRINTFORMW 「あんまり煩いと噛んじゃうよ～」
				ELSEIF RAND:4 == 0
					PRINTFORMW 「みんなおしゃべりだね～」
				ELSEIF RAND:3 == 0
					PRINTFORMW 「色々面白い話持ってるね～」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「あはは、面白いね～」
				ELSE
					PRINTFORMW 「魔理沙みたいにおしゃべりだね～」
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
