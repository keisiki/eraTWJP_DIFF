;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K15_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K15_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K15_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		CALL HPH_PRINT("「あぁッHPH 許してぇっHPH」","L")
		PRINTFORMW 咲夜はその白い珠のような尻を痛めつけられ、喘ぎ声を上げている
		PRINTFORMW そのまま咲夜の尻がもも色になるまでその光景は続いた……
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「やっ！いたっ、なんでっ！？」
		PRINTFORMW 咲夜は特に意味も無く尻を叩かれている屈辱を噛み締めている
		RETURN 1
	ENDIF
ENDIF
RETURN 1

