;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K112_140
CALL M_KOJO_MESSAGE_COM_K112_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K112_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
			PRINTFORMW 「え…それってどういう…」
			PRINTFORMW 「お゛…ごっ…！」
		ENDIF
	RETURN 1
	ENDIF
	;基本セット
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF TALENT:恋人
			PRINTFORMW 「お゛っ%UNICODE(0x2665) *1%…ごっ%UNICODE(0x2665) *1%…お゛っ%UNICODE(0x2665) *1%…」
			PRINTFORMW （早くっ%UNICODE(0x2665) *1%…出してっ%UNICODE(0x2665) *1%…）
		ELSEIF TALENT:恋慕
			PRINTFORMW 「ん゛っ…ぐっ…う゛っ…」
			PRINTFORMW （あっ%UNICODE(0x2665) *1%…大きくなった%UNICODE(0x2665) *1%…出るのね%UNICODE(0x2665) *1%）
		ELSEIF TALENT:思慕
			PRINTFORMW 「お゛ごっ…う゛っ…ぐっ…」
			PRINTFORMW （まさか…このまま出すの…？）
		ENDIF
	ELSE
		IF TALENT:恋人
			PRINTFORMW 「ん゛お゛っ%UNICODE(0x2665) *1%…お゛っ%UNICODE(0x2665) *1%…ぐっ%UNICODE(0x2665) *1%…」
			PRINTFORMW （あっ%UNICODE(0x2665) *1%…駄目%UNICODE(0x2665) *1%…気持ちいい%UNICODE(0x2665) *1%…）
		ELSEIF TALENT:恋慕
			PRINTFORMW 「う゛っ…ぶっ…お゛っ…」
			PRINTFORMW （こんな%UNICODE(0x2665) *1%…道具みたいに…%UNICODE(0x2665) *1%）
		ELSEIF TALENT:思慕
			PRINTFORMW 「お゛ごっ…う゛っ…ぐっ…」
			PRINTFORMW （く…苦しい…）
		ENDIF
	ENDIF
ENDIF
RETURN 1

