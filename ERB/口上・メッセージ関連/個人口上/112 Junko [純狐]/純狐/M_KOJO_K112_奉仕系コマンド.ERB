;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K112_80
CALL M_KOJO_MESSAGE_COM_K112_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K112_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		IF NOWEX:MASTER:射精
			IF TALENT:恋人
				PRINTFORMW 「あ%UNICODE(0x2665) *1% 出そうなのね%UNICODE(0x2665) *1%」
				PRINTFORMW 「いっぱい出してちょうだい%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「そろそろ出そうなのね%UNICODE(0x2665) *1%」
			ELSEIF TALENT:思慕
				PRINTFORMW （ビクビクしてきたわ…出そうなのかしら）
			ENDIF
		ELSE
			IF TALENT:恋人
				SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「ほらほら%UNICODE(0x2665) *1% 出しちゃってもいいのよ%UNICODE(0x2665) *1%」
				CASE 1
					PRINTFORMW 「%CALLNAME:MASTER%の気持ちいいところは全部知ってるんだから%UNICODE(0x2665) *1%」
				CASE 2
					PRINTFORMW 「ほーら%UNICODE(0x2665) *1% こうすると気持ちいいんでしょ%UNICODE(0x2665) *1%」
				ENDSELECT
			ELSEIF TALENT:恋慕
				SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「ヌルヌルになっちゃってるわ%UNICODE(0x2665) *1%」
				CASE 1
					PRINTFORMW 「気持ちいいのね%UNICODE(0x2665) *1% 顔が緩んでるわよ%UNICODE(0x2665) *1%」
				CASE 2
					PRINTFORMW 「すごく固くて熱いの…%UNICODE(0x2665) *1%」
				ENDSELECT
			ELSEIF TALENT:思慕
				SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「こんな感じで大丈夫かしら…」
					PRINTFORMW 「痛くない…？」
				CASE 1
					PRINTFORMW 「あんまりこういうことしたことないの」
					PRINTFORMW 「うまくできてるといいんだけど…」
				CASE 2
					PRINTFORMW 「気持ちよくなってくれると嬉しいわ」
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

