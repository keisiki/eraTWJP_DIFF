;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K112_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;今日会ってない。
	IF !TCVAR:26:今日会ったか
		;-------------------------------------------------
		;小分岐
		;-------------------------------------------------
		;MASTERが入ってきた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 1
			;怒ってるとき
			IF CFLAG:318 == 1
				PRINTFORMW 「………」
			;屋外かつ満月
			ELSEIF NAME_MOON(DAY:3) == "十五夜 -中秋の名月- " && OPENPLACE(CFLAG:MASTER:現在位置) >= 2
				PRINTFORMW 「………」
				PRINTFORMW 「………」
				PRINTFORMW 純狐は無言で月を眺めている…
			ELSEIF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
				IF RAND:3 == 0
					PRINTFORMW 「あら、ご機嫌よう」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「こんにちは、何か用かしら？」
				ELSE
					PRINTFORMW 「こんにちは、%CALLNAME:MASTER%」
					PRINTFORMW 「今日は何をするの？」
				ENDIF
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「……」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「……何かしら」
				ELSE
					PRINTFORMW 「わざわざ会いに来たの？」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが入ってきた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 2
			;怒ってるとき
			IF CFLAG:318 == 1
				PRINTFORMW 「………」
			ELSEIF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
				IF RAND:3 == 0
					PRINTFORMW 「あら、ご機嫌よう」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「こんにちは、%CALLNAME:MASTER%」
					PRINTFORMW 「会いに来ちゃったわ」
				ELSE
					PRINTFORMW 「こんにちは、%CALLNAME:MASTER%」
					PRINTFORMW 「今日は何をするの？」
				ENDIF
			ELSE
				IF RAND:2 == 0
					PRINTFORMW 「……何かしら」
				ELSE
					PRINTFORMW 「……」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;風呂のやつって浴場で遭遇と違うのかわからん
		;-------------------------------------------------
		;TARGETが風呂に入ってきて一緒に入る
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 3
			PRINTFORMW 「一緒に入ってもいいかしら？」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが風呂に入ってきて出て行った
