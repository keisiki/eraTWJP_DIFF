		;-------------------------------------------------
		IF LOCAL:1 && ARG == 4
			PRINTFORMW 「あら、ごめんなさい」
			PRINTFORMW 「すぐいなくなるから」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが風呂に入ってきて追い出された
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 5
			PRINTFORMW 「私が入ってもいいかしら？」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;外出先で遭遇した
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 6
			;怒ってるとき
			IF CFLAG:318 == 1
				PRINTFORMW 「………」
			;屋外かつ満月
			ELSEIF NAME_MOON(DAY:3) == "十五夜 -中秋の名月- " && OPENPLACE(CFLAG:MASTER:現在位置) >= 2
				PRINTFORMW 「………」
				PRINTFORMW 「………」
				PRINTFORMW 純狐は無言で月を眺めている…
			ELSEIF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
				IF RAND:3 == 0
					PRINTFORMW 「あら、ご機嫌よう」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「こんにちは、何か用かしら？」
				ELSE
					PRINTFORMW 「こんにちは、%CALLNAME:MASTER%」
					PRINTFORMW 「今日は何をするの？」
				ENDIF
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「……」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「……何かしら」
				ELSE
					PRINTFORMW 「わざわざ会いに来たの？」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとデート中に遭遇した
		;ARG:1にデート相手
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
