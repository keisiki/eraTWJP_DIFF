						PRINTFORMW 「うーん…」
						PRINTFORMW 「いいけど…変なことに使わないでね…？」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
						PRINTFORMW 「え…、嫌よ…」
						PRINTFORMW 「何に使うつもりなのよ…」
					ENDIF
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

