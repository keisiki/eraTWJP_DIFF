;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K112_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		IF TALENT:恋人 || TALENT:恋慕
			IF RAND:2 == 0
				PRINTFORMW 「おはよう、%CALLNAME:MASTER%」
				PRINTFORMW 「寝顔がかわいかったからつい眺めちゃったわ」
			ELSE
				PRINTFORMW 「おはよう、%CALLNAME:MASTER%」
				PRINTFORMW 「今日も良い一日になるといいわね」
			ENDIF
		ELSEIF TALENT:思慕
			PRINTFORMW 「おはよう、%CALLNAME:MASTER%」
		ELSE
			PRINTFORMW 「……気にしないで」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人 || TALENT:恋慕
			IF RAND:2 == 0
				PRINTFORMW 「おはよう、%CALLNAME:MASTER%」
				PRINTFORMW 「寝顔がかわいかったからつい眺めちゃったわ」
			ELSE
				PRINTFORMW 「おはよう、%CALLNAME:MASTER%」
				PRINTFORMW 「今日も良い一日になるといいわね」
			ENDIF
		ELSE
			PRINTFORMW 「おはよう、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORMW 「……ん…%CALLNAME:MASTER%…」
			PRINTFORMW 純狐は幸せそうに寝ている。
		ELSE
			PRINTFORMW 「………」
			PRINTFORMW 純狐はぐっすりと寝ている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
			IF RAND:2 == 0
				PRINTFORMW 「おはよう%CALLNAME:MASTER%、早起きね」
			ELSE
				PRINTFORMW 「おはよう%CALLNAME:MASTER%、」
				PRINTFORMW 「…寝坊しちゃったかしら」
			ENDIF
		ELSE
			PRINTFORMW 「……」
		ENDIF
		RETURN 1
	ENDIF
	;その他
	;PRINTFORMW 「あら、おはよう、%CALLNAME:MASTER%」
ENDIF
RETURN 1

