@M_KOJO_EVENT_K112_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰る（定時になって寝る）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「あら、もうこんな時間」
			PRINTFORMW 「それじゃあ、失礼するわね」
			PRINTFORMW 「明日も会えると嬉しいわ」
		ELSEIF TALENT:恋慕 || TALENT:思慕
			PRINTFORMW 「あら、もうこんな時間」
			PRINTFORMW 「それじゃあ、失礼するわね」
		ELSE
			PRINTFORMW 目を離している間に純狐はいなくなっていた。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
			PRINTFORMW 「ふわぁ…眠くなってきたわね…」
		ELSE
			PRINTFORMW 純狐の表情が少しだけ眠そうになっている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会話というよりは寝言？）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			IF RAND:10 == 0
				PRINTFORMW 「大丈夫よ……はく…」
				PRINTFORMW 純狐は寝言を言っているようだ。
				
			;これだけ確率を低くしたかったがこれで合ってるか？
			
			ELSEIF RAND:3 == 0
				PRINTFORMW 「……あなた…」
				PRINTFORMW 純狐は寝言を言っているようだ。
			ELSEIF RAND:2 == 0
				PRINTFORMW 「%CALLNAME:MASTER%…すきよ…」
				PRINTFORMW 純狐は寝言を言っているようだ。
			ELSE
				PRINTFORMW 「…嫦…娥…」
				PRINTFORMW 純狐は寝言を言っているようだ。
			ENDIF
		ELSE
			PRINTFORMW 「…嫦…娥…」
			PRINTFORMW 純狐は寝言を言っているようだ。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;イタズラ中
	;記入チェック（=0, 非表示、1, 表示）
	;LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG == 4
		;PRINTFORMW 
		;RETURN 1
	;ENDIF
	;-------------------------------------------------
	;衰弱中（恋慕付き拠点内キャラがMAIN_MAPで衰弱状態になった場合か、お招き部屋で相手が衰弱した場合に発生
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
			PRINTFORMW 「つ、疲れちゃった、わ…」
		ELSE
			PRINTFORMW 純狐の表情が少しだけ辛そうになっている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（部屋まで運ばれる）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
			PRINTFORMW 「あら、張り切りすぎちゃった？」
		ELSE
			PRINTFORMW 意識が消える瞬間、純狐がこっちを見ているのが見えた。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが自室まで戻ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;そのまま着替えて寝た
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 0
			PRINTFORMW 「おやすみなさい、%CALLNAME:MASTER%」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部屋から追い出された
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
				PRINTFORMW 「ごめんなさい、そろそろ寝たいの」
			ELSE
				PRINTFORMW 「そろそろ寝たいの、出ていってもらえるかしら」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとのＨを止めさせて寝た
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
				PRINTFORMW 「あの、そろそろ寝たいのだけれど…」
			ELSE
				PRINTFORMW 「そろそろ寝たいの、出ていってもらえるかしら」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;非在住TARGETがMAIN_MAPで休憩
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;前のターンから一緒の部屋にいる状態で相手が寝る（お招き部屋含む（
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML
ENDIF
RETURN 1

