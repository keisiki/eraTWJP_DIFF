@M_KOJO_MESSAGE_COM_K69_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	PRINTFORML 
	IF 陥落状態() >= 4
		IF CFLAG:MASTER:現在位置 == CFLAG:70:現在位置 && ((MAIN_MAP != 2 && CFLAG:MASTER:現在位置 == 220) || MAIN_MAP == 2)
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 『今日も仲よさそうですねぇ』
					PRINTFORML 「ふふ、伴侶がおるというのは良いものじゃぞ」
					PRINTFORMW そう言ってマミゾウがしなだれかかってくる
					PRINTFORML 『きっとそうなんでしょうね。私には縁は無さそうですけど』
					PRINTFORML 「ちゃんと着飾れば、嬢ちゃんにも良い相手は出来るさね。こやつほどの相手ではないがの」
					PRINTFORMW そう言って、抱き付いてくるマミゾウに、小鈴が羨ましそうな表情を浮かべている
				CASE 1
					PRINTFORML 「嬢ちゃんには、良い相手はおらんのかの？」
					PRINTFORML 腕を絡めてくるマミゾウの言葉に、小鈴が困ったように笑う
					PRINTFORMW 『私にはまだ早いですよ。それに、かび臭い本なんかに興味持つ男性ってあまりいないし』
					PRINTFORML 「そんなことを言っておると、良縁を逃すぞい？」
					PRINTFORML 『んー、今はソレよりも面白いことも多いですし』
					PRINTFORMW そう言いながらも、羨ましそうに組んでいる腕を見つめてきた
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ふふ、なんじゃ儂の話なぞ、面白いもんでもなかろう」
					PRINTFORML マミゾウの言葉に、そんな事は無いと真顔で告げる%CALLNAME:MASTER%
					PRINTFORML 「ふむ、そういわれるとなんだか、くすぐったいのう」
					PRINTFORMW そう言いながら、僅かに頬を赤らめるマミゾウに、心臓が跳ねた
				CASE 1
					PRINTFORML 「ああ、儂の話を知っておるのか。……しておらんぞ、本当じゃぞ」
					PRINTFORML 何をと、問い掛けられない雰囲気に、ただ苦笑を浮かべる
					PRINTFORML 「む、信じておらんな。アレは逃げる口実を探すためじゃったんじゃからな」
					PRINTFORMW ムキになるあたり、実はと思ってしまう%CALLNAME:MASTER%だった
				CASE 2
					PRINTFORML 「ふふ、長年生きておるが、またこうして連れ添いができるとはのう」
					PRINTFORML そう呟くマミゾウが、僅かに陰の有る笑みを浮かべた
					PRINTFORML 「ん？　なんじゃ、その心配そうな顔は？」
					PRINTFORMW 何でもないと答え、マミゾウに笑顔を返した
			ENDSELECT
		ENDIF
	ELSEIF 陥落状態() >= 2
		IF CFLAG:MASTER:現在位置 == CFLAG:70:現在位置 && ((MAIN_MAP != 2 && CFLAG:MASTER:現在位置 == 220) || MAIN_MAP == 2)
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 『こんにちは、今日はどうされたんですか』
					PRINTFORML 「ふふ、でえとじゃよ、うらやましかろう？　お嬢ちゃんも良い相手をみつけんとの」
					PRINTFORMW そう言って腕を組んでくるマミゾウに、小鈴は苦笑を浮かべている
				CASE 1
					PRINTFORML 「お、嬢ちゃん\@ ((MAIN_MAP != 2 && CFLAG:MASTER:現在位置 == 220) || (MAIN_MAP == 2 && CFLAG:MASTER:現在位置 == 213)) ? そんなめかし込んで、どこに出掛けるつもりじゃ？ # 外で会うのは珍しいのう、どこに行くつもりじゃ？ \@」
					PRINTFORML 『こんにちは。ちょっと友人の家にお呼ばれしてるんです\@ ((MAIN_MAP != 2 && CFLAG:MASTER:現在位置 == 220) || (MAIN_MAP == 2 && CFLAG:MASTER:現在位置 == 213)) ? けど、なにかご入り用ですか？ #  \@』
					PRINTFORMW そう言って笑いあう二人に、なんとなく疎外感を覚える%CALLNAME:MASTER%だった
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「ふふ、お前さんと出会えたのは、良かったことのひとつではあるの」
					PRINTFORMW なぜか頬が熱くなるのを感じながら、マミゾウの言葉に生返事を返した
				CASE 1
					PRINTFORML 「ふむ、まあ儂としては、ここの暮らしは嫌いではないのう」
					PRINTFORMW 楽しげなマミゾウに、なぜか嬉しくなる%CALLNAME:MASTER%だった
				CASE 2
					PRINTFORML 「こちらに来て、確かに良かったとは思うの」
					PRINTFORMW そう呟くマミゾウの目は、どこか遠いところに向いている
			ENDSELECT
		ENDIF
	[SKIPSTART]
	ELSEIF 陥落状態() == 1
		IF CFLAG:MASTER:現在位置 == CFLAG:70:現在位置 && ((MAIN_MAP != 2 && CFLAG:MASTER:現在位置 == 220) || MAIN_MAP == 2)
			SELECTCASE RAND:2
				CASE 0
				CASE 1
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
				CASE 1
				CASE 2
			ENDSELECT
		ENDIF
	[SKIPEND]
	ELSE
		IF CFLAG:MASTER:現在位置 == CFLAG:70:現在位置 && ((MAIN_MAP != 2 && CFLAG:MASTER:現在位置 == 220) || MAIN_MAP == 2)
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 『あ、最近、おもしろい本が入ったんです、よかったらどうですか？』
					PRINTFORMW 「ふむ、見せて\@ CFLAG:MASTER:現在位置 == 280 ? もらう # もらいに行く \@とするか、の」
				CASE 1
					PRINTFORML 『こんにちは、今日も仲よさそうですね』
					PRINTFORMW 「ふふ、実際はどうかわからんが、の」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「ふむ、幻想郷とは面白いところじゃのう」
				CASE 1
					PRINTFORMW 「外の世界？　さほど変わったもんでもないぞい」
				CASE 2
					PRINTFORMW 「正直、外では妖怪よりも人間の方がおそろしいのう」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
