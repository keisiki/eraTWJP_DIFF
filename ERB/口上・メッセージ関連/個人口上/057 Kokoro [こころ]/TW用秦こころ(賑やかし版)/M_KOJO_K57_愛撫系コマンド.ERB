				CALL HPH_PRINT,@"「きゃっHPH　んHPH　そう、この…指、が…HPH　私を、狂わせるんだ……HPH　はぁぁHPH」","W"
			CASE 15
				PRINTFORML こころの身体を愛撫しながら下腹部に指を這わせ、子宮の上から指圧でマッサージする
				PRINTFORML メスの本能に訴える刺激はこころの身体を淫靡に染め上げ、こぼす吐息は徐々に火照った熱を宿していく
				CALL HPH_PRINT,@"「はっHPH　あッHPH　そこ、は……HPH　んッHPH　んんッHPH」","L"
				CALL HPH_PRINT,@"「からだ、が…HPH　HPH　あつ、い…HPH　HPH　あッHPH　HPH　HPH」","W"
			CASE 16
				CALL HPH_PRINT,@"「ん…HPH　お前の指が、私のワレメを擦って……HPH　はぁ…HPH」","L"
				CALL HPH_PRINT,@"「あHPH　んんHPH　えっちな音、してる……HPH」","W"
