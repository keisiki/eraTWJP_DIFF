;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K129_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K129_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K129_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「ん……！んん～～～！」
		PRINTFORMW 「無理やり……おちんぽを突っ込まないでください……%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「んん～～～%UNICODE(0x2665) *1%」
			PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると喉で巧みにそれを促している。
			PRINTFORMW （早く%CALLNAME:MASTER%さんの精子で満たされたいな……%UNICODE(0x2665) *1%）
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ん%UNICODE(0x2665) *1%ん%UNICODE(0x2665) *1%」
			PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると喉で巧みにそれを促している。
		ELSE
			PRINTFORMW 「んんっ%UNICODE(0x2665) *1%ん%UNICODE(0x2665) *1%」
			PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると喉で巧みにそれを促している。
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「んんん～！」
			PRINTFORMW 久侘歌は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んん……ん～～～！！！」
			PRINTFORMW 久侘歌は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ELSE
			PRINTFORMW 「んん～！！！」
			PRINTFORMW 久侘歌は%CALLNAME:MASTER%に陰茎を突っ込まれて呻いている。
		ENDIF
	ENDIF
ENDIF
RETURN 1

