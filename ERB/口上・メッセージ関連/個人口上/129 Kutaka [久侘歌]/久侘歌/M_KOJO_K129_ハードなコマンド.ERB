;==================================================
;150,胸の谷間を犯す
;==================================================
@M_KOJO_MESSAGE_COM_K129_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K129_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K129_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「む、無理やりおっぱいに…！？」
		RETURN 1
	ENDIF
	;基本セット
	;胸3以上
	IF ABL:胸 > 2
		IF RAND:3 == 0
			PRINTFORMW 「おっぱい犯されちゃいました%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW （無理やりされるのもなんだか感じてしまいます……%UNICODE(0x2665) *1%）
		ELSE
			PRINTFORMW 「んん……こんなにおっぱいで擦られるのが好きだなんてとんだおっぱい星人さんで御座いますね♪」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「脈打ってて熱くて……%CALLNAME:MASTER%さんを感じます……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あ、あまり無理やりは……！」
		ELSE
			PRINTFORMW 「んん……少し痛いです……」
		ENDIF
	ENDIF
ENDIF
RETURN 1
