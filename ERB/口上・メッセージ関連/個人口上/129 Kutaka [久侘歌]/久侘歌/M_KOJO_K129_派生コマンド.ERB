@M_KOJO_MESSAGE_SCOM_K129_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「ん……口でご奉仕させていただきます♪」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「んん～～～%UNICODE(0x2665) *1%」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「早くおちんぽ汁を出してください%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……%UNICODE(0x2665) *1%……ちゅっ%UNICODE(0x2665) *1%……出してください%UNICODE(0x2665) *1%」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ%UNICODE(0x2665) *1%」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「口の中%CALLNAME:MASTER%の精で満たしてください……%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「ん～」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「もっと気持ちよくしてあげますね」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「ん……%UNICODE(0x2665) *1%……れろ……%UNICODE(0x2665) *1%」
				PRINTFORMW 「……美味しい……%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「……おちんちん汁いっぱい出してください%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「じゅる%UNICODE(0x2665) *1%れろっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「ん～」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
				PRINTFORMW 「もっと気持ちよくしてあげますね」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んん……」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ELSE
				PRINTFORMW 「んんっ」
				PRINTFORMW 久侘歌は%CALLNAME:MASTER%の射精を感じ取ると舌使いで巧みにそれを促している。
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん……ちゅるる……%UNICODE(0x2665) *1%」
			PRINTFORMW 「……こんな感じでしょうか？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……れろっ……変な味です……」
		ELSE
			PRINTFORMW 「……んん……」
			PRINTFORMW 久侘歌はどこかぎこちなく陰茎を口に含んだ。
		ENDIF
	ENDIF
ENDIF
