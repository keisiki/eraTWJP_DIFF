	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K133_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;今日会ってない。
	IF !TCVAR:133:今日会ったか
		;-------------------------------------------------
		;小分岐
		;-------------------------------------------------
		;MASTERが入ってきた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 1
			;仕事中、今日会ってない。
			IF CFLAG:行動 == 5 && !TCVAR:133:今日会ったか
				PRINTFORMW 「仕事中だ、後にしてくれ」
			ELSE
				;正月、バレンタイン、ハロウィン、クリスマスの場合
				IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
					CALL SPECIALDAY_EVENT_K133
				ELSEIF CFLAG:318 == 1
					PRINTFORMW 「……私は今機嫌が悪いんだ、蹴るぞ」
				ELSE
					;早朝
					IF TIME:2 == 1 || TIME:2 == 2
						PRINTFORMW 「おはようだな、%CALLNAME:MASTER%！」
					;昼
					ELSEIF TIME:2 == 3 || TIME:2 == 4
						PRINTFORMW 「こんにちはだな、%CALLNAME:MASTER%」
					;夕方・夜
					ELSEIF TIME:2 == 5 || TIME:2 == 6
						PRINTFORMW 「こんばんはだな、%CALLNAME:MASTER%」
					ENDIF
					IF TALENT:恋人
						PRINTFORMW 「私に会いに来てくれたのか！嬉しいな！」
					ELSEIF TALENT:恋慕
						PRINTFORMW 「わざわざ会いに来てくれるなんて嬉しいぞ！」
					ELSEIF TALENT:思慕
						PRINTFORMW 「私に会いに来るほど暇なのか？まあ嬉しいがな」
					ENDIF
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが入ってきた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 2
			;仕事中、今日会ってない。
			IF CFLAG:行動 == 5 && !TCVAR:133:今日会ったか
				PRINTFORMW 「仕事中だ、後にしてくれ」
			ELSE
				;正月、バレンタイン、ハロウィン、クリスマスの場合
				IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
					CALL SPECIALDAY_EVENT_K133
				ELSEIF CFLAG:318 == 1
					PRINTFORMW 「……私は今機嫌が悪いんだ、蹴るぞ」
				ELSE
					;早朝
					IF TIME:2 == 1 || TIME:2 == 2
						PRINTFORMW 「おはようだな、%CALLNAME:MASTER%！」
					;昼
					ELSEIF TIME:2 == 3 || TIME:2 == 4
						PRINTFORMW 「こんにちはだな、%CALLNAME:MASTER%」
					;夕方・夜
					ELSEIF TIME:2 == 5 || TIME:2 == 6
						PRINTFORMW 「こんばんはだな、%CALLNAME:MASTER%」
					ENDIF
					IF TALENT:恋人
						PRINTFORMW 「お前の匂いを辿ってみればこんなところに来てしまったぞ！どうしてくれようか？」
					ELSEIF TALENT:恋慕
						PRINTFORMW 「お前の居場所は分かりやすいな！こそこそしてなくて嬉しいぞ！」
					ELSEIF TALENT:思慕
						PRINTFORMW 「ふふん、今日は何をしているんだ？」
					ENDIF
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが風呂に入ってきて一緒に入る
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 3
			IF TALENT:恋人
				PRINTFORMW 「なんだ%CALLNAME:MASTER%か、一緒に洗ってやるぞ！」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「いい機会だ、背中を流してやるよ」
			ELSEIF TALENT:思慕
				PRINTFORMW 「お？先客か？」
				PRINTFORMW 「私は気にしないからお前も気にすること無いぞ！」
			ELSE
				PRINTFORMW 「ん？先に入ってるのか？まあお互い気にすること無いな！」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが風呂に入ってきて出て行った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 4
			IF TALENT:恋人
				PRINTFORMW 「一人の方がいいようだな、失礼した」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ゆっくりしていくといいぞ！私は気が長いからな！」
			ELSEIF TALENT:思慕
				PRINTFORMW 「これは失礼、のぞき趣味は無くてな」
			ELSE
				PRINTFORMW 「悪かったな、気にせず浸かっていてくれ」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが風呂に入ってきて追い出された
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 5
			IF TALENT:恋人
				PRINTFORMW 「そのうち背中を流してやるから気にするな！」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「まだ私の背中を流すには早いぞ！」
			ELSEIF TALENT:思慕
				PRINTFORMW 「残念だが私の湯場だ、出て行ってもらえるかな？」
			ELSE
				PRINTFORMW 「私の裸体に興味なんて無いだろう？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;外出先で遭遇した
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 6
			;仕事中、今日会ってない。
			IF CFLAG:行動 == 5 && !TCVAR:133:今日会ったか
				PRINTFORMW 「仕事中だ、後にしてくれ」
			ELSE
				;正月、バレンタイン、ハロウィン、クリスマスの場合
				IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
					CALL SPECIALDAY_EVENT_K133
				ELSEIF CFLAG:318 == 1
					PRINTFORMW 「……私は今機嫌が悪いんだ、蹴るぞ」
				ELSE
					;早朝
					IF TIME:2 == 1 || TIME:2 == 2
						PRINTFORMW 「おはようだな、%CALLNAME:MASTER%！」
					;昼
					ELSEIF TIME:2 == 3 || TIME:2 == 4
						PRINTFORMW 「こんにちはだな、%CALLNAME:MASTER%」
					;夕方・夜
					ELSEIF TIME:2 == 5 || TIME:2 == 6
						PRINTFORMW 「こんばんはだな、%CALLNAME:MASTER%」
					ENDIF
					IF TALENT:恋人
						PRINTFORMW 「お前の匂いを辿ってみればこんなところに来てしまったぞ！どうしてくれようか？」
					ELSEIF TALENT:恋慕
						PRINTFORMW 「お前の居場所は分かりやすいな！こそこそしてなくて嬉しいぞ！」
					ELSEIF TALENT:思慕
						PRINTFORMW 「ふふん、今日は何をしているんだ？」
					ENDIF
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとデート中に遭遇した
		;ARG:1にデート相手
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 7
			IF TALENT:恋人
				PRINTFORMW 「……」
				PRINTFORMW 「……なんでもない！」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「色男だな！ますます惚れたぞ！」
			ELSE
				PRINTFORMW 「これは失礼、まさか逢い引きとはな」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

;==================================================
;特別な日イベント用関数
;==================================================
;-------------------------------------------------
@SPECIALDAY_EVENT_K133
;-------------------------------------------------
	;謹賀新年
	IF DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1
