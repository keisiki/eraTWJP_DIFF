;==================================================
;脱衣口上
;==================================================
@M_KOJO_EVENT_K133_28(ARG,ARG:1)
;口上主はTARGET
;ARGに脱がし方選択
;ARG:1（ARG=3の場合のみあり）地の文の反応
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;全裸にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「ふふっ、いい筋肉だろ？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「やはり鍛えておくと自分に自信が出るな！」
				ELSE
					PRINTFORMW 「お前に見られても恥ずかしくないように鍛えていたんだぞ？」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;下着姿にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「こっちの方がいいのか？あまり下着の良しあしは分からなくてな……」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「存分に私の筋肉を味わうといい！」
				ELSE
					PRINTFORMW 「どうだ？見事な腹筋だろ？」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;半脱ぎ
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「人間の趣味はわからんな……」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「こっちの方が興奮するのか？」
				ELSE
					PRINTFORMW 「裸の方が楽なんだがな……」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;パンツちょうだい
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
				;-------------------------------------------------
				CASE 0
				;交換です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「%CALLNAME:MASTER%のパンツもくれるならいいぞ！」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「お前のパンツと交換ならいいぞ！大切にするからな！」
					ELSE
						PRINTFORMW 「交換という条件ならいいぞ！」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 1
				;OK（嬉々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「大切にしろよ！」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「私のことをこれで思い出すといいぞ！」
					ELSE
						PRINTFORMW 「変なことに使っても私は寛大だから許すぞ！」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「仕方ないなぁ、%CALLNAME:MASTER%は」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「こんなのでいいのか？わからんな……」
					ELSE
						PRINTFORMW 「大切に履けよ……？」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「嫌だが？」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「そういうのは良くないと思うぞ！」
					ELSE
						PRINTFORMW 「何に使う気だ？」
					ENDIF
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

