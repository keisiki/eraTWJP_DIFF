@M_KOJO_MESSAGE_COM_K109_411_1

#DIM 正解
#DIM 経験値
#DIM 制限時間
#DIM 倍率
#DIM FEINT
#DIM イカサマ
VARSET LOCAL
イカサマ = 0
制限時間 = 2000 - (ABL:[[ドレミー]]:戦闘能力 - ABL:MASTER:戦闘能力) * 100

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特訓デイリーからお借りしました
	PRINTFORMDW あなたは%CALLNAME:TARGET%と弾幕ごっこの練習をすることにした
	SELECTCASE RAND:5
		CASE 0
			PRINTFORMW 「いきますよ。夢符「緋色の悪夢」！ 」
		CASE 1
			PRINTFORMW 「躱せますか？ 夢符「藍色の愁夢」！ 」
		CASE 2
			PRINTFORMW 「さあ、いきますよ。夢符「刈安色の迷夢」！ 」
		CASE 3
			PRINTFORMW 「ついてこれますか？ 夢符「ドリームキャッチャー」！ 」
		CASE 4
			PRINTFORMW 「これを躱せたらたいしたものですよ。月符「紺色の狂夢」！」
	ENDSELECT
	RESETCOLOR
	PRINTFORML 
	CALL ASK_YN("集中する","ちょっとタンマ")
	IF RESULT
		CALL M_KOJO_COLOR_K109
		PRINTFORML 「どうかしましたか？」
		PRINTFORML 「――え？ やっぱり止める？ は、はあ……」
		PRINTFORMDW 急にお腹が痛くなったあなたは弾幕ごっこの練習を中断した
		RESETCOLOR
	ELSE
		正解 = RAND:4
		経験値 = MIN(ABL:[[ドレミー]]:戦闘能力 * 5, 5)
		SELECTCASE RAND:20
			;曖昧
			CASE IS <= 11
				LOCALS:0 = たぶん上
				LOCALS:1 = きっと右
				LOCALS:2 = おそらく左
				LOCALS:3 = やや下
				倍率 = 120
			;ごっちゃ
			CASE 12 TO 15
				LOCALS:0 = 上
				LOCALS:1 = MIGI
				LOCALS:2 = ひだり
				LOCALS:3 = DOWN
				倍率 = 120
			;スペイン語
			CASE 16,17,18
				LOCALS:0 = encima de
				LOCALS:1 = a la derecha
				LOCALS:2 = a la izquierda
				LOCALS:3 = abajo de
				倍率 = 320
			;スワヒリ語
			CASE 19
				LOCALS:0 = juu
				LOCALS:1 = kulia
				LOCALS:2 = kushoto
				LOCALS:3 = chini
				倍率 = 320
		ENDSELECT
		;あなたの戦闘能力が高いと５分の一の確率でフェイントをかけてくる
		IF ABL:MASTER:戦闘能力 > 3 && !RAND:3
			FEINT = 1
			制限時間 += 400
			倍率 += 80
		ELSE
			FEINT = 0
		ENDIF
		CALL FISHER_YATES_SHAFFLE(4)
		PRINTFORMW %CALLNAME:TARGET%の攻撃を避けよう（クリックで開始）
		PRINTFORM %CALLNAME:TARGET%の
		SETCOLOR C_YELLOW
		IF FEINT
			PRINTFORM %LOCALS:(RAND:4)%
			RESETCOLOR
			PRINTFORM と見せかけて%LOCALS:(3 - 正解)%
		ELSE
			PRINTFORM %LOCALS:(3 - 正解)%
			RESETCOLOR
		ENDIF
		PRINTFORML からの攻撃！
		$LOOP_IKASAMA
		FOR LOCAL:10,0,4
			LOCAL = SHAFFLE_ARRAY:(LOCAL:10)
			PRINTFORML [{LOCAL}] - %LOCALS:LOCAL%に避ける
		NEXT
		SIF !イカサマ && BASE:MASTER:TSP > 500
			PRINTFORML [{4}] - 時間停止
		TINPUT 制限時間, -1, 1
		IF RESULT == 正解
			PRINTFORML %CALLNAME:MASTER%は攻撃を回避した！
			CALL M_KOJO_COLOR_K109
			PRINTFORMDL ……………
			PRINTFORMDL ………
			PRINTFORMDW …
			PRINTFORML 「ふぅ……お疲れ様でした」
			PRINTFORMDL 練習を終えたあなたと%CALLNAME:TARGET%は並んでお茶を飲んでいる
			PRINTFORML 「やりますね。私も久々に燃えました」
			PRINTFORML 「またやる時は言ってください。お付き合いしますよ」
			PRINTFORMDW あなたは%CALLNAME:TARGET%に褒めてもらった。
			SIF イカサマ
				PRINTFORMDW 本当は時間を止めてズルをしたんだが……バレてないみたいだ
			RESETCOLOR
			BASE:MASTER:体力 -= 100 * (1 + ABL:[[ドレミー]]:戦闘能力)
			BASE:MASTER:気力 -= 100 * (1 + ABL:[[ドレミー]]:戦闘能力)
			経験値 = 経験値 * 倍率 / 100
			EXP:MASTER:戦闘経験 += 経験値
			EXP:[[ドレミー]]:戦闘経験 += 経験値 * 2
			CFLAG:[[ドレミー]]:好感度 += 経験値 * 2
			CFLAG:[[ドレミー]]:信頼度 += 経験値 * 2
			PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:MASTER%）
			PRINTFORML 戦闘経験 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			PRINTFORML 好感度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			PRINTFORML 信頼度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
		ELSEIF RESULT == 4 && !イカサマ && BASE:MASTER:TSP > 500
			イカサマ = 1
			倍率 /= 5
			PRINTFORMW %CALLNAME:MASTER%は時間停止能力で攻撃を見極めた！
			GOTO LOOP_IKASAMA
		ELSE
			CALL M_KOJO_COLOR_K109
			PRINTFORMDL 失敗した……
			PRINTFORML 「残念。惜しかったですね」
			PRINTFORMDL 回避に失敗したあなたはボロボロになって地面に横たわっている
			PRINTFORML 「またお願いします。私も、結構楽しかったので」
			PRINTFORMDW そう言い微笑む%CALLNAME:TARGET%に、あなたは弱々しくサムズアップを返した
			RESETCOLOR
			BASE:MASTER:体力 -= 500 * (1 + ABL:[[ドレミー]]:戦闘能力)
			BASE:MASTER:気力 -= 500 * (1 + ABL:[[ドレミー]]:戦闘能力)
			PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:TARGET%）
			EXP:[[ドレミー]]:戦闘経験 += 経験値
			CFLAG:[[ドレミー]]:好感度 += 経験値 / 2
			CFLAG:[[ドレミー]]:信頼度 += 経験値 / 2
			PRINTFORML 好感度 ＋{経験値}（%CALLNAME:TARGET%）
			PRINTFORML 信頼度 ＋{経験値}（%CALLNAME:TARGET%）
		ENDIF
	ENDIF
ENDIF
RETURN 1
