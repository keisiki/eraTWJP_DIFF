;==================================================
;150,胸の谷間を犯す
;==================================================
@M_KOJO_MESSAGE_COM_K109_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K109_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
		SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「ぁっHPHそんな、乱暴に…っHPH」","L"
			PRINTFORMDL %CALLNAME:MASTER%はドレミーの胸を寄せ上げるように掴むと、パンパンと腰を打ち付ける
			PRINTFORMDW 同時に指で乳首をこね回してやると、ドレミーは乱暴に胸を犯されているにも関わらずトロ顔を晒し『使われる』幸福に打ち震えた
		CASE 1
			CALL HPH_PRINT,@"「もうっHPH大人しくしてなきゃダメじゃないですかHPH」","L"
			PRINTFORMDL ドレミーはまるでいたずらっ子を窘めるような口調で奉仕を続けている
			PRINTFORMDW 躾とばかりにドレミーが胸をギュウッと押し上げると、竿全体が肉の海に圧迫される快感に%CALLNAME:MASTER%は思わず呻きを上げた
		CASE 2
			CALL HPH_PRINT,@"「んぅっHPHおっぱい強く掴んじゃダメですよ…っHPH」","L"
			PRINTFORMDL 胸で肉棒を扱かれる快感に思わずドレミーの胸を鷲掴みにしてしまう
			PRINTFORMDL ドレミーは口では抵抗しているが、どこか嬉しそうにしている…
			CALL HPH_PRINT,@"「ヘコヘコ腰振りながら私のおっぱいにカウパー擦り付けて…しょうがない人ですねぇ…HPH」","L"
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		CALL AUTO_AEGI(NO:TARGET)
		RETURN 1
	ENDIF
ENDIF
RETURN 1
