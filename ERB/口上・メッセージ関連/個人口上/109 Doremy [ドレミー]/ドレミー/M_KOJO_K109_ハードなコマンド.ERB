;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K109_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K109_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORML 「喉の奥までなんて…そんなの…っオ゛エッ！？」
			PRINTFORMDW %CALLNAME:MASTER%は有無を言わせず喉奥に突き入れた
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「お゛ッ…ごぉ゛ッHPH」","L"
				PRINTFORMDL 射精が近付き、喉奥に突き入れる速度が上がってくるとドレミーは涙と鼻水で顔をぐちゃぐちゃにしながら嘔吐く
				PRINTFORMDW %CALLNAME:MASTER%がそろそろ射精すぞと宣言すると、ドレミーは全身の力を抜き受け入れ態勢に入った
			CASE 1
				CALL HPH_PRINT,@"「お゛ほ゛ッHPHごぁッHPHんぐぅうッ…HPH」","L"
				PRINTFORMDL ドレミーを気遣う余裕の無くなった%CALLNAME:MASTER%は、ただひたすらに喉奥へと肉棒を突き入れる
				PRINTFORMDW 胃がひっくり返るような感覚の連続にドレミーはただ耐えることしかできない…
			CASE 2
				CALL HPH_PRINT,@"「ごッ、ぇ゛エ゛ッ…ん゛こ゛ッ…ごっHPH」","L"
				PRINTFORMDL ぐぽっ、ぐぽっ、とまるでオナホのようにドレミーの喉を犯している
				PRINTFORMDW 射精が近くなった%CALLNAME:MASTER%は、排泄に向けて腰を振る速度を上げた
		ENDSELECT
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「ぶッHPH…ふぅ゛う゛…ぇぐッHPH」","L"
				PRINTFORMDL ドレミーはまるで物のように扱われることに不思議な昂揚感を感じていた
				PRINTFORMDW そんな様子を知ってか知らずか、%CALLNAME:MASTER%はドレミーに視線を向けることなくひたすらに喉奥に突き込んだ
			CASE 1
				CALL HPH_PRINT,@"「お゛ぶッ！？…フーッHPH…がッ…フーッHPH」","L"
				PRINTFORMDL %CALLNAME:MASTER%はドレミーを押し倒すと顔面に跨り、まるで床オナをするかのように喉奥に肉棒を突き入れた
				PRINTFORMDW ドレミーは最初の内は%CALLNAME:MASTER%の尻を掴み暴れていたが、やがて大人しくなった…
			CASE 2
				CALL HPH_PRINT,@"「ぐ、る゛しッ…お゛ッ、ごッ、ごぉッHPH」","L"
				PRINTFORMDL %CALLNAME:MASTER%はドレミーの頭をガッチリとホールドすると、自分だけが気持ち良くなる為に喉を犯していく
				PRINTFORMDW 気持ち良さそうに涎を垂らす%CALLNAME:MASTER%の姿を見て、ドレミーは股を濡らし子宮を疼かせた…
		ENDSELECT
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ッッ…がっ、ぐぷっ、オ゛エッ…」
				PRINTFORMDL 喉奥を犯されているドレミーが嘔吐くが、食道の入り口は肉棒がピッチリと塞ぎ胃液の一滴すら通る隙間はない
				PRINTFORMDW %CALLNAME:MASTER%はドレミーの頭を撫で陰茎を引き抜くと、一気に突き込んだ
			CASE 1
				PRINTFORML 「ん゛ん゛ッ…エ゛ッ…ゴ、エ゛ッ…」
				PRINTFORMDL ドレミーは潰れたカエルのような呻きを上げながら、咽頭に与えられる暴力に耐えている
				PRINTFORMDW 締まりが弱いと思った%CALLNAME:MASTER%が喉をグッと掴むと、ドレミーは半分白目を剥き手をだらんと垂らした
			CASE 2
				PRINTFORML 「ッッ………」
				PRINTFORMDL ドレミーは喉に突き入れられた肉棒の衝撃に目を見開き硬直している
				PRINTFORMDW 嗜虐心が湧いた%CALLNAME:MASTER%が鼻を摘まむと、呼吸を封じられたドレミーはバタバタと暴れた
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ぐ、ぅ……ぶ、ふっ…」
				PRINTFORMDL ドレミーは喉奥に突き入れられた肉棒に目を白黒させている
				PRINTFORMDW %CALLNAME:MASTER%はそんなドレミーの様子を見て少しペースを緩めた
			CASE 1
				PRINTFORML 「ぶえ゛ッ…んごっ…んく…」
				PRINTFORMDL 不意にズンッと喉に突き入れると、ドレミーは豚のような声を上げビクンと身体を跳ねさせた
				PRINTFORMDW %CALLNAME:MASTER%はその様子に満足そうに微笑むと、ピストンを再開した…
			CASE 2
				PRINTFORML 「お゛ぇッ…やっ…ぐぅっ…」
				PRINTFORMDL 乱暴な抽挿に抗議の声を上げようとしたドレミーの食道に陰茎を突き入れ封殺する
				PRINTFORMDW 食道壁に亀頭を擦りつけるように引き抜くと、ドレミーは鼻水を垂らしながら嘔吐いた
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
