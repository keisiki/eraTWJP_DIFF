;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K109_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K109_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K109_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「この格好、お互いの大事なとこが全部見えちゃって……」
		CALL HPH_PRINT,@"「なんというか、恥ずかし……ぁっHPH」","L"
		PRINTFORMDW %CALLNAME:MASTER%がドレミーの秘所を舐め上げると、彼女の腰がビクンと跳ねた
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;舌技術３以上
	ELSEIF ABL:舌 >= 3
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ふぅ、んん……っHPH 私も、イッちゃいそう……HPH」","L"
					CALL HPH_PRINT,@"「ねえ、一緒に……HPH ひゃんっHPH 一緒にイッて……っHPH」","L"
					PRINTFORMDL ムワッとした熱気と共に、ピチャピチャと性器を啜り合う音が辺りに響く
					PRINTFORMDW 互いに腰の抜けるような快感を味わいながら、高みへとのぼっていった
				CASE 1
					CALL HPH_PRINT,@"「ほら……私のおまんこ、見えますよね……？HPH」","L"
					CALL HPH_PRINT,@"「イきたい、イかせてって……ヒクヒクってしてる……HPH」","L"
					PRINTFORMDL ドレミーは左右にゆっくりと腰を動かし、%CALLNAME:MASTER%の視線を釘付けにしている
					CALL HPH_PRINT,@"「ちゃんと私をイかせられたら……そうですね……HPH」","L"
					CALL HPH_PRINT,@"「腰が抜けて立てなくなるくらい、気持ち良く射精させて……んあぁっHPH」","L"
					PRINTFORMDW その言葉を聞いた瞬間、既に限界寸前だった%CALLNAME:MASTER%はドレミーの秘所にむしゃぶりついた
				CASE 2
					CALL HPH_PRINT,@"「ねえ……イきたい……？HPH イきたいの……？HPH」","L"
					CALL HPH_PRINT,@"「それなら、私も気持ち良くして……？HPH」","L"
					PRINTFORMDL グイグイと秘所を押し付けてくるドレミーの求めに応じて舌でナカを穿ってやる
					PRINTFORMDL とめどなく溢れる愛液を口内に溜めゴクリと音を立てて嚥下してやると、ドレミーの膣がキューッと締まった
					CALL HPH_PRINT,@"「く、ぅんっHPH 今ので少しイッちゃった……HPH」","L"
					CALL HPH_PRINT,@"「ちゃんと気持ち良くさせてくれたので……私も、イかせてあげますね……ちゅっ、ぢゅぽっHPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ぢゅぷっ、じゅる……っHPH…あ、んぅっHPH」","L"
					PRINTFORMDL %CALLNAME:MASTER%の激しい責めにドレミーは思わず肉棒から口を離し見悶えた
					CALL HPH_PRINT,@"「もう、加減してくださいよ……HPH %MASTERNAME:109%のを気持ち良くできないじゃないですか……HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「ふ……っHPH ん……っHPH ぢゅずる……っHPH」","L"
					PRINTFORMDL ドレミーの激しい責めに思わず奉仕するのも忘れてしまう
					CALL HPH_PRINT,@"（ふふ……HPH %MASTERNAME:109%ったら、私のお口にヘコヘコ腰振っちゃって……HPH）","L"
					PRINTFORMDW 腰を小刻みに振る%CALLNAME:MASTER%の姿に気を良くしたドレミーはさらに激しく責め立てていった…
				CASE 2
					CALL HPH_PRINT,@"「ふ……っHPH れろ……ちゅぽ……っHPH」","L"
					CALL HPH_PRINT,@"「んはぁ……っHPH %MASTERNAME:109%、気持ち良いですか……？HPH」","L"
					CALL HPH_PRINT,@"「…ふふ、そうですか……HPH 私も、気持ち良くて……HPH」","L"
					CALL HPH_PRINT,@"「ほら……見て……HPH アソコが、キュッキュッてなっちゃってるの……HPH」","L"
					PRINTFORMDW 『よく見える』%CALLNAME:MASTER%がそう言うとドレミーの蜜穴から大量の愛液が溢れ出てきた
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ふふ、%MASTERNAME:109%のココ……キューって縮みあがってますよ……HPH」","L"
					PRINTFORMDL ドレミーが肉棒から口を離し、指先で陰嚢を撫で上げる
					CALL HPH_PRINT,@"「これ、もう射精そうって合図ですよね……HPH」","L"
					CALL HPH_PRINT,@"「それじゃあ、気持ち良くしてあげないと……HPH あむ……ぢゅる、ぢゅっHPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「ふふ、もう射精そうですか……？HPH」","L"
					CALL HPH_PRINT,@"「それならほら……んっHPH 私もイかせてください……HPH」","L"
					PRINTFORMDL %CALLNAME:MASTER%の奉仕が止まっているのを責めるようにドレミーが秘所を顔に押し付けてくる
					CALL HPH_PRINT,@"「クスッ、苦しい……？HPH でも、そっちの方が気持ち良いですよね……？HPH」","L"
					CALL HPH_PRINT,@"「ぁんっHPH その調子……HPH 頑張ったご褒美にイかせてあげますね……HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「ん、むぅっHPH そんな、激しくしたらいけませんよ……HPH」","L"
					CALL HPH_PRINT,@"「気持ち良くて、集中できなくて……%MASTERNAME:109%のこと、イかせてあげられない……っHPH」","L"
					PRINTFORMDL %CALLNAME:MASTER%はドレミーの秘所を貪りながら、いいから咥えろとばかりに腰を突き上げる
					CALL HPH_PRINT,@"「ぅ、んん……っHPH ぁ、はぁあ……っHPH 本当に、しょうがないひと……っHPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL 愛液を味わいながら、軽く腰を浮かして肉棒をドレミーの口に押し込む
					CALL HPH_PRINT,@"「ふぐっ！？ んん………んぐ……っHPH」","L"
					CALL HPH_PRINT,@"「ぷ、ぁ……ぢゅる……ぢゅるる……っHPH」","L"
					PRINTFORMDW ドレミーは少しえずきながらも肉棒に奉仕を始めた
				CASE 1
					CALL HPH_PRINT,@"「そんな、音立てたら、ぁ……っHPH はぁ、ぁあっHPH」","L"
					PRINTFORMDL ドレミーの腰をガッチリとホールドし、ぢゅるぢゅると愛液を吸い上げる
					PRINTFORMDW その激しさにドレミーはすっかり奉仕を忘れ、腰を震わせ喘ぎよがった……
				CASE 2
					PRINTFORMDL ドレミーに肉棒を舐めさせながら、秘貝に舌を這わせていく
					CALL HPH_PRINT,@"「ん……っHPH れる……っHPH じゅ……っHPH」","L"
					PRINTFORMDW 快感に打ち震えながらも、ドレミーの奉仕は止まらなかった
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
