@M_KOJO_MESSAGE_SCOM_K109_3_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF TALENT:処女
			PRINTFORML 「%MASTERNAME:109%、それ以上は……ッ痛」
			PRINTFORMDL ドレミーの処女膣を%CALLNAME:MASTER%の指が往復する
			PRINTFORML 「ねえ、お願い……本当に破れちゃうから……ッ！？」
			PRINTFORMW 「ぅ、くぅ……ッ どうして聞いてくれないのですか……ッ」
		ELSE
			CALL HPH_PRINT,@"「っあ！？HPH な……何を……っHPH」","W"
			PRINTFORMDW %CALLNAME:MASTER%は指をクイッと曲げ、膣内のザラザラとした所を小刻みに擦る
			PRINTFORMDW 強すぎる快感にドレミーは何度も腰を跳ねさせ、トプトプと愛液を溢れさせる…
			CALL HPH_PRINT,@"「待ってっHPH まってまってまってッッHPH これ、ダメ……っHPH ダメだから……っHPH」","W"
			PRINTFORMDW 腰をガッシリと固定し執拗に擦り上げると、ドレミーは頭を抱え声にならない嬌声を上げ続ける
			PRINTFORMDW 暫し女の弱点で遊んだ%CALLNAME:MASTER%が指を引き抜くと、女陰はトロトロに蕩けとめどなく蜜を滴らせていた…
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;処女
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TALENT:処女
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「痛ッ、ぁ、う……っ」
				PRINTFORML 「お願い……もう少し優しくして……っ」
				PRINTFORMDW Gスポットを刺激しようと試みるが、ドレミーが痛がりそれどころではなかった
			CASE 1
				PRINTFORML 「……いくらほぐす為とはいえ……」
				PRINTFORML 「初めてが指というのは……少し……」
				PRINTFORMDL 本当に破いてしまうわけじゃない。%CALLNAME:MASTER%はそう言い聞かせながらドレミーの膣内を愛撫する
				PRINTFORML 「……ん……ぅッ」
				PRINTFORMDL ドレミーが痛みに顔を顰めたので指を抜くと、少しだけ血が付着していた
				PRINTFORMDW うっかり膜を傷付けてしまったようだ……気を付けなければ
			CASE 2
				PRINTFORML 「う、嘘……血が……」
				PRINTFORML 「まさか……破いてしまったのですか……？」
				PRINTFORMDW 粘膜を傷付けてしまっただけだ。%CALLNAME:MASTER%は適当に誤魔化した
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;バイブ挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:バイブ
		;Ｖ感覚６以上
		IF ABL:Ｖ感覚 >= 6
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「んふぅッHPH %MASTERNAME:109%ッHPH 待ってッHPH 止めてッHPH」","L"
					PRINTFORMDL 小刻みに蠢くバイブがドレミーのＧスポットを揺らす
					PRINTFORMDL いくら叫んでも、機械は無情に女の弱点を攻撃し続けた
					CALL HPH_PRINT,@"「ぉ、おッHPH ダメッHPH イくッHPH イクイクイグッHPH HPH HPH」","W"
				CASE 1
					PRINTFORMDL %CALLNAME:MASTER%はバイブを限界まで引き抜いた後、振動のスイッチを最強に切り替えた
					PRINTFORMDL そしてそのまま、膣の天井を擦るようにゆっくりと押し戻していく……
					CALL HPH_PRINT,@"「あ、来ちゃう……ッHPH バイブが、絶対イッちゃうところに届いちゃう……ッHPH」","L"
					CALL HPH_PRINT,@"「……ッぁHPH ぉ―――ッHPH HPH HPH は、ぁああッHPH」","W"
					PRINTFORMDW 振動するバイブがＧスポットを擦った瞬間、ドレミーは盛大に潮を噴きながらアクメに達した……
				CASE 2
					CALL HPH_PRINT,@"「ねぇ……ねえッHPH %MASTERNAME:109%ッHPH」","L"
					CALL HPH_PRINT,@"「そんな浅いトコじゃなくて……気持ち良くなれるトコ、擦って……っHPH」","L"
					CALL HPH_PRINT,@"「めちゃくちゃにイきたいの……ッHPH ぁ、はぁああんっHPH HPH HPH」","L"
					PRINTFORMDW お望み通りＧスポットを擦り上げてやると、ドレミーは壊れたように腰を痙攣させながら盛大にイきまくった……
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ん、はぁ……っHPH 振動が、伝わってくる……っHPH」","L"
					PRINTFORMDL 指では決して味わえない振動で女の弱点を揺すられ、ドレミーは悩ましげに腰をくねらせている
					PRINTFORMDL %CALLNAME:MASTER%は悪戯してみたくなり、振動の段階を一つ上げてみた
					CALL HPH_PRINT,@"「ぉ、あッHPH や、やめてッHPH 弱めてッHPH ぅんんッHPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「こんなものより、私は%MASTERNAME:109%の……っHPH」","L"
					PRINTFORMDL 膣内に挿入したバイブを焦らすようにゆっくりと動かしていく
					PRINTFORMDL ドレミーは複雑な様子だが、Ｇスポットを重点的に擦ってやるとすぐに表情が蕩けた
					CALL HPH_PRINT,@"「ふ、ぅう……っHPH でも、これ……ちょっと良いかも……っHPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「やっ、ただでさえ敏感なんですから、そこは……っHPH」","L"
					CALL HPH_PRINT,@"「ぐ、ぅっHPH んんん～～～ッッHPH」","L"
					PRINTFORMDL いつもは飄々としているドレミーが快楽に翻弄され可愛らしく喘いでいる
					PRINTFORMDW その光景が%CALLNAME:MASTER%を非常に昂らせた……
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;Ｖ感覚６以上
	ELSEIF ABL:Ｖ感覚 >= 6
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「はぁ、ん……っHPH 気持ち良くて……おかしくなっちゃいそう……ッHPH」","L"
				PRINTFORMDL まだまだ余裕そうだ。%CALLNAME:MASTER%はドレミーの下腹部を押さえＧスポットを執拗に刺激した
				CALL HPH_PRINT,@"「あ、ひ、待ってっHPH ほんと、おかしく……ッHPH」","L"
				CALL HPH_PRINT,@"「ぅ、ぐ、んぅんぅうっHPH HPH HPH」","L"
				PRINTFORMDW あまりの快感にドレミーはのたうちながら獣のような嬌声を上げている……
			CASE 1
				CALL HPH_PRINT,@"「私のココ……ただでさえ敏感だったのに……HPH」","L"
				CALL HPH_PRINT,@"「%MASTERNAME:109%に開発されたせいで……おかしくなっちゃいましたね……HPH」","L"
				PRINTFORMDL ドレミーはそう言うと、どこか満足そうにＧスポットの辺りを撫でた
				CALL HPH_PRINT,@"「ほら……触って……HPH いつもみたいに、めちゃくちゃにイかせて……っHPH」","W"
			CASE 2
				CALL HPH_PRINT,@"「んっHPH そこ、感じちゃう……っHPH」","L"
				CALL HPH_PRINT,@"「もっと……もっと擦って……っHPH」","L"
				PRINTFORMDL Ｇスポットへの愛撫がお気に召したのか、ドレミーは腰を揺らして続きを求めてきた
				CALL HPH_PRINT,@"「ひぁ、あっHPH すごっHPH 気持ち良い……っHPH」","W"
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「そ、そこ……ザラザラってしてるの分かりますか……？HPH」","L"
				CALL HPH_PRINT,@"「そこが……私が一番気持ち良くなれる場所……んぁっHPH」","L"
				CALL HPH_PRINT,@"「そうっHPH そうやって、トントンって優しくして……っHPH」","W"
			CASE 1
				CALL HPH_PRINT,@"「あんまり乱暴にしてはダメですよ……？HPH」","L"
				CALL HPH_PRINT,@"「刺激が強すぎても苦しいだけなので……んっHPH」","L"
				PRINTFORMDL %CALLNAME:MASTER%は指の腹で軽く触れるようにドレミーのザラザラとした天井を擦り上げた
				CALL HPH_PRINT,@"「あっHPH そのまま、続けてください……っHPH」","W"
			CASE 2
				CALL HPH_PRINT,@"「ぁ、やぁっHPH こんなの、すぐイッちゃう……ッHPH」","L"
				PRINTFORMDL Ｇスポットを刺激され感じまくっているドレミーは自らの痴態に思わず顔を覆ってしまう
				PRINTFORMDW %CALLNAME:MASTER%がクチュクチュとわざとらしく音を立ててやるとドレミーは悲鳴のような嬌声を上げた
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
