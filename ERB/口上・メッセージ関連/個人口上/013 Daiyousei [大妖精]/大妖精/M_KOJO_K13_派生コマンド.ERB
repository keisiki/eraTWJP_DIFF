;==================================================
;5,手淫フェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K13_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K13_5_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K13_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
			;時姦中
			IF FLAG:70
				PRINTFORMW 
			ELSE
				PRINTFORMW 「こう…ですか…あむっ」
				PRINTFORMW 「んむっ…ふぅ…れろ…」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…ちゅ……%UNICODE(0x2665) *1%」
			PRINTFORMW 「…ん…出るの…？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「はむっ、んちゅ…」
		ELSE
			PRINTFORMW 「レロレロ…じゅぷ…」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「…んぐぅ…うぅ…んっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ちゅるっ……ン%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「ズッ、ヂュバッ%UNICODE(0x2665) *1%」
			PRINTFORMW 「れりゅれりゅ…%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「んっ……ぐっ……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んぐっ…うぇ…」
			PRINTFORMW 「口が疲れちゃいます…」
		ELSE
			PRINTFORMW 「んん…れろ…」
			PRINTFORMW (変な味がするよぉ…）
		ENDIF
	ENDIF
ENDIF
