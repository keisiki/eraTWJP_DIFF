;==================================================
@M_KOJO_MESSAGE_COM_K13_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K13_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K13_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 
			ELSE
				PRINTFORMW 大妖精は怯えながらも%CALLNAME:MASTER%の男根を幼い手で包み込んだ。
				PRINTFORMW 「ヒッ…」
				PRINTFORMW 大妖精は怯えるあまり勢いあまって男根を握り潰しかけた…。
				PRINTFORMW 「ご、ごめんなさい。加減がわからなくて…」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「あ…なんだか震えてます…」
			PRINTFORMW 「射精しそうなんですね。んっ…んっ…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「はち切れそう…」
			PRINTFORMW 大妖精は両手で%TINKO(PLAYER)%を包み込みしごくペースをあげていく。 
		ELSE
			PRINTFORMW 大妖精は顔を蒸気させながら一心腐乱に%TINKO(PLAYER)%を擦りあげる。
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「切ない顔してかわいそう…今楽にしてあげますからね」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「先っぽを強くコスってあげると…ビクッて動くんですね」
			PRINTFORMW 「フフ、カワイイ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 大妖精は指先を絡みつかせ%TINKO(PLAYER)%を刺激する…。
			PRINTFORMW 「もうちょっとガンバってくださいね…」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「早く…私に欲しいな…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ビュッビュッてセーシ出してあげますからね%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「根元も亀頭もぜーんぶいっぱいしごいてあげます%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ま、まだでないんですか？」
			PRINTFORMW 「は、早く出してください…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「やあぁ…怖い」
			PRINTFORMW 「こ、こっちに向けないください…」
		ELSE
			PRINTFORMW 大妖精はつたない手つきで%TINKO(PLAYER)%を擦っている。
			PRINTFORMW 快感で%TINKO(PLAYER)%が跳ねつくたびに大妖精の目に涙が溜まっていく。
		ENDIF
	ENDIF
ENDIF
RETURN 1

