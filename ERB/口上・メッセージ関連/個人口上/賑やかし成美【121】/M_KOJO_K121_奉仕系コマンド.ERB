;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K121_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K121_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K121_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「……………」
		PRINTFORML 成美が合わせた両の手に、肉棒を突き入れた……
		PRINTFORMW 成美は見て見ぬふりをしている……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		;この後射精する(射精寸前の描写)
		IF NOWEX:PLAYER:射精
			CALL HPH_PRINT,@"「…………出していいよ、%CALLNAME:MASTER%……HPH」","L"
			PRINTFORMW 成美は小声で呟くと、両手に力を籠めた……。
		ELSE
			PRINTFORML 「………………♪」
			PRINTFORMW 成美が手をすりすりとする度に、射精欲が高まって行く……
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

