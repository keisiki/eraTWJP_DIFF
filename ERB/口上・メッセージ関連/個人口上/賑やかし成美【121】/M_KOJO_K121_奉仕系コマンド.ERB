	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「…………」
		PRINTFORML 地蔵をしている成美の口に、肉棒を挿入した…… 
		PRINTFORMW 成美は戸惑いながらも、地蔵を続けるつもりのようだ……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「…………」
		PRINTFORMW 地蔵として微動だにしない成美だが、口内では舌が淫らにうねっている……。
		IF NOWEX:PLAYER:射精
			CALL HPH_PRINT,@"「んむっ……HPH………………こくん…………」","L"
			PRINTFORMW 成美の喉が静かに鳴った……。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

