							LOCALS += "はぁ…"
						CASE 6
							LOCALS += "ふぅ"
						CASE 7
							LOCALS += "………"
						CASE 8
							LOCALS += "ん…"
					ENDSELECT
					SELECTCASE RAND:4
						CASE 0
							LOCALS += "…"
						CASEELSE
							LOCALS += K53_ハート付与()
					ENDSELECT
				ENDIF
			ENDIF
		;イラマチオ
		CASE 8
			IF K53_PVOICE_FRAG:8
				SELECTCASE RAND:9
					CASE 0
						LOCALS += "おぼぅっ"
					CASE 1
						LOCALS += "んぶっ"
					CASE 2
						LOCALS += "んぼっ"
					CASE 3
						LOCALS += "うぶっ"
					CASE 4
						LOCALS += "うぶぅっ"
					CASE 5
						LOCALS += "むっ"
					CASE 6
						LOCALS += "むぅ"
					CASE 7
						LOCALS += "むぶっ"
					CASE 8
						LOCALS += "おぶっ"
				ENDSELECT
				IF CFLAG:睡眠
					SELECTCASE RAND:5
						CASE 0
							LOCALS += "？"
						CASE 1
							LOCALS += "…"
						CASEELSE
							LOCALS += K53_ハート付与()
					ENDSELECT
				ELSE
					SELECTCASE RAND:5
						CASE 0
							LOCALS += "！"
						CASE 1
							LOCALS += "！？"
						CASEELSE
							LOCALS += K53_ハート付与()
					ENDSELECT
				ENDIF
			ENDIF
		;声がまん・口ふさぎ
		CASE 9
			IF K53_PVOICE_FRAG:9
				;刺激が大きい場合キス感覚外処理
				IF K53_刺激LV("快感") >= 2 || K53_刺激LV("苦痛") >= 1 || K53_刺激LV("反感") >= 1
					SELECTCASE RAND:9
						CASE 0
							LOCALS += "んっんっんっ"
						CASE 1
							LOCALS += "んっ"
						CASE 2
							LOCALS += "んむぅ～～！？"
						CASE 3
							LOCALS += "んんっ！"
						CASE 4
							LOCALS += "んうぅぅ～～っ"
						CASE 5
							LOCALS += "んんんっ！？"
						CASE 6
							LOCALS += "んぶっ"
						CASE 7
							LOCALS += "んむぅっ"
						CASE 8
							LOCALS += "～～～っ"	
					ENDSELECT
					SELECTCASE RAND:2
						CASE 0
							LOCALS += K53_ハート付与()
					ENDSELECT
				ELSEIF K53_刺激LV("快感") >= 0
					SELECTCASE RAND:9
						CASE 0
							LOCALS += "ん"
						CASE 1
							LOCALS += "んっ"
						CASE 2
							LOCALS += "ん～"
						CASE 3
							LOCALS += "んぅっ"
						CASE 4
							LOCALS += "ふっ"
						CASE 5
							LOCALS += "ふぅっ"
						CASE 6
							LOCALS += "っぅ"
						CASE 7
							LOCALS += "っふ"
						CASE 8
							LOCALS += "うぅぅっ"	
					ENDSELECT
					SELECTCASE RAND:2
						CASE 0
							LOCALS += K53_ハート付与()
						CASE 1
							LOCALS += "…"
					ENDSELECT
				ENDIF
			ENDIF
	ENDSELECT
	
	;文字数が一定に達したら終了
	SIF STRLENSU(LOCALS) > K53_NUM
		BREAK
REND

;*********************************************************************************
;あえぎ声描画
PRINTFORML 「%LOCALS%」
PRINTW


