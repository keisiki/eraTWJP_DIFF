			PRINTFORML 典が何やら怪しい液体を口移しで飲ませてきた
			PRINTFORML 疲れていた身体が軽くなっていく…
			PRINTW 
			BASE:MASTER:体力 = MAXBASE:MASTER:体力
			BASE:MASTER:気力 = MAXBASE:MASTER:気力
			BASE:MASTER:精力 = MAXBASE:MASTER:精力
			BASE:MASTER:勃起 = MAXBASE:MASTER:勃起
			BASE:体力 = MAXBASE:体力
			BASE:気力 = MAXBASE:気力
			SETCOLOR 0xFF69B4
			CALL HPH_PRINT,@"「これでまた頑張れますよねHPH」","L"
			CALL HPH_PRINT,@"「さあHPH　子作り再開しましょうHPH」","L"
			PRINTW 
			TCVAR:360 += 1
		ELSE
			IF TCVAR:発情
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「まだ終わっちゃ嫌ですぅHPH　もっとしましょうよぉHPHHPH」","L"
				IF HAS_PENIS(MASTER)
					CALL HPH_PRINT,@"「がんばれHPH　がんばれっHPH　おちんちんがんばれっHPH」","L"
					RESETCOLOR
					PRINTFORML 典が半勃ちになったペニスにキスの雨を降らせてきた…
				ENDIF
				PRINTW 
			ELSE
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「ふふ、この甘ボッキしたおちんちん触るの、大好きです…%UNICODE(0x2665) *1%」
					PRINTFORML 典は半勃ちのペニスを手の平に乗せて遊んでいる…
					PRINTW 
				CASE 1
					PRINTFORML 「お疲れですか？　少し、休憩いたしましょう」
					PRINTFORML 典は優しい表情で%CALLNAME:MASTER%の頭を撫でてくれている
					PRINTW 
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「ふふ、ゆっくりと楽しみましょうね」
		CASE 1
			PRINTFORML 「おやおや、もう休憩ですか？」
		ENDSELECT
		PRINTW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

