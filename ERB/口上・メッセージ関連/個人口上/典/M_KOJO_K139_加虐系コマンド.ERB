;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K139_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K139_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:発情
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
			ELSE
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「ひぃんっHPH　いぎっHPH　いぃっHPHHPH」","L"
				RESETCOLOR
				PRINTFORML パンッ、パンッと乾いた音が響くたび、典の嬌声が響く
				PRINTW 
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「いたいのにHPH　いたいのにきもちいいですっHPH　なんでぇHPH」","L"
				RESETCOLOR
				PRINTFORML 発情によって極限まで性感が高まった典の体は、痛みすらも快楽に強制的に塗り替えていく
				PRINTFORML 尻を叩かれる衝撃は快楽の波としてま、子宮にまで到達していた
				PRINTW 
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「だめっHPH　たたかないでっHPH　いたいっHPH　あぐっHPH　あぁーっHPH　あぁーっHPH」","L"
				RESETCOLOR
				PRINTW 
			ENDIF
		ELSE
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
				IF ABL:マゾっ気 >= 2
					CALL HPH_PRINT,@"「こおぉんっHPH」","W"
					PRINTFORML 「あ、今のは…違うの…お姉ちゃん、別に気持ちよくなんか…」
					PRINTW 
					PRINTFORML %CALLNAME:MASTER%は無言で手を振り下ろした
					CALL HPH_PRINT,@"「いひいぃっHPH」","W"
					CALL HPH_PRINT,@"「だっ、だめっHPH　痛いからっ、やめっ、やめなさっ、ひぃんっHPH」","L"
					PRINTW 
				ELSE
					PRINTFORML 「こぉんっ！！？」
					PRINTFORML 「ちょ、ちょっと…お尻を叩くのは…こおぉんっ！！」
					PRINTFORML 「お姉ちゃん、怒ります、よぉっ！」
					PRINTFORML 「痛っ、痛いからっ、ぶたないでっ、あっ！」
					PRINTW 
				ENDIF
			ELSE
				IF ABL:マゾっ気 >= 2
					CALL HPH_PRINT,@"「こおぉんっHPH」","W"
					PRINTFORML 「あ、今のは違うんです、気持ちよく、なんかこぉぉぉぉん！！？」
					PRINTW 
					PRINTFORML 二発、三発と典の尻を叩くたびに、丸い尻に紅葉がいくつも浮かび上がっていく
					CALL HPH_PRINT,@"「だめっHPH　おしりたたいちゃだめっHPH　いたいのやですっHPH　こぉぉぉん！！」","L"
					PRINTW 
				ELSE
					PRINTFORML 「こぉんっ！！？」
					IF GROUPMATCH(TALENT:MASTER:体型,2,3)
						PRINTFORML （痛い痛い痛いっ！！？　本気で痛いっ！！！）
						PRINTW 
						PRINTFORML バシィィィィィン！
						PRINTFORML 「いぎいぃ！！？」
						PRINTW 
						PRINTFORML 「あ、あ…%CALLNAME:MASTER%どの、おやめください…」
						PRINTFORML 典は目の端に涙をため、本気でガタガタと震えている
						PRINTW 
					ELSE
						PRINTFORML 「ちょ、ちょっと…お尻を叩くのは…こおぉんっ！！」
						PRINTFORML 「痛っ、おやめくださいっ、やめっ、こぉぉんっ！」
						PRINTW 
					ENDIF
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

