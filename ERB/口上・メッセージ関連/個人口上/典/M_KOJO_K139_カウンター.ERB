				ENDSELECT
			CASE 2
				CALL HPH_PRINT,@"「どうしましたっHPH　腰が浮いてますよっHPH」","L"
			ENDSELECT
			RESETCOLOR
			PRINTW 
		ENDIF
	ENDIF
;発情してない
ELSE
	IF TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行
		IF TCVAR:MASTER:イきそう == 1
			CALL HPH_PRINT,@"（可愛い…HPH　我慢してるっHPH）","L"
			IF SOURCE:快Ｖ >= 7000
				CALL HPH_PRINT,@"「もう少し、頑張ってHPH　お姉ちゃんも、もうすぐイきますからっHPH」","L"
			ELSE
				SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ほらHPH　頑張ってくださいHPH」","L"
					CALL HPH_PRINT,@"「そんなよわよわちんちんじゃ、お姉ちゃんをイかせられませんよHPHHPH」","L"
				CASE 1
					CALL HPH_PRINT,@"（歯をくいしばって一生懸命腰振ってる%CALLNAME:MASTER%どの、可愛い…HPH）","L"
					CALL HPH_PRINT,@"（ほらっHPH　もっと典のために腰を振ってHPH　お姉ちゃんを%CALLNAME:MASTER%どののチン堕ちさせてっHPHHPH）","L"
				CASE 2
					CALL HPH_PRINT,@"「まだイったらダメですよっHPH　お姉ちゃんをもっと気持ちよくしてくださいっHPH」","L"
					CALL HPH_PRINT,@"（ああっHPH　涙目の%CALLNAME:MASTER%どのっHPH　可愛いっ、可愛いっHPHHPH　もっと苛めたいっHPHHPH）","L"
				ENDSELECT
				PRINTW 
			ENDIF
		ELSE
			SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「ふふHPH　涙目になって、可愛い…HPH」","L"
				PRINTFORML 典は%CALLNAME:MASTER%の上で腰を振りながら、乳首を指でコリコリと責め立てている…
				IF TALENT:MASTER:性別 == 2
					CALL HPH_PRINT,@"「男のだって、乳首で感じるんですよHPH　ほら、この通りHPH」","L"
					IF TALENT:MASTER:Ｂ感度 == 1
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%どのは敏感すぎますけどねHPH　ほらHPH　またおちんちんが跳ねたHPHHPH」","L"
					ENDIF
				ENDIF
				PRINTW 
			CASE 1
				CALL HPH_PRINT,@"「こうやってぇ、おちんちんの先っぽだけ、にゅこにゅこされるの好きですかHPH」","L"
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%どののおちんちん、とっても気持ちいいですHPH」","L"
			CASE 2
				CALL HPH_PRINT,@"「出たり入ったりするところ、見えますか？HPH」","L"
				CALL HPH_PRINT,@"「お姉ちゃんと%CALLNAME:MASTER%どの、セックスしているんですよHPH」","L"
			ENDSELECT
		ENDIF
	;子供じゃない
	ELSE
		IF TCVAR:MASTER:イきそう == 1
			CALL HPH_PRINT,@"「%CALLNAME:MASTER%どののイくときの顔、好きです…HPH　とっても、可愛いんですもの…っHPH","L"
			IF SOURCE:快Ｖ >= 7000
				CALL HPH_PRINT,@"「典もっ、もうすぐイきますっHPH　一緒に、一緒にっHPHHPH」","L"
			ELSE
				CALL HPH_PRINT,@"「あらあら、もうイきそうなんですか？HPH」","L"
				CALL HPH_PRINT,@"「ふふ、%CALLNAME:MASTER%どののおちんちんは堪え性のない雑魚ちんぽですねぇHPH","L"
				CALL HPH_PRINT,@"「ざぁこHPH　ざぁこHPH　管狐ごときに無様にイかされちゃえHPHHPH」","L"
			ENDIF
		ELSE
			CALL HPH_PRINT,@"「あっHPH　もっとHPH　強く突いてっHPHHPH」","L"
			CALL HPH_PRINT,@"「%CALLNAME:MASTER%どののちんぽっHPH　ちんぽ好きぃっHPHHPH」","L"
		ENDIF
	ENDIF
ENDIF
