;==================================================
;50,ゴム装着
;==================================================
@M_KOJO_MESSAGE_COM_K139_50
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K139_50_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_50_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:恋人
		IF TCVAR:発情
			SETCOLOR 0xFF69B4
			CALL HPH_PRINT,@"「えぇ～HPH　こっちは授精する気満々ですのにぃHPH　生でしましょうよぉHPHHPH」","L"
			RESETCOLOR
		ELSE
			IF TALENT:妊娠願望
				PRINTFORML 「えぇ～～、二人で子作りしようって決めたじゃないですかぁ」
			ELSE
				IF TALENT:MASTER:万能孕ませ || TALENT:MASTER:妖怪孕ませ
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%どのの種なら、典など一発で孕んでしまいますものね…HPH」","L"
					SIF TALENT:MASTER:濃厚精液
						CALL HPH_PRINT,@"「でも、ゴムが破けてしまわないか、典は心配です…HPH」","L"
				ELSE
					SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「子作りはせずに、純粋にセックスを愉しみたいのですね…HPH」","L"
						CALL HPH_PRINT,@"「ふふ、一箱使い切るくらいたくさんしましょうHPH」","L"
					CASE 1
						CALL HPH_PRINT,@"「典は%CALLNAME:MASTER%どのの子を身籠っても困りはしませんが…HPH」","L"
						PRINTFORML 「%CALLNAME:MASTER%どのはそうはいきませんよね」
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
		PRINTW 
		RETURN 1
	;それ以外
	ELSE
		IF TEQUIP:PLAYER:コンドーム
			IF ESTRUS_CYCLE(TARGET) == 15
				PRINTFORML 「今日は生でも大丈夫ですのに…妊娠が気になりますか？」
			ELSE
				CALL HPH_PRINT,@"「ふふ、私、コンドームを付けてするのも嫌いじゃないですよHPH」","L"
				CALL HPH_PRINT,@"「わざわざコンドーム付けるってことは、子作り目的じゃなくて、セックスが目的じゃないですかHPH」","L"
				CALL HPH_PRINT,@"「ああ、この方は、私を犯したくてしょーがないんだなーって、想像しちゃうと濡れちゃうんです…HPH」","L"
			ENDIF
		ELSE
			CALL HPH_PRINT,@"「やっぱり、生でなさりたいですよね…HPH」","L"
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1