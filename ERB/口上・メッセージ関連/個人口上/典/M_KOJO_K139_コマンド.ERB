;==================================================
;189,新妻プレイ
;==================================================
@M_KOJO_MESSAGE_COM_K139_189
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K139_189_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_189_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;服を着てない（ノーブラ・パンツありはブラを自由に変更できないので多分できない）
		IF TEQUIP:5 && TEQUIP:0 & 0
			PRINTFORML 「これが噂に聞く『裸エプロン』というものですか…」
			;下着あり
			IF EQUIP:下半身下着１ || EQUIP:下半身下着２
				PRINTFORML 「まあ、一応下着だけは着けておりますけど」
				PRINTFORML 「それとも、下着も脱いだ方がよろしいですか？　ア・ナ・タ%UNICODE(0x2665) *1%」
				PRINTW 
			ELSE
				PRINTFORML 「なんだかスースーして落ち着かないものですねぇ」
				PRINTFORML 「ひょっとして、この格好で家事をせよ、と？」
				CALL HPH_PRINT,@"「いけません、こんな格好で掃除なんかしたら、大事なところが丸見えになってしまいます…HPH」","L"
				CALL HPH_PRINT,@"「そして、ムラムラした%CALLNAME:MASTER%どのに襲われてしまうかもしれません…HPH」","L"
				CALL HPH_PRINT,@"「もしそうなったら…典は抵抗できずに組み敷かれてしまうかも…HPH」","L"
				PRINTW 
			ENDIF
		ELSE
			PRINTFORML 「おお、これは新鮮なものですねぇ」
			PRINTFORML 「私、どちらかというと割烹着派なものでして」
			PRINTFORML 「こちらの方が確かに新妻感は出ておりますね%UNICODE(0x2665) *1%」
			PRINTW 
			PRINTFORML 「というわけで、定番のアレ、やっておきますか？」
			CALL ASK_YN("一番いいやつを頼む","大丈夫だ、問題ない")
			IF !RESULT
				PRINTFORMW 「かしこまりました♪」
				PRINTFORML 「アナタ、今日もお疲れ様%UNICODE(0x2665) *1%」
				PRINTFORML 「ご飯になさいます？　お風呂になさいます？　それとも、わ・た・し？」
				PRINTW 
			ELSE
				PRINTFORML 「ああん、いけずぅ」
				PRINTFORML 典は胸元で手を組み、体をくねくねと揺らしている
				PRINTW 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;開始
	IF TEQUIP:新妻プレイ
		PRINTFORML 「ふふ、またでございますか？」
		IF TEQUIP:5 && TEQUIP:0 & 0
			IF DAY:2 == 4
				PRINTFORML 「寒いっ！　寒いですようっ！」
				PRINTFORML 典は身体を抱きながらブルブル震えている
				PRINTW 
			;下着あり
			ELSEIF EQUIP:下半身下着１ || EQUIP:下半身下着２
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「下着は脱がさないのは、なんだかマニアックですねぇ」
				CASE 1
					PRINTFORML 「下着を脱がさないのは、何かこだわりがあるのでしょうか？」
					PRINTFORML 「典としては、%CALLNAME:MASTER%どのが喜んでくださるなら何でもいいのですけどね」
				ENDSELECT
				PRINTW 
			ELSE
				SELECTCASE RAND:3
				CASE 0
					IF TALENT:TARGET:バストサイズ >= 0
						SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「%CALLNAME:MASTER%どの、典の裸エプロン、いかがですか？」
							PRINTFORML 典は胸を強調するように前かがみの姿勢をとっている
						CASE 1
							PRINTFORML 「エプロンから胸がはみ出てしまいそうです…」
							PRINTFORML 「あんまり、隠せていないですね」
						CASE 2
							SELECTCASE RAND:2
							CASE 0
								PRINTFORML 胸がないせいで、桜色の突起がチラチラと見える…
								PRINTFORML 「胸が擦れちゃって…ちょっと変な感じです…%UNICODE(0x2665) *1%」
							CASE 1
								PRINTFORML 「ふふ、横から見えるおっぱいというものも乙なものでしょう♪」
							ENDSELECT
						ENDSELECT
					ELSE
						PRINTFORML 「私にもっと胸があれば、%CALLNAME:MASTER%どのを悩殺できるのですが…」
						PRINTFORML 隙間から見えるちっぱいと桜色の突起こそが至高と教えてやった
						CALL HPH_PRINT,@"「もう…%CALLNAME:MASTER%どのの、えっち…HPH」","L"
					ENDIF
					PRINTW 
				CASE 1
					PRINTFORML 「ふふ、お尻は見せてあげません%UNICODE(0x2665) *1%」
					PRINTFORML 典はわざと焦らすように、お尻を尻尾で隠している
				CASE 2
					PRINTFORML 「お掃除してるときに、後ろから覗いては駄目ですよ？」
				ENDSELECT
				PRINTW 
			ENDIF
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「わざわざこれに着替えて致すというのも…」
				PRINTFORML 「これが『しちゅえーしょんぷれい』というものでしょうか？」
			CASE 1
				PRINTFORML 「アナタ、今日はいっぱいしてくださいね…%UNICODE(0x2665) *1%」
			CASE 2
				PRINTFORML 「これを付けてたら%CALLNAME:MASTER%どのの新妻に見えるでしょうか…%UNICODE(0x2665) *1%」
