@M_KOJO_MESSAGE_COM_K139_621_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TALENT:139:機嫌 == -1 || CFLAG:139:ブチギレ == 1
		PRINTFORML 「…………」
		PRINTFORML 典はムスッとした表情で釣り糸を垂らしている…
		PRINTW 
	ELSE
		PRINTFORML 「おっさかーな、おっさかーなー♪　つーれるっかなー♪」
		PRINTFORML 釣りをしているときの典は何とも無邪気だった
		PRINTW 
	ENDIF
	
	SELECTCASE TFLAG:194
	;典が釣った
	CASE IS >= 1000
		SELECTCASE TFLAG:193
		CASE 0
			PRINTFORML 「えいやっ！　あれ、何もかかってない…」
			PRINTFORML ボウズなのを見て%CALLNAME:MASTER%が笑うと、典はムっとした表情を見せた
			PRINTFORML 「ふーんだ。次は大物を釣って見せますからねー」
			PRINTW 
		;普通の魚
		CASE 1 TO 4
			PRINTFORML 「見てください。%CALLNAME:MASTER%どの、典が釣りましたよ♪」
			PRINTFORML ピチピチと跳ねる%FISH_NAME(TFLAG:193)%を見せながら、典が誇らしげに笑みを浮かべた
			PRINTW 
		;うまい魚
		CASE 11 TO 15
			PRINTFORML 「%CALLNAME:MASTER%どの！　%CALLNAME:MASTER%どの！　%FISH_NAME(TFLAG:193)%です！」
			PRINTFORML 「%FISH_NAME(TFLAG:193)%が釣れましたよ！（ﾄﾞﾔｧ）」
