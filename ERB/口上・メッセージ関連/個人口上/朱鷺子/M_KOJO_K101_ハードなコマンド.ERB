;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K101_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K101_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K101_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「んんんんっ！？んっ～～～～」
		PRINTFORMW 朱鷺子は苦しそうに%CALLNAME:MASTER%の陰茎を咥えている。
		RETURN 1
	;基本セット
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「んんっ～～～～～……んんんっ…ぷはぁっ…げほっ」
			PRINTFORMW 朱鷺子は涙目で%CALLNAME:MASTER%を見ている。
		CASE 0
			PRINTFORMW 「んっ！？ ～～～～～～～っ！！！」
			PRINTFORMW 朱鷺子は突然口に入れられた剛直に目を見開いている。
		ENDSELECT
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 「あぐっ…んぐっ…んっ～～～～～」
		PRINTFORMW 朱鷺子は苦しそうにしながらも何とか%CALLNAME:MASTER%を気持ち良くさせようとしている。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「んんんんんっ～～～～～」
		PRINTFORMW 「げほげほげほっ……酷いわ…」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

