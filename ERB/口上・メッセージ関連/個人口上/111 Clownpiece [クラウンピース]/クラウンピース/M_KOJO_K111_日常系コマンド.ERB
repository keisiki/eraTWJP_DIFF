	;27,えっちした後（満足した）
	ELSEIF !GETBIT(TCVAR:111:359, 27) && TCVAR:[[クラウンピース]]:355 && TALENT:[[クラウンピース]]:機嫌 > 0
		CALL M_KOJO_K111_TALKSET("状況会話", 27)
	;28,ほろ酔い
	ELSEIF !GETBIT(TCVAR:111:359, 28) && ALCOHOL_FACE(111) == "ほろ酔い"
		CALL M_KOJO_K111_TALKSET("状況会話", 28)

[SKIPSTART]
	ELSEIF !GETBIT(TCVAR:111:359, ) && 
		CALL M_KOJO_K111_TALKSET("状況会話", )
[SKIPEND]

	;恋人
	ELSEIF TALENT:111:恋人
		CALL M_KOJO_K111_TALKSET("恋人")
	;恋慕
	ELSEIF TALENT:111:恋慕
		CALL M_KOJO_K111_TALKSET("恋慕")
	;通常
	ELSE
		CALL M_KOJO_K111_TALKSET("その他")
	ENDIF
ENDIF
