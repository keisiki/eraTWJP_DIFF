;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K111
	;地の文改変型
	PRINTFORMDL %CALLNAME:TARGET%の愚痴を聞いてやった
	PRINTW 
	SELECTCASE TFLAG:SELECTCOMの分岐
	;失敗
	CASE 1
		;最初から不機嫌
