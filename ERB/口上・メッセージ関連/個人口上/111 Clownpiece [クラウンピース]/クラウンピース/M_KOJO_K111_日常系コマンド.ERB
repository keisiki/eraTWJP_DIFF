	ELSEIF !GETBIT(TCVAR:111:359, 19) && CFLAG:[[クラウンピース]]:現在位置 == CFLAG:[[純狐]]:現在位置 && !M_KOJO_K111_JUNKO_KURASHI()
		CALL M_KOJO_K111_TALKSET("状況会話", 19)
