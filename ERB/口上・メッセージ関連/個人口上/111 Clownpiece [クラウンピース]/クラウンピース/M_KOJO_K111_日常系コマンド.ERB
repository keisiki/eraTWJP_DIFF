	ELSEIF !GETBIT(TCVAR:111:359, 22) && TIME:7 == 1 && M_KOJO_K111_EARTH(CFLAG:[[クラウンピース]]:現在位置) && OUTROOF(CFLAG:[[クラウンピース]]:現在位置)
		CALL M_KOJO_K111_TALKSET("状況会話", 22)
