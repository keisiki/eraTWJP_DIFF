;621,釣りをする
;TFLAG:193(0=釣れない 1～4=魚 11～15=うまい魚 610=木材 131=守矢の守札 21=ローター 609=氷の鱗 606=龍の爪)
;TFLAG:194(1000以上ならTARGETが釣った)
;==================================================
@M_KOJO_MESSAGE_COM_K111_621
CALL TRAIN_MESSAGE
DRAWLINE
CALL M_KOJO_MESSAGE_COM_K111_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K111_621_1
#DIM CHARA
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;地の文カットは危ないので廃止
IF LOCAL
	CALL M_KOJO_COLOR_K111
	SELECTCASE TFLAG:193
	;釣れない
	CASE 0
		IF TALENT:111:恋慕
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「あ～～釣れない～～っ！」
			CASE 2
				PRINTFORMW 「さかなさかなさかな～♪」
			CASEELSE
				PRINTFORMW 「%CALLNAME:MASTER%、タイクツ～」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「……ひまー！！」
			CASE 2
				PRINTFORMW 「……これ、楽しい？」
			CASEELSE
				PRINTFORMW 「……なんにも釣れないね……」
			ENDSELECT
		ENDIF
	;木片
	CASE 610
		IF TFLAG:194 > 1000
			PRINTFORMW 「見て！ なんか釣れた！！」
			PRINTFORMW 「……むぅ～～！」
		ELSE
			PRINTFORMW 「きゃははは！ はずれ～！」
		ENDIF
	;紙切れ
	CASE 131
		PRINTFORMW 「……？ なにこれ？？ ゴミ？」
	;カエル
	CASE 700
		PRINTFORMW 「きゃははは！ カエルカエルー！ ゲロゲロ！ ゲロゲロ！」
	;ローター
	CASE 21
		IF TFLAG:194 > 1000
			PRINTFORMW 「ん？ これって……\/\/\/」
		ELSE
			PRINTFORML 「ん？ これって……」
			PRINTFORMW 「%CALLNAME:MASTER%、どんだけスケベなのよ～！」
		ENDIF
	CASEELSE
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			LOCALS = %FISH_NAME(TFLAG:193)%
		CASEELSE
			LOCALS = %ITEMNAME:(TFLAG:193)%
		ENDSELECT
		SELECTCASE LOCALS
		CASE "氷の鱗", "龍の爪"
			PRINTFORMW 「なにこれー！？ きれい～～っ！！」
		CASE "龍涎香"
			PRINTFORMW 「なにこれ、ゴミじゃん」
		CASE "ピラニア"
			PRINTFORMW 「うぉ～ピラニアじゃーん！ イカす～！」
		CASE "ウナギ", "ヤツメウナギ", "ナマズ"
			PRINTFORMW 「きゃははは！ ぬるぬる～！」
		CASEELSE
			IF LOCAL:1 >= 80
				PRINTFORMW 「わお！ でっかーい！！」
			ELSEIF INRANGE(TFLAG:193, 11, 20) || INRANGE(TFLAG:193, 24, 60)
				PRINTFORMW 「おー！ すごい！ それ、食べるの？」
			ELSEIF TALENT:111:恋慕 || TALENT:111:思慕
				PRINTFORMW 「わっ！ すごい！ これはなーに？？」
			ELSE
				PRINTFORMW 「おー！ やるじゃ～ん！」
			ENDIF
		ENDSELECT
	ENDSELECT
ENDIF
RETURN 1
