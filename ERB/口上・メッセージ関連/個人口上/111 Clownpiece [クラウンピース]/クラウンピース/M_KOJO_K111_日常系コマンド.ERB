		CALL M_KOJO_K111_TALKSET("状況会話", 11, ABL:[[ルナ]]:親密)
	;12,スターの話
	ELSEIF !GETBIT(TCVAR:111:359, 12) && CFLAG:[[クラウンピース]]:現在位置 == CFLAG:[[スター]]:現在位置
		CALL M_KOJO_K111_TALKSET("状況会話", 12, ABL:[[スター]]:親密)
	;13,チルノの話
	ELSEIF !GETBIT(TCVAR:111:359, 13) && CFLAG:[[クラウンピース]]:現在位置 == CFLAG:[[チルノ]]:現在位置
		CALL M_KOJO_K111_TALKSET("状況会話", 13, ABL:[[チルノ]]:親密)
	;14,ラルバの話
	ELSEIF !GETBIT(TCVAR:111:359, 14) && CFLAG:[[クラウンピース]]:現在位置 == CFLAG:[[ラルバ]]:現在位置
		CALL M_KOJO_K111_TALKSET("状況会話", 14, ABL:[[ラルバ]]:親密)
	;15,大妖精の話
	ELSEIF !GETBIT(TCVAR:111:359, 15) && CFLAG:[[クラウンピース]]:現在位置 == CFLAG:[[大妖精]]:現在位置
		CALL M_KOJO_K111_TALKSET("状況会話", 15, ABL:[[大妖精]]:親密)
	;16,リリーホワイトの話
	ELSEIF !GETBIT(TCVAR:111:359, 16) && CFLAG:[[クラウンピース]]:現在位置 == CFLAG:[[リリーＷ]]:現在位置
		CALL M_KOJO_K111_TALKSET("状況会話", 16, ABL:[[リリーＷ]]:親密)
	;17,リリーブラックの話
	ELSEIF !GETBIT(TCVAR:111:359, 17) && CFLAG:[[クラウンピース]]:現在位置 == CFLAG:[[リリーＢ]]:現在位置
		CALL M_KOJO_K111_TALKSET("状況会話", 17, ABL:[[リリーＢ]]:親密)
	;18,ヘカーティアとの話,ヘカーティアと純狐は純狐邸に住んでいると高頻度で発生してしまうので除外
	ELSEIF !GETBIT(TCVAR:111:359, 18) && CFLAG:[[クラウンピース]]:現在位置 == CFLAG:[[ヘカーティア]]:現在位置 && !M_KOJO_K111_JUNKO_KURASHI()
		CALL M_KOJO_K111_TALKSET("状況会話", 18)
