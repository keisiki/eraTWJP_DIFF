;==================================================
;350,押し倒す（時姦時イタズラする
;TFLAG:194 (0=成功 1=人目がある　2=野外は嫌でござりまする　3=多人数プレイは勘弁　4=合意があってもムードが足りぬ）
;TCVAR:抱き寄せ初回　1=普通に合意ゲット　2=先にキスぐらいしろよ　3=媚薬or発情には勝てなかったよお…　4=悔しい…刻印さえなければﾋﾞｸﾋﾞｸﾝ
;5=強引に迫られると断れぬ(素質臆病持ち専用) 6=まんざらでもない　7=駄目　8=論外
;==================================================
@M_KOJO_MESSAGE_COM_K111_350
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K111_350_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K111_350_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K111
	;押し倒され値	ENDURE(111) >= RAND:300 + 300で真,しかしRANDの値は変化するのでここでは400を超えたら押し倒されることにする
	IF FLAG:時間停止 == 1
	;野外失敗
	ELSEIF TFLAG:194 == 2
		IF ENDURE(111) > 400
			PRINTFORML 「えっ\/\/\/ そんな……\/\/\/」
			PRINTFORMW 「こ、ここはお外だよ……\/\/\/」
		ELSE
			PRINTFORML 「わっ！ なにっ！？」
			PRINTFORMW 「こ、ここはお外だよ……？」
		ENDIF
	;人目失敗
	ELSEIF TFLAG:194 == 1
		IF ENDURE(111) > 400
			PRINTFORML 「えっ\/\/\/ そんな……\/\/\/」
			PRINTFORMW 「人が見てるよ……\/\/\/」
		ELSE
			PRINTFORML 「わっ！ なにっ！？」
			PRINTFORMW 「人が見てるよ……？」
		ENDIF
	;乱交失敗
	ELSEIF TFLAG:194 == 3
		PRINTFORMW 「えっ、いや……%CALLNAME:MASTER%と二人きりがいい……」
	;ムード失敗
	ELSEIF TFLAG:194 == 4
		PRINTFORML 「うわっ！ やめてっ！」
		PRINTFORMW 「もう！ ムードってのを考えてよね～」
	;泥酔失敗
	ELSEIF TFLAG:194 == 5
		PRINTFORMW 「んん～～？ %CALLNAME:MASTER%、どうひたのぉ～～？？」
		PRINTL 
		PRINTFORMDW 素面の目撃者がいるのに酔姦するわけにいかない
	;強制失敗
	ELSEIF TFLAG:194 == 100 || TCVAR:111:抱き寄せ初回 == 98
		PRINTFORML 「%M_KOJO_K111_AUTO("喘ぎ：小", "…！")% ヤ、ヤダッ！」
		PRINTFORMW 「今はそんな気分じゃないのっ！！」
		RETURN 1
	;泥酔
	ELSEIF TCVAR:111:泥酔 == 1
		PRINTFORMW 「ふわぁ～～にゃ～～にぃ～～？ おねんねぇ～～？？」
	;乱交
	ELSEIF GET_TARGETNUM() > 1
		IF FIRSTTIME("乱交")
			PRINTFORMW 「えっ！ ほ、他の子と一緒にするの……？」
			PRINTFORMW 「ま、まぁいっか……」
		ELSE
			PRINTFORML 「%TOFULL(TOSTR(GET_TARGETNUM()))%ぴーってやつ？」
			PRINTFORMW 「ちゃんとびょーどーに相手してよ？」
		ENDIF
	;お泊り１
	ELSEIF GROUPMATCH(CFLAG:[[クラウンピース]]:現在位置, 連れ込み宿, ラブホ, CFLAG:MASTER:初期位置) && CFLAG:TARGET:うふふ != 2
		PRINTFORMW 「%M_KOJO_K111_AUTO("喘ぎ：小", "❤")%」
	;お泊り２
	ELSEIF M_KOJO_K111_HOUSE(CFLAG:[[クラウンピース]]:現在位置) == 1 && CFLAG:TARGET:うふふ != 2
		PRINTFORMW 「%M_KOJO_K111_AUTO("喘ぎ：小", "❤")%」
	;それ以外
	ELSEIF !TCVAR:351
		PRINTFORMW 「%M_KOJO_K111_AUTO("喘ぎ：小", "…！")%」
	ENDIF
	;先に押し倒された
	;うふふフラグは押し倒した時点で変更されるんでした
	;押し倒し返しは小傘口上さんを参考にして専用TCVARを作ることに
;	IF !FLAG:70 && !TFLAG:194 && CFLAG:111:うふふ == 2
	IF TCVAR:351
		PRINTFORML 
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「%M_KOJO_K111_AUTO("喘ぎ：小", "❤")%」
			PRINTFORMW 「もー、わかったわかった！ 今度は%CALLNAME:MASTER%の番ね❤」
		CASE 2
			PRINTFORMW 「フフッ❤ %CALLNAME:MASTER%も興奮してきた？」
			PRINTFORMW 「いいよ❤ いっしょに気持ちよくなろ♪」
		CASEELSE
			PRINTFORMW 「きゃはっ♪ その気になってくれた？」
			PRINTFORMW 「うれしいっ❤ 好きにしていいよ❤」
		ENDSELECT
	ELSEIF !FLAG:70 && !TFLAG:194
		PRINTFORML 
		SELECTCASE TCVAR:111:抱き寄せ初回
		;普通の初回,キスがまだ,クラウンピースはキスが先だとか後だとか大して気にしてない
		CASE 1, 2
			PRINTFORMW 「きゃっ！ ちょ、ちょっと！」
			PRINTFORMW 「……%CALLNAME:MASTER%、本気なの……？」
			PRINTFORMW 「あたい、こういうの初めてだから、よくわかんないけど……」
			PRINTFORMW 「いいよ、%CALLNAME:MASTER%❤ やさしくしてね❤」
		;媚薬or発情
		CASE 3
			PRINTFORML 「ふわぁ……%CALLNAME:MASTER%、なんだかあたい、頭がぼうっとして……」
			PRINTFORMW 「はぁ……もう、好きにして……」
		;時姦
		CASE 4
			PRINTFORML 「なにするの！ いや！」
			PRINTL 
			PRINTFORMDW 口ではそういいつつも%CALLNAME:TARGET%は%CALLNAME:MASTER%に抱きついてきた
			PRINTL 
			PRINTFORMW 「えっ！？ なんで……どうして……？」
;		CASE 5
;			PRINTFORML 押しの弱い%CALLNAME:TARGET%は、%CALLNAME:MASTER%を拒むことができなかった…
		;もうちょっと
		CASE 6
			PRINTFORML 「ふぇ……そんな……」
			PRINTFORMW 「いやだ……ごめんね%CALLNAME:MASTER%……」
		;ダメ
		CASE 7
			PRINTFORML 「んわぁっ！ なにするんだよう！」
			PRINTFORMW 「そーゆーのいけないんだよ？」
		;早すぎ
		CASE 8
