@M_KOJO_MESSAGE_COM_K111_330_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K111
	IF FLAG:時間停止
	ELSEIF M_KOJO_K111_FUKIGEN([[クラウンピース]])
		;地の文カット
		PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の%SHOW_BOTTOM(TARGET,1)%に手をかけた…
		PRINTW 
		PRINTFORMW 「やめてっ！！」
		PRINTFORMDW 思いっきりビンタされた……
	;スカートめくり成功
	ELSEIF TFLAG:193 >= 0
		PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の隙を突いて%SHOW_BOTTOM(TARGET,1)%をめくり上げた！
		PRINTW 
		;地の文カット
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「きゃあっ！」
		CASE 2
			PRINTFORML 「ひゃあっ！」
		CASEELSE
			PRINTFORML 「わわっ！」
		ENDSELECT
		SELECTCASE RAND:5
		CASE 1
			PRINTFORMW 「この～！ やったなー！」
		CASE 2
			PRINTFORMW 「んも～❤ エッチなんだから～！」
		CASE 3
			PRINTFORMW 「むぐぐ～！ %CALLNAME:MASTER%もやるな～！」
		CASE 4
			PRINTFORMW 「ゆ、油断してただけなんだからねっ！」
		CASEELSE
			PRINTFORMW 「くそ～！ やられた～！」
		ENDSELECT
	;スカートめくり失敗
	ELSEIF TALENT:[[クラウンピース]]:思慕 || TALENT:[[クラウンピース]]:恋慕
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「ちょっと！ やめてよ！」
		CASE 2
			PRINTFORMW 「もー、すぐそういうことするー！」
		CASEELSE
			PRINTFORML 「えっち！」
		ENDSELECT
	ELSE
		PRINTFORMW 「わっ！ なにするの！！」
	ENDIF
ENDIF
RETURN 1
