@M_KOJO_MESSAGE_COM_K111_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
;CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K111_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K111_300_1
#DIM POINT
#DIM デート会話, 120
;日が変わってたらデート会話リセット
SIF !TCVAR:111:353
	VARSET デート会話
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	RESETCOLOR
	DRAWLINE
	;ここに口上書いて下さい
	IF ALCOHOL_FACE(111) == "酔いどれ" || ALCOHOL_FACE(111) == "ぐでぐで"
		PRINTFORML %CALLNAME:MASTER%は%CALLNAME:TARGET%に話し掛けてみたが、%CALLNAME:TARGET%は呂律が回っておらず何を言っているかも聴き取れない
	;会話不能の地の文は恋慕でない場合は省略できず、恋慕の時は表示されないので、ここではどちらの場合も同じように表示されるようにする
	ELSEIF TCVAR:111:会話不能
		IF TALENT:111:恋慕
			PRINTFORML %CALLNAME:TARGET%に話しかけようとしたが言葉が見つからない
			PRINTFORML あたりに気まずい空気が流れた…
		ENDIF
	ELSE
		;デート会話
		IF FLAG:デート相手 == [[クラウンピース]] && DATENAME_SPOT() != "道中" && !デート会話:(CFLAG:111:現在位置 / 10)
			CLEARLINE 2
			PRINTFORML %DATENAME_SPOT()%でデート
			DRAWLINE
		ENDIF
		PRINTFORM %CALLNAME:MASTER%は%CALLNAME:TARGET%と
		IF TALENT:111:恋慕 && !M_KOJO_K111_FUKIGEN(111)
			PRINTDATA
				DATAFORM 隣り合って
				DATAFORM 少し照れながら
				DATAFORM 微笑みを交わしながら
				DATAFORM 和やかな雰囲気で
				DATAFORM 少し熱くなりながら
				DATAFORM じゃれ合いながら
			ENDDATA
		ELSE
			PRINTDATA
				DATAFORM くだらない
				DATAFORM たわいのない
				DATAFORM 世間
				DATAFORM 平和な
				DATAFORM よくある
				DATAFORM ちょっとした
				DATAFORM 可笑しな
			ENDDATA
		ENDIF
		IF TFLAG:SELECTCOMの分岐 > 0
			PRINTFORML 話をして盛り上がった
		ELSEIF TFLAG:SELECTCOMの分岐 < 0
			PRINTFORML 話をしていたが話が途切れてしまった……
		ELSE
			PRINTFORML 話をした
		ENDIF
	ENDIF
	PRINTW 
	CALL M_KOJO_COLOR_K111
	IF ALCOHOL_FACE(111) == "酔いどれ" || ALCOHOL_FACE(111) == "ぐでぐで"
		IF !RAND:2
			PRINTFORML 「きゃははは♪ おしゃけだ～いしゅき♪ おしゃけしゅきな%CALLNAME:MASTER%もしゅきぃ❤」
			PRINTFORMW 「～～ッ！！＠＃！＄％！？＊＊＊＊ッ！！」
		ELSE
			PRINTFORML 「しょれでぇ～～、ごひゅびんひゃまがぁ～～～ぶぁ～～～～～って！」
			PRINTFORMW 「れもあらいはひゅ～～ぐ、ひゅ～～～～ぐひゃひぇひゅへ、ひゃ～はははは！！」
		ENDIF
		PRINTFORML 
		PRINTFORMDW ほとんど会話にならなかった……
	ELSEIF M_KOJO_K111_FUKIGEN(111)
		PRINTFORML 「なーに！？」
		PRINTFORMW 「あたいじゃなくて、他の娘と喋ってれば？」
	;会話不能
	ELSEIF TCVAR:111:会話不能
		PRINTFORML 「その話さっきしたー」
		PRINTFORMW 「つまんないー」
	;失敗
	ELSEIF TFLAG:SELECTCOMの分岐 == -1
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「ねー、適当に相槌打ってるでしょー」
			PRINTFORMW 「興味ないならいいもん！」
		CASE 2
			PRINTFORML 「%CALLNAME:MASTER%は時々むづかしい話をするよね」
			PRINTFORMW 「つまんないよそれー」
		CASEELSE
			PRINTFORML 「……ふーん」
			PRINTFORMW 「何の話かよく判らないや」
		ENDSELECT
	;夏,野外,汗フラグなし,昼間,地上,晴れで確定
	ELSEIF DAY:2 == 2 && OUTROOF(CFLAG:111:現在位置) && !TCVAR:111:350 && INRANGE(TIME, 600, 960) && M_KOJO_K111_EARTH(CFLAG:111:現在位置) && 天候 < 2
		;暑いの大好きだった
		SELECTCASE RAND:5
		CASE 1
			PRINTFORML 「%CALLNAME:MASTER%、暑い？ 暑そうだね？」
			PRINTFORMW 「きゃはっ！ いいわねぇ！ 暑さに歪むその表情、あたい大好物なの♪」
			TALENT:111:機嫌 = 1
;			PRINTFORML 「毎日暑いね……」
;			PRINTFORMW 「灼熱地獄に比べたらこれでも涼しい方なんだろうケド」
		CASE 2
			PRINTFORML 「全く、こうも毎日暑いと……」
			PRINTFORMW 「最っ高に血湧き肉躍るね！！」
;			PRINTFORMW 「地上もこんなに暑いなんて……」
		CASE 3
			PRINTFORML 「あたいは地獄生まれ地獄育ち！」
			PRINTFORMW 「夏の暑い日差し大好きなんだ～♪」
		CASE 4
			PRINTFORML 「この強烈な日差しを浴びると身体の内からパワーが込み上がってくる！」
			PRINTFORMW 「うおぉー！ テンション上がってきたー！！」
			TALENT:111:機嫌 = 1
		CASEELSE
			PRINTFORML 「灼熱地獄はこの日差しよりずっと暑いんだけどさ」
			PRINTFORMW 「やっぱり太陽の暑さはエネルギーに溢れていて質が違うんだよね～」
;			PRINTFORML 「ぐでー……あついー……」
;			PRINTFORMW 「%CALLNAME:MASTER%、この暑さなんとかならないのー？」
		ENDSELECT
		PRINTL 
		PRINTFORMDL クラウンピースは夏用の短いタイツを履いているが
		PRINTFORMDW ムンムンとした熱気でだいぶ蒸れているようだ……。
		TCVAR:111:350 = 1
	;プレゼント会話１（プレゼントのお礼）,フラグ発生から5日以上
	ELSEIF CFLAG:[[クラウンピース]]:1005 && DAY - CFLAG:[[クラウンピース]]:1005 >= 5
		PRINTFORMW 「あ、そうだ！ 今日は%CALLNAME:MASTER%にこれを渡そうと思ってたの」
		;12日以上
		SIF DAY - CFLAG:[[クラウンピース]]:1005 >= 12
			PRINTFORML 「%CALLNAME:MASTER%は忘れちゃってるかもしれないけど……」
		PRINTFORMW 「ほら、前に話したプレゼントのお礼だよっ！」
		SELECTCASE RAND:8
		CASE 1
			LOCALS:1 = 狂気の
		CASE 2
			LOCALS:1 = 地獄の
		CASE 3
			LOCALS:1 = 道化の
		CASE 4
			LOCALS:1 = 手作りの
		CASEELSE
			LOCALS:1 = スター＆ストライプの
		ENDSELECT
		SELECTCASE RAND:8
		CASE 1
			LOCALS:2 = ハンカチ
		CASE 2
			LOCALS:2 = ストラップ
		CASE 3
			LOCALS:2 = チャーム
		CASE 4
			LOCALS:2 = マグカップ
		CASE 5
			LOCALS:2 = ランプ
		CASE 6
			LOCALS:2 = ステッキ
		CASE 7
			LOCALS:2 = マナストーン
		CASEELSE
			IF TALENT:MASTER:性別 == 1
				LOCALS:2 = リボン
			ELSE
				LOCALS:2 = ネクタイ
			ENDIF
		ENDSELECT
		CALL GIFT_ORDER_MADE_FROM_CHARA([[クラウンピース]], LOCALS:1, LOCALS:2)
		PRINTL 
		CALL COLORMESSAGE(@"クラウンピースから【%RESULTS%】を貰った！", C_AQUA, 2)
		PRINTL 
		PRINTFORMW 「あ、あたいのお礼なんだから、感謝の言葉はいらないわよっ！」
		CFLAG:[[クラウンピース]]:1005 = 0
	;プレゼント会話２（クリスマスプレゼント）,フラグ発生から1日以上12日以内
	ELSEIF CFLAG:[[クラウンピース]]:1006 && INRANGE(DAY - CFLAG:[[クラウンピース]]:1006, 1, 12)
		PRINTFORMW 「あ、あのさ%CALLNAME:MASTER%……こ、これあげるよ」
		;年越してる
		SIF DAY - CFLAG:[[クラウンピース]]:1006 > 7
			PRINTFORML 「もうだいぶ経っちゃってるけど……」
		PRINTFORMW 「ク、クリスマスプレゼント欲しかったんでしょ！」
		PRINTFORMW 「だからこれ、あげるの！」
		SELECTCASE RAND:8
		CASE 1
			LOCALS:1 = クリスマスの
		CASE 2
			LOCALS:1 = ベツレヘムの
		CASE 3
			LOCALS:1 = サンタクロースの
		CASE 4
			LOCALS:1 = トナカイの
		CASE 5
			LOCALS:1 = 聖夜の
		CASE 6
			LOCALS:1 = 雪降る
		CASE 7
			LOCALS:1 = スター＆ストライプの
		CASEELSE
			LOCALS:1 = 雪と星の
		ENDSELECT
		SELECTCASE RAND:8
		CASE 1
			LOCALS:2 = 時計
		CASE 2
			LOCALS:2 = ランプ
		CASE 3
			LOCALS:2 = 手袋
		CASE 4
			LOCALS:2 = カード
		CASEELSE
			LOCALS:2 = スノードーム
		ENDSELECT
		CALL GIFT_ORDER_MADE_FROM_CHARA([[クラウンピース]], LOCALS:1, LOCALS:2)
		PRINTL 
		CALL COLORMESSAGE(@"クラウンピースから【%RESULTS%】を貰った！", C_AQUA, 2)
		PRINTL 
		PRINTFORMW 「ら、来年は%CALLNAME:MASTER%の番なんだからねっ！！」
		CFLAG:[[クラウンピース]]:1006 = 0
	;デート会話
	ELSEIF FLAG:デート相手 == [[クラウンピース]] && DATENAME_SPOT() != "道中" && !デート会話:(CFLAG:111:現在位置 / 10)
		POINT = CFLAG:111:現在位置 / 10
		;▼博麗神社
		SELECTCASE STR:(POINT + 6000)
		CASE "桜並木の鳥居"
			;春
			IF DAY:2 == 1
				PRINTFORML 「ここの桜、とっても綺麗だよね！」
				PRINTFORMW 「初めて見たときはそりゃあもう感動しちゃった！」
			;春以外
			ELSE
				PRINTFORML 「ここの桜、春になるととっても綺麗なんだよ！」
				PRINTFORMW 「毎年春になったらお花見を襲撃するんだー♪」
			ENDIF
		CASE "神社境内"
			PRINTFORML 「%CALLNAME:MASTER%も博麗の巫女には注意しなよ！」
			PRINTFORMW 「イタズラがバレると真っ先にやってきて、ぶっ飛ばされるんだから」
		CASE "神社本堂"
			PRINTFORML 「神社って神様がいるんでしょ？」
			PRINTFORMW 「ここにいる神様はご主人様より強いのかな？」
		CASE "神社縁側"
			;TW内の設定では別荘ということにしておく
			PRINTFORML 「ここの地下にはあたいの別荘があるんだー」
			PRINTFORMW 「なんでか知らないけど、神社の地下があたいの気質に合うんだよね」
		CASE "妖精の大樹"
			;誰かいる
			IF GROUPMATCH(CFLAG:[[クラウンピース]]:現在位置, CFLAG:[[サニー]]:現在位置, CFLAG:[[ルナ]]:現在位置, CFLAG:[[スター]]:現在位置)
				PRINTFORML 「ここはあたいの友達の妖精の住処だよ」
				PRINTFORMW 「ちょっと遊んでいくかー！」
			;留守
			ELSE
				PRINTFORML 「ここはあたいの友達の妖精の住処なんだけど……」
				PRINTFORMW 「今は留守みたいだなぁ……」
			ENDIF
		CASE "夢幻遺跡"
			PRINTFORML 「なぁにここー？ ひろーい……」
			PRINTFORMW 「これなんだろ？ えいっ」
		;▼命蓮寺
		CASE "お地蔵通り"
			PRINTFORML 「あたい、この辺はあんまり来ないんだ」
			PRINTFORMW 「なんでか忘れたけど、お寺は妖精には良くない所らしいよ」
		CASE "枝垂れ柳の下"
			PRINTFORML 「お、いい具合に不気味な木だなぁ！」
			PRINTFORMW 「いかにもお化けが出てきそうじゃない？」
		CASE "命蓮寺境内"
			PRINTFORML 「ここが噂の妖怪寺か～」
			PRINTFORMW 「うん、たしかに妖しい魔力を感じるぞ」
		CASE "命蓮寺方丈"
			PRINTFORML 「ねぇ%CALLNAME:MASTER%、ここの人たちはなにをしているの？」
			PRINTFORMW 「なんだかみんな忙しそうにしてるけど」
		CASE "墓地"
			PRINTFORML 「わ～！ お墓よ～♪」
			PRINTFORMW 「みんな地獄へおいで～♪」
		CASE "神霊廟広場"
			PRINTFORML 「おわー！ でっかくて派手な門！」
			PRINTFORMW 「ここに住んでるヤツがいかに尊大かよく判るね～」
		CASE "神霊廟道場"
			PRINTFORML 「ここは仙人のドージョーなの？」
			PRINTFORMW 「仙人か～、友人様がなにか言ってた気がするな～」
		;▼人里
		CASE "広場"
			PRINTFORML 「お！ ちょうどいいカモがいるじゃない」
			PRINTFORMW 「ちょっとイタズラしてやろうよ！」
		CASE "商家町"
			PRINTFORML 「ねぇ%CALLNAME:MASTER%！ あそこ行きたい！」
			PRINTFORMW 「あれ買って！ あれ買って！ ねっ！ ねっ！」
		CASE "呑屋小道"
			PRINTFORML 「お、この辺りは居酒屋があるのか」
			PRINTFORMW 「どうりでお酒の匂いがすると思った」
		CASE "寺子屋"
			PRINTFORML 「人間はここで勉強するの？ ふーん」
			PRINTFORMW 「ここにお絵かきしていい？」
		CASE "大衆浴場"
			IF DAY * 1440 + TIME - EX:MASTER:前回の入浴時刻 < 360
				PRINTFORML 「ふぅ～！ さっぱり～！」
				PRINTFORMW 「でもなんで%CALLNAME:MASTER%と一緒に入っちゃだめだったの？」
			ELSE
				PRINTFORML 「わ～！ お風呂～♪」
				PRINTFORMW 「釜茹で風呂はないのかー？」
			ENDIF
		CASE "連れ込み宿"
			;入った時点で合意取得するのね…,うふふ前か後かは絶頂したかで判断するので事前に絶頂させてるとちょっと齟齬
			IF EX:111:Ｃ絶頂 + EX:111:Ｖ絶頂 + EX:111:Ａ絶頂 + EX:111:Ｂ絶頂 + EX:111:Ｍ絶頂
				IF TALENT:111:処女
					PRINTFORMW 「……\/\/\/」
				ELSE
					PRINTFORMW 「……❤」
				ENDIF
			ELSE
				IF TALENT:111:処女
					PRINTFORMW 「ねぇ、ここって……\/\/\/」
				ELSE
					PRINTFORMW 「はやくっ❤ はやくっ❤」
				ENDIF
			ENDIF
		CASE "長屋街"
			PRINTFORML 「へー、人間はこうやってまとまって暮らしてるんだー」
			PRINTFORMW 「%CALLNAME:MASTER%はどうしてここに住まないの？」
		;▼霧の湖
		CASE "霧の湖"
			IF DAY:2 == 2
				PRINTFORML 「きゃははー♪ 涼しそう～～！」
				PRINTFORMW 「%CALLNAME:MASTER%も一緒に泳ごうよっ♪」
			ELSE
				PRINTFORML 「ほんとに霧が深いのねー」
				PRINTFORMW 「これじゃあ晴れててもテンション上がらなそうだなぁ」
			ENDIF
		CASE "氷のかまくら"
			PRINTFORML 「あっ、ここチルノの住処だー！」
			PRINTFORMW 「きゃははっ♪ ちょっと物色してやりましょ！」
		CASE "廃洋館"
			PRINTFORML 「なにここ？ 誰も住んで……」
			PRINTFORMW 「わっ！ な、なんか今へんなのが……」
		CASE "正門"
			PRINTFORML 「吸血鬼の館か～、たしかにやたら紅いわねー」
			PRINTFORMW 「入っちゃだめなの？ ふふん♪ あたいは妖精なのよ？」
		CASE "広間"
			PRINTFORML 「お？ 妖精が働いてる……」
			PRINTFORMW 「奴隷として働かされているのかな？」
		CASE "食堂"
			PRINTFORMW 「こんな広いところでご飯食べるの？ すごーい！」
		CASE "大図書館"
			PRINTFORML 「わっ、すごい！ これ全部本？」
			PRINTFORMW 「まぁあたいは微塵も興味ないんだけど」
		CASE "地下室"
			PRINTFORML 「おっと、ここは随分と強い狂気を感じるぞ！」
			PRINTFORMW 「このヤバイくらいの威圧感……たまらないわ！」
		;▼迷いの竹林
		CASE "竹林入り口"
			PRINTFORML 「%CALLNAME:MASTER%は太陽の畑を知ってる？」
			PRINTFORMW 「あたいはよく遊びに来るんだー」
		CASE "迷いの小道", "斜角の竹林"
			PRINTFORML 「あれ？ ここさっきも来たよ？」
			PRINTFORMW 「%CALLNAME:MASTER%、迷子になっちゃったの？」
		CASE "兎の洞穴"
			PRINTFORML 「ん！ なんだかここヒンヤリする！」
			PRINTFORMW 「へぇ、洞窟の中はあんまり気温が変わらないんだ」
		CASE "永遠亭"
			PRINTFORML 「う……くさい……」
			PRINTFORMW 「ここ病院ってやつ？ あたいここヤダ……」
		CASE "無名の丘"
			PRINTFORML 「わはー！ きれいなお花畑～！ まるで夢みたい！」
			PRINTFORMW 「えへ、一度お花の冠作ってみたかったんだ～♪」
		CASE "太陽の畑"
			PRINTFORML 「この辺りは妖精がたくさん住み着いてて毎日賑わってるのよ」
			PRINTFORMW 「まぁ一種の集会場みたいなものかな？」
		CASE "永遠亭奥ノ院"
			PRINTFORML 「へぇ、病院の奥ってお屋敷になってるんだ」
			PRINTFORMW 「さーてどこにイタズラをしてやろうかな」
		CASE "夢幻館"
			PRINTFORML 「このお屋敷なんだろー？」
			PRINTFORMW 「なんだか不思議な感じがするね」
		;▼魔法の森
		CASE "森の入り口"
			PRINTFORML 「うわ～、すごい瘴気……」
			PRINTFORMW 「ゾクゾクしてたまらないわー♪」
		CASE "香霖堂"
			PRINTFORML 「なにここ？ 古道具屋？」
			PRINTFORMW 「楽しそうなものある？」
		CASE "古木の大樹"
			PRINTFORML 「でっかい木ー」
			PRINTFORMW 「およ？ 妖精が住んでいた名残があるぞ」
		CASE "霧雨魔法店"
			PRINTFORML 「ここがあの魔法使いの家かぁ」
			PRINTFORMW 「案外ちっちゃいんだね」
		CASE "人形の洋館"
			PRINTFORML 「ここは誰の家ー？」
			PRINTFORMW 「あ、お人形！ え、今動いたような……」
		CASE "再思の道"
			PRINTFORML 「おお…なんて不気味な場所なんだ……」
			PRINTFORMW 「あの花とか、地獄に咲く花にちょっと似てるかも」
		CASE "無縁塚"
			PRINTFORML 「なんだろうこのでっかい石」
			PRINTFORMW 「こいつはなんだかヤバそうな気配がするわね……」
		;▼冥界
		CASE "中有の道"
			PRINTFORML 「ここが噂の屋台街道か～」
			PRINTFORMW 「%CALLNAME:MASTER%、なんか買ってよ～！」
		CASE "三途の川"
			PRINTFORML 「三途の河は知ってるよ！」
			PRINTFORMW 「見るのは初めてだけど…うーん、案外普通の河ってカンジ？」
		CASE "マヨヒガ"
			PRINTFORML 「こんなところにお屋敷？」
			PRINTFORMW 「地上にも物好きが居るもんだな～」
		CASE "彼岸"
			PRINTFORML 「死者は彼岸で審判を受けて、普通は転生する時が来るまで冥界に留まるんだよ」
			PRINTFORMW 「――って話は聞いてたけど、来るのは初めて～♪」
		CASE "白玉楼庭"
			PRINTFORML 「でっかい庭だなぁ～」
			PRINTFORMW 「こんな庭があったら毎日遊び放題だね」
		CASE "白玉楼"
