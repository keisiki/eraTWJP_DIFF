;==================================================
;332,お酒をふるまう
;TFLAG:193(0=成功 -1=失敗 -2=大失敗 -5=ひじりん)
;==================================================
@M_KOJO_MESSAGE_COM_K111_332
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K111_332_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K111_332_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K111
	;デフォルト（ぼくのかんがえたクラウンピースちゃん…もとい自前CSV（受け入れてくれた作者さんには大感謝））
	;では[酒に強い]のでまずい＞どぶろく,おいしい＞清酒
	;口上が出る時点で既に酒気は加算されてる,ぐでぐで口上は果たして表示できるのか
	IF ALCOHOL_FACE(111) == "ぐでぐで"
		PRINTFORMW 「きゃははは♪ おしゃけだ～いしゅき♪♪♪」
	;現在は無い
	ELSEIF ALCOHOL_FACE(111) == "へべれけ"
		PRINTFORMW 「もっともっとぉ～～♪」
	ELSEIF ALCOHOL_FACE(111) == "酔いどれ"
		PRINTFORMW 「えへへ～♪ まだまだぁ～～♪」
	ELSE
		PRINTFORMW 「わー！ おさけおさけー♪」
	ENDIF
	;どぶろく
	IF ALCOHOL_TASTE(TFLAG:194) < 10
		IF ALCOHOL_FACE(111) == "ぐでぐで"
			PRINTFORMW 「なぁんだぁこのさけはぁー！！ 淹れた奴をよんでくぉーい！！」
		ELSEIF ALCOHOL_FACE(111) == "酔いどれ"
			PRINTFORMW 「んぁ～♪ こんなんじゃ、じぇんじぇん酔えないよぉ～～」
		ELSE
			PRINTFORMW 「んー♪ ちょっと物足りないかな～」
		ENDIF
	;清酒
	ELSEIF ALCOHOL_TASTE(TFLAG:194) >= 15
		IF ALCOHOL_FACE(111) == "ぐでぐで"
			PRINTFORMW 「こりぇ～～♪ こりぇあたいしゅきなのぉ～♪ こりぇしゅきな%CALLNAME:MASTER%もしゅきぃ❤」
		ELSEIF ALCOHOL_FACE(111) == "酔いどれ"
			PRINTFORMW 「おひゃけおいひ～ね～♪ %CALLNAME:MASTER%はこんなおいひぃもにょをどこにかくしてりゅのかにゃ～～？？」
		ELSE
			PRINTFORMW 「これおいしい～♪ まだあるんでしょ？ あけよっ♪ ねっ♪」
		ENDIF
	;鬼殺し
	ELSE
		IF ALCOHOL_FACE(111) == "ぐでぐで"
			PRINTFORMW 「～～ッ！！＠＃！＄％！？＊＊＊＊ッ！！」
		ELSEIF ALCOHOL_FACE(111) == "酔いどれ"
			PRINTFORMW 「きっちゅい～～！ こんなことして%CALLNAME:MASTER%はぁ～なにをしゅるきだぁ～～！？」
		ELSE
			PRINTFORMW 「きゃー！ これきついわー！ テンションあがってきたよー！！」
		ENDIF
	ENDIF
ENDIF
RETURN 1
