@M_KOJO_K111_TALKSET(ARGS, ARG, ARG:1)
#DIM 汎用会話数
#DIM 恋慕会話数
汎用会話数 = 18
恋慕会話数 = 44
IF ARGS == "恋人"
	SELECTCASE RAND:(9 + 恋慕会話数 + 汎用会話数)
	CASE 1
		PRINTFORML 「恋人はクラウンピ～ス♪」
		PRINTFORMW 「って歌、どお！？」
	CASE 2
		PRINTFORML 「どうして浮気や不倫はイケナイ事なんだろう？」
		PRINTFORMW 「だって人を好きになるのは本能じゃない？」
		PRINTFORMW 「あ、でも、だからこそ一人に決めるってことに価値があるのかぁ」
	CASE 3
		PRINTFORML 「うーん……………………」
		PRINTFORMW 「ねぇ%CALLNAME:MASTER%、『愛』ってなんだろう？」
	CASE 4
		PRINTFORMW 「%CALLNAME:MASTER%はあたいのどこがスキ～？」
	CASE 5
		PRINTFORML 「もしあたいが他のオトコとエッチしてたらどうする？」
		PRINTFORMW 「%CALLNAME:MASTER%は怒る？ それとも泣いちゃう？」
	CASE 6
		PRINTFORML 「ご主人様に、あたいは好きな子にイタズラしちゃうタイプって言われたの！」
		PRINTFORMW 「なによそれー！ はずかしっ！」
	CASE 7
		PRINTFORML 「妖精は極度に力を失うと、石桜という魔石に姿を変えて眠ってしまうの」
		PRINTFORMW 「もし万が一あたいがそうなってしまったら、%CALLNAME:MASTER%、あたいを守ってね……」
	CASE 8
		IF CFLAG:[[クラウンピース]]:子供人数 > 1
			PRINTFORML 「もしも%CALLNAME:MASTER%が挫けそうになったら、あたいや%CHILDNAME:[[クラウンピース]]:(CFLAG:[[クラウンピース]]:子供人数 % 10)%達が居ることを思い出してね！」
		ELSEIF CFLAG:[[クラウンピース]]:子供人数
			PRINTFORML 「もしも%CALLNAME:MASTER%が挫けそうになったら、あたいや%CHILDNAME:[[クラウンピース]]:1%が居ることを思い出してね！」
		ELSE
			PRINTFORML 「もしも%CALLNAME:MASTER%が挫けそうになったら、あたいが居ることを思い出してね！」
		ENDIF
		PRINTFORMW 「あたいも困難に出くわした時は%CALLNAME:MASTER%を想うから♪」
	CASE 9
		SELECTCASE M_KOJO_K111_MASTER_IMPRESSION()
		CASE "ロリコン"
			PRINTFORMW 「あたいはねー、%CALLNAME:MASTER%のちょっとエッチなトコロがスキ～❤」
		CASE "清掃中毒者"
			PRINTFORMW 「あたいはねー、掃除に夢中になってる%CALLNAME:MASTER%がスキ～❤」
		CASE "料理人"
			PRINTFORMW 「あたいはねー、お料理作ってる%CALLNAME:MASTER%がスキ～❤」
		CASE "弾幕強者"
			PRINTFORMW 「あたいはねー、%CALLNAME:MASTER%の強くて頼れるトコロがスキ～❤」
		CASE "昆虫王"
			PRINTFORMW 「あたいはねー、虫捕りに夢中になってる%CALLNAME:MASTER%がスキ～❤」
		CASE "モテ男"
			;ずるしてません？
			PRINTFORMW 「%CALLNAME:MASTER%、ズルして浮気してない？」
		CASE "インテリ"
			PRINTFORMW 「あたいはねー、%CALLNAME:MASTER%の頭が冴えてるトコロがスキ～❤」
		CASE "饒舌"
			PRINTFORMW 「あたいはねー、いつも楽しいお話してくれる%CALLNAME:MASTER%がスキ～❤」
		CASE "金持ち"
			PRINTFORMW 「あたいはねー、お金に目がない%CALLNAME:MASTER%がスキ～❤」
		CASEELSE
			PRINTFORMW 「あたいはねー、%CALLNAME:MASTER%の冴えないトコロがスキ～❤」
		ENDSELECT
	CASEELSE
		ARGS = 恋慕
		RESTART
	ENDSELECT
ELSEIF ARGS == "恋慕"
	SELECTCASE RAND:(恋慕会話数 + 汎用会話数)
	CASE 1
		PRINTFORML 「ここはいいね、苦しそうにしてる人がいなくて」
		PRINTFORMW 「地獄は怨嗟の声しか聞こえないから退屈でしょうがないよ」
	CASE 2
		PRINTFORML 「%CALLNAME:MASTER%は何が好きなのー？」
		PRINTFORMW 「あたいはねー……」
	CASE 3
		PRINTFORML 「この服かわいいでしょー！」
		PRINTFORMW 「ご主人様に作ってもらったのよ♪」
	CASE 4
		PRINTFORML 「今日は何して遊ぼっかなー」
		PRINTFORMW 「カブでも育ててみようかな」
	CASE 5
		PRINTFORML 「友人様はねー、お話してくれたりー、遊んでくれたりー」
		PRINTFORMW 「ナデナデしてくれたりー、ぎゅーってしてくれたりー、とっても優しいんだ～♪」
	CASE 6
		PRINTFORML 「そういえば%CALLNAME:MASTER%ってどんな能力があるの？」
		PRINTFORMW 「あたいと本気で戦ったらどうなるかな？」
	CASE 7
		IF TALENT:[[クラウンピース]]:恋人
			PRINTFORML 「人間と妖怪の恋物語は聞いたことがあるけど」
			PRINTFORML 「人間と妖精の恋物語なんて聞いたことがないわ」
			PRINTFORMW 「きっとあたいたちが初めてね❤」
		ELSE
			PRINTFORML 「あたいは妖精で、%CALLNAME:MASTER%は人間……」
			PRINTFORMW 「う～ん、%CALLNAME:MASTER%はどう思う？」
		ENDIF
	CASE 8
		PRINTFORML 「ねえ%CALLNAME:MASTER%ー」
		PRINTFORMW 「えへへ、呼んだだけー」
	CASE 9
		PRINTFORML 「あーあ、%CALLNAME:MASTER%が地獄に堕ちてくれたら地獄もちょっとは楽しくなるのになー」
