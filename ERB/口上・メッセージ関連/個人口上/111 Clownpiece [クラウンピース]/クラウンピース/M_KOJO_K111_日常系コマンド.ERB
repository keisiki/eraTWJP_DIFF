;	ELSEIF !GETBIT(TCVAR:111:359, 23) && M_KOJO_K111_EARTH(CFLAG:[[クラウンピース]]:現在位置) && OUTROOF(CFLAG:[[クラウンピース]]:現在位置) && !RAND:50
;		CALL M_KOJO_K111_TALKSET("状況会話", 23)
	;24,一緒に住んでる時の話,6日に1回
	ELSEIF !GETBIT(TCVAR:111:359, 24) && M_KOJO_K111_JUNKO_KURASHI() && DAY % 6 == 0
		CALL M_KOJO_K111_TALKSET("状況会話", 24)
