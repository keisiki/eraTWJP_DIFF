SIF DAY:2 == 4 && DAY:3 == 14
	TALENT:111:機嫌 = MAX(TALENT:111:機嫌, 0)
;ハロウィン
IF DAY:2 == 3 && DAY:3 == 30
	CALL M_KOJO_EVENT_K111_1_HALLOWEEN
;元旦（2年目以降）
ELSEIF DAY:2 == 1 && DAY:3 == 1 && DAY != 1
	CALL M_KOJO_EVENT_K111_1_GANTAN
