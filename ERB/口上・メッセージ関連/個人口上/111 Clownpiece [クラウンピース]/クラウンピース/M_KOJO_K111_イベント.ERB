@M_KOJO_EVENT_K111_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた＆TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (ARG == 1 || ARG == 2) && !TCVAR:111:358
		CALL M_KOJO_EVENT_K111_1_MAINMAP(ARG)
		RETURN 1
	ENDIF
;	;-------------------------------------------------
;	;TARGETが入ってきた
;	;記入チェック（=0, 非表示、1, 表示）
;	LOCAL:1 = 0
;	;-------------------------------------------------
;
;	;統合しました
;
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;恥薄いと大体入ってくる
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「えへ\/\/\/ じゃあ一緒に入ろっ\/\/\/」
		CALL M_KOJO_EVENT_K111_1_END(ARG)
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「ごめんねー、お邪魔しましたー」
		CALL M_KOJO_EVENT_K111_1_END(ARG)
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「ふふーん♪ あたいが入るから%CALLNAME:MASTER%は出てってね♪」
		CALL M_KOJO_EVENT_K111_1_END(ARG)
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6 && !TCVAR:111:358
		;ハロウィン
		IF DAY:2 == 3 && DAY:3 == 30
			CALL M_KOJO_EVENT_K111_1_HALLOWEEN
		;元旦（2年目以降）
		ELSEIF DAY:2 == 1 && DAY:3 == 1 && DAY != 1
			CALL M_KOJO_EVENT_K111_1_GANTAN
		;クリスマスイブ
		ELSEIF DAY:2 == 4 && DAY:3 == 24 && !TCVAR:[[クラウンピース]]:今日の贈り物
			CALL M_KOJO_EVENT_K111_1_XMAS
		ELSE
			PRINTFORML 「あ！ %CALLNAME:MASTER%！」
			IF TALENT:111:恋慕
				;月
				IF CFLAG:111:現在位置 >= 1000 && CFLAG:111:現在位置 < 1100
					;恋人
					IF TALENT:111:恋人
						PRINTFORMW 「会いに来てくれたの！？」
						PRINTFORMW 「わーい♪ %CALLNAME:MASTER%大好きー❤」
					ELSE
						PRINTFORMW 「なにしにきたの？」
						PRINTFORMW 「せっかくだし遊ぼうよー！」
					ENDIF
				ELSE
					SELECTCASE RAND:5
					CASE 1
						PRINTFORMW 「こんなところで会うとは奇遇ね！」
					CASE 2
						PRINTFORMW 「ねえねえ、一緒にどこか行かない？」
					CASE 3
						PRINTFORMW 「ひとりでおでかけ？ あたいと遊ぼうよ～」
					CASE 4
						PRINTFORMW 「ねぇ%CALLNAME:MASTER%～！ 暇なら一緒に遊ばないー？」
					CASEELSE
						PRINTFORMW 「遊ぼっ遊ぼっ♪」
					ENDSELECT
				ENDIF
			ELSE
				PRINTFORMW 「ねえねえ、なにしてるのー？」
			ENDIF
			;バレンタイン
			SIF DAY:2 == 4 && DAY:3 == 14
				CALL M_KOJO_EVENT_K111_1_VALENTINE
		ENDIF
		CALL M_KOJO_EVENT_K111_1_END(ARG)
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7 && !TCVAR:111:358
		;ハロウィン
		IF DAY:2 == 3 && DAY:3 == 30
			CALL M_KOJO_EVENT_K111_1_HALLOWEEN
		;元旦（2年目以降）
		ELSEIF DAY:2 == 1 && DAY:3 == 1 && DAY != 1
			CALL M_KOJO_EVENT_K111_1_GANTAN
		;クリスマスイブ
		ELSEIF DAY:2 == 4 && DAY:3 == 24 && !TCVAR:[[クラウンピース]]:今日の贈り物
			CALL M_KOJO_EVENT_K111_1_XMAS
		ELSE
			PRINTFORML 「あ！ %CALLNAME:MASTER%！」
			IF TALENT:111:恋慕
				;ヘカーティアとデート
				IF FLAG:デート相手 == [[ヘカーティア]]
					PRINTFORML 「ご、ご主人様……！」
					PRINTFORMW 「えっと、あの、あたいはここに居ても良いですか……？」
				;純狐とデート
				ELSEIF FLAG:デート相手 == [[純狐]]
					PRINTFORML 「ゆ、友人様……！」
					PRINTFORMW 「えっと、あの、あたいはここに居ても良いですか……？」
				;三月精orチルノとデート
				ELSEIF GROUPMATCH(FLAG:デート相手, [[サニー]], [[ルナ]], [[スター]], [[チルノ]])
					;ここはフルネーム
					PRINTFORML 「ゲッ！ お前は%NAME:(FLAG:デート相手)%！」
					IF TALENT:[[クラウンピース]]:恋人
						PRINTFORMW 「%CALLNAME:MASTER%はあたいのダーリンなんだから！ あんたは離れなさいよー！」
					ELSE
						PRINTFORMW 「%CALLNAME:MASTER%は私と遊ぶんだから！ 離れなさいよ！」
					ENDIF
				;ラルバとデート,ちょっと仲良し
				ELSEIF FLAG:デート相手 == [[ラルバ]]
					PRINTFORMW 「%CALLNAME:(FLAG:デート相手)%と一緒に遊んでるの？」
					IF TALENT:[[クラウンピース]]:恋人
						PRINTFORMW 「一応言っておくけど、%CALLNAME:MASTER%はあたいのダーリンなんだからね！」
					ELSE
						PRINTFORMW 「いいなぁ！ あたいも混ぜてよ！」
					ENDIF
				;妖精とデート
				ELSEIF TALENT:(FLAG:デート相手):妖精
					PRINTFORMW 「なによその妖精、仲良いのー？」
					SIF TALENT:[[クラウンピース]]:恋人
						PRINTFORMW 「%CALLNAME:MASTER%はあたいのダーリンなんだからねっ！」
				;それ以外
				ELSE
					PRINTFORMW 「むっ、だーれその娘？」
					SIF TALENT:[[クラウンピース]]:恋人
						PRINTFORMW 「%CALLNAME:MASTER%はあたいのダーリンなんだからねっ！」
				ENDIF
			ELSE
				;ヘカーティアとデート
				IF FLAG:デート相手 == [[ヘカーティア]]
					PRINTFORML 「ご主人様も！」
					PRINTFORMW 「お二人でなにしてたんですか？」
				;純狐とデート
				ELSEIF FLAG:デート相手 == [[純狐]]
					PRINTFORML 「友人様も！」
					PRINTFORMW 「お二人でなにしてたんですか？」
				;三月精orチルノorラルバとデート
				ELSEIF GROUPMATCH(FLAG:デート相手, [[サニー]], [[ルナ]], [[スター]], [[チルノ]], [[ラルバ]])
					PRINTFORML 「おっ、%CALLNAME:(FLAG:デート相手)%もいるじゃん！」
					PRINTFORMW 「なにしてるの？ あたいも混ぜてよー！」
				ELSE
					PRINTFORMW 「ねえねえ、なにしてるのー？」
				ENDIF
			ENDIF
			;バレンタイン
			SIF DAY:2 == 4 && DAY:3 == 14
				CALL M_KOJO_EVENT_K111_1_VALENTINE
		ENDIF
		CALL M_KOJO_EVENT_K111_1_END(ARG)
		RETURN 1
	ENDIF
