;SENSE        贈り物に設定された属性。STRCOUNT(DISH_TASTE, "ほげほげ/")を使うと特定の属性を判定できます。
;GIFT_ID      これ単体では使いません。以下の式内関数を使うとデータを抽出できます（こだわりたい人用）
;　GET_GIFTDATA(GIFT_ID, "回数")    貰った（あげた）回数
;　GET_GIFTDATA(GIFT_ID, "得点")    得点（1～999）,口上テンプレ内では用意されてます。
;　GET_GIFTDATA(GIFT_ID, "日付")    貰った（あげた）日付（1～9999）
;　GET_GIFTDATA(GIFT_ID, "場所")    買った場所（デートの行き先）
;　GET_GIFTDATA(GIFT_ID, "形容詞")  形容詞番号（贈り物_形容詞.erb参照）
;　GET_GIFTDATA(GIFT_ID, "本体")    本体番号（贈り物_本体.erb参照）
;--------------------------------------------------------------------------------------------------
;デート帰りに口上主があなたに贈り物を渡す（陥落や好感度に関わらず親密5以上から発生）
;評価点は701～999（渡すキャラ本人の評価）
;--------------------------------------------------------------------------------------------------
IF ARG == 1
	SELECTCASE RAND:3
	CASE 1
		PRINTFORMW 「あ、待って待って！ %CALLNAME:MASTER%！」
		PRINTFORMW 「今日はね、%CALLNAME:MASTER%にとっておきのプレゼントがあるの！」
	CASE 2
		PRINTFORMW 「今日は%CALLNAME:MASTER%にね、サプライズがありまーす！」
		PRINTFORMW 「ジャーン！」
	CASEELSE
		PRINTFORMW 「そうだ！ 今日は%CALLNAME:MASTER%にプレゼントを用意してきたの！」
		PRINTFORMW 「はい、これあげる！」
	ENDSELECT
	PRINTL 
	PRINTFORMDL クラウンピースからのプレゼントは%GIFT_NAME%だった。
	PRINTW 
	IF STRCOUNT(SENSE, "(クレイジーな|地獄の|狂気の)")
		PRINTFORML 「どう？ 最高にキメてるでしょ～！」
		PRINTFORMW 「あたいだと思って大事にしてねっ❤」
	ELSEIF STRCOUNT(SENSE, "(かわいい|怖い)")
		PRINTFORML 「どう？ カワイイでしょ～！」
		PRINTFORMW 「%CALLNAME:MASTER%に似合うと思って選んだんだー」
	ELSE
		PRINTFORML 「どう？ イカすでしょ～！」
		PRINTFORMW 「でも%CALLNAME:MASTER%にはちょっと似合わないかもね～」
	ENDIF
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の終わりに今日貰った贈り物を大事にする（700点以上）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 2
	IF STRCOUNT(SENSE, "(地獄の|狂気の|クレイジーな|骨|妖術)")
		CALL M_KOJO_COLOR_K111_HECA
		PRINTFORML 「あらピース、コレどうしたの？ 随分イカしてるじゃない」
		PRINTW 
		CALL M_KOJO_COLOR_K111
		PRINTFORML 「えへへ～♪ いいでしょ～！ %CALLNAME:MASTER%に貰ったんですよ～♪」
		PRINTW 
		CALL M_KOJO_COLOR_K111_HECA
		IF TALENT:[[ヘカーティア]]:恋慕
			PRINTFORML 「あらぁ～！ さすがは%CALLNAME:MASTER%ね、いいセンスしてるわ♪」
		ELSEIF TALENT:[[ヘカーティア]]:思慕
			PRINTFORML 「へぇ～、なかなかいいセンスしてるわね。見直しちゃったわ」
		ELSE
			PRINTFORML 「ふーん、なかなかいいセンスしてるのね」
		ENDIF
	ELSEIF STRCOUNT(SENSE, "(ポップ|ロリータ|かわいい)")
		CALL M_KOJO_COLOR_K111
		PRINTFORML 「ねぇねぇご主人様！ 見てください、コレ！」
		PRINTW 
		CALL M_KOJO_COLOR_K111_HECA
		PRINTFORML 「あら、カワイらしいわねぇ。あなたにお似合いよん、ピース♪」
		PRINTW 
		CALL M_KOJO_COLOR_K111
		PRINTFORML 「えへへ～！ %CALLNAME:MASTER%に貰ったんですよ～♪」
		PRINTW 
		CALL M_KOJO_COLOR_K111_HECA
		IF TALENT:[[ヘカーティア]]:恋慕
			PRINTFORML 「やっぱり！ %CALLNAME:MASTER%はピースの事大好きだからねぇん」
		ELSEIF TALENT:[[ヘカーティア]]:思慕
			PRINTFORML 「なるほど～。%CALLNAME:MASTER%ってばピースの事よく判ってるじゃない」
		ELSE
			PRINTFORML 「ふーん、悪い人間ではないようねぇ」
		ENDIF
	ELSE
		CALL M_KOJO_COLOR_K111_HECA
		PRINTFORML 「あら、ピース、どうしたのそれ？」
		PRINTW 
		CALL M_KOJO_COLOR_K111
		PRINTFORML 「ふふふ、いいでしょ～♪ コレはですねぇ、%CALLNAME:MASTER%が買ってくれたんですよ～♪」
		PRINTFORML 「あっ！ ご主人様には渡しませんよ！」
		PRINTW 
		CALL M_KOJO_COLOR_K111_HECA
		IF TALENT:[[ヘカーティア]]:恋慕
			IF CFLAG:[[ヘカーティア]]:貰った贈り物
				PRINTFORML 「フフン♪ 私はもっといいモノ持ってるわよん❤」
			ELSEIF CFLAG:[[ヘカーティア]]:贈り物ストック
				PRINTFORML 「フフン♪ 私だって持ってるわよん」
				SIF GET_GIFTDATA(CFLAG:[[ヘカーティア]]:贈り物ストック, "得点") < 400
					PRINTFORML （質に入れちゃったけど……）
			ELSE
				PRINTFORML 「ううむ…私には何も寄越さないのに……まぁいいわ」
			ENDIF
		ELSEIF TALENT:[[ヘカーティア]]:思慕
			PRINTFORML 「あらあら、随分とご機嫌ねぇ」
		ELSE
			PRINTFORML 「別に取ったりしないわよん」
		ENDIF
	ENDIF
	PRINTFORML 「ピース、無くさないよう大事にするのよ」
	PRINTW 
	CALL M_KOJO_COLOR_K111
	PRINTFORMW 「うん！ あたいの宝物にするー❤」
	CFLAG:[[クラウンピース]]:1007 ++
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の終わりに今日貰った贈り物をお気に入りにする（↑の大事にするから続き）（700点以上かつ最高得点）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 3
	PRINTL 
	IF TALENT:[[クラウンピース]]:恋人
		PRINTFORML 「そうだ！ いつもコレを持っておけば無くさないし――」
		PRINTFORMW 「いつでも%CALLNAME:MASTER%が見守ってくれる気がする……」
	ELSEIF TALENT:[[クラウンピース]]:恋慕
		PRINTFORML 「そうだ！ いつもコレを持っておけば無くさないし――」
		PRINTFORMW 「……なんだか%CALLNAME:MASTER%を感じていられる気がする……」
	ELSE
		PRINTFORML 「そうだ！ いつもコレを持っておけば無くさないぞ～」
	ENDIF
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の終わりに今日貰った贈り物をとりあえず保管する（400～699点）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 4
	;たまに介入してくる
	IF !RAND:3
		CALL M_KOJO_COLOR_K111_JUNKO
		PRINTFORML 「あら、何かしらこれ？」
		PRINTW 
		CALL M_KOJO_COLOR_K111
		PRINTFORML 「あ！ 友人様！ これはですね、%CALLNAME:MASTER%に貰ったんですよー！」
		PRINTW
		CALL M_KOJO_COLOR_K111_JUNKO
		IF TALENT:[[純狐]]:恋慕
			IF CFLAG:[[純狐]]:貰った贈り物
				PRINTFORML 「そう……まぁ私の物に比べたら……」
			ELSEIF CFLAG:[[純狐]]:贈り物ストック
				PRINTFORML 「そう……私だけじゃなかったのね……」
			ELSE
				;漢字で書くと誤解されそうなので平仮名で……
				PRINTFORML 「そう……私もねだってみようかしら……」
			ENDIF
		ELSEIF TALENT:[[純狐]]:思慕
			PRINTFORML 「%CALLNAME:MASTER%……成程ね」
		ELSE
			PRINTFORML 「そう……良かったわね」
		ENDIF
		PRINTFORML 「それ、どうするの？」
		PRINTW
		CALL M_KOJO_COLOR_K111
		PRINTFORMW 「そうですねぇ、とりあえず飾って置こうかなぁ」
		PRINTL …
		PRINTL ………
		PRINTL ……………
		PRINTW 
	ENDIF
	CALL M_KOJO_COLOR_K111
	SELECTCASE CFLAG:[[クラウンピース]]:1007 % 5
	CASE 1
		PRINTFORMW 「よし！ ここに置こうっと」
	CASE 2
		PRINTFORMW 「よく判んないしこのへんにでも飾っておこ」
	CASE 3
		PRINTFORMW 「ここを%CALLNAME:MASTER%置き場にしようかな～」
	CASE 4
		PRINTFORMW 「うーん、どこにしまっておこう……」
	CASEELSE
		PRINTFORML 「あ！ 貰ったのすっかり忘れてた！」
		PRINTFORMW 「もう適当に置いておけばいっか」
	ENDSELECT
	IF CFLAG:[[クラウンピース]]:1007 >= 3
		SELECTCASE CFLAG:[[クラウンピース]]:1007 % 4
		CASE 1
			PRINTFORMW 「気づいたらこんなに貰ってたんだなぁ」
		CASE 2
			PRINTFORMW 「これ以上は置く場所に困っちゃうかも……」
		CASE 3
			PRINTFORMW 「うんうん、充実してきた♪」
		CASEELSE
			PRINTFORMW 「すごく充実してきたぞ～♪」
		ENDSELECT
	ENDIF
	CFLAG:[[クラウンピース]]:1007 ++
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の終わりに今日貰った贈り物をこっそり質に入れる（400点未満）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 5
	CALL M_KOJO_COLOR_K111_HECA
	PRINTFORML 「あらピース、コレどうしたの？」
	PRINTW 
	CALL M_KOJO_COLOR_K111
	PRINTFORML 「それはですねぇ、%CALLNAME:MASTER%に貰ったんですけど……」
	PRINTW 
	CALL M_KOJO_COLOR_K111_HECA
	IF TALENT:[[ヘカーティア]]:恋慕
		IF CFLAG:[[ヘカーティア]]:贈り物ストック
			PRINTFORML 「へ、へぇ……よ、良かったじゃないの……」
			PRINTFORML （私にじゃなくて良かったわぁ～）
		ELSE
			PRINTFORML 「へ、へぇ……羨ましいような、そうでもないような……」
		ENDIF
	ELSEIF TALENT:[[ヘカーティア]]:思慕
		PRINTFORML 「あらら……そんな残念な人間だったのかしら……」
	ELSE
		PRINTFORML 「あらら、随分とナンセンスな人間ねぇ……」
	ENDIF
	PRINTFORML 「まぁ、貰ったものなんだし、大事にするのよん」
	PRINTW
	CALL M_KOJO_COLOR_K111
	PRINTFORML 「えー！？ イヤですよこんなの！」
	PRINTFORMW 「『シニガミ』に出しちゃおーっと！」
	;地獄界の大手フリマサービス『シニガミ』は、処分品を効率よく、かつ迅速に市場に流せる便利なサービスなのだ。
	RETURN 1
ENDIF
RETURN 1


;==================================================
;ムシバトル
;ムシバトルの口上は少し特殊で、PRINTを使うのではなく、
;台詞一行のみをRESULTSに入れてください
;ムシの名前は %MUSHI_NAME% で表示できます
;ムシの名前の最長はアレキサンドラトリバネアゲハの全角14文字です
;文字数制限があります。はみ出ないように気をつけてください
;ARGは残りムシ数か順位です
;==================================================
@M_KOJO_MESSAGE_COM_K111_MUSHI_BATTLE(ARGS, ARG)
#DIM 残りムシ数
#DIM 順位
SELECTCASE ARGS
CASE "試合前"
	;-------------------------------------------------
	;キャラセレクト時,ARGは使えません
	;全角50文字くらいまで
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「フッフッフ……地獄を見ることになるよ」
	ENDIF
CASE "戦場に出した"
	;-------------------------------------------------
	;"【（キャラ）】は《（ムシ）》を戦場に出した！」"の直後
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「いけっ！ %MUSHI_NAME%！」
	ENDIF
CASE "攻撃時"
	;-------------------------------------------------
	;"【（キャラ）】《（ムシ）》の『（攻撃名）』！」"の直前
	;全角40文字まで（カギカッコ含む）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	残りムシ数 = ARG
	;-------------------------------------------------
	IF LOCAL:1
		RESULTS = 「ぶっとばせー！%MUSHI_NAME%！」
	ENDIF
CASE "倒れた時"
