		CASE 601 TO 1080
			;遅起き（4時～10時以外）,迷ったけど10時は許す！
			IF !INRANGE(TIME:3, 240, 600)
				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「おはよ～%CALLNAME:MASTER%、もうお昼だよ～」
				CASE 2
					PRINTFORML 「おはよう、ねぼすけー！」
				CASEELSE
					PRINTFORML 「あ、%CALLNAME:MASTER%ー、いつまで寝てるの～？」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「あ、%CALLNAME:MASTER%、%M_KOJO_EVENT_K111_1_HELLO()%」
				CASE 2
					PRINTFORML 「%M_KOJO_EVENT_K111_1_HELLO()%、%CALLNAME:MASTER%！」
				CASEELSE
					PRINTFORML 「あ、%CALLNAME:MASTER%、グッダフタヌ～ン♪」
				ENDSELECT
			ENDIF
		;18時～
		CASEELSE
			;夜型？（4時～18時以外）
