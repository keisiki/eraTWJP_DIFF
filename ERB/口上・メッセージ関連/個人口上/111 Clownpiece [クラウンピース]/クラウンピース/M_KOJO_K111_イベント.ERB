				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「おはよ～%CALLNAME:MASTER%、もう夜だよ～」
				CASE 2
					PRINTFORML 「おはよう、スーパーねぼすけ！」
				CASEELSE
					PRINTFORML 「あ、%CALLNAME:MASTER%ー、寝すぎー」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「あ、%CALLNAME:MASTER%、%M_KOJO_EVENT_K111_1_HELLO()%」
				CASE 2
					PRINTFORML 「%M_KOJO_EVENT_K111_1_HELLO()%、%CALLNAME:MASTER%！」
				CASEELSE
					PRINTFORML 「あ、%CALLNAME:MASTER%、グッディーブニ～ン♪」
				ENDSELECT
			ENDIF
		ENDSELECT
	;来訪
	ELSE
		PRINTFORML 「あ、%CALLNAME:MASTER%！」
	ENDIF
	;▲ここまで挨拶
	;まれにこける
	IF !RAND:200
		PRINTFORML 「あたっ！！」
		PRINTL 
		PRINTFORMDW クラウンピースは駆け寄ってこようとして盛大にすっ転んだ。
	;溜まってるとたまに
	ELSEIF TALENT:[[クラウンピース]]:恋慕 && CFLAG:111:溜まってる度 >= 750 && !RAND:5
		PRINTFORML 「ねぇ～%CALLNAME:MASTER%～、あそぼ～❤」
		PRINTL 
		PRINTFORMDW クラウンピースは甘えるように抱きついてきた。
	ELSE
		SELECTCASE RAND:12
		CASE 1
			PRINTFORMW 「およ？ ほっぺにごはんつぶが付いてるよっ！」
		CASE 2
			PRINTFORMW 「今日は何して遊ぼうかな～」
		CASE 3
			PRINTFORMW 「いまひま？ ねえひま？？」
		CASE 4
			PRINTFORMW 「ちょうど遊びたいと思ってたんだー！」
		CASE 5
