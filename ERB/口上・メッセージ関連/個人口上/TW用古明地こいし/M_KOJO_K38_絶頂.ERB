					PRINTFORMW 
				ELSE
					PRINTFORML 「きゃ%UNICODE(0x2764)% んん%UNICODE(0x2764)% %CALLNAME:MASTER%のザーメン…こいしの、ナカに…%UNICODE(0x2764)%」
					PRINTFORMW 
				ENDIF
			CASE 6
				IF ESTRUS_CYCLE(TARGET) == 150
					PRINTFORML 「あ、はぁぁ%UNICODE(0x2764)% 危険日なのに…いっぱい…中出しされてる…%UNICODE(0x2764)%」
					PRINTFORML 「赤ちゃんできちゃうかも…なのに…%UNICODE(0x2764)% 私の身体…%CALLNAME:MASTER%に種付けされるの悦んじゃってる…%UNICODE(0x2764)%」
					PRINTFORMW 
				ELSE
					PRINTFORML 「あぁ、すごい……%UNICODE(0x2764)% こいしのナカに中出しされちゃってる…%UNICODE(0x2764)%」
					PRINTFORML 「はふ……気持ち、いぃ……%UNICODE(0x2764)%」
					PRINTFORMW 
				ENDIF
			CASE 7
				PRINTFORML 「きゃ%UNICODE(0x2764)% ん……%UNICODE(0x2764)% ドクドクって…こいしの、ナカに……%UNICODE(0x2764)%」
				PRINTFORML 「熱いの…どんどん広がって……素敵……%UNICODE(0x2764)%」
				PRINTFORMW 
			CASE 8
				PRINTFORML 「あ、はぁ……%UNICODE(0x2764)% 熱いのいっぱい…注がれちゃってる%UNICODE(0x2764)%」
				PRINTFORML 「……これ、すごぉい%UNICODE(0x2764)%%UNICODE(0x2764)%%UNICODE(0x2764)%」
				PRINTFORMW 
			CASE 9
				PRINTFORML 「はぁ…%UNICODE(0x2764)% はぁ…%UNICODE(0x2764)% んっ%UNICODE(0x2764)% あついの、こいしのナカで広がってく…%UNICODE(0x2764)%」
				PRINTFORML 「%CALLNAME:MASTER%に愛された証が、私を満たしていくのを感じるの…%UNICODE(0x2764)%」
				PRINTFORMW
 			CASEELSE
				IF EX:膣内精液 >= 1200
					PRINTFORML 「す、ご%UNICODE(0x2764)% せーえきで、こいしのおなか……ふくらんじゃってる%UNICODE(0x2764)%」
					PRINTFORML 「……%CALLNAME:MASTER%のけだもの%UNICODE(0x2764)%」
					PRINTFORMW 
				ELSE
					PRINTFORML 「ぁ……%UNICODE(0x2764)% %CALLNAME:MASTER%になかに出されるの……すき%UNICODE(0x2764)%」
					PRINTFORMW 
				ENDIF
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:MASTER:濃厚精液
			IF RAND:3 == 0
				PRINTFORML 「あ、……ナカ……に」
				PRINTFORML こいしは虚ろな目で射精が続く結合部に視線を向けている
				PRINTFORMW 
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORML 「は……ぁ……妊娠、しちゃう……」
				PRINTFORMW 
				RETURN 1
			ELSE
				PRINTFORML 「あ……あぁ……まだ、出……て……」
				PRINTFORMW 
				RETURN 1
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORML 「あ、はぁ……おなか、どくどくってしてる」
				PRINTFORML 「……あつい……沁みこんでくる、みたい」
				PRINTFORMW 
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORML 「は……ぁ、すご……ぃ」
				PRINTFORMW 
				RETURN 1
			ELSE
				PRINTFORML 「はっ……はッ……は……ぁ」
				PRINTFORML 「すご……私のナカで、まだ、射精してる……」
				PRINTFORMW 
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
