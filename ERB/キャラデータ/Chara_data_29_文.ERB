;-------------------------------------------------------------------------------------------
@IMAGE_I29_特殊表情(CHARA)
#DIM CHARA

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF FLAG:立ち絵種類
		;顔絵
	ELSE
		;立ち絵
	ENDIF
ELSE
	;通常画像
	IF INRANGE(CFLAG:CHARA:現在位置, 200, 299) && !CHECK_NUDE(CHARA)
		SIF CHECK_BLUSH(CHARA, @"%GET_BASESTYLE(CHARA)%", "_人里")
			RESULTS = 赤面
		SIF BASE:CHARA:ムード >= MAXBASE:CHARA:ムード*8/10
			RESULTS = LOVE
	ENDIF
ENDIF

