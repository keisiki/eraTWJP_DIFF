@CHARAMOVE_DATA_29(ARG,ARG:2)
LOCAL = 29
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 3, 5, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 31 TO 38, 51 TO 55, 71 TO 74, 81 TO 84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ29
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ29(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "文")

;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（スカート）セット")
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "兜巾"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "下駄"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "ネクタイ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "鴉の濡れ羽色の")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "Ｖカットショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "三角ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[文]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;特殊衣装設定
CASE "おうちデート衣装"
	SIF MAIN_MAP == 2
		CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "キャスケット文セット"))
CASE "人里デート衣装"
	CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "キャスケット文セット"))

CASE "固有コマンド"
	CALLF MAKE_STR(V_NAME, "新聞購読")
CASE "固有コマンド実行可能"
	CALLF MAKE_INT(V_NAME, !FLAG:新聞購読)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/獣肉/虫肉/和食/煮物/ジャンキー/濃厚/豪華/機能的/野趣/サクサク/天狗流/辛い/辛い/しょっぱい/しょっぱい/" + "秋物/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "洋食/すっぱい/すっぱい/卵/卵/卵/外界風/外界風/外界風/鳥肉/鳥肉/鳥肉/鳥肉/鳥肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "得意料理＊秋物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/宝石/秩序/かわいい/レトロ/知的/不思議/壮大/" + "風/鳥/文具/コイン/風の/秋の/" * 3 + "カラスの/カエデの/天狗の/疾風の/手帳/ネクタイ/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/ポップ/パワフル/ロリータ/" + "弾/小手/" * 3 + "カメの/河童の/鬼の/" * 10)

;農民関連
;天狗も水田持ってそう
;山っぽさをだすならワサビなども可
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "米農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "お米")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "水田")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "妖怪の山・山麓")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 7)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 732)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 450)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 12)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "依頼なし発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 102)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼29(ARG, O_DATA, V_NAME)
ENDSELECT

;IMAGE_I29_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I29_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ENDIF

