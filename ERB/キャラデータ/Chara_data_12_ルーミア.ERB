@CHARAMOVE_DATA_12(ARG,ARG:2)
LOCAL = 12
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;各私室,風呂
			CASE 512, 514, 521, 522, 523, 526, 531, 535, 537
				RETURN -100
			;居間,リビング,二階
			CASE 511, 525, 530
				RETURN -5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 5, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,15, 51,53,56,57, 61,62
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ12
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ12(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ルーミア")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "袖無し上着"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "闇を固めたような")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "漆黒のショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "漆黒のキャミソール")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[ルーミア]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/肉料理/揚げ物/甘い/濃厚/豪華/野趣/サクサク/ガッツリ/ガッツリ/" + "カエル/しょっぱい/グロテスク/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "野菜/すっぱい/淡白/質素/あっさり/あっさり/あっさり/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "濃い味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "怖い/ロリータ/" + "黒/闇/生体品/" * 3 + "妖魔の/宵闇の/闇の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "知的/" + "白/光/日/照明/" * 3)

ENDSELECT

