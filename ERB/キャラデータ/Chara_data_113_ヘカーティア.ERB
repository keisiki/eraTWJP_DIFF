@CHARAMOVE_DATA_113(ARG,ARG:2)
LOCAL = 113
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;綿月亭には行かない
			CASE 1022 TO 1049
				RETURN -100
			;出るとき用
			CASE 1020, 1021
				RETURN -10
			;静かの海は渡らない
			CASE 1001 TO 1015
				RETURN -100
			;物置はあまり行かない
			CASE 1065
				RETURN -5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 6, 10
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 61,62,67, 109
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ113
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ113(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ヘカーティア")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：アクセサリ"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("アクセサリ", "鎖"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "三色スカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "地獄Ｔシャツ"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "首輪"))
CASE "普段着：その他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他２", "ベルト"))
CASE "普段着：その他３"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他３", "惑星"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "見るだけで地獄に落ちそうな")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "『GoToHell』パンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "チューブトップブラ")
CASE "固有下着着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "パン/スパイス/麺類/パイ/ガッツリ/ジャンキー/濃厚/豪華/鮮やか/サクサク/地獄風/外界風/" + "洋食/辛い/しょっぱい/" * 2 + "グロテスク/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "中華/あっさり/ヘルシー/淡白/野趣/すっぱい/すっぱい/質素/質素/月都風/月都風/月都風/月都風/月都風/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "地獄風/魔力/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/青/黄/高級/混沌/怖い/奇妙/派手/パワフル/威厳/壮大/マニアック/" + "闇/宝石/妖術/冥王の/月の/地上の/異界の/" * 3 + "パンデモニックな/紺珠の/女神の/地獄の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "シンプル/レトロ/シック/繊細/" + "安物/" * 3)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
