@CHARAMOVE_DATA_53(ARG,ARG:2)
LOCAL = 53
SELECTCASE ARG
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;夢幻館には行かない
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;他人の家には入らない
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;来客用トイレ,永琳私室,輝夜私室,裏庭,物置,倉庫,地下室はあんまり行かない
			CASE 406, 413, 414, 422, 424, 427, 430
				RETURN -5
			;てゐ私室,鈴仙私室,庭がちょっと好き
			CASE 410, 411, 421
				RETURN 3
			;縁側,居間,兎の洞穴,斜角の竹林,迷いの小道によく居る
			CASE 409, 416, 431, 433, 434
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 4, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 41 TO 47, 51,52,53,56,57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ53
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ53(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "てゐ")
