@CHARAMOVE_DATA_26(ARG, ARG:2)
LOCAL = 26
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;神社の居間
			CASE P9居間
				RETURN 5
			;藍,橙の私室はあまり入らない
			CASE P635橙私室, P638藍私室
				RETURN -5
			;是非曲直庁、畜生界は行かない
			CASE P610是非曲直庁ホール TO P618空き部屋, P640異界の関所 TO P655尤魔宅の風呂
				RETURN -100
			;他人の家には入らない
			CASE P608潤美の家, P609瓔花の家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 神社本堂, 神社縁側
				RETURN 1
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE 広間 , 食堂, 大図書館
				RETURN 1
			CASE 森の入り口, 香霖堂, 古木の大樹, 霧雨魔法店, 人形の洋館, 再思の道, 無縁塚
				RETURN 1
			CASE 中有の道, 三途の川, マヨヒガ, 白玉楼庭, 白玉楼
				RETURN 1
			CASE 守矢神社境内, 守矢神社本殿
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
		;土曜の夜は白玉楼へ遊びに行く
		SIF YOUBI_MATCH(DAY, "土") && INRANGE(TIME, 960, 1200)
			TCVAR:LOCAL:引力点 = P628居間
ENDSELECT

@EXIST_キャラデータ26
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ26(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "紫")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：アクセサリ"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("アクセサリ", "傘"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ガーターストッキング"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
	ENDIF
CASE "普段着：ワンピース"
	IF DAY:2 == 1
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "道士服"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "イブニングドレス"))
	ENDIF

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "怪しげな光沢を放つ")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "シルクサテンショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "シルクサテンブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[紫]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/肉料理/葉物/ヘルシー/芸術的/上品/家庭的/ふわふわ/外界風/和食/和食/甘い/甘い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "ガッツリ/ジャンキー/野趣/しょっぱい/しょっぱい/" + "虫肉/天界風/月都風/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "妖力/妖力/得意料理＊和食＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "高級/美しい/芸術的/レトロ/シック/知的/威厳/巫術/" + "紫/妖術/護符/式神の/夢幻の/" * 3 + "黒死蝶の/生と死の/境界の/隙間の/幻葬の/傘/夢と現の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "ポップ/ロリータ/" + "安物/月/小手/" * 3 + "フェムトファイバーの/天人の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_26
DISH_NAME = ウミガメのスープ
RESULTS = 軽食
CALL 材料設定("得体の知れない肉")
DISH_TASTE = 汁物/肉料理/すっぱい/あっさり/質素/上品/謎料理/
DISH_COMMENT = ウミガメの肉を使ったスープ。幻想郷に海は無いが……。
