@CHARAMOVE_DATA_124(ARG,ARG:2)
LOCAL = 124
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 5, 6, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21, 22, 23, 51, 52, 53, 54, 56, 57, 61, 62, 63, 81, 82, 83
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ124
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ124(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "隠岐奈")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "烏帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "ロングブーツ"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "道士服"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "夜空を投影した")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "星辰のショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "星辰のブラ")
CASE "固有下着着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/煮物/ヘルシー/豪華/芸術的/上品/ぷるぷる/辛い/辛い/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "質素/しょっぱい/しょっぱい/" + "カエル/虫肉/謎料理/" * 3 + "天狗流/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "エンチャント/加護/絢爛/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黄/高級/混沌/美しい/芸術的/レトロ/派手/知的/威厳/" + "巫術/美術品/四季の/" * 3 + "七星の/後戸の/秘匿された/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "洋風/シック/ロリータ/" + "安物/" * 3 + "天狗の/ヴァカンスの/" * 10)
ENDSELECT

