@CHARAMOVE_DATA_13(ARG, ARG:2)
LOCAL = 13
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;三妖精宅内に行きやすい
			CASE P27三妖精の家・広間 TO P34スター私室
				RETURN 5
			;よく居る
			CASE P343湖畔, P345チルノのかまくら, P346大妖精の棲家, P347霧の湖
				RETURN 5
			;納屋と地下
			CASE P303守衛小屋, P304空き小屋, P305納屋, P328地下廊下 TO P333大浴場
				RETURN -100
			;チルノ以外の私室には入らない
			CASE P311パチュリー私室, P312小悪魔私室, P320咲夜私室, P321空き部屋, P326レミリア私室, P327小バルコニー
				RETURN -100
			CASE P338廃洋館・風呂, P340ルナサ私室, P341メルラン私室, P342リリカ私室, P344レティのかまくら
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 妖精の大樹
				RETURN 1
			CASE 霧の湖, 氷のかまくら, 正門
				RETURN 1
			CASE 森の入り口, 古木の大樹, 再思の道
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ13
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ13(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "大妖精")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "自作らしい")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "毛糸のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "毛糸のブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[大妖精]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/果物/葉物/パイ/すっぱい/あっさり/ヘルシー/淡白/質素/鮮やか/家庭的/ふわふわ/きらめき/甘い/甘い/甘い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "ガッツリ/辛い/辛い/" + "虫肉/スパイス/濃厚/グロテスク/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "薄味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "緑/植物/かわいい/繊細/ロリータ/" + "風/" * 3 + "風の/花の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "怖い/派手/パワフル/威厳/" + "刃物/弾/小手/キセル/" * 3)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "依頼なし発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼13(ARG, O_DATA, V_NAME)
ENDSELECT

