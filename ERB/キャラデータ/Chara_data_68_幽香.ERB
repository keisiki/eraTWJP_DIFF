@CHARAMOVE_DATA_68(ARG,ARG:2)
LOCAL = 68
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;永遠亭の奥には行かない
			CASE 405, 407 TO 430
				RETURN -100
			;他人の家には入らない
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;倉庫にはあまり行かない
			CASE 456
				RETURN -5
			;太陽の畑,ダイニング,リビング,庭によく居る
			CASE 438, 453, 461, 466
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 4, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 41,42,43,46,47, 51,53,56,57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ68
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ68(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "幽香")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")


CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：アクセサリ"
	CALLF MAKE_INT(V_NAME, 6);CLOTHES_ID("アクセサリ", "傘"))
CASE "普段着：靴下"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ガーターストッキング"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
	ENDIF
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：外衣"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外衣", "ベスト"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "スカーフタイ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "色気と可愛さと両立する")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "花柄プリーツパンティ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "花柄ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[幽香]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)
	
;CASE "固有コマンド"
;	CALLF MAKE_STR(V_NAME, "弾幕勝負")
;CASE "固有コマンド実行可能"
;	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "パン/ミルク/洋食/汁物/ケーキ/ガッツリ/ヘルシー/夏物/豪華/上品/鮮やか/ふわふわ/夢幻風/しょっぱい/しょっぱい/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "辛い/辛い/" + "カエル/虫肉/質素/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "夢幻風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "緑/かわいい/オシャレ/レトロ/パワフル/" + "土/植物/" * 3 + "ヒマワリの/花の/華の/夢幻の/種/傘/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "繊細/マニアック/" + "火/刃物/" * 3)

;農民関連
;「かき」と読みます
;園芸用の草花のこと
;仕事の手伝いと被ってるがどうしよう
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "花卉農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "花の苗")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "お花畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "迷いの竹林")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 4)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 438)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 15)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_68
DISH_NAME = ひまわりワッフル
RESULTS = デザート
CALL 材料設定("甘味材")
DISH_TASTE = 洋食/卵/ミルク/甘い/ガッツリ/濃厚/温かい/夏物/芸術的/サクサク/夢幻風/
DISH_COMMENT = ひまわりを象ったカワイイワッフル。できれば夏に食べたい。
RETURN 1

;IMAGE_I68_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I68_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF EQUIP:CHARA:上半身上着２ == 3
			RESULTS = _パジャマ
	ENDIF
ENDIF

