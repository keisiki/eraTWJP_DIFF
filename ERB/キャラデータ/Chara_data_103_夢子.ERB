@CHARAMOVE_DATA_103(ARG,ARG:2)
LOCAL = 103
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;魔理沙寝室,アリスの部屋,神綺の部屋
			CASE 512, 521, 522
				RETURN -10
			;自室
			CASE 523
				;7～22時は避ける,寝起きと寝る前は行きやすく
				IF BETWEENTIME(420,1320)
					RETURN -10
				ELSE
					RETURN 10
				ENDIF
			;研究室
			CASE 524
				RETURN -10
			;リビング
			CASE 525
				;10～17時外は行きやすい
				IF BETWEENTIME(600,1020)
					RETURN 0
				ELSE
					RETURN 5
				ENDIF
			;空き部屋と倉庫
			CASE 528, 531
				RETURN -100
			;魔理沙家,エレン家,妖怪の棲み処
			CASE 511 TO 515, 535, 536, 537, 541, 542, 543, 544
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 51 TO 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ103
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ103(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "夢子")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "ホワイトブリム"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "エプロンドレス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "謎の重みと冷たさがある")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "夢子のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "夢子のブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "パン/ミルク/洋食/葉物/パイ/ヘルシー/淡白/豪華/上品/家庭的/ふわふわ/魔力/魔界風/甘い/甘い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "濃厚/野趣/しょっぱい/しょっぱい/カエル/カエル/カエル/虫肉/虫肉/虫肉/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "魔界風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/美しい/シンプル/洋風/繊細/" + "神霊/刃物/" * 3 + "メイドの/魔界の/懐剣/護刀/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/和風/パワフル/ロリータ/" + "生体品/" * 3)
ENDSELECT

