@CHARAMOVE_DATA_77(ARG,ARG:2)
LOCAL = 77
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
		;連動処理（穣子の方が高いときは穣子を参照）
		IF TALENT:78:恋慕 || (TALENT:78:思慕 && !TALENT:77:思慕)
			CFLAG:LOCAL:現在位置 = CFLAG:78:遭遇位置 ? CFLAG:78:遭遇位置 # SUKIMA()
		ELSE
			CFLAG:LOCAL:現在位置 = CFLAG:77:遭遇位置 ? CFLAG:77:遭遇位置 # SUKIMA()
		ENDIF
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;私室
			CASE 717
				;起床後と就寝前以外は避ける
				IF BETWEENTIME(1200,420)
					RETURN 10
				ELSE
					RETURN -10
				ENDIF
			;畑
			CASE 715
				;畑によく居る
				IF BETWEENTIME(540,1400)
					RETURN 10
				ELSE
					RETURN -5
				ENDIF
			;アジト,厄家,百合家,華扇家,隠れ家,小屋
			CASE 704, 706, 709, 720 TO 730, 733, 734
				RETURN -100
			CASEELSE
				;秋は活発になる
				IF DAY:2 == 3
					CFLAG:LOCAL:移動率補正 = 5
				ELSE
					CFLAG:LOCAL:移動率補正 = -5
				ENDIF
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 7, 8
				RETURN 1
			CASE 2
				SIF DAY:2 == 3
					RETURN 1
				RETURN 0
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 71,72,73, 81,82,83,84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ77
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ77(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "静葉")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "髪飾り"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "秋の終焉を体現する")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "紅葉のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "紅葉のブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/米/スパイス/葉物/あっさり/ヘルシー/淡白/芸術的/野趣/鮮やか/サクサク/甘い/甘い/辛い/辛い/秋物/秋物/秋物/秋物/秋物/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "ジャンキー/濃厚/しょっぱい/しょっぱい/" + "カエル/虫肉/冬物/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "絢爛/得意料理＊秋物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "美しい/芸術的/和風/派手/繊細/" + "朱/風/植物/美術品/" * 3 + "紅葉の/秋の/秋風の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "威厳/" + "氷/刃物/弾/小手/機械/" * 3 + "夏の/サクラの/ヒマワリの/スイカの/" * 5  + "春の/冬の/春風の/" * 10)

;農民関連
;葡萄がついてるのは妹のほうだが
;葡萄ならオサレ感あるし紅葉もするし果実も色づくし
;変えるなら穣子を葡萄にして静葉は漆（非食用の有用植物、かつ紅葉する）とか？？？
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "山葡萄農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "ヤマブドウ")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "山葡萄畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "妖怪の山・山麓")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 7)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 715)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 4)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
