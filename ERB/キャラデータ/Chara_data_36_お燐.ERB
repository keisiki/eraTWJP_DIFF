@CHARAMOVE_DATA_36(ARG,ARG:2)
LOCAL = 36
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			CASE 18, 43, 44, 51, 52
				RETURN -100
			;お空以外の私室や使わない部屋
			CASE 903, 904, 910, 916, 918, 919, 920, 925, 930, 935, 936, 937, 938, 940, 941, 942, 943
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 1, 7, 9
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 15, 76, 91,92,93,96,97
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ36
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ36(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "お燐")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ネグリジェ）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "尻尾を邪魔しないローライズの")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ハーフバックショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "フリルブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/肉料理/揚げ物/ガッツリ/ジャンキー/濃厚/温かい/野趣/家庭的/サクサク/地底風/甘い/甘い/しょっぱい/しょっぱい/" + "グロテスク/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "野菜/あっさり/ヘルシー/冷たい/辛い/辛い/スパイス/スパイス/スパイス/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "地獄風/地底風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/混沌/怖い/奇妙/" + "火/骨/幽霊/妖術/生体品/" * 3 + "ネコの/死霊の/地霊の/廃獄の/ドクロの/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "水/氷/小手/" * 3 + "イヌの/オオカミの/" * 10)

;農民関連
;キウイはマタタビの仲間でネコをメロメロにする
;果実にその効果はないみたいだが
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "キウイ農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "キウイ")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "キウイ農園")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "地底")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 9)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 926)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 8)
ENDSELECT

