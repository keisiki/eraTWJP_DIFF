@CHARAMOVE_DATA_118(ARG,ARG:2)
LOCAL = 118
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;三妖精宅内に行きやすい
			CASE 26 TO 34
				RETURN 5
			;夢幻館には庭以外行かない
			CASE 450
				RETURN -10
			CASE 450 TO 465
				RETURN -100
			;永遠亭の奥には行かない
			CASE 405, 407 TO 430
				RETURN -100
			;他人の家には入らない
			CASE 432, 437, 440, 442, 443, 445
				RETURN -100
			;自宅,無明の丘,太陽の畑,夢幻館庭によく居る
			CASE 439, 436, 438, 466
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 3, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 31, 32, 34, 51 TO 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ118
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ118(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ラルバ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "碧色に煌めく")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "鱗粉染めパンティ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "キャミソール")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "葉物/ドリンク/あっさり/夏物/芸術的/野趣/鮮やか/ぷるぷる/野菜/野菜/甘い/甘い/甘い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "ガッツリ/しょっぱい/しょっぱい/肉料理/肉料理/肉料理/魚料理/魚料理/魚料理/" + "虫肉/" * 10)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "得意料理＊夏物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黄/かわいい/ポップ/派手/ロリータ/" + "虫/" * 3  + "揚羽蝶の/ヒマワリの/夏の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "シック/知的/" + "氷/鳥/キセル/" * 3 + "蜘蛛糸の/蜘蛛の/冬の/" * 10)
ENDSELECT

