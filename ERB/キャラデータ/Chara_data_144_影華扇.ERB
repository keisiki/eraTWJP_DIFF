CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "濃い味/得意料理＊豪快＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/混沌/美しい/大陸風/知的/" + "食器/生体品/鎖の/闇/骨/死生/妖術/酒/" * 3 + "鬼の/死霊の/呪術の/鬼力の/茨の/華の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/繊細/ポップ/ロリータ/" + "仙人の/機械/" * 3 + "河童の/天狗の/" * 10)

;特殊衣装設定
CASE "おうちデート衣装"

ENDSELECT

