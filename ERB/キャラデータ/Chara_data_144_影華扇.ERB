@CHARAMOVE_DATA_144(ARG, ARG:2)
LOCAL = 144
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 地獄, 彼岸, 中有の道, 三途の川
				RETURN 1
			CASE 灼熱地獄跡, 旧地獄街道, 旧地獄温泉, 地獄の深道
				RETURN 1
			CASE 神社境内, 神社縁側, 神社本堂
				RETURN 1
			CASE 麓の樹海, 仙人の屋敷, 森の深奥
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
ENDSELECT

@EXIST_キャラデータ144
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ144(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "影華扇")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "シニヨンキャップ"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "タイツ"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "道士服"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
