@CHARAMOVE_DATA_93(ARG,ARG:2)
LOCAL = 93
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
		;例外処理
		IF !CFLAG:LOCAL:遭遇位置
			CFLAG:LOCAL:現在位置 = SUKIMA()
		ELSE
			CFLAG:LOCAL:現在位置 = CFLAG:LOCAL:遭遇位置
			IF !GETBIT(FLAG:プール, 3)
				SIF CFLAG:LOCAL:現在位置 != 610 && !AT_HOME(LOCAL) && !CFLAG:LOCAL:デート中
					CFLAG:LOCAL:現在位置 = 霧の湖
			ENDIF
		ENDIF
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		IF !GETBIT(FLAG:プール, 3)
			SELECTCASE ARG:2
				CASE 347, 348
					RETURN 0
				CASEELSE
					RETURN -100
			ENDSELECT
		ELSE
			SELECTCASE ARG:2
				;納屋と屋敷の三階と地下、及び各私室には入らない
				CASE 303, 304, 305, 311, 312, 318, 320, 321, 323, 325, 340, 341, 342, 344, 345, 346, 347, 348
					RETURN -100
			ENDSELECT
		ENDIF
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 3
				RETURN 1
			CASE 4
				SIF GETBIT(FLAG:プール, 3)
					RETURN 1
				RETURN 0
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 31
				RETURN 1
			CASE 32,33,34, 41,42,43
				SIF GETBIT(FLAG:プール, 3)
					RETURN 1
				RETURN 0
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ93
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ93(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "わかさぎ姫")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：着物"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("着物", "着物"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "帯"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "人魚の王道であろう")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "貝殻ビキニ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "貝殻ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[わかさぎ姫]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 99)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/葉物/すっぱい/あっさり/ヘルシー/淡白/冷たい/質素/野趣/家庭的/ぷるぷる/おいしい水/甘い/甘い/甘い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "ガッツリ/ジャンキー/辛い/辛い/" + "グロテスク/謎料理/" * 5 + "魚料理/" * 10)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "おいしい水/得意料理＊淡白＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/鉱物/かわいい/芸術的/ポップ/シック/" + "水/宝石/魚/" * 3 + "魚の/人魚の/深海魚の/古代魚の/鱗/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/怖い/派手/威厳/" + "氷/雷/刃物/弾/小手/キセル/" * 3 + "火/" * 5)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_93
DISH_NAME = マーメイドパフェ
RESULTS = デザート
CALL 材料設定("氷の鱗")
DISH_TASTE = 洋食/アイス/パフェ/果物/甘い/甘い/冷たい/夏物/豪華/芸術的/鮮やか/ぷるぷる/きらめき/
DISH_COMMENT = 人魚姫をイメージしたブルーオーシャンなパフェ。見てるだけで涼しい。
RETURN 1

;IMAGE_I93_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;   RESULTSに特殊服装を格納すること
;   書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I93_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF CFLAG:CHARA:うふふ && TALENT:CHARA:バストサイズ > 0
			RESULTS = _はだけ
	ENDIF
ENDIF

