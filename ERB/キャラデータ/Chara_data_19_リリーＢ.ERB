@CHARAMOVE_DATA_19(ARG,ARG:2)
LOCAL = 19
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;三妖精宅内に行きやすい
			CASE 26 TO 34
				RETURN 5
			;アジト,厄家,秋家,華扇家,隠れ家,小屋
			CASE 704, 706, 715 TO 719, 720 TO 730, 733, 734
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2
				SIF DAY:2 == 1
					RETURN 1
				RETURN 0
			CASE 5, 6, 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 56,57, 61,62,64, 71,72,73
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ19
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ19(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "リリーＢ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ほんのり暖かい")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "黒春告精のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "黒春告精のキャミソール")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/スパイス/洋食/葉物/濃厚/温かい/質素/野趣/鮮やか/ふわふわ/冥界風/きらめき/辛い/辛い/辛い/春物/春物/春物/春物/春物/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "和食/淡白/冷たい/甘い/甘い/しょっぱい/しょっぱい/ミルク/ミルク/ミルク/夏物/夏物/夏物/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "得意料理＊春物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "植物/かわいい/ロリータ/" + "黒/風/" * 3 + "サクラの/ユリの/春の/春風の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "氷/刃物/弾/小手/キセル/" * 3 +"白/夏の/カエデの/スイカの/紅葉の/" * 5 + "冬の/秋の/秋風の/" * 10)
ENDSELECT

