@CHARAMOVE_DATA_91(ARG,ARG:2)
LOCAL = 91
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;神霊廟メンバーの私室
			CASE 127, 128
				RETURN -5
			;離れと墓地と命蓮寺内部
			CASE 103 TO 122
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13,15,16, 61,62,65,66
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ91
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ91(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "屠自古")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "烏帽子"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "護符"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "重さのない")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "霊体ショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "霊体ブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)


;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/揚げ物/あっさり/淡白/上品/鮮やか/豪快/ふわふわ/仙術/和食/和食/しょっぱい/しょっぱい/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "洋食/ガッツリ/濃厚/質素/甘い/甘い/カエル/カエル/カエル/虫肉/虫肉/虫肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "刺激的/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "緑/美しい/レトロ/威厳/" + "雷/幽霊/" * 3 + "亡霊の/稲妻の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "ポップ/繊細/" + "護符/生体品/" * 3)

;農民関連
;豪族組で相談したら土地も用意できそうだけどそこまでしてくれるか？
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "大根農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "大根")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "大根畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "人里")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 2)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 205)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 12)
ENDSELECT

