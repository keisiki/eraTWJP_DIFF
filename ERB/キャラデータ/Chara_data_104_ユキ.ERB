@CHARAMOVE_DATA_104(ARG,ARG:2)
LOCAL = 104
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;自室,マイの部屋,広場
			CASE 234, 235, 202
				RETURN 5
			;あなたの部屋
			CASE 230
				IF TALENT:LOCAL:恋慕
					RETURN 5
				ELSE
					RETURN -100
				ENDIF
			;行かない
			CASE 220, 225, 221, 222, 224, 226
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23,27, 51,52,53,56,57
				RETURN 1 
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ104
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ104(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ユキ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "魔界で流行りの")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "フレアパンティ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "ブラレット")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "卵/スパイス/和食/揚げ物/あっさり/濃厚/機能的/鮮やか/豪快/ぷるぷる/魔界風/辛い/辛い/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "淡白/野趣/しょっぱい/しょっぱい/" + "カエル/虫肉/謎料理/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "魔界風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/かっこいい/ポップ/洋風/派手/パワフル/" + "魔法/" * 3 + "魔界の/真紅の/禁断の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "シック/繊細/レトロ/和風/")
ENDSELECT

