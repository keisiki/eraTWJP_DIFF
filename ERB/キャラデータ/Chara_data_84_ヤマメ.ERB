@CHARAMOVE_DATA_84(ARG,ARG:2)
LOCAL = 84
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;行かない
			CASE 901 TO 930, 935, 936, 937, 939, 940, 942, 943
				RETURN -100
			;大通りと酒場に行きやすい
			CASE 931, 933
				RETURN 5
			;汚れに惹かれる
			CASEELSE
				RETURN YOGORE:(ARG:2) / 300
		ENDSELECT
	;仕事時間帯
	CASE 5
		RETURN 0
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 9
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 91,92,93
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ84
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ84(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ヤマメ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "上着"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ジャンパースカート"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "黒地に蜘蛛の巣を刺繍した")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "スパイダー柄ショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "スパイダー柄ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[ヤマメ]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/和食/汁物/ドリンク/濃厚/質素/野趣/グロテスク/とろとろ/地底風/しょっぱい/しょっぱい/すっぱい/すっぱい/" + "虫肉/" * 5)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "辛い/辛い/パン/パン/パン/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "地底風/得意料理＊虫肉＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/茶/かわいい/シック/" + "虫/毒/生体品/" * 3 + "蜘蛛糸の/蜘蛛の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "派手/パワフル/ロリータ/" + "動物/鳥/機械/" * 3)
ENDSELECT


