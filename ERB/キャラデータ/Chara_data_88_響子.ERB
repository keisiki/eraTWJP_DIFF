@CHARAMOVE_DATA_88(ARG,ARG:2)
LOCAL = 88
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
		RETURN 0
	;移動確率
	CASE 2
		;悪天候時、外なら移動しやすく
		SIF OUTROOF(CFLAG:(NO:88):現在位置) && ((TIME:5 & 4) || (TIME:5 & 8))
			RETURN 10
		RETURN 80
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室（経堂と物置除く）
			CASE 113, 114, 115, 116, 117, 118
				RETURN -5
			;離れと墓地と神霊廟内部
			CASE 119, 121, 125 TO 133
				RETURN -100
			;境内,居間に行きやすい
			CASE 102, 108
				RETURN 5
			;白蓮が本堂にいる時はやってくる
			CASE 103
				SIF CFLAG:55:現在位置 == 103
					RETURN 10
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11 TO 15, 71,72,73
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ88
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ88(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "響子")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "渋い妖怪の絵が描かれた")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "プリントショーツ(山彦)")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[響子]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "鳥肉/ミルク/麺類/パイ/すっぱい/あっさり/濃厚/質素/野趣/家庭的/もちもち/" + "甘い/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "辛い/辛い/グロテスク/グロテスク/グロテスク/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "濃い味/得意料理＊家庭的＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "緑/かっこいい/ポップ/派手/" + "パワフル/音楽/" * 3 + "イヌの/山の/経典/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "シック/威厳/知的/" + "闇/" * 3)
ENDSELECT

