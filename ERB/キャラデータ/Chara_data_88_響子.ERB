@CHARAMOVE_DATA_88(ARG, ARG:2)
LOCAL = 88
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
		;土日はみすちーと連動
		IF YOUBI_MATCH(DAY, "土日")
			RESULT:1 = [[ミスティア]]
			RESULT:2 = [[響子]]
		ENDIF
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113ナズーリン私室, P115星私室, P114ムラサ私室, P116一輪私室, P117ぬえ私室, P118白蓮私室, P109女苑私室
				RETURN -5
			;離れ、マミゾウの棲家には入らない,神霊廟内部は行かない
			CASE P119離れ, P137マミゾウの棲家, P125屠自古私室 TO P134蔵, P138鐘楼, P139邪仙楼
				RETURN -100
			;居間に行きやすい
			CASE P108居間
				RETURN 5
			;境内にもっと行きやすい
			CASE P102境内
				RETURN 10
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE お地蔵通り, 枝垂れ柳の下, 命蓮寺境内, 命蓮寺方丈, 墓地
				RETURN 1
			CASE 麓の樹海, 玄武の沢, 大蝦蟇の池
				RETURN 1
			CASE 絶景の丘
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;白蓮が本堂にいる時はやってくる
		SIF CFLAG:[[白蓮]]:現在位置 == P103本堂 && !CFLAG:[[白蓮]]:うふふ && SHIRAHU([[白蓮]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ88
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ88(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "響子")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "渋い妖怪の絵が描かれた")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "プリントショーツ(山彦)")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[響子]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "鳥肉/ミルク/麺類/パイ/すっぱい/あっさり/濃厚/質素/野趣/家庭的/もちもち/" + "甘い/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "辛い/辛い/グロテスク/グロテスク/グロテスク/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "濃い味/得意料理＊家庭的＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "緑/かっこいい/ポップ/派手/" + "パワフル/音楽/" * 3 + "イヌの/山の/経典/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "シック/威厳/知的/" + "闇/" * 3)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_88
DISH_NAME = わんこそば
RESULTS = 主食
CALL 材料設定("汎用")
DISH_TASTE = 和食/麺類/しょっぱい/あっさり/淡白/温かい/温かい/上品/芸術的/
DISH_COMMENT = お椀にアツアツのそばを一口だけ入れて食べるのを繰り返す変わったそば。犬のワンコは関係ない。
DISH_TIME = 30
RETURN 1

