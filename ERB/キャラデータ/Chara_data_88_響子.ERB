;例の通り、STRFINDを使用してください
;-------------------------------------------------------------------------------------------
@IMAGE_I88_特殊エフェクトチェック(CHARA, エフェクト)
#DIM CHARA
#DIMS エフェクト

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
ELSE
	;通常画像
	;SIF STRFIND(エフェクト, "母乳") != -1
	;	RETURN 1
	SIF STRFIND(エフェクト, "ぶっかけ") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "お口") != -1
		RETURN 1
ENDIF

RETURN 0

;IMAGE_I88_特殊エフェクト(CHARA)--------------------------------------------------------------------
;キャラクター特有のエフェクト表示条件
;使用時注意点:
;	RESULTS配列に特殊エフェクトを格納
;	RETURNで追加するエフェクト数を返却すること
;-------------------------------------------------------------------------------------------
@IMAGE_I88_特殊エフェクト(CHARA)
#DIM CHARA
#DIM DYNAMIC エフェクト数 = 0
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF FLAG:立ち絵種類
		;顔絵
	ELSE
		;立ち絵
	ENDIF
ELSE
	;通常画像
	IF FLAG:立ち絵種類
		;顔絵
		IF CFLAG:CHARA:顔に精液
				;胸ぶっかけ
				RESULTS:エフェクト数 = ぶっかけ
				エフェクト数 += 1
			ENDIF
			IF CFLAG:CHARA:口に精液
				;フェラ
				RESULTS:エフェクト数 = お口
				エフェクト数 += 1
			ENDIF
	ELSE
		;立ち絵
	ENDIF
ENDIF

RETURN エフェクト数

