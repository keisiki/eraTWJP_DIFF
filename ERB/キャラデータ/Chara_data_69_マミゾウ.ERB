@CHARAMOVE_DATA_69(ARG,ARG:2)
LOCAL = 69
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室（経堂と物置除く）
			CASE 113, 114, 115, 116, 118
				RETURN -100
			;神霊廟メンバーの私室
			CASE 125, 127, 128
				RETURN -100
			;離れと墓地,その他行かない場所
			CASE 119, 121, 103, 122, 131, 132
				RETURN -100
			;客間に行きやすい
			CASE 109, 129
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13,15,16, 21,22,23,27
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

