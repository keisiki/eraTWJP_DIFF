	CALLF MAKE_STR(V_NAME, "すっぱい/すっぱい/パン/パン/パン/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "ドッキリ味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "茶/混沌/美しい/奇妙/和風/知的/" + "付喪神/妖術/眼鏡/酒/" * 3 + "タヌキの/葉っぱの/キセル/" * 5)
