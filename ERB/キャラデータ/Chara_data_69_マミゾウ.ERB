CASE "人里デート衣装"
	CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "マミゾウ和装セット"))

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/スパイス/和食/煮物/濃厚/芸術的/野趣/豪快/もちもち/外界風/月見/辛い/辛い/しょっぱい/しょっぱい")
