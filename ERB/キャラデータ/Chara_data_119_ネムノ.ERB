@CHARAMOVE_DATA_119(ARG,ARG:2)
LOCAL = 119
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 71, 72, 73
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ119
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ119(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ネムノ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

;あえて色は決めない
CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "色の濃淡が面白い")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "手染めふんどし")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "手染めさらし")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[ネムノ]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/キノコ/和食/鍋物/おつまみ/ガッツリ/濃厚/秋物/質素/野趣/家庭的/とろとろ/しょっぱい/しょっぱい/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "パン/チーズ/洋食/あっさり/淡白/外界風/甘い/甘い/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "おふくろの味/得意料理＊秋物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "茶/美しい/シンプル/シック/" + "秩序/刃物/" * 3 + "山の/聖域の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/洋風/ロリータ/" + "機械/" * 5)
ENDSELECT

