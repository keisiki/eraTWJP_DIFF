@CHARAMOVE_DATA_73(ARG,ARG:2)
LOCAL = 73
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;酒屋,自室,広場
			CASE 211, 231, 202
				RETURN 5
			;あなたの部屋
			CASE 230
				IF TALENT:LOCAL:恋慕
					RETURN 5
				ELSE
					RETURN -100
				ENDIF
			;行かない
			CASE 220, 225, 221, 222, 224, 226, 
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13,15, 21,22,23,27
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ73
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ73(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "蛮奇")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：外衣"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外衣", "ケープマント"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：その他１"
;とりあえず冬限定にしてみる
	SIF DAY:2 == 4
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "マフラー"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "グラデーションが美しい")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "薄紅色のショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "薄紅色のブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[蛮奇]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "卵/スパイス/中華/汁物/あっさり/ジャンキー/淡白/機能的/上品/鮮やか/もちもち/辛い/辛い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "米/ガッツリ/野趣/すっぱい/すっぱい/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "薄味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "かわいい/オシャレ/大陸風/シック/マニアック/" + "赤/" * 3 + "騎士の/マント/マスク/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "派手/知的/ロリータ/" + "書物/" * 3)

;農民関連
;首が落ちる＋赤色
;あんま人付き合いなさそうだけど一応
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "椿農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "椿油")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "椿農園")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "人里")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 2)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 206)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 350)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 6)
ENDSELECT

