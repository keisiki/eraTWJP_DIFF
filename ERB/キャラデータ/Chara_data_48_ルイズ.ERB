@CHARAMOVE_DATA_48(ARG,ARG:2)
LOCAL = 48
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2,5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 51,53,54,56,57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ48
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ48(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ルイズ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "セーラー服"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "旅歩き用の")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "カジュアルショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "カジュアルブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "チーズ/スパイス/洋食/汁物/ジャンキー/淡白/豪華/鮮やか/グロテスク/もちもち/魔界風/外界風/甘い/甘い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "野趣/しょっぱい/しょっぱい/カエル/カエル/カエル/虫肉/虫肉/虫肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "魔界風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/美しい/オシャレ/ポップ/繊細/" + "魔法/自然/" * 3 + "地上の/楽園の/霊天の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "奇妙/レトロ/パワフル/ロリータ/マニアック/" + "小手/" * 3)
ENDSELECT

