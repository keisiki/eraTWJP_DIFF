@CHARAMOVE_DATA_90(ARG,ARG:2)
LOCAL = 90
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室（経堂と物置除く）
			CASE 113, 114, 115, 116, 118
				RETURN -100
			;神霊廟メンバーの私室
			CASE 125, 127, 128
				RETURN -100
			;離れ
			CASE 119
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,15,16, 21,22,23, 61,62
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
