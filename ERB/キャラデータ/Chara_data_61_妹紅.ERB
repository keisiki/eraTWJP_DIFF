@CHARAMOVE_DATA_61(ARG, ARG:2)
LOCAL = 61
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;夢幻館には行かない
			CASE P450玄関 TO P466庭
				RETURN -100
			;永遠亭の奥には行かない
			CASE P405調剤室, P407廊下 TO P430地下室
				RETURN -100
			;他人の家には入らない
			CASE P432影狼の家, P437メディスンの家, P439ラルバの家, P443ミスティアの家
				RETURN -100
			;斜角の竹林,迷いの小道,夜雀の屋台,自宅,竹林入口によく居る
			CASE P433斜角の竹林, P434迷いの小道, P444夜雀の屋台, P445妹紅の家, P447竹林入口
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 広場, 商家町, 呑屋小道, 寺子屋, 長屋街
				RETURN 1
			CASE 竹林入り口, 迷いの小道, 斜角の竹林, 兎の洞穴, 永遠亭, 無名の丘, 太陽の畑
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ61
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ61(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "妹紅")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ズボン"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ズボン", "指貫袴"))
CASE "普段着：上半身上着２"
	IF DAY:2 == 2
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "半袖ブラウス"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
	ENDIF
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "サスペンダー"))
CASE "普段着：その他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他２", "護符"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "無自覚お洒落な")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "護符パンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/スパイス/和食/鍋物/あっさり/温かい/質素/鮮やか/家庭的/ふわふわ/辛い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "アイス/ガッツリ/濃厚/冷たい/甘い/甘い/すっぱい/すっぱい/謎料理/謎料理/謎料理/月見/月見/月見/月都風/月都風/月都風/月都風/月都風/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "大火力/得意料理＊鳥肉＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "かわいい/和風/パワフル/壮大/" + "赤/火/巫術/護符/" * 3 + "鳳凰の/不滅の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "ポップ/繊細/ロリータ/マニアック/" + "死生/機械/" * 3 + "水/氷/月/王女の/蓬莱の/" * 5)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_61
DISH_NAME = 焼き鳥
RESULTS = 軽食
CALL 材料設定("鳥肉", "ヤマドリ")
DISH_TASTE = 和食/おつまみ/肉料理/鳥肉/しょっぱい/濃厚/温かい/家庭的/
DISH_COMMENT = 不死鳥のように鮮やかに焼いた鳥肉。味はいたって普通である。
RETURN 1

;IMAGE_I61_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I61_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

