		CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "頭巾"))
	ELSEIF CFLAG:ARG:口上セレクタ == 0 && CFLAG:ARG:基本服装セット == (100 + [[影狼]]) && TCVAR:ARG:363 == 101
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "麦藁帽子"))
