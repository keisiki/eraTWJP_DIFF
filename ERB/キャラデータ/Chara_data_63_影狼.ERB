@CHARAMOVE_DATA_63(ARG,ARG:2)
LOCAL = 63
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;夢幻館には行かない
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;永遠亭の奥には行かない
			CASE 405, 407 TO 430
				RETURN -100
			;他人の家には入らない
			CASE 437, 439, 440, 442, 443, 445
				RETURN -100
			;自宅,斜角の竹林,迷いの小道,夜雀の屋台,竹林入口によく居る
			CASE 432, 433, 434, 444, 447
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 3, 4, 5, 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 31, 41,42,43,46,47
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ63
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ63(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "影狼")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：外衣"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外衣", "ケープ"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "ブローチ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "“菊に盃”の絵柄をあしらった")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "麻布のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "麻布のブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "揚げ物/甘い/ガッツリ/濃厚/豪華/野趣/鮮やか/サクサク/月見/しょっぱい/しょっぱい/しょっぱい/肉料理/肉料理/肉料理/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "あっさり/淡白/辛い/辛い/" + "カエル/虫肉/スパイス/キノコ/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "濃い味/得意料理＊肉料理＊/")
;贈り物関連
CASE "感性：好き"
	{
	CALLF MAKE_STR(V_NAME, "和風/イヌの/血に飢えた/動物/繊細/" + "シンプル/赤/自然/" * 2 + 
	"装飾/月/牙/爪/芸術的/美しい/シック/" * 3
	 + "オシャレ/オオカミの/満月の/" * 5)
	}
CASE "感性：嫌い"
	{
	CALLF MAKE_STR(V_NAME, "奇妙/金属/" + "派手/ロリータ/マニアック/" * 2 
	+ "壮大/威厳/刃物/" * 3 + "機械/" * 5)
	}
ENDSELECT

