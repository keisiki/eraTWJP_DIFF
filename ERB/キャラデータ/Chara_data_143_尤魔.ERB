@CHARAMOVE_DATA_143(ARG, ARG:2)
LOCAL = 143
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE P612法廷 TO P639八雲邸居間
				RETURN -100
			;他人の家には入らない
			CASE P608潤美の家, P609瓔花の家
				RETURN -100
			;霊長園には行かない
			CASE P642霊長園 TO P645磨弓の部屋
				RETURN -100
			;他所の風呂には行かない
			CASE P652早鬼宅の風呂, P646八千慧宅の風呂
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 中有の道, 三途の川, 彼岸, 地獄, 畜生界
				RETURN 1
			CASE 旧地獄街道, 灼熱地獄跡
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ143
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ143(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "尤魔")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（スカート＋フード）セット")
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "スプーン"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "モコモコの")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ひつじさんパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "ひつじさんブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "中華/ジャンキー/濃厚/畜生流/" + "辛い/ガッツリ/豪快/" * 3)
CASE "料理：嫌いな味"
	;例外的に嫌いな味が極端に少ない
	CALLF MAKE_STR(V_NAME, "質素/あっさり/淡白/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "畜生流/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/混沌/怖い/知的/" + "強欲な/血/" * 3  + "ヒツジの/ワシの/剛欲な/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/派手/芸術的/" + "神霊/" * 3 + "はにわの/カワウソの/ワシの/カメの/龍の/はにわ/" * 10)
ENDSELECT

