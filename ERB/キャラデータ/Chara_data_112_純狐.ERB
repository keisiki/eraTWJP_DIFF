@CHARAMOVE_DATA_112(ARG,ARG:2)
LOCAL = 112
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;綿月亭には行かない
			CASE 1022 TO 1049
				RETURN -100
			;出るとき用
			CASE 1020, 1021
				RETURN -10
			;静かの海は渡らない
			CASE 1001 TO 1015
				RETURN -100
			;物置はあまり行かない
			CASE 1065
				RETURN -5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 4, 10
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 42,43,45, 109
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ112
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ112(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "純狐")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "冕冠"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：着物"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("着物", "着物"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "帯"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ヒップラインを美しく支える")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ガードルショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "レースブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[純狐]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "カエル/肉料理/卵/中華/汁物/あっさり/芸術的/上品/鮮やか/とろとろ/地獄風/しょっぱい/しょっぱい/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "野趣/甘い/甘い/虫肉/虫肉/虫肉/謎料理/謎料理/謎料理/謎料理/謎料理/" + "月都風/月見/" * 10)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "純化した味/得意料理＊中華＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/美しい/シンプル/大陸風/シック/威厳/" + "光/宝石/" * 3 + "ユリの/神霊の/純粋な/殺意の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "ポップ/ロリータ/マニアック/" + "黄/安物/生体品/" * 3 + "月/海神の/" * 10)
ENDSELECT

