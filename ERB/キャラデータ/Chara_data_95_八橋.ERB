@CHARAMOVE_DATA_95(ARG,ARG:2)
LOCAL = 95
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
		;連動処理（弁々と一緒）
		IF TALENT:95:恋慕 || (TALENT:95:思慕 && !TALENT:95:思慕)
			CFLAG:LOCAL:現在位置 = CFLAG:95:遭遇位置 ? CFLAG:95:遭遇位置 # SUKIMA()
		ELSE
			CFLAG:LOCAL:現在位置 = CFLAG:94:遭遇位置 ? CFLAG:94:遭遇位置 # SUKIMA()
		ENDIF
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;自室,九十九姉妹の部屋,広場
			CASE 208, 209, 210, 202
				RETURN 5
			;あなたの部屋
			CASE 230
				IF TALENT:LOCAL:恋慕
					RETURN 5
				ELSE
					RETURN -100
				ENDIF
			;行かない
			CASE 220, 225, 221, 222, 224, 226
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 51,52,53,56,57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ95
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ95(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "八橋")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "カチューシャ"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "古着をリメイクした")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "緩めの紐ショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "素朴な紐ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[八橋]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/肉料理/和食/煮物/濃厚/豪華/芸術的/家庭的/ぷるぷる/甘い/甘い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "淡白/辛い/辛い/" + "カエル/虫肉/キノコ/グロテスク/謎料理/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "濃い味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "茶/かわいい/芸術的/和風/シック/" + "音楽/楽器/" * 3 + "嵐の/旋律の/爪/ピック/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "洋風/繊細/" + "刃物/生体品/小手/" * 3 + "バチ/スティック/" * 5 + "精霊銀の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
