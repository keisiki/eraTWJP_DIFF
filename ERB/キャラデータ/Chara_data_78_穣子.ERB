@CHARAMOVE_DATA_78(ARG, ARG:2)
LOCAL = 78
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
		RESULT:1 = [[静葉]]
		RESULT:2 = [[穣子]]
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;各私室
			CASE P704河童のアジト, P706厄神の家, P729仙人の屋敷・玄関 TO P730仙人の屋敷・庭
				RETURN -100
			CASE P733正邪の隠れ家, P736山姥の家, P737招き猫の家, P740山童のアジト
				RETURN -100
				;畑によく居る
			CASE P715秋姉妹の家・畑
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 広場, 商家町, 呑屋小道
				;秋の間だけ
				SIF DAY:2 == 3
					RETURN 1
			CASE 麓の樹海, 玄武の沢, 大蝦蟇の池, 森の深奥
				RETURN 1
			CASE 九天の滝, 絶景の丘, 山の湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ78
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ78(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "穣子")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "エプロンドレス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ほんのり甘い香りがする")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "焼き芋色のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "焼き芋色のブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[穣子]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/イモ/米/キノコ/煮物/ガッツリ/濃厚/豪華/野趣/家庭的/もちもち/甘い/甘い/しょっぱい/しょっぱい/" + "秋物/" * 5)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "淡白/質素/すっぱい/すっぱい/" + "カエル/虫肉/冬物/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "豊穣/得意料理＊秋物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "朱/かわいい/オシャレ/派手/" + "土/植物/果実/食器/" * 3 + "ブドウの/紅葉の/秋の/豊穣の/種/秋風の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "氷/刃物/弾/小手/機械/" * 3 + "夏の/サクラの/ヒマワリの/スイカの/" * 5  + "春の/冬の/春風の/" * 10)

;農民関連
;米でもいいが天狗と被るのだよ
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "芋農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "お芋")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "お芋畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "妖怪の山・山麓")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 7)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 715)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 4)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_78
DISH_NAME = スイートポテト
RESULTS = デザート
CALL 材料設定("イモ")
DISH_TASTE = 洋食/野菜/甘い/濃厚/温かい/秋物/野趣/家庭的/
DISH_COMMENT = サツマイモを使った和製洋菓子。海外では通じない。
RETURN 1

;IMAGE_I78_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I78_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
	ENDIF
ENDIF

