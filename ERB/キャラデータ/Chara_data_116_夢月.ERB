@CHARAMOVE_DATA_116(ARG,ARG:2)
LOCAL = 116
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;永遠亭の奥には行かない
			CASE 405, 407 TO 430
				RETURN -100
			;他人の家には入らない
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;倉庫にはあまり行かない
			CASE 456
				RETURN -5
			;ダイニング,リビング,自室によく居る
			CASE 453, 461, 457
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 4
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 41, 42, 47, 49
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ116
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ116(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "夢月")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ネグリジェ）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "ホワイトブリム"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "エプロンドレス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "いかにもメイドっぽい")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "花柄白レースパンティ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "花柄白レースブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "パン/卵/洋食/汁物/ケーキ/あっさり/淡白/豪華/上品/鮮やか/ふわふわ/夢幻風/甘い/甘い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "野趣/しょっぱい/しょっぱい/" + "カエル/虫肉/スパイス/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "夢幻風/メイド流/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/混沌/かわいい/洋風/シック/威厳/" + "妖術/禁断の/" * 3 + "メイドの/夢幻の/悪魔の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/和風/派手/" + "神霊/" * 3)
ENDSELECT

