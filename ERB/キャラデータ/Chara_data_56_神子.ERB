@CHARAMOVE_DATA_56(ARG,ARG:2)
LOCAL = 56
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;神霊廟メンバーの私室
			CASE 125, 127
				RETURN -5
			;離れと墓地と命蓮寺内部
			CASE 103 TO 122
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 11,12,13,16, 21,22,23,27
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ56
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ56(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "神子")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "髪飾り"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "袖無し上着"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "ブレスレット"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "オーダーメイドの")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "純絹レースショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "純絹ソフトブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[神子]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/魚料理/煮物/あっさり/ヘルシー/淡白/豪華/上品/鮮やか/サクサク/仙術/月都風/天界風/外界風/" + "和食/甘い/すっぱい/" * 2)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "洋食/質素/辛い/辛い/" + "カエル/虫肉/濃厚/謎料理/冥界風/地獄風/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "絢爛/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "紫/金/高級/秩序/美しい/オシャレ/和風/派手/知的/威厳/" + "光/日/宝石/巫術/ミミズクの/マント/" * 3 + "黄金の/神霊の/仙人の/七星の/厳つがましい/救世の/宝剣/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "黒/混沌/シンプル/シック/繊細/ロリータ/" + "安物/闇/死生/生体品/" * 3)
ENDSELECT

