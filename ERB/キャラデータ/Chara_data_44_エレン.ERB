@CHARAMOVE_DATA_44(ARG,ARG:2)
LOCAL = 44
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;一応ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;私室は避ける
			CASE 535
				RETURN -10
			;魔理沙家,アリス家,妖怪の棲み処
			CASE 511 TO 515, 521 TO 531, 541, 542, 543, 544
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 3, 4, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 6, 21,22,23,27, 31,37, 41,42,43,46,47, 51,53,54,56,57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ44
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ44(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "エレン")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ネグリジェ）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "エプロンドレス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ふりふりふわふわの")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "リボンフリルショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "キャミソール")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "卵/キノコ/洋食/葉物/パイ/あっさり/ヘルシー/淡白/芸術的/鮮やか/家庭的/ふわふわ/月見/甘い/甘い/甘い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "ガッツリ/濃厚/辛い/辛い/" + "カエル/虫肉/グロテスク/謎料理/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "得意料理＊ふわふわ＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黄/かわいい/ポップ/不思議/" + "月/魔法/" * 3 + "ふわふわの/ネコの/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "パワフル/威厳/" + "妖術/小手/機械/" * 3)
ENDSELECT

