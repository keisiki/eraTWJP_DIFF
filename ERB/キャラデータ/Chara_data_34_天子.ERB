@CHARAMOVE_DATA_34(ARG,ARG:2)
LOCAL = 34
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;あれでもいい所のお嬢様なので一応ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 71,72,73, 81,82,83,84,89
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ34
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ34(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "天子")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "桃"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "一切の縫い目がない")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "桃色パンティ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "ブラキャミ")
CASE "固有下着着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

CASE "固有コマンド"
	CALLF MAKE_STR(V_NAME, "天候操作")
CASE "固有コマンド実行可能"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "果物/スパイス/中華/麺類/パイ/ガッツリ/ジャンキー/濃厚/豪華/上品/鮮やか/ふわふわ/天界風/甘い/甘い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "野菜/あっさり/ヘルシー/淡白/質素/野趣/すっぱい/すっぱい/カエル/カエル/カエル/虫肉/虫肉/虫肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "天界風/絢爛/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/虹/混沌/美しい/奇妙/ポップ/派手/壮大/威厳/" + "土/刃物/" * 3 + "要石の/モモの/天使の/天人の/極光の/天界の/緋想の/非想非非想の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/シンプル/シック/繊細/知的/" + "雷/死生/書物/生体品/ネクタイ/" * 3 + "悪魔の/" * 5)

;農民関連
;もも
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "桃農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "桃")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "桃農園")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "天界")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 8)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 833)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 600)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 7)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_34
DISH_NAME = 天界フルーツパフェ
RESULTS = デザート
CALL 材料設定("仙桃")
DISH_TASTE = 洋食/パフェ/アイス/ミルク/甘い/甘い/冷たい/豪華/芸術的/上品/鮮やか/豪快/ぷるぷる/ふわふわ/天界風/絢爛/
DISH_COMMENT = 仙桃をふんだんに使った贅沢なパフェ。とびきり甘い。
RETURN 1

;IMAGE_I34_特殊服装(CHARA)-------------------------------------------------------------------
