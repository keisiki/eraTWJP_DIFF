@CHARAMOVE_DATA_14(ARG,ARG:2)
LOCAL = 14
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;三妖精宅内に行きやすい
			CASE 26 TO 34
				RETURN 5
			;湖畔、レティ、チルノ、大ちゃん、霧の湖に行きやすい
			CASE 343, 344, 345, 346, 347
				RETURN 5
			;納屋と屋敷の三階と地下、及び各私室には入らない
			CASE 303, 304, 305, 311, 312, 318, 320, 321, 323, 325, 340, 341, 342, 348
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 3, 5, 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,5, 31,32,34, 51 TO 57, 71,72,73
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ14
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ14(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "チルノ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ひんやり冷たい")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "氷精のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "氷精のキャミソール")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[チルノ]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

CASE "固有コマンド"
	CALLF MAKE_STR(V_NAME, "アイスを買う")
CASE "固有コマンド実行可能"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "カエル/魚料理/汁物/アイス/濃厚/野趣/豪快/ぷるぷる/きらめき/" + "甘い/冷たい/夏物/謎料理/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "鍋物/辛い辛い/" + "野菜/スパイス/温かい/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "氷結/得意料理＊夏物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/かっこいい/ポップ/パワフル/ロリータ/壮大/" + "氷/夏の/" * 3 + "ヒマワリの/氷精の/氷の/氷結の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "赤/繊細/知的/" + "書物/キセル/" * 3 + "火/" * 5)

;農民関連
;最強とか天才とかそういう言葉が好きそう
;寒冷地の作物って感じがするしこの子に任せるっきゃない
;収穫したテンサイは紅魔館に出荷され砂糖に加工されるのであった
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "甜菜農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "テンサイ")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "甜菜畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "霧の湖")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 3)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 343)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 250)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 9)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "依頼なし発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼14(ARG, O_DATA, V_NAME)
ENDSELECT

