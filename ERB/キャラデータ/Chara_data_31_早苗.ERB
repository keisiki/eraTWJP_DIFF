@CHARAMOVE_DATA_31(ARG,ARG:2)
LOCAL = 31
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;守矢神社分社によくいる
			CASE 21
				RETURN 10
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 71,72,73,76, 81,82,83,84,85
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ31
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ31(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "早苗")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")
CASE "特殊衣装着用"
	;春か夏に1/7でセーラー服着用
	SIF (DAY:2 == 1 || DAY:2 == 2) && !RAND:7
		CALLF MAKE_STR(V_NAME, 74)
CASE "普段着：アクセサリ"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("アクセサリ", "袖"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "髪飾り"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
	ENDIF
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "守矢の巫女服"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "早苗とっておきの")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "サイドリボンショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "フロントホックブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[早苗]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/魚料理/和食/揚げ物/辛い/ジャンキー/濃厚/ぷるぷる/外界風/甘い/甘い/甘い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "すっぱい/すっぱい/妖力/妖力/カエル/カエル/カエル/虫肉/虫肉/虫肉/" + "グロテスク/謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "奇跡/外界風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "緑/果実/秩序/かわいい/オシャレ/ポップ/未来/派手/不思議/壮大/マニアック/" + "光/神霊/巫術/護符/カエルの/ヘビの/客星の/" * 3 + "巫女の/奇跡の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/シンプル/レトロ/シック/" + "妖怪/小手/キセル/" * 3 + "博霊の/妖怪の/" * 10)

;農民関連
;神社は土地持ってそう、神田
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "米農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "お米")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "水田")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "守谷神社")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 8)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 814)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 500)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 5)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_31
DISH_NAME = やしょうま
RESULTS = デザート
CALL 材料設定("甘味材")
DISH_TASTE = 和食/団子/甘い/上品/もちもち/神力/
DISH_COMMENT = 涅槃会で食べる平たい団子。これがないと信州の春は訪れないらしい。
RETURN 1

;IMAGE_I31_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I31_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF EQUIP:CHARA:上半身上着１ == 2
			RESULTS = _セーラー
	ENDIF
ENDIF

