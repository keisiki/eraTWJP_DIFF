@CHARAMOVE_DATA_66(ARG, ARG:2)
LOCAL = 66
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、畜生界は行かない
			CASE P610是非曲直庁ホール TO P618空き部屋, P640異界の関所 TO P655尤魔宅の風呂
				RETURN -100
			;他人の家には入らない
			CASE P608潤美の家, P609瓔花の家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 枝垂れ柳の下, 墓地
				RETURN 1
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE 中有の道, 三途の川, マヨヒガ, 彼岸, 白玉楼庭, 白玉楼
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ66
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ66(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "幽々子")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：着物"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("着物", "着物"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "肌襦袢"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "帯"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "桜の柄が鮮やかな")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "和装ショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "和装ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[幽々子]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/肉料理/揚げ物/おつまみ/ガッツリ/濃厚/芸術的/上品/家庭的/もちもち/冥界風/月見/和食/和食/甘い/甘い/すっぱい/すっぱい/" + "春物/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "しょっぱい/あっさり/淡白/天界風/天界風/天界風/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "冥界風/得意料理＊春物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "ピンク/美しい/芸術的/和風/派手/繊細/" + "死生/幽霊/扇/食器/サクランボの/眠りの/" * 3 + "黒死蝶の/サクラの/亡霊の/春の/冥界の/死出の/死の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "洋風/パワフル/ロリータ/" + "神霊/刃物/生体品/小手/" * 3 + "不滅の/" * 10)

;農民関連
;ザクロに設定したのは独断
;冥界（死）＆女性に縁のある果物としてチョイス
;最初は米農家にしたけど彼岸で米栽培してるのはとても違和感が
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "柘榴農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "ザクロ")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "柘榴畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "冥界")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 6)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 630)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 500)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 13)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_66
DISH_NAME = 桜餅
RESULTS = デザート
CALL 材料設定("甘味材")
DISH_TASTE = 和食/甘い/あっさり/春物/芸術的/上品/鮮やか/もちもち/冥界風/
DISH_COMMENT = 餅菓子を桜の葉で包んだ和菓子。餅の色も桜色。
RETURN 1

;IMAGE_I66_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I66_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:0 % 2
			RESULTS = _日替わり
	ENDIF
ENDIF

