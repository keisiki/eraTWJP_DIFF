@CHARAMOVE_DATA_156(ARG, ARG:2)
LOCAL = 156
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
		;6分の1で残無にくっつく
		SIF CFLAG:LOCAL:今日の運勢 % 6
			RETURN [[残無]]
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P605地獄
				RETURN 5
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE P612法廷 TO P639八雲邸居間
				RETURN -100
			;霊長園,組長の家には行かない
			CASE P642霊長園 TO P655尤魔宅の風呂
				RETURN -100
			;他人の家には入らない
			CASE P608潤美の家, P609瓔花の家, P607残無の家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 神社縁側
				RETURN 1
			CASE 中有の道, 三途の川, 彼岸, 地獄
				RETURN 1
			CASE 麓の樹海, 玄武の沢, 大蝦蟇の池, 森の深奥
				RETURN 1
			CASE 九天の滝, 絶景の丘, 山の湖
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
		;恋慕になったらずっと付いてくるモード（嘘）
;		SIF TALENT:LOCAL:恋慕
;			TCVAR:LOCAL:引力点 = CFLAG:MASTER:現在位置
		;8分の1で残無にくっつく
		SIF INRANGE(TIME, 600, 1080) && CFLAG:LOCAL:今日の運勢 % 8
			TCVAR:LOCAL:引力点 = CFLAG:[[残無]]:現在位置
ENDSELECT

@EXIST_キャラデータ156
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ156(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "日狭美")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ネグリジェ＋キャップ）セット")
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "チャイナドレス"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "ロングブーツ"))
CASE "普段着：腕部装束"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("腕部装束", "ロンググローブ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "扇情的な")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ヤマブドウパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "ヤマブドウブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[日狭美]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/葉物/淡白/豪華/上品/芸術的/ぷるぷる/鬼の魔力/地獄風/甘い/甘い/すっぱい/すっぱい/" + "たけのこ/" * 3 + "ヤマブドウ/" * 5)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "辛い/辛い/濃厚/機能的/" + "虫肉/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "地獄風/得意料理：野趣/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "紫/美しい/芸術的/威厳/" + "土/果実/" * 3 + "ブドウの/黄泉の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "安物/火/黄/繊細/ロリータ/" + "金属/" * 3)
ENDSELECT

