@CHARAMOVE_DATA_102(ARG,ARG:2)
LOCAL = 102
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;魔理沙寝室,アリスの部屋,夢子の部屋
			CASE 512, 521, 523
				RETURN -10
			;自室
			CASE 522
				;7～22時は避ける,寝起きと寝る前は行きやすく
				IF BETWEENTIME(420,1320)
					RETURN -10
				ELSE
					RETURN 10
				ENDIF
			;研究室
			CASE 524
				RETURN -10
			;リビング
			CASE 525
				;10～17時外は行きやすい
				IF BETWEENTIME(600,1020)
					RETURN 0
				ELSE
					RETURN 5
				ENDIF
			;空き部屋と倉庫
			CASE 528, 531
				RETURN -100
			;魔理沙家,エレン家,妖怪の棲み処
			CASE 511 TO 515, 535, 536, 537, 541, 542, 543, 544
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 51 TO 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ102
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ102(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "神綺")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "イブニングドレス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "無から創造された")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "魔界神様のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "魔界神様のブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "パン/スパイス/洋食/鍋物/パイ/あっさり/ヘルシー/豪華/上品/家庭的/もちもち/魔力/メイド流/魔界風/甘い/甘い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "中華/ガッツリ/質素/すっぱい/すっぱい/" + "カエル/虫肉/謎料理/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "魔界風/おふくろの味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/高級/混沌/美しい/威厳/壮大/" + "魔法/妖術/" * 3 + "魔界蝶の/魔神の/魔界の/神話の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/ロリータ/マニアック/" + "安物/巫術/" * 3)
ENDSELECT

