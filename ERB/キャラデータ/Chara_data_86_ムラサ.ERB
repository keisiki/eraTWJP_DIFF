@CHARAMOVE_DATA_86(ARG, ARG:2)
LOCAL = 86
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113ナズーリン私室, P115星私室, P116一輪私室, P117ぬえ私室, P118白蓮私室, P109女苑私室, P136響子私室
				RETURN -5
			;離れ、マミゾウの棲家には入らない,神霊廟内部は行かない
			CASE P119離れ, P137マミゾウの棲家, P125屠自古私室 TO P134蔵, P138鐘楼, P139邪仙楼
				RETURN -100
			;居間に行きやすい
			CASE P108居間
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE お地蔵通り, 枝垂れ柳の下, 命蓮寺境内, 命蓮寺方丈, 墓地
				RETURN 1
			CASE 中有の道, 三途の川
				RETURN 1
			CASE 玄武の沢, 間欠泉地下センター
				RETURN 1
			CASE 九天の滝, 山の湖
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;白蓮が本堂にいる時はやってくる
		SIF CFLAG:[[白蓮]]:現在位置 == P103本堂 && !CFLAG:[[白蓮]]:うふふ && SHIRAHU([[白蓮]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ86
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ86(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "水蜜")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ズボン"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ズボン", "ショートズボン"))
CASE "普段着：上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "セーラー服"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "名前代わりに錨のマークが書かれた")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ボーイッシュショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "タンクトップブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[ムラサ]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/スパイス/汁物/カレー/ジャンキー/濃厚/冷たい/機能的/豪快/グロテスク/ぷるぷる/辛い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "揚げ物/甘い/甘い/カエル/カエル/カエル/虫肉/虫肉/虫肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "刺激的/おいしい水/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/混沌/怖い/" + "水/幽霊/妖術/" * 3 + "海の/深海の/錨の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "知的/威厳/" + "光/火/護符/小手/" * 3)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_86
DISH_NAME = 深海料理
RESULTS = 主食
CALL 材料設定("うまい魚")
DISH_TASTE = 魚料理/しょっぱい/ガッツリ/淡白/野趣/豪快/グロテスク/霊障/
DISH_COMMENT = 深海魚を使ったちょっとグロテスクな料理。
RETURN 1

;IMAGE_I86_特殊服装(CHARA)-------------------------------------------------------------------
