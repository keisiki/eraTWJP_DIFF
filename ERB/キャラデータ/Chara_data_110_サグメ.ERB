@CHARAMOVE_DATA_110(ARG,ARG:2)
LOCAL = 110
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:住み込みキャラ != LOCAL
					RETURN -100
			;夢幻館には行かない
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;他人の家には入らない
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;物置,倉庫,地下室は行かない
			CASE 424, 427, 430
				RETURN -100
			;縁側,居間によく居る
			CASE 409, 416
				RETURN 5
			;静かの海の方へは行かない
			CASE 1016, 1060 TO 1072
				RETURN -100
			;空き家には行かない
			CASE 1007
				RETURN -100
			;詰め所、食料庫は行かない
			CASE 1023 TO 1025, 1028
				RETURN -100
			;玉兎街方面はあまり行かない
			CASE 1003, 1004, 1010 TO 1013
				RETURN -5
			;月の都によく居る
			CASE 1002
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 10
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 101, 105 TO 108
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
