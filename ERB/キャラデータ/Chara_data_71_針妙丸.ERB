@CHARAMOVE_DATA_71(ARG, ARG:2)
LOCAL = 71
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;主に居間に居る
			CASE P9居間
				RETURN 5
			;私室にはあまり入らない
			CASE P15霊夢私室, P52離れ
				RETURN -5
			;他所
			CASE P27三妖精の家・広間 TO P35三妖精の家・地下室, P37夢幻遺跡・一階通路 TO P51夢幻遺跡・倉庫, P56あうんの棲家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 神社縁側
				RETURN 1
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE お地蔵通り, 枝垂れ柳の下, 命蓮寺境内, 神霊廟広場
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ71
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ71(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "針妙丸")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "お椀"))
CASE "普段着：着物"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("着物", "着物"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "肌襦袢"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "帯"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "大きくても小さくてもピッタリサイズな")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "小人のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "小人のキャミソール")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[針妙丸]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/卵/和食/葉物/ジャンキー/豪華/上品/鮮やか/ふわふわ/甘い/甘い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "質素/辛い/辛い/謎料理/謎料理/謎料理/" + "カエル/虫肉/鬼の魔力/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "得意料理＊上品＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/秩序/かっこいい/オシャレ/ポップ/繊細/巫術/" + "装飾/" * 3 + "小人の/小さな小さな/針/茶碗/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "パワフル/知的/" + "キセル/" * 3 + "鬼の/鬼力の/" * 10)

;農民関連
;ころぽっくる
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "蕗農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "フキ")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "蕗畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "博麗神社")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 350)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 3)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_71
DISH_NAME = こびとのパン
RESULTS = 軽食
CALL 材料設定("汎用")
DISH_TASTE = 洋食/パン/甘い/あっさり/機能的/サクサク/魔力/
DISH_COMMENT = 食べると周りの地形を把握できる小人の為に作られた魔法のパン
RETURN 1

;IMAGE_I71_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I71_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ENDIF

