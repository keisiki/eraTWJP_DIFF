@CHARAMOVE_DATA_134(ARG,ARG:2)
LOCAL = 134
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;鯢呑亭(仕事があるので10までいかない）
			CASE 238
				RETURN 5
			;広場
			CASE 202
				RETURN 5
			;酒屋, 食料品店, 八百屋
			CASE 211, 216, 227
				RETURN 10
			;自室はあまり居ない
			CASE 239
				RETURN -5
			;あなたの部屋
			CASE 230
				IF TALENT:LOCAL:恋慕
					RETURN 5
				ELSE
					RETURN -100
				ENDIF
			;他のお店とかは行かない
			CASE 212, 213, 217, 218, 220, 221, 222, 223, 224, 225, 226, 228, 229, 231 TO 237
				RETURN -100
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1, 2, 3, 4, 11, 12, 13, 14, 21, 22, 23
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ134
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ134(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "美宵")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン＋フード）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ワイシャツ"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "地下足袋"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "いかがわしき")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "クジラパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "鯨鵬ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[美宵]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/魚料理/和食/煮物/おつまみ/ガッツリ/ヘルシー/質素/芸術的/家庭的/もちもち/甘い/甘い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "安物食材/ジャンキー/すっぱい/すっぱい/チーズ/チーズ/チーズ/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "プロの味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/かわいい/ポップ/壮大/" + "水/酒/食器/海の/記憶の/" * 3 + "クジラの/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "洋風/ロリータ/" + "雷/" * 3 + "忘却の/" * 10)
ENDSELECT

