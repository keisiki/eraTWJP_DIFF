@CHARAMOVE_DATA_80(ARG, ARG:2)
LOCAL = 80
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
		;日曜日は連動
		IF YOUBI_MATCH(DAY, "日")
			RESULT:1 = [[阿求]]
			RESULT:2 = [[小鈴]]
		ENDIF
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;稗田邸,阿求私室
			CASE P221稗田邸, P229阿求私室
				RETURN 5
			;慧音の家,鈴奈庵,小鈴の部屋
			CASE P224慧音宅, P213鈴奈庵, P226小鈴私室
				RETURN 5
			;私室には行かない
			CASE P208雷鼓の部屋, P209八橋の部屋, P210弁々の部屋
				RETURN -100
			CASE P231蛮奇の部屋, P232蓮子の部屋, P233メリーの部屋, P234ユキの部屋, P235マイの部屋, P239美宵の部屋
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 神社縁側
				RETURN 1
			CASE 広場, 商家町, 呑屋小道, 長屋街
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;小鈴の仕事日はちょいちょい会いに行く
		SIF YOUBI_MATCH(DAY, "水木") && INRANGE(TIME, 720, 1020) && CFLAG:LOCAL:今日の運勢 % 2 == 0
			TCVAR:LOCAL:引力点 = P213鈴奈庵
ENDSELECT

@EXIST_キャラデータ80
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ80(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "阿求")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "髪飾り"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "行灯袴"))
CASE "普段着：着物"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("着物", "着物"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "肌に優しい絹100％の")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "シームレスショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/和食/葉物/あっさり/ヘルシー/淡白/芸術的/上品/家庭的/とろとろ/外界風/甘い/甘い/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "辛い/ガッツリ/ジャンキー/濃厚/" * 2 + "カエル/スパイス/冥界風/地獄風/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "薄味/稗田家秘伝の味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "茶/美しい/芸術的/レトロ/知的/" + "文具/書物/" * 3 + "記憶の/知識の/記録/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/パワフル/ロリータ/" + "火/刃物/生体品/小手/キセル/ネズミの/" * 3 + "死神の/死の/忘却の/" * 10)

;農民関連
;人里のとっぷに君臨する
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "米農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "お米")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "水田")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "人里")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 2)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 206)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 550)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 9)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_80
DISH_NAME = 求聞雑炊
RESULTS = 軽食
CALL 材料設定("高級食材")
DISH_TASTE = 和食/汁物/米/野菜/豆/すっぱい/ヘルシー/淡白/温かい/質素/質素/家庭的/とろとろ/秘術/薬膳/
DISH_COMMENT = 稗田家に伝わる、記憶力を増幅させると言われる雑炊。健康にも良い。
RETURN 1

;IMAGE_I80_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I80_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:0 % 2 && !TCVAR:CHARA:発情
			RESULTS = _日替わり
	ENDIF
ENDIF

