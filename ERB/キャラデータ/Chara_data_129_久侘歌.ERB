@CHARAMOVE_DATA_129(ARG,ARG:2)
LOCAL = 129
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;八雲邸、畜生界は行かない（入り口部分は出る時用）
			CASE 630, 631, 640, 641
				RETURN -10
			CASE 630 TO 639, 640 TO 652
				RETURN -100
			;彼岸によくいる
			CASE 604
				RETURN 10
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 6, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 61, 62, 64, 65, 66, 67, 71, 72, 73, 75, 81, 82, 83, 84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ129
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ129(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "久侘歌")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "ロングブーツ"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "リボンチョーカー"))
CASE "普段着：その他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他２", "ひよこ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ピヨピヨ音の出る")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ひよこパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "ひよこブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/米/葉物/あっさり/淡白/質素/上品/鮮やか/ふわふわ/冥界風/甘い/甘い/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "揚げ物/ガッツリ/濃厚/しょっぱい/しょっぱい/カエル/カエル/カエル/" + "謎料理/" * 5 + "鳥肉/卵/" * 10)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "冥界風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黄/秩序/美しい/ポップ/繊細/" + "鳥/" * 3 + "ニワトリの/勇気の/タマゴの/卵/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/奇妙/マニアック/" + "弾/小手/生体品/機械/" * 3 + "カラスの/" * 10)
ENDSELECT

