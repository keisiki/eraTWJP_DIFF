@CHARAMOVE_DATA_92(ARG,ARG:2)
LOCAL = 92
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;神霊廟メンバーの私室
			CASE 125, 128
				RETURN -5
			;離れと墓地と命蓮寺内部
			CASE 103 TO 122
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13,15,16, 21,22,23
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ92
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ92(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "布都")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "烏帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "水干"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ミニスカート"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "誰かに騙されて穿いているらしい")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "女児パンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "サラシ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/卵/汁物/ジャンキー/淡白/豪華/上品/家庭的/もちもち/仙術/天界風/和食/和食/甘い/甘い/甘い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "洋食/ガッツリ/濃厚/カエル/カエル/カエル/虫肉/虫肉/虫肉/辛い/辛い/辛い/辛い/" + "グロテスク/謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "薄味/得意料理＊和食＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/白/秩序/かわいい/レトロ/壮大/" + "巫術/護符/食器/" * 3 + "仙人の/風水の/皿/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/怖い/知的/" + "死生/機械/" * 3 )
ENDSELECT

