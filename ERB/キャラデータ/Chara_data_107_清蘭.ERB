@CHARAMOVE_DATA_107(ARG,ARG:2)
LOCAL = 107
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;静かの海の方へは行かない
			CASE 1016, 1060 TO 1072
				RETURN -100
			;ドレミー宅には行かない
			CASE 1050 TO 1059
				RETURN -100
			;空き家には行かない
			CASE 1007
				RETURN -100
			;門番詰め所は行かない
			CASE 1023
				RETURN -100
			;二階はあまり行かない
			CASE 1040
				RETURN -5
			;玉兎街方面,玉兎食堂,玉兎詰め所によく居る
			CASE 1003, 1004, 1010 TO 1013, 1024, 1025
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 4, 8, 10
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 41 TO 45, 83, 105 TO 108
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
;鈴瑚
@EXIST_キャラデータ107
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ107(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "清蘭")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "裾の方がスケスケの")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "シースルードロワ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/中華/汁物/あっさり/淡白/豪華/野趣/鮮やか/もちもち/玉兎流/月都風/月見/甘い/甘い/甘い/ニンジン/ニンジン/ニンジン/ニンジン/ニンジン/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "質素/" + "カエル/獣肉/虫肉/スパイス/" * 3 + "辛い/辛い/辛い/辛い/" + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "玉兎流/薄味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "秩序/かわいい/シンプル/繊細/" + "青/弾/" * 3 + "浅葱色の/ウサギの/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "怖い/パワフル/知的/" + "刃物/生体品/小手/" * 3 + "橘色の/" * 10)
ENDSELECT

