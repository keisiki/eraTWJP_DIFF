@CHARAMOVE_DATA_139(ARG,ARG:2)
LOCAL = 139
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;保留
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 1, 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1, 2, 3, 4, 11, 12, 13, 14, 16, 21, 22, 23, 71, 72, 73, 81, 82, 83, 84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ139
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ139(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "典")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン＋フード）セット")

CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "チャイナドレス（ズボン）"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "足袋"))
	
CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "蠱惑的な")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "純白パンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "番の管狐ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[典]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/肉料理/肉料理/卵/卵/卵/揚げ物/甘い/甘い/辛い/辛い/あっさり/淡白/豪華/上品/鮮やか/とろとろ/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "すっぱい/すっぱい/濃厚/質素/質素/野趣/野趣/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "隠し味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "高級/白/混沌/かわいい/シンプル/シック/知的/" + "純白の/キツネの/薬瓶/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "黒/秩序/派手/威厳/" + "安物/護符/" * 3 + "タヌキの/小手/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_139
DISH_NAME = きつねごはん
RESULTS = 主食
CALL 材料設定("ニンジン")
DISH_TASTE = 和食/米/野菜/しょっぱい/あっさり/淡白/温かい/質素/家庭的/
DISH_COMMENT = 煮込んだ油揚げとニンジンを混ぜたご飯。懐かしい味わい。
RETURN 1
