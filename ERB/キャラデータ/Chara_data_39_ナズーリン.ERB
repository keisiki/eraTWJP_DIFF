@CHARAMOVE_DATA_39(ARG,ARG:2)
LOCAL = 39
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			CASE 18, 43, 44, 51, 52
				RETURN -100
			;命蓮寺メンバーの私室（経堂と物置除く）
			CASE 114, 115, 116, 117, 118
				RETURN -5
			;離れと墓地と神霊廟内部
			CASE 119, 121, 125 TO 133
				RETURN -100
			;居間に行きやすい
			CASE 108
				RETURN 5
			;白蓮が本堂にいる時はやってくる
			CASE 103
				SIF CFLAG:55:現在位置 == 103
					RETURN 10
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 5, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11 TO 15, 21,22,23, 51 TO 57, 61,62
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ39
#LOCALSIZE 1
#LOCALSSIZE 1

