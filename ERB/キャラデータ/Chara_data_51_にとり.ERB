	CALLF MAKE_STR(V_NAME, "化学的/おいしい水/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/技術/秩序/かっこいい/シンプル/未来/知的/マニアック/" + "水/金属/道具/カメの/" * 3 + "キュウリの/河童の/機械仕掛けの/光学技術の/機械/" * 5)
