@CHARAMOVE_DATA_16(ARG,ARG:2)
LOCAL = 16
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		;6時～18時は寝てる、最近のお嬢様はサマータイム制などと申して起床・就寝時間をずらしております。
		IF DAY:2 == 2
			CFLAG:16:就寝時間 = 300
			CFLAG:16:起床時間 = 1140
		ELSEIF DAY:2 == 4
			CFLAG:16:就寝時間 = 420
			CFLAG:16:起床時間 = 1020
		ELSE
			CFLAG:16:就寝時間 = 360
			CFLAG:16:起床時間 = 1080
		ENDIF
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;お嬢様なので一応ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;納屋と屋敷外には出ないし、空き部屋にも行かない
			CASE 304, 305, 321, 329, 330, 334 TO 348
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 3
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 34 TO 38
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ16
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ16(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "レミリア")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ネグリジェ）セット")

CASE "普段着：アクセサリ"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("アクセサリ", "傘"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ものすごく高級な")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "フルレースショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "キャミソール")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[レミリア]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "高級食材/汁物/ケーキ/濃厚/豪華/上品/鮮やか/ふわふわ/メイド流/外界風/月見/洋食/洋食/甘い/甘い/甘い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "和食/あっさり/ジャンキー/淡白/野趣/" + "安物食材/カエル/虫肉/質素/" * 3 + "辛い/" * 4  + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "得意料理＊洋食＊/絢爛/")

;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "高級/宝石/混沌/かわいい/奇妙/洋風/派手/ロリータ/威厳/" + "赤/血/美術品/人形/" * 3 + "永遠に紅い/コウモリの/悪魔の/紅魔の/満月の/血の/運命の/傘/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "青/秩序/シンプル/和風/繊細/" + "安物/水/光/護符/小手/キセル/メイドの/" * 3 + "日/太陽石の/" * 5)
;農民関連
;西洋っぽいので小麦の供給元に
;霧の湖近辺は冷害が怖い
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "小麦農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "小麦")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "小麦畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "紅魔館")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 3)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 301)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 600)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 16)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_16
DISH_NAME = 真紅のブラッドスープ
RESULTS = 軽食
CALL 材料設定("トマト")
DISH_TASTE = 洋食/汁物/野菜/獣肉/甘い/濃厚/温かい/上品/鮮やか/鮮やか/鮮やか/グロテスク/絢爛/血の魔力/
DISH_COMMENT = 生き血を啜るが如く紅く染まったスープ。
RETURN 1

