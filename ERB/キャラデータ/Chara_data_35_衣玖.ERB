@CHARAMOVE_DATA_35(ARG,ARG:2)
LOCAL = 35
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;空気を読んでひとんちの風呂とかトイレにはいかない
			CASE 6, 12, 13, 14, 17, 18, 30, 32, 33, 34, 35, 39, 40, 51, 52, 53
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23,27, 71,72,73, 81,82,83,84,89
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ35
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ35(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "衣玖")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：外衣"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外衣", "ケープ"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "フリルブラウス"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "羽衣"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "羽衣のように軽やかな")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "薄絹のショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "薄絹のブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[衣玖]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/鍋物/甘い/あっさり/ヘルシー/淡白/芸術的/鮮やか/家庭的/ぷるぷる/神力/天界風/すっぱい/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "しょっぱい/しょっぱい/ジャンキー/ジャンキー/ジャンキー/外界風/外界風/外界風/" + "魚料理/謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "天界風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/美しい/シンプル/シック/魚/" + "雷/" * 3 + "龍の/龍神の/天女の/稲妻の/羽衣/鱗/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/ロリータ/" + "土/火/刃物/弾/小手/" * 3)

ENDSELECT

