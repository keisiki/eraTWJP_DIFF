@CHARAMOVE_DATA_133(ARG, ARG:2)
LOCAL = 133
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE P612法廷 TO P639八雲邸居間
				RETURN -100
			;他人の家には入らない
			CASE P608潤美の家, P609瓔花の家
				RETURN -100
			;霊長園には行かない
			CASE P642霊長園 TO P645磨弓の部屋
				RETURN -100
			;他所の風呂には行かない
			CASE P646八千慧宅の風呂, P655尤魔宅の風呂
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 中有の道, 三途の川, 彼岸, 地獄, 畜生界
				RETURN 1
			CASE 麓の樹海, 玄武の沢, 大蝦蟇の池
				RETURN 1
			CASE 九天の滝, 絶景の丘, 山の湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ133
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ133(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "早鬼")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "テンガロンハット"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "ロングブーツ"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "スカーフ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "蹄鉄の印章が施された")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ブラックシェイプショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "黒翼ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[早鬼]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/スパイス/中華/鍋物/ガッツリ/ジャンキー/濃厚/温かい/豪華/野趣/豪快/サクサク/畜生流/" + "辛い/ニンジン/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "質素/" + "甘い/すっぱい/あっさり/淡白/" * 2 + "天界風/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "畜生流/得意料理＊豪快＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/赤/混沌/かっこいい/シンプル/パワフル/壮大/" + "ウマの/オオカミの/天馬の/スカーフ/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/繊細/知的/" + "神霊/書物/機械/" * 3 + "はにわの/カワウソの/ワシの/カメの/龍の/はにわ/" * 10)
ENDSELECT

