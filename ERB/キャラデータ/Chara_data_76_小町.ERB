@CHARAMOVE_DATA_76(ARG, ARG:2)
LOCAL = 76
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;八雲邸、畜生界は行かない
			CASE P630八雲邸玄関 TO P655尤魔宅の風呂
				RETURN -100
			;他人の家、他宅の私室等には入らない
			CASE P608潤美の家, P609瓔花の家
				RETURN -100
			;映姫様の私室はあまり入らない
			CASE P615映姫の部屋
				RETURN -5
			;法廷はなるべく入らない
			CASE P612法廷
				RETURN -10
			;賽の河原～彼岸によく居る
			CASE P602賽の河原, P603積石塚, P604彼岸
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 神社縁側
				RETURN 1
			CASE 呑屋小道
				RETURN 1
			CASE 中有の道, 三途の川, 彼岸, 白玉楼庭, 地獄
				RETURN 1
			CASE 麓の樹海, 玄武の沢, 大蝦蟇の池, 仙人の屋敷
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ76
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ76(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "小町")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "髪飾り"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "下駄"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "足袋"))
CASE "普段着：着物"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("着物", "着物"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "肌襦袢"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "帯"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "オトナの魅力溢れる")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "タンガショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "レースブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[小町]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/スパイス/和食/揚げ物/おつまみ/ガッツリ/ジャンキー/濃厚/豪華/鮮やか/豪快/霊障/冥界風/辛い/辛い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "ヘルシー/化学的/すっぱい/すっぱい/質素/質素/カエル/カエル/カエル/虫肉/虫肉/虫肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "冥界風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/美しい/和風/派手/" + "死生/酒/" * 3 + "死神の/彼岸の/死出の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "洋風/繊細/ロリータ/マニアック/" + "ネクタイ/" * 3)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "依頼なし発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼76(ARG, O_DATA, V_NAME)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_76
DISH_NAME = 人魂わたあめ
RESULTS = デザート
CALL 材料設定("彷徨う魂")
DISH_TASTE = 和食/甘い/淡白/淡白/冷たい/質素/ふわふわ/霊障/冥界風/
DISH_COMMENT = 冥界で人気の希薄なわたあめ。ヒンヤリする。
RETURN 1

