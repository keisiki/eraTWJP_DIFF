@CHARAMOVE_DATA_98(ARG, ARG:2)
LOCAL = 98
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;夢幻館には行かない
			CASE P450玄関 TO P466庭
				RETURN -100
			;他人の家には入らない
			CASE P432影狼の家, P437メディスンの家, P439ラルバの家, P443ミスティアの家, P445妹紅の家
				RETURN -100
			;縁側,居間によく居る
			CASE P409縁側, P416居間
				RETURN 5

			;静かの海の方へは行かない
			CASE P1016入り江, P1060純狐邸・玄関 TO P1072ヘカーティア私室
				RETURN -100
			;ドレミー宅には行かない
			CASE P1050ドレミー宅・居間 TO P1055ドレミー宅・トイレ
				RETURN -100
			;詰め所、食料庫は行かない
			CASE P1023門番詰め所 TO P1025玉兎詰め所, P1028食料庫
				RETURN -100
			;中庭、四阿によく居る
			CASE P1035中庭, P1036四阿
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 月の都, 綿月亭, 玉兎の街, 桃林～砂浜
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ98
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ98(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "依姫")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "サロペットスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "半袖ブラウス"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "ブレスレット"))
CASE "普段着：その他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他２", "サッシュベルト"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "一切の穢れなき")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "無邪の下衣")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "清浄の肌衣")
CASE "固有下着着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/麺類/ヘルシー/質素/上品/芸術的/もちもち/月都風/月見/おいしい水/中華/中華/甘い/甘い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "洋食/ジャンキー/野趣/すっぱい/すっぱい/" + "カエル/虫肉/グロテスク/" * 3 + "謎料理/生命力/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "月都風/切れ味/神力/神力/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/秩序/美しい/シンプル/大陸風/シック/威厳/" + "水/月/神霊/巫術/刃物/護符/" * 3 + "海神の/海の/穢れなき/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/奇妙/洋風/派手/ロリータ/マニアック/" + "土/闇/死生/生体品/キセル/" * 3 + "地上の/パンデモニックな/" * 10)
ENDSELECT

;IMAGE_I98_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I98_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:衣装一時変更 == 55 || CFLAG:CHARA:基本服装セット == 55
			IF FLAG:立ち絵種類
				;顔絵
				RESULTS = _バニー
			ELSE
				;立ち絵
				RESULTS = _逆バニー
			ENDIF
		ENDIF
	ENDIF
ENDIF

