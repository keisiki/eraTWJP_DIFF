@CHARAMOVE_DATA_98(ARG,ARG:2)
LOCAL = 98
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;夢幻館には行かない
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;他人の家には入らない
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;物置,倉庫,地下室は行かない
			CASE 424, 427, 430
				RETURN -100
			;縁側,居間によく居る
			CASE 409, 416
				RETURN 5
			;静かの海の方へは行かない
			CASE 1016, 1060 TO 1072
				RETURN -100
			;ドレミー宅には行かない
			CASE 1050 TO 1059
				RETURN -100
			;空き家には行かない
			CASE 1007
				RETURN -100
			;詰め所、食料庫は行かない
			CASE 1023 TO 1025, 1028
				RETURN -100
			;中庭、四阿によく居る
			CASE 1035, 1036
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 10
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 105 TO 108
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ98
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ98(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "依姫")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "サロペットスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "半袖ブラウス"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "ブレスレット"))
CASE "普段着：その他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他２", "サッシュベルト"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "一切の穢れなき")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "無邪の下衣")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "清浄の肌衣")
CASE "固有下着着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/麺類/ヘルシー/質素/上品/芸術的/もちもち/月都風/月見/おいしい水/中華/中華/甘い/甘い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "洋食/ジャンキー/野趣/すっぱい/すっぱい/" + "カエル/虫肉/グロテスク/" * 3 + "謎料理/生命力/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "月都風/切れ味/神力/神力/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/秩序/美しい/シンプル/大陸風/シック/威厳/" + "水/月/神霊/巫術/刃物/護符/" * 3 + "海神の/海の/穢れなき/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/奇妙/洋風/派手/ロリータ/マニアック/" + "土/闇/死生/生体品/キセル/" * 3 + "地上の/パンデモニックな/" * 10)
ENDSELECT

