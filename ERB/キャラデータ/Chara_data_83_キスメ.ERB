@CHARAMOVE_DATA_83(ARG,ARG:2)
LOCAL = 83
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;せまいところが好き
			CASE 3, 5, 6, 12, 14, 16, 17, 18, 23, 38, 40, 51, 53,
				RETURN 10
			;行かない
			CASE 901 TO 930, 935, 936, 937, 939, 940, 941, 943
				RETURN -100
			;大通りと酒場に行きやすい
			CASE 931, 933
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 9
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 91,92,93
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ83
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ83(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "キスメ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "髪飾り"))
CASE "普段着：着物"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("着物", "着物"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ちっちゃくて可愛らしい")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "桃色ふんどし")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[キスメ]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/和食/揚げ物/あっさり/ジャンキー/淡白/質素/野趣/グロテスク/とろとろ/地底風/甘い/甘い/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "ガッツリ/辛い/辛い/スパイス/スパイス/スパイス/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "地底風/ドッキリ味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/混沌/かわいい/レトロ/シック/" + "火/水/骨/生体品/" * 3 + "地底の/骨の/ドクロの/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "派手/パワフル/威厳/" + "小手/機械/" * 3)
ENDSELECT

