@CHARAMOVE_DATA_122(ARG,ARG:2)
LOCAL = 122
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
		;連動処理（里乃の方が高いときは里乃を参照）
		IF TALENT:123:恋慕 || (TALENT:123:思慕 && !TALENT:123:思慕)
			CFLAG:LOCAL:現在位置 = CFLAG:123:遭遇位置 ? CFLAG:123:遭遇位置 # SUKIMA()
		ELSE
			CFLAG:LOCAL:現在位置 = CFLAG:122:遭遇位置 ? CFLAG:122:遭遇位置 # SUKIMA()
		ENDIF
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 4, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21, 22, 23, 41, 42, 43, 46, 47, 51, 52, 53, 56, 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ122
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ122(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "舞")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "烏帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "踊りやすそうな")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ボーイズショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "スポーツブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[舞]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/鍋物/ガッツリ/ジャンキー/濃厚/豪華/芸術的/豪快/ふわふわ/甘い/甘い/辛い/辛い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "野菜/質素/すっぱい/すっぱい/天狗流/天狗流/天狗流/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "エンチャント/濃い味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "緑/混沌/かっこいい/ポップ/派手/不思議/" + "音楽/楽器/" * 3 + "竹の/クレイジーな/後戸の/肉体の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "シック/繊細/知的/" + "書物/" * 3 + "天狗の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_122
DISH_NAME = タナバタスターソーメン
RESULTS = 主食
CALL 材料設定("たけのこ")
DISH_TASTE = 和食/麺類/しょっぱい/あっさり/淡白/冷たい/冷たい/質素/上品/家庭的/生命力/きらめき/
IF FESTIVAL() == "七夕"
	DISH_TASTE += "縁起物/"
ELSE
	DISH_TASTE += "季節外れ/"
ENDIF
DISH_COMMENT = 七夕の日（夏の月7日）に食べる煌びやかな素麺。味は普通。
RETURN 1
