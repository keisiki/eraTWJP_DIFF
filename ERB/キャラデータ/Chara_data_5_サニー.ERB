@CHARAMOVE_DATA_5(ARG,ARG:2)
LOCAL = 5
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
		;連動処理（三人の内一番高いキャラにあわせる）
		IF TALENT:7:恋慕 || (TALENT:7:思慕 && !TALENT:6:思慕 && !TALENT:5:思慕)
			CFLAG:LOCAL:現在位置 = CFLAG:7:遭遇位置 ? CFLAG:7:遭遇位置 # SUKIMA()
		ELSEIF TALENT:6:恋慕 || (TALENT:6:思慕 && (!TALENT:7:思慕 || !TALENT:5:思慕))
			CFLAG:LOCAL:現在位置 = CFLAG:6:遭遇位置 ? CFLAG:6:遭遇位置 # SUKIMA()
		ELSE
			CFLAG:LOCAL:現在位置 = CFLAG:5:遭遇位置 ? CFLAG:5:遭遇位置 # SUKIMA()
		ENDIF
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		;24時～6時は寝てる
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		;特になし
		SELECTCASE ARG:2
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 3, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4,5, 31,34, 51 TO 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ5
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ5(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "サニー")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ネグリジェ）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "ホワイトブリム"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "太陽のマークが刺繍された")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ワンポイントショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "ハーフトップブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[サニー]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;農民関連
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "レタス農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "レタス")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "レタス畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "博麗神社")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 25)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 200)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "ミルク/キノコ/洋食/揚げ物/辛い/ガッツリ/ジャンキー/濃厚/温かい/豪華/野趣/豪快/トロトロ/きらめき/甘い/甘い/甘い/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "質素/すっぱい/すっぱい/野菜/野菜/野菜/グロテスク/グロテスク/グロテスク/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "濃い味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "朱/果実/かわいい/ポップ/派手/パワフル/ロリータ/" + "日/生体品/" * 3 + "太陽の/陽光の/トリニタリアンな/反射の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "シック/知的/" + "闇/星/月/書物/キセル/" * 3)

ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_5
DISH_NAME = サニーサイドアップ
RESULTS = 軽食
CALL 材料設定("汎用")
DISH_TASTE = 洋食/卵/卵/甘い/あっさり/鮮やか/豪快/
DISH_COMMENT = 片面を焼いた目玉焼き。派手な装飾付き。
DISH_TIME = 10
RETURN 1

;IMAGE_I5_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I5_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

