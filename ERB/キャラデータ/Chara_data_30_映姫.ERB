@CHARAMOVE_DATA_30(ARG,ARG:2)
LOCAL = 30
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;小町の私室
			CASE 616
				RETURN -5
			;八雲邸、畜生界は行かない（入り口部分は出る時用）
			CASE 630, 631, 640, 641
				RETURN -10
			CASE 630 TO 639, 640 TO 652
				RETURN -100
			;他人の家、他宅の私室等には入らない
			CASE 608, 609, 623, 624, 627
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13, 21,22,23,27, 61,62,64,65,66,67
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
