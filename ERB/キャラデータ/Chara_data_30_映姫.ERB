;IMAGE_I30_特殊エフェクトチェック(CHAR, エフェクト)--------------------------------------------
;キャラクター特有のエフェクト判定  特殊エフェクトを直接指定したい場合に参照
;引数 "エフェクト" に固有のエフェクトが含まれるかを判定する
;例の通り、STRFINDを使用してください
;-------------------------------------------------------------------------------------------
@IMAGE_I30_特殊エフェクトチェック(CHARA, エフェクト)
#DIM CHARA
#DIMS エフェクト

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	SIF STRFIND(エフェクト, "怒り") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "ハート") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "睡眠") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "母乳") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "精液上") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "精液中") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "精液下") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "泣き") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "キラ") != -1 && FLAG:立ち絵種類
		RETURN 1
	SIF STRFIND(エフェクト, "ダミー") != -1
		RETURN 1
ELSE
	;通常画像
ENDIF

RETURN 0

;IMAGE_I30_特殊エフェクト(CHARA)--------------------------------------------------------------------
;キャラクター特有のエフェクト表示条件
;使用時注意点:
;	RESULTS配列に特殊エフェクトを格納
;	RETURNで追加するエフェクト数を返却すること
;-------------------------------------------------------------------------------------------
@IMAGE_I30_特殊エフェクト(CHARA)
#DIM CHARA
#DIM DYNAMIC エフェクト数 = 0
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	;辛いもの食べるor作るで星が湧く
	IF GROUPMATCH(SELECTCOM, 413, 414, 415) && FLAG:立ち絵種類
		SIF TALENT:CHARA:機嫌 < 0 || CFLAG:CHARA:衣装一時変更 == 62 || !SHIRAHU(CHARA) || MARK:CHARA:3
			GOTO SKIP
		IF DISHNAME == "地獄ラーメン（極辛）" || TCVAR:30:360 == TIME
			RESULTS:エフェクト数 = キラ
			エフェクト数 += 1
			RETURN エフェクト数
		ENDIF
	ENDIF
	$SKIP
	IF TCVAR:CHARA:無理矢理 || (CFLAG:CHARA:1600 && CFLAG:CHARA:うふふ)
		RESULTS:エフェクト数 = 泣き
		エフェクト数 += 1
	ENDIF
	IF TALENT:CHARA:母乳体質 && NOWEX:CHARA:噴乳 && CHECK_NUDE(CHARA)
		RESULTS:エフェクト数 = 母乳
		エフェクト数 += 1
	ENDIF
	;口上から指定する顔絵エフェクト盛り合わせ
	;もう少しスマートなやり方無いもんかね
	IF TFLAG:192 <= -5
		RESULTS:エフェクト数 = ダミー
		エフェクト数 += 1
		IF TFLAG:192 <= -6
			RESULTS:エフェクト数 = 照れ
			エフェクト数 += 1
			IF TFLAG:192 <= -7
				RESULTS:エフェクト数 = ハート
				エフェクト数 += 1
				IF TFLAG:192 <= -8
					RESULTS:エフェクト数 = 汗
					エフェクト数 += 1
					IF TFLAG:192 <= -9
						RESULTS:エフェクト数 = 吐息
						エフェクト数 += 1
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	IF FLAG:立ち絵種類
		;顔絵
		IF CFLAG:CHARA:口に精液 || TCVAR:CHARA:363 == TIME
			;顔ぶっかけ
			RESULTS:エフェクト数 = 精液上
			エフェクト数 += 1
			;朝フェラ後紅潮
			IF TCVAR:CHARA:363 == TIME
				RESULTS:エフェクト数 = 照れ
				エフェクト数 += 1
			ENDIF
		ENDIF
		IF CFLAG:CHARA:手に精液 && !(CHECK_NUDE(CHARA) == 0 || (TALENT:CHARA:妊娠 && CFLAG:CHARA:妊娠経過日数 >= 41))
			;手コキ
			RESULTS:エフェクト数 = 精液手
			エフェクト数 += 1
		ENDIF
		;着衣ぶっかけどうしよう。ちょっとオーバーかも
		IF CFLAG:CHARA:顔に精液
			;胸ぶっかけ
			RESULTS:エフェクト数 = 精液下
			エフェクト数 += 1
		ENDIF
	ELSE
		;立ち絵
		IF CFLAG:CHARA:顔に精液
			;胸ぶっかけ
			RESULTS:エフェクト数 = 精液上
			エフェクト数 += 1
		ENDIF
		IF CFLAG:CHARA:手に精液 && !(TALENT:CHARA:妊娠 && CFLAG:CHARA:妊娠経過日数 >= 41)
			;手コキ
			RESULTS:エフェクト数 = 精液手
			エフェクト数 += 1
		ENDIF
		IF (CFLAG:CHARA:膣内射精 || CFLAG:CHARA:アナル射精) && GROUPMATCH(NUDITYCHECK(CHARA), 0, 2)
			;中出し
			RESULTS:エフェクト数 = 精液下
			エフェクト数 += 1
		ENDIF
	ENDIF
	SIF FLAG:時間停止
		RETURN エフェクト数
	IF BASE:CHARA:ムード >= MAXBASE:CHARA:ムード*8/10
		IF !(TCVAR:CHARA:無理矢理 || CFLAG:CHARA:1600 || 仕事中チェック(CHARA)) 
			RESULTS:エフェクト数 = ハート
			エフェクト数 += 1
		ENDIF
	ELSEIF MARK:CHARA:反発刻印 > 0 || CFLAG:CHARA:ブチギレ
		RESULTS:エフェクト数 = 怒り
		エフェクト数 += 1
	ELSEIF CFLAG:CHARA:睡眠 && TFLAG:100
		RESULTS:エフェクト数 = 睡眠
		エフェクト数 += 1
	ENDIF
ELSE
	;通常画像
	IF FLAG:立ち絵種類
		;顔絵
	ELSE
		;立ち絵
	ENDIF
ENDIF

RETURN エフェクト数

