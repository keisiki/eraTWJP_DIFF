@CHARAMOVE_DATA_52(ARG,ARG:2)
LOCAL = 52
SELECTCASE ARG
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;夢幻館には行かない
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;他人の家には入らない
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;来客用トイレ,永琳私室,輝夜私室,裏庭,物置,倉庫,地下室はあんまり行かない
			CASE 406, 413, 414, 422, 424, 427, 430
				RETURN -5
			;永遠亭入口,待合室,診察室,調剤室,てゐ私室,鈴仙私室,庭がちょっと好き
			CASE 401, 403, 404, 405, 410, 411, 421
				RETURN 3
			;縁側,居間によく居る
			CASE 409, 416
				RETURN 5
			;ドレミー宅には行かない
			CASE 1050 TO 1059
				RETURN -100
			;空き家には行かない
			CASE 1007
				RETURN -100
			;門番詰め所は行かない
			CASE 1023
				RETURN -100
			;二階はあまり行かない
			CASE 1040
				RETURN -5
			;玉兎街方面,玉兎食堂,玉兎詰め所によく居る
			CASE 1003, 1004, 1010 TO 1013, 1024, 1025
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 4
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 41 TO 45
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ52
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ52(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "鈴仙")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ワイシャツ"))
CASE "普段着：上半身上着１"
;夏はブレザーを脱ぐ
	SIF DAY:2 != 2
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "ブレザー"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "ネクタイ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "兎尻尾にリボンで結わえた")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ボーダーショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "ボーダーブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/果物/葉物/ヘルシー/上品/機能的/家庭的/もちもち/玉兎流/月都風/月見/甘い/甘い/すっぱい/すっぱい/ニンジン/ニンジン/ニンジン/ニンジン/ニンジン/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "野趣/辛い/辛い/" + "カエル/獣肉/スパイス/虫肉/グロテスク/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "玉兎流/得意料理＊ヘルシー＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/かわいい/繊細/知的/" + "ウサギの/地上の/狂気の/薬瓶/ネクタイ/やみかわな/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/怖い/奇妙/派手/ロリータ/" + "刃物/生体品/小手/" * 3)

;特殊衣装
CASE "おうちデート衣装"
	SIF MAIN_MAP == 2
		CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "うどん薬売りセット"))
CASE "人里デート衣装"
	CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "うどん薬売りセット"))

CASE "固有コマンド"
	CALLF MAKE_STR(V_NAME, "薬を買う")
CASE "固有コマンド実行可能"
	CALLF MAKE_INT(V_NAME, 1)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_52
DISH_NAME = 狂気の赤い目玉焼き
RESULTS = 軽食
CALL 材料設定("汎用")
DISH_TASTE = 卵/しょっぱい/あっさり/ヘルシー/温かい/家庭的/鮮やか/グロテスク/妖力/玉兎流/
DISH_COMMENT = 赤い瞳のような目玉焼き。見た目はグロテスクだが栄養価は高い。
RETURN 1

;IMAGE_I52_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I52_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF EQUIP:CHARA:上半身上着１ != 4
			RESULTS = _ブラウス
	ENDIF
ENDIF

