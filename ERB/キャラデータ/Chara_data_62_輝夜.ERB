	IF LOCAL >= 5
		RESULTS:0 = 眯笑
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 睡眠
		RESULTS:1 = 睡眠
	ENDIF
	RETURN
ENDIF

IF CFLAG:62:ブチギレ
	RESULTS:0 = 怒{RAND:4+1}
	RESULTS:1 = 怒{RAND:4+1}
	RETURN
ELSEIF BASE:62:怒り > 600 || TALENT:62:機嫌 < 0 || CFLAG:62:不機嫌 == 1
	RESULTS:0 = 不悦
	RETURN
ENDIF

IF TALENT:62:恋人
	IF BASE:62:ムード >= 100
		RESULTS:0 = 笑顔
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
ELSEIF TALENT:62:恋慕 || TALENT:62:愛欲
	IF BASE:62:ムード >= 300
		RESULTS:0 = 笑顔
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
ELSEIF TALENT:62:思慕 || TALENT:62:セフレ
	IF BASE:62:ムード >= 600
		RESULTS:0 = 笑顔
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
ELSE
	IF BASE:62:ムード >= 1200
		RESULTS:0 = 笑顔
	ELSE
		RESULTS:0 = 通常
	ENDIF
ENDIF

IF RESULTS:0 == "通常"
	IF MARK:62:反発刻印 <= 0
		RESULTS:1 = 微开口
	ENDIF
ELSEIF RESULTS:0 == "笑顔"
	IF PALAM:62:欲情 >= PALAMLV:10 || TCVAR:62:発情 || CFLAG:62:溜まってる度 >= 900
		RESULTS:1 = 开口
	ELSEIF MARK:62:反発刻印 <= 0 && (TFLAG:103==109 || TARGET==109)
		RESULTS:1 = 貘笑
	ENDIF
ENDIF

RETURN


