@CHARAMOVE_DATA_25(ARG,ARG:2)
LOCAL = 25
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;藍さまは礼儀をわきまえているのでひとんちの風呂とかトイレにはいかない
			CASE 17, 19, 53
				SIF FLAG:15 != LOCAL
					RETURN -100
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51, 52
				RETURN -100
			;紫様の私室
			CASE 633
				RETURN -5
			;是非曲直庁、畜生界は行かない（入り口部分は出る時用）
			CASE 610, 613, 640, 641
				RETURN -10
			CASE 610 TO 618, 640 TO 652
				RETURN -100
			;他人の家、他宅の私室等には入らない
			CASE 608, 609, 623, 624, 627
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 6, 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,3,4, 23, 61,62,63,65,66, 71,72,73,74
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ25
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ25(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "藍")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
	ENDIF
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "道士服"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "大きな尻尾穴が開いた")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "Ｏバックショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "フルカップブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[藍]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/卵/和食/中華/揚げ物/ガッツリ/上品/家庭的/機能的/もちもち/辛い/辛い/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "しょっぱい/しょっぱい/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "妖力/妖力/得意料理＊和食＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/秩序/美しい/和風/シック/知的/" + "妖術/書物/" * 3 + "キツネの/式神の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "ポップ/ロリータ/マニアック/" + "水/小手/" * 3 + "タヌキの/" * 10)

;農民関連
;おあげ
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "大豆農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "大豆")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "大豆畑")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "人里")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 2)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 205)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 600)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 5)
ENDSELECT

