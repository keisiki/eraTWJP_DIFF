@CHARAMOVE_DATA_43(ARG,ARG:2)
LOCAL = 43
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;かせんハウス上階
			CASE 724 TO 728
				;7～17時は避ける,それ以外もあまり行かない
				;※華扇は仕事で道場に行くので脱出は簡単
				IF BETWEENTIME(420,1020)
					RETURN -10
				ELSE
					RETURN -5
				ENDIF
			;外に出やすいように屋敷内は避ける
			CASE 720 TO 723
				RETURN -10
			CASE 729, 730
				RETURN -5
			;各私室
			CASE 704, 706, 709, 715 TO 719, 733, 734
				RETURN -100
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23, 71,72,73,75, 81,82,83,84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

