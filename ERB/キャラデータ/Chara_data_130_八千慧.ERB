@CHARAMOVE_DATA_130(ARG,ARG:2)
LOCAL = 130
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE 610, 613, 620, 621, 630, 631
				RETURN -10
			CASE 610 TO 639
				RETURN -100
			;他人の家には入らない
			CASE 608, 609
				RETURN -100
			;霊長園には行かない
			CASE 642 TO 645
				RETURN -100
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 5, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1, 2, 3, 4, 21, 22, 23, 51, 53, 54, 56, 57, 61, 62, 64, 67, 68
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
