@CHARAMOVE_DATA_15(ARG, ARG:2)
LOCAL = 15
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;納屋には入らない、屋敷外には出ない
			CASE P305納屋, P334廃洋館・入り口 TO P348霧の湖・湖底
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 神社縁側
				RETURN 1
			CASE 広場, 商家町, 呑屋小道, 長屋街
				RETURN 1
			CASE 霧の湖, 正門, 広間, 食堂, 大図書館, 地下室
				RETURN 1
			CASE 森の入り口, 香霖堂, 霧雨魔法店, 人形の洋館
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ15
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ15(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "咲夜")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "ホワイトブリム"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ガーターストッキング"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
	ENDIF
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "エプロンドレス"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "リボン"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ガーター付きの上品な")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "高級シルクショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "高級シルクブラ")
CASE "固有下着着用条件"
	SIF !RAND:(3 - (2 * 勝負下着判定([[咲夜]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

CASE "固有コマンド"
	CALLF MAKE_STR(V_NAME, "紅魔館出張サービス")
CASE "固有コマンド実行可能"
	IF TFLAG:出張お掃除 && (FLAG:水やり代行 || !ITEM:150)
	ELSEIF GET_MAPNAME(MAIN_MAP) == "紅魔館"
	ELSE
		CALLF MAKE_INT(V_NAME, 1)
	ENDIF

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/スパイス/中華/煮物/甘い/淡白/豪華/上品/家庭的/サクサク/絢爛/" + "すっぱい/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "質素/野趣/しょっぱい/しょっぱい/虫肉/虫肉/虫肉/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "隠し味/秘術/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/美しい/シンプル/洋風/シック/" + "時計/食器/" * 3 + "瀟洒な/メイドの/時の/時空の/懐中時計/ナイフ/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "パワフル/ロリータ/" + "キセル/" * 3)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "依頼なし発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼15(ARG, O_DATA, V_NAME)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_15
DISH_NAME = 紅魔ランチ
RESULTS = 主食
CALL 材料設定("高級食材")
DISH_TASTE = 洋食/お子様ランチ/パン/ミルク/甘い/ガッツリ/濃厚/温かい/豪華/上品/鮮やか/秘術/メイド流/
DISH_COMMENT = 咲夜がお嬢様の為に創作したお子様ランチ。
RETURN 1

;IMAGE_I15_特殊服装(CHARA)-------------------------------------------------------------------
