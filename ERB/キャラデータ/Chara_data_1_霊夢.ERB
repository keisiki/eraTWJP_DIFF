;オブジェクト本体
@キャラデータ1(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "霊夢")

;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：アクセサリ"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("アクセサリ", "袖"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "リボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
	ENDIF
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "博麗の巫女服"))

CASE "固有コマンド"
	CALLF MAKE_STR(V_NAME, "おみくじ契約")
CASE "固有コマンド実行可能"
	CALLF MAKE_INT(V_NAME, !FLAG:おみくじ契約)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/洋食/葉物/あっさり/淡白/質素/上品/家庭的/ふわふわ/" + "甘い/すっぱい/" * 2 + "春物/煎餅/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "辛い/妖力/" * 2 + "グロテスク/謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "薄味/得意料理＊和食＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/かわいい/和風/" + "神霊/巫術/護符/キツネの/春の/永遠の/" * 3 + "蓮華蝶の/サクラの/巫女の/楽園の/封魔の/針/お祓い棒/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/洋風/ロリータ/マニアック/" + "小手/機械/" * 3 + "妖怪/" * 5 + "守矢の/妖怪の/" * 10)

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "小さな赤いリボンがフロントに付いている")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "フロントリボンショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "リボンブラジャー")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;農民関連
;神社は土地持ってそう、神田
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "米農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "お米")
CASE "農民：畑の種類"
	CALLF MAKE_STR(V_NAME, "水田")
CASE "農民：畑のマップ"
	CALLF MAKE_STR(V_NAME, "博麗神社")

CASE "農民：マップID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 5)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "依頼なし発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 103)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼1(ARG, O_DATA, V_NAME)
ENDSELECT

