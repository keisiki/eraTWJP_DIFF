@CHARAMOVE_DATA_42(ARG, ARG:2)
LOCAL = 42
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;天界には行かない
			CASE P830玄雲海 TO P842比那名居邸物置
				RETURN -100
			;私室やお風呂は入らない
			CASE P817神奈子私室, P818諏訪子私室, P820早苗私室
				RETURN -100
			CASE P851久侘歌の小屋, P852駒草の小屋, P853魅須丸の小屋, P824トイレ, P825風呂
				RETURN -100
			;自宅に引きこもりがち
			CASE P809はたて宅
				RETURN 5
			;虹龍洞は無理
			CASE P855虹龍洞, P856百々世の住処
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE 正門, 広間 , 食堂, 大図書館
				RETURN 1
			CASE 麓の樹海, 玄武の沢, 大蝦蟇の池
				RETURN 1
			CASE 九天の滝, 絶景の丘, 山の湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ42
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ42(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "はたて")

;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "兜巾"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "下駄"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "ネクタイ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "今時の少女らしい")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "フリルスキャンティ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "フリルブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "魚料理/獣肉/虫肉/葉物/ガッツリ/濃厚/豪華/機能的/鮮やか/ふわふわ/天狗流/外界風/甘い/甘い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "あっさり/辛い/辛い/卵/卵/卵/地底風/地底風/地底風/鳥肉/鳥肉/鳥肉/鳥肉/鳥肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "濃い味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "紫/かわいい/オシャレ/ポップ/不思議/" + "風/鳥/文具/風の/コイン/" * 3 + "カラスの/天狗の/疾風の/ストラップ/キーホルダー/ネクタイ/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "シンプル/レトロ/シック/" + "刃物/弾/小手/" * 3 + "カメの/河童の/鬼の/" * 10)
;ENDSELECT
;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "依頼なし発生率"
	CALLF MAKE_INT(V_NAME, 0)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 8)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼42(ARG, O_DATA, V_NAME)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_42
DISH_NAME = プリンセスデザート
RESULTS = デザート
CALL 材料設定("甘味材")
DISH_TASTE = 甘い/甘い/甘い/芸術的/濃厚/豪華/鮮やか/ふわふわ/
DISH_COMMENT = 華やかでキュートなデザート。ただし滅茶苦茶甘い
RETURN 1

;IMAGE_I42_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I42_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:衣装一時変更 == 55 || CFLAG:CHARA:基本服装セット == 55
			IF FLAG:立ち絵種類
				;顔絵
				RESULTS = _バニー
			ELSE
				;立ち絵
				RESULTS = _バニー
			ENDIF
		ENDIF
	ENDIF
ENDIF

