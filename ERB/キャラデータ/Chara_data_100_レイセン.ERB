@CHARAMOVE_DATA_100(ARG,ARG:2)
LOCAL = 100
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;夢幻館には行かない
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;他人の家には入らない
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;物置,倉庫,地下室は行かない
			CASE 424, 427, 430
				RETURN -100
			;縁側,居間によく居る
			CASE 409, 416
				RETURN 5
			;静かの海の方へは行かない
			CASE 1016, 1060 TO 1072
				RETURN -100
			;ドレミー宅には行かない
			CASE 1050 TO 1059
				RETURN -100
			;空き家には行かない
			CASE 1007
				RETURN -100
			;門番詰め所は行かない
			CASE 1023
				RETURN -100
			;二階はあまり行かない
			CASE 1040
				RETURN -5
			;玉兎街方面,玉兎食堂,玉兎詰め所によく居る
			CASE 1003, 1004, 1010 TO 1013, 1024, 1025
				RETURN 5
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 10
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 105 TO 108
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
;朱鷺子
@EXIST_キャラデータ100
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ100(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "レイセン")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ワイシャツ"))
CASE "普段着：上半身上着１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着１", "ブレザー"))
CASE "普段着：その他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("その他１", "ネクタイ"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "地味さ以外に非の打ち所がない")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "玉兎支給パンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "玉兎支給ブラ")
CASE "固有下着着用条件"
	SIF !RAND:(1 + (2 * 勝負下着判定([[レイセン]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "野菜/米/中華/葉物/あっさり/ヘルシー/質素/機能的/家庭的/玉兎流/月都風/月見/甘い/甘い/すっぱい/すっぱい/ニンジン/ニンジン/ニンジン/ニンジン/ニンジン/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "しょっぱい/しょっぱい/" + "カエル/獣肉/虫肉/生命力/" * 3 + "グロテスク/謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "玉兎流/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黄/秩序/かわいい/繊細/" + "月/護符/モモの/" * 3 + "ウサギの/海神の/ネクタイ/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/怖い/パワフル/威厳/" + "刃物/小手/" * 3)
ENDSELECT

