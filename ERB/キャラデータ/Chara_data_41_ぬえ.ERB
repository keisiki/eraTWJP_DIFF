@CHARAMOVE_DATA_41(ARG, ARG:2)
LOCAL = 41
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113ナズーリン私室, P114ムラサ私室, P115星私室, P116一輪私室, P118白蓮私室, P109女苑私室, P136響子私室
				RETURN -5
			;離れには入らない,神霊廟内部は行かない
			CASE P119離れ, P125屠自古私室 TO P134蔵, P138鐘楼, P139邪仙楼
				RETURN -100
			;居間、マミゾウの棲家に行きやすい
			CASE P108居間, P137マミゾウの棲家
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE お地蔵通り, 枝垂れ柳の下, 命蓮寺境内, 命蓮寺方丈, 墓地
				RETURN 1
			CASE 麓の樹海, 玄武の沢, 大蝦蟇の池, 森の深奥
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
		;白蓮が本堂にいる時はやってくる
		SIF CFLAG:[[白蓮]]:現在位置 == P103本堂 && !CFLAG:[[白蓮]]:うふふ && SHIRAHU([[白蓮]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ41
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ41(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ぬえ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "モザイクでぼやける")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "正体不明パンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "正体不明ブラ")
CASE "固有下着着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/スパイス/麺類/ガッツリ/ジャンキー/濃厚/鮮やか/グロテスク/ぷるぷる/辛い/辛い/しょっぱい/しょっぱい/" + "謎料理/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "あっさり/ヘルシー/質素/甘い/甘い/野菜/野菜/野菜/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "未知の味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/混沌/怖い/奇妙/不思議/" + "闇/妖術/生体品/酒/" * 3 + "謎の/正体不明の/夜空の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/シンプル/知的/" + "光/書物/照明/ネクタイ/" * 3 + "真実の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_41
DISH_NAME = 未確認飛行料理
RESULTS = 主食
CALL 材料設定("ダークマター")
DISH_TASTE = スパイス/未知の味/未知の味/未知の味/中立温度/謎料理/妖力/
DISH_COMMENT = 正体不明の宙を舞う料理。非常に食べづらい。
RETURN 1


