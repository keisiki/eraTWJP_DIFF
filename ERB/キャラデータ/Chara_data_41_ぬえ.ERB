@CHARAMOVE_DATA_41(ARG,ARG:2)
LOCAL = 41
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室（経堂と物置除く）
			CASE 113, 114, 115, 116, 118
				RETURN -5
			;離れと墓地と神霊廟内部
			CASE 119, 121, 125 TO 133
				RETURN -100
			;経堂,居間に行きやすい
			CASE 105, 108
				RETURN 5
			;白蓮が本堂にいる時はやってくる
			CASE 103
				SIF  CFLAG:55:現在位置 == 103
					RETURN 10
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 11 TO 15, 71,72,73
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ41
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ41(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "ぬえ")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ニーソックス"))
CASE "普段着：ワンピース"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ワンピース", "ワンピース"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "モザイクでぼやける")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "正体不明パンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "正体不明ブラ")
CASE "固有下着着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "肉料理/スパイス/麺類/ガッツリ/ジャンキー/濃厚/鮮やか/グロテスク/ぷるぷる/辛い/辛い/しょっぱい/しょっぱい/" + "謎料理/" * 3)
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "あっさり/ヘルシー/質素/甘い/甘い/野菜/野菜/野菜/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "未知の味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/混沌/怖い/奇妙/不思議/" + "闇/妖術/生体品/酒/" * 3 + "謎の/正体不明の/夜空の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/シンプル/知的/" + "光/書物/照明/ネクタイ/" * 3 + "真実の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_41
DISH_NAME = 未確認飛行料理
RESULTS = 主食
CALL 材料設定("ダークマター")
DISH_TASTE = スパイス/未知の味/未知の味/未知の味/中立温度/謎料理/妖力/
DISH_COMMENT = 正体不明の宙を舞う料理。非常に食べづらい。
RETURN 1


