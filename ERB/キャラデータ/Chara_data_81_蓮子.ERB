@CHARAMOVE_DATA_81(ARG, ARG:2)
LOCAL = 81
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
		RESULT:1 = [[蓮子]]
		RESULT:2 = [[メリー]]
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;遺跡によく行く
			CASE P36夢幻遺跡前 TO P51夢幻遺跡・倉庫
				RETURN 5
			;蓮メリの部屋,広場
			CASE P232蓮子の部屋, P233メリーの部屋, P202中央広場
				RETURN 5
			;私室には行かない
			CASE P221稗田邸, P224慧音宅, P222寺子屋, P226小鈴私室, P229阿求私室
				RETURN -100
			CASE P208雷鼓の部屋, P209八橋の部屋, P210弁々の部屋
				RETURN -100
			CASE P231蛮奇の部屋, P234ユキの部屋, P235マイの部屋, P239美宵の部屋
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 神社本堂, 神社縁側, 夢幻遺跡
				RETURN 1
			CASE お地蔵通り, 枝垂れ柳の下, 命蓮寺境内, 墓地, 神霊廟広場
				RETURN 1
			CASE 広場, 商家町, 呑屋小道, 長屋街
				RETURN 1
			CASE 竹林入り口, 迷いの小道, 斜角の竹林, 兎の洞穴, 無名の丘, 太陽の畑
				RETURN 1
			CASE 中有の道, 三途の川
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
ENDSELECT

@EXIST_キャラデータ81
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ81(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "蓮子")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "普段着：外衣"
	SIF DAY:2 == 1
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("外衣", "ケープ"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：靴下"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "ロングスカート"))
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "下半身が引き締まって見える")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "ドット柄ショーツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "ヌーブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "米/スパイス/中華/麺類/ドリンク/あっさり/ジャンキー/機能的/上品/家庭的/サクサク/月都風/甘い/甘い/しょっぱい/しょっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "すっぱい/すっぱい/虫肉/虫肉/虫肉/グロテスク/グロテスク/グロテスク/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "化学的/得意料理＊ジャンキー＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/高級/かっこいい/シンプル/レトロ/シック/知的/不思議/マニアック/" + "星/月/文具/遺物/エニグマティクな/" * 3 + "量子の/未知の/魅知の/幻想の/手帳/ネクタイ/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/派手/ロリータ/" + "小手/" * 3 + "外界の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/デザート）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_81
DISH_NAME = 栄養効率サプリメント
RESULTS = 軽食
CALL 材料設定("栄養ドリンク", "スポーツドリンク")
DISH_TASTE = あっさり/ヘルシー/機能的/機能的/機能的/薬効/外界風/
DISH_COMMENT = 栄養を凝縮した錠剤。料理なのにまるで食べた気がしない。
DISH_MAXLIMIT = 600
RETURN 1

;IMAGE_I81_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;   RESULTSに特殊服装を格納すること
;   書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I81_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

