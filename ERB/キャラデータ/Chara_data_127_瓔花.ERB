@CHARAMOVE_DATA_127(ARG,ARG:2)
LOCAL = 127
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;八雲邸と彼岸から先は行かない（入り口部分は出る時用）
			CASE 604, 610, 613, 620, 621, 630, 631, 640, 641
				RETURN -10
			CASE 610 TO 652
				RETURN -100
			;潤美の家には行かない
			CASE 608
				RETURN -100
			;積石塚に引きこもる
			CASE 603
				RETURN 10
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 61, 62
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ127
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ127(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "瓔花")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（ズボン）セット")

CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "ブラウス"))
CASE "普段着：スカート"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("スカート", "スカート"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "サンダル"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ふっくらフワフワ")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "クラゲパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "クラゲブラ")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "ミルク/卵/汁物/ドリンク/あっさり/淡白/冷たい/質素/野趣/家庭的/冥界風/おいしい水/甘い/甘い/すっぱい/すっぱい/")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "揚げ物/ガッツリ/濃厚/温かい/しょっぱい/しょっぱい/" + "カエル/肉料理/魚料理/スパイス/地獄風/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "冥界風/幸運/おいしい水/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "灰/鉱物/かわいい/シンプル/ポップ/ロリータ/シック/繊細/" + "水/" * 3 + "石の/クラゲの/かざぐるま/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "派手/パワフル/知的/威厳/" + "火/死生/刃物/弾/キセル/機械/" * 3)
ENDSELECT

