CASE "名前"
	CALLF MAKE_STR(V_NAME, "ブラジャー")
CASE "装備部位"
	CALLF MAKE_STR(V_NAME, "「上半身下着１」")
;旧装備処理との照会用、上半身下着１の1
CASE "つける"
	IF TALENT:ARG:幼稚 || TALENT:ARG:幼児／幼児退行
		CALLF MAKE_INT(V_NAME, 0)
	ELSE
		CALLF MAKE_INT(V_NAME, 1)
	ENDIF
CASE "生地厚さ"
	CALLF MAKE_INT(V_NAME, 2)
ENDSELECT



