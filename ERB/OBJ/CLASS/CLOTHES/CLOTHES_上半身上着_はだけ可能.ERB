#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "名前"
	CALLF MAKE_STR(V_NAME, "ブラウス")
CASE "装備部位"
	CALLF MAKE_STR(V_NAME, "「上半身上着２」")
;旧装備処理との照会用、上半身上着２の1
ENDSELECT


