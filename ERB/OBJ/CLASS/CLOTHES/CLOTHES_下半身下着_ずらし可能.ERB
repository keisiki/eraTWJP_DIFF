@勝負下着判定(ARG)
#FUNCTION
IF !TALENT:ARG:無知
	SIF (CFLAG:ARG:既成事実 & 合意_うふふ || TALENT:ARG:恋慕) && CFLAG:ARG:翌日来訪
		RETURNF 1
	IF DAY:2 == 1
		SELECTCASE DAY:3
			CASE 14
				SIF TALENT:ARG:恋慕
					RETURNF 1
		ENDSELECT
	ELSEIF DAY:2 == 4
		SELECTCASE DAY:3
			CASE 14, 24, 25
				SIF TALENT:ARG:恋慕
					RETURNF 1
		ENDSELECT
	ENDIF
ENDIF
RETURNF 0

